                                                            continue;
                                                        }
                                                        if(typeof(recomment_arr_video_ngay_hien_tai) != "undefined" && recomment_arr_video_ngay_hien_tai.indexOf(parseInt(v_object_recomend[j]["c_new_id"]))  >= 0){
                                                            continue;
                                                        }
