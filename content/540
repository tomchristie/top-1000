		// Ga video load luôn được chạy
		
		function inViewport( element ){  
			// Get the elements position relative to the viewport
			var bb = element.getBoundingClientRect();
			// Check if the element is outside the viewport
			// Then invert the returned value because you want to know the opposite
			return !(bb.top > innerHeight || bb.bottom < 0);
		}
		var zplayer_29457VpElm = document.getElementById( "zplayer_29457" );
		
		var viszplayer_29457 = (function () {
		  var stateKey, eventKey, keys = {
			hidden: "visibilitychange",
			webkitHidden: "webkitvisibilitychange",
			mozHidden: "mozvisibilitychange",
			msHidden: "msvisibilitychange"
		  };
		  for (stateKey in keys) {
			if (stateKey in document) {
			  eventKey = keys[stateKey];
			  break;
			}
		  }
		  return function (c) {
			if (c) document.addEventListener(eventKey, c);
			return !document[stateKey];
		  }
		})();
        
        var videoAds1Elmzplayer_29457 = document.getElementById("v-24hContainer_zplayer_29457");
        document.addEventListener("DOMContentLoaded", function () {
            if(videoAds1Elmzplayer_29457){
                var videoAdsInitzplayer_29457 = false;
                var checkTabActivezplayer_29457 = setInterval(function () {
					if (inViewport(videoAds1Elmzplayer_29457) && viszplayer_29457() && !videoAdsInitzplayer_29457 && typeof(window.videojs) !== "undefined" && (typeof(google) !== "undefined" || adBlockCheckedStatus) && videoObj) {
                        initvideozplayer_29457();
                        clearInterval(checkTabActivezplayer_29457);
                    }
                }, 100);
            }
        }, false);
