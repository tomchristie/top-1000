                                                            continue;
                                                        }
                                                    }
													arr_recomend_video[k] = v_object_recomend[j];
													k++;
                                                }
                                                var v_total_page_recommend =0;
                                                v_total_page_recommend = parseInt(Math.ceil(parseInt(arr_recomend_video.length)/parseInt(v_object_recomend[0]["c_number_items"])));
