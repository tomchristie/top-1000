    var td_table_name = 'pageviews',
      oid = 'b18a644a0e17bc7f';

    a1tracker.send(oid, function (err, jsonp) {
      var aoneuid = jsonp.aoneuid,
        segmentIds = jsonp.segments,
        aonesegments = jsonp.aonesegments;

      var data = {
        aoneid: aoneuid,
        aone_segmentids: segmentIds,
        td_global_id: 'td_global_id'
      };

      if (aonesegments.demographic) data.aone_demographic = aonesegments.demographic;
      if (aonesegments.interest) data.aone_interest = aonesegments.interest;
      if (aonesegments.zipcode) {
        data.aone_zipcode_home = aonesegments.zipcode.home;
        data.aone_zipcode_work = aonesegments.zipcode.work;
      }
      td.set(td_table_name, data);
      td.trackPageview(td_table_name);
    });
  })();
</script>

