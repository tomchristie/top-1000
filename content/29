<script type="text/javascript">
function setCookie(cname, cvalue, exdays) {
    var d = new Date();
    d.setTime(d.getTime() + (exdays*24*60*60*1000));
    var expires = "expires="+ d.toUTCString();
    document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
}

function getCookie(cname) {
    var name = cname + "=";
    var decodedCookie = decodeURIComponent(document.cookie);
    var ca = decodedCookie.split(';');
    for(var i = 0; i <ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0) == ' ') {
            c = c.substring(1);
        }
        if (c.indexOf(name) == 0) {
            return c.substring(name.length, c.length);
        }
    }
    return "";
}

jQuery( document ).ready(function() {
   if(getCookie('IsCookiesMessageClicked') == "true"){
     jQuery('#cookie-auth').hide();
   }else{
     setCookie('IsCookiesMessageClicked', 'false', '1');
	 jQuery('#cookie-auth').show();
   }
   
   jQuery('.ico-close').on('click', function(){
	 setCookie('IsCookiesMessageClicked', 'true', '1');
     jQuery('#cookie-auth').hide();

   });
});
</script>
             <div id="cookie-auth">
         	<!-- png -->
		<div class="cookie-content">
	        	<img src="https://www.swiftic.com/wp-content/themes/florida-wp/images/ico.png" class="ico">
        		<span class="cookie-text">This site uses cookies to provide you with a great user experience. By using Swiftic, you are accepting our use of cookies.</span>
		</div>
                <img src="https://www.swiftic.com/wp-content/themes/florida-wp/images/ico-close.png" class="ico-close">
             </div>

