                                  topCount = 0,
                                  midCount = 0,
                                  botCount = 0,
                                  topRSCount = 0,
                                  rightRSCount = 0,
                                  uniqueResultsDisplayed = 0,
                                  csaTopModule = "" || "csaTop",
                                  csaMiddleModule = "" || "csaMiddle",
