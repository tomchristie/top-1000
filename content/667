                                    $(".PartialSearchBoxRounded").removeClass("lss");
                                    $(".PartialSearchBox-rounded").removeClass("lss");
                                }
                            });
                            observer.observe(target, {attributes: true});
                        
}
                }
            });
        })();
    }
