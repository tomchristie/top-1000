$(".zs21-list-3").each(function(){
	var rowNum = Math.round($(this).find(".li-1").height()/parseFloat($(this).find(".li-1").css('line-height')));
	if(rowNum==1){
		$(this).find(".li-6").show()
	}
})
</script>
    </div>
    </div>

