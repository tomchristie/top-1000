		if ($(window).scrollTop() > 200) {
			$(".return-to-top").fadeIn(300);
		} else {
			$(".return-to-top").fadeOut(300);
		}
	});

$(".return-top").click(function() {
	$('body,html').animate({
		scrollTop: 0
	}, 500);
}); 
</script>

