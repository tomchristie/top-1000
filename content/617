(function(win, doc) {
  
function preloadCriticalPath() {
  var head = doc.getElementsByTagName('head')[0],
      assets = ["https://http2.mlstatic.com/frontend-assets/ui-navigation/5.15.2/mercadolibre/navigation-desktop.css","https://http2.mlstatic.com/frontend-assets/ui-navigation/5.15.2/mercadolibre/navigation-mobile.css"],
      link,
      i;
  for(i = 0; i < assets.length; i++) {
    link = doc.createElement('link');
    link.rel = 'prefetch';
    link.href = assets[i];
    head.appendChild(link);
  }
}
  if (doc.readyState === 'complete') {
    setTimeout(preloadCriticalPath, 0);
  } else {
    win.addEventListener('load', function() {
      preloadCriticalPath();
    });
  }
})(window, document);
</script>
