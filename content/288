<script>
  (function() {
    function e() {
      if (document.querySelector(".home-kv-carousel .first-image")) {
        var b = document.querySelector(".home-kv-carousel .first-image .first-image__desktop"),
          c = document.querySelector(".home-kv-carousel .first-image .first-image__mobile");
        var a = window;
        var d = "inner";
        "innerWidth" in window || (d = "client", a = document.documentElement || document.body);
        a = a[d + "Width"];
        a = 768 > a ? "360" : 768 <= a && 1366 > a ? "768" : "1366";
        d = 1.5 > window.devicePixelRatio ? 1 : 2;
        f !== a && ("768" === a || "1366" === a ? (c = b.dataset[a + "w" + d + "xSrc"],
          c !== b.src && (b.src = c)) : "360" === a && (b = c.dataset[a + "w" + d + "xSrc"], b !== c.src && (c.src = b)), f = a)
      }
    }
    var f;
    e();
    window.addEventListener("resize", e)
  })();
</script>



    
        
  

    
    
        
    

    
    
        
    

    
    
        
    
        
    

    
    
        
    
        
    


