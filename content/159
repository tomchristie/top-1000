	<script type="text/javascript" src="https://utrack.hexun.com/dp/dplus_config_ver1.0.1.js"></script>
	<script type="text/javascript" src="https://utrack.hexun.com/dp/hexun_dplus_ver1.0.1.js"></script>
	<script type="text/javascript" src="https://web.hexun.com/pc/modules/hxpctraffic.0.0.1.min.js"></script>
	<script>
	var hxpctraffic = function (appId, args) {
		typeof jQuery.track !== "undefined" && jQuery.track(appId, true, args.name, args, function (res) {}, function (err) {});
	};
	var czgFunction = function () {
		this.islogin = false;
		this.isPartner = false;
		return this;
	};
	czgFunction.prototype = {
		init: function () {
			var t = this;
			t.getIsLogin();
			t.bindEvent();
			hxpctraffic("chuangzuoguanPC", {
				"TYPE": "新闻列表页",
				"PAGENAME": "和讯网首页",
				"EVENTNAME": "浏览事件"
			});
		},
		getIsLogin: function () {
                 var t = this;
            var regHost = 'https://regtool.hexun.com';
            var isHttps = 'https:' === document.location.protocol;
            if (!isHttps) {
                regHost = 'http://reg.tool.hexun.com';
            }
            $.ajax({
                url: regHost + "/wapreg/checklogin.aspx?format=json&encode=utf-8",
                type: "GET",
                dataType: "jsonp",
                success: function (data) {
                    t.isLogin = data.islogin == "True" ? true : false;
                    if (t.isLogin) {
                        t.getIsPartner(data.userid);
                    }
                }
            });
        },

		getIsPartner: function (id) {
			var t = this;
			$.ajax({
				url: "https://partnerwriter.hexun.com/api/partner/get_partnershow_info?partnerId="+id,
				type: "GET",
				dataType: "jsonp",
				success: function (data) {
					t.isPartner = data.resultKey == "ok" ? true : false;
					if (t.isPartner) {
						$("#toczgnews a").attr("href", "https://writer.hexun.com/#/wzedit");

$("#toczgvideo a").attr("href", "https://web.hexun.com/pc/admin/videoOfflineAdmin.html");
$("#toczgvideo a").attr("target", "_blank");

						$("#toczgadmin a").attr("href", "https://writer.hexun.com/");
						$("#toczgadmin a").attr("target", "_blank");
					} else {
						$("#toczgnews a").attr("href", "https://jinshuju.net/f/cudPXe");
						$("#toczgnews a").attr("target", "_blank");
						$("#toczgvideo a").attr("href", "https://jinshuju.net/f/cudPXe");
						$("#toczgvideo a").attr("target", "_blank");
						$("#toczgadmin a").attr("href", "https://jinshuju.net/f/cudPXe");
						$("#toczgadmin a").attr("target", "_blank");
					}
				}
			});
		},
		bindEvent: function () {
			var t = this;
			$("#toczgnews a").click(function () {
				hxpctraffic("chuangzuoguanPC", {
					"TYPE": "新闻列表页",
					"PAGENAME": "和讯网首页",
					"EVENTNAME": "点击事件",
					"FUNCTION": "点击写文章按钮"
				});
				if (!t.isLogin) {
					popupLogin();
					return false;
				}
			});
			$("#toczgvideo a").click(function () {
				hxpctraffic("chuangzuoguanPC", {
					"TYPE": "新闻列表页",
					"PAGENAME": "和讯网首页",
					"EVENTNAME": "点击事件",
					"FUNCTION": "点击发视频按钮"
				});
				if (!t.isLogin) {
					popupLogin();
					return false;
				} 
			});
			$("#toczgadmin a").click(function () {
				hxpctraffic("chuangzuoguanPC", {
					"TYPE": "新闻列表页",
					"PAGENAME": "和讯网首页",
					"EVENTNAME": "点击事件",
					"FUNCTION": "点击创作官中心按钮"
				});
				if (!t.isLogin) {
					popupLogin();
					return false;
				}
			});
			$("#toczgindex").click(function () {
				hxpctraffic("chuangzuoguanPC", {
					"TYPE": "新闻列表页",
					"PAGENAME": "和讯网首页",
					"EVENTNAME": "点击事件",
					"FUNCTION": "点击创作官频道入口按钮"
				});
			});
			$("#toczgfollow").click(function () {
				hxpctraffic("chuangzuoguanPC", {
					"TYPE": "新闻列表页",
					"PAGENAME": "和讯网首页",
					"EVENTNAME": "点击事件",
					"FUNCTION": "点击和首我的关注按钮"
				});
			});
			$(".czg-popup .btn, .czg-popup .close-btn").click(function(){
				$(".czg-popup").hide();
			});
		}
	};
	$(function(){
		var czgFun = new czgFunction();
		czgFun.init();
	});
	</script>
	<!-- 创作官 end -->

