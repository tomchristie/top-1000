(function(){
$(".globalnavCQbanner:not(.loaded)").each(function(index){
if( window.location.pathname !== $(this).attr('data-pathname') ){
var bannerImgOnLoadSrc = $(this).attr('data-dtSrc');  
var hpPlatform = 'desktop';
if ($(window).width() <= 480) {
bannerImgOnLoadSrc = $(this).attr('data-mobSrc');  
