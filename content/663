var resizeTimer;
$(window).on('resize', function(e) {
clearTimeout(resizeTimer);
resizeTimer = setTimeout(function() {
// Run code here, resizing has "stopped"
checkBannerSize();
}, 250);
});
}else{
$( ".globalnavCQbanner" ).hide();
}
}); 
function checkBannerSize(){
$(".globalnavCQbanner.loaded").each(function(index){

if( window.location.pathname !== $(this).attr('data-pathname') ){  
if ($(window).width() <= 480 && $(".globalnavCQbanner.desktop").length>0 ) {
$("img.globalCQbannerimg").attr('src', $(this).attr('data-mobSrc') );
$(this).removeClass('desktop').addClass('mobile');
// console.log('now mobile');
}else if($(window).width() > 480 && $(".globalnavCQbanner.mobile").length>0 ) {
$("img.globalCQbannerimg").attr('src', $(this).attr('data-dtSrc') );
$(this).removeClass('mobile').addClass('desktop');
//console.log('now desktop');
}
}
});
}
}()); 
