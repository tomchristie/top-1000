<script>
	var url = 'https://advert.wanfangdata.com.cn/' + 'advertmanager/advert/advertJsonp?positionId=3'
	$.ajax({
		type: "get",
		url: url,
		jsonp: "jsonpcallback",
		dataType: "jsonp",
		success: function (data) {
			if (data.code === 200) {
				$('.adImgUrl').attr('href', data.url)
				$('.adPopupBg').attr('src', data.picture)
				var time = Math.ceil(new Date().getTime() / 1000) // 当前时间
				var expires = new Date(new Date(new Date().getTime() + 1 * 24 * 60 * 60 * 1000).setHours(0, 0, 0, 0)) // cookie 过期时间
				var adCookie = data.num // 弹出次数
				var adNumCookie = $.cookie('AdPopupNum') // 缓存弹出次数
				var adIdCookie = $.cookie('AdIdCookie') // 缓存广告id
				if (adNumCookie == null || adNumCookie == '' || adNumCookie == undefined) {
					$('#adPopup').show()
					$.cookie('AdPopupNum', 1, { expires: expires })
					$.cookie('AdIdCookie', data.advertId, { expires: expires })
				} else if (adIdCookie != data.advertId) {
					$('#adPopup').show()
					$.cookie('AdPopupNum', 1, { expires: expires })
					$.cookie('AdIdCookie', data.advertId, { expires: expires })
				} else if (adNumCookie != adCookie && adNumCookie < adCookie) {
					$('#adPopup').show()
					$.cookie('AdPopupNum', ++adNumCookie, { expires: expires })
				}
			}
		}
	});
	function handleClose() {
		$('#adPopup').hide()
	}
</script>
