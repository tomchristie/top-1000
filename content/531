<div class="mt_idx_body4 mgt34">
    <!-- 포토.영상 -->
    <h3 class="catit">
        <span class="lmt"><a href="https://news.mt.co.kr/photonews/index.html">포토</a>&nbsp;&middot;&nbsp;<a href="https://news.mt.co.kr/photonews/photovod_star.html?type=vod&cate=1000">영상</a></span>
    </h3>

    <script type="text/javascript">
        $(window).load(function () {
            var idx = document.getElementById('fx').value;

			textAutoHeight(idx);
        });
		function textAutoHeight(idx) {
			if ($('#photoVod_' + idx).is(':visible')) {
                var element = $('#photoVod_' + idx + ' .bundle.ty2 .thum');
				var element2 = $('#photoVod_' + idx + ' .bundle.ty2 .subject');
                var elmH = element.height();
				var elm2H = element2.height();
				var elm3H = elmH + elm2H + 11; //제목 상단의 여백(11px) 포함
				var bdH = $('.bundle.ty2').height() - elm3H - 6 - 7;
				var txtH = $('#photoVod_' + idx + ' .bundle.ty2 .txt');
				// console.log('elmH = '+elmH+' / elm2H = '+elm2H+' elm3H = '+elm3H+' bdH = '+bdH);

				if (elm3H >= 252) {
                    $(txtH).css('display', 'none');
                } else if(252 > elm3H && elm3H > 231) {
                    $(txtH).css('display', 'block').css('height', 22 + 'px');
                } else if(232 > elm3H && elm3H > 210) {
                    $(txtH).css('display', 'block').css('height', 40 + 'px');
                } else {
                    $(txtH).css('display', 'block').css('height', bdH + 'px');
                }
            }
		}
        function photoVodtab(direction) {
            var idx = document.getElementById('fx').value;
            if (direction == "prev") {
                idx = parseInt(idx) - 1;
                if (idx < 0) {
                    idx = '1';
                }
            } else {
                idx = parseInt(idx) + 1;
                if (idx == '2') {
                    idx = 0;
                }
            }
            document.getElementById('fx').value = idx;

            for (i = 0; i < 2; i++) {
                document.getElementById('photoVod_' + i).style.display = "none";
            }
            document.getElementById('photoVod_' + idx).style.display = "block";

			textAutoHeight(idx);
        }
    
    </script>
    <div class="mt_idx_body4_limit">
        <div class="photovod">
            <ul class="group" id="photoVod_0">
                <li class="bundle ty1">
                    <ul class="list">
