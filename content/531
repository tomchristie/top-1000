                    $('#mt_header').css('padding-right', '220px').css('padding-left', '220px');
                    $('#container').css('padding-right', '220px').css('padding-left', '220px');
                    $('#mt_footer').css('padding-right', '220px').css('padding-left', '220px');
                 //   $('.wrap_banner').css('padding-right', '220px').css('padding-left', '220px');
                    $('#aside2017_open').css('display', 'none');
					$('.rightbn_wrap').css('display', 'none');
                } else if (boolAsd && !boolLfb) {
                    $('#wrap').css('min-width', '1464px');
                    $('#mt_header').css('padding-right', '220px').css('padding-left', '0');
                    $('#container').css('padding-right', '220px').css('padding-left', '0');
                    $('#mt_footer').css('padding-right', '220px').css('padding-left', '0');
                //    $('.wrap_banner').css('padding-right', '220px').css('padding-left', '0');
                    $('#aside2017_open').css('display', 'none');
					$('.rightbn_wrap').css('display', 'none');
                } else if (!boolAsd && boolLfb) {
                    $('#wrap').css('min-width', '1404px');
                    $('#mt_header').css('padding-left', '190px').css('padding-right', '190px');
                    $('#container').css('padding-left', '190px').css('padding-right', '190px');
                    $('#mt_footer').css('padding-left', '190px').css('padding-right', '190px');
                //    $('.wrap_banner').css('padding-left', '190px').css('padding-right', '190px');
                    $('#aside2017_open').css('display', 'block');
                } else if (!boolAsd && !boolLfb) {
                    $('#wrap').css('min-width', '1114px');
                    $('#mt_header').css('padding-left', '0').css('padding-right', '0');
                    $('#container').css('padding-left', '0').css('padding-right', '0');
                    $('#mt_footer').css('padding-left', '0').css('padding-right', '0');
                //    $('.wrap_banner').css('padding-left', '0').css('padding-right', '0');
                    $('#aside2017_open').css('display', 'block');
                }
