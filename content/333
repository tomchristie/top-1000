            function isLtIe8() {
                var DEFAULT_VERSION = 8;
                var ua = navigator.userAgent.toLowerCase();
                var v;
                if (ua.indexOf("msie") > -1) {
                    v = ua.match(/msie ([\d.]+)/)[1];
                    if (v <= DEFAULT_VERSION) {
                        return true;
                    }
                } else {
                    return false;
                }
            }
