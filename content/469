<script type="text/javascript">
	$(function () {
		var maskLayer2 = $('#maskLayer2'),
			floatingLayer2 = $('#login_box2'),
			pageH = $(document.body).height(),
			pageW = $(document.body).width(),
			winH = $(window).height(),
			maxH = Math.max(winH, pageH),
			floatingLayer2_x = (pageW - floatingLayer2.width()) / 2,
			floatingLayer2_y = (winH - floatingLayer2.height()) / 2;

		maskLayer2.hide();
		floatingLayer2.hide();

		$('.loginclose').click(function () {
			floatingLayer2.hide();
			maskLayer2.hide();
		});
	});

	function checkLogin() {
		if (!ret || !ret.loginFlag) {
			showLogin();
			return false;
		}
		return true;
	}

	function showLogin() {
		var maskLayer2 = $('#maskLayer2'),
			floatingLayer2 = $('#login_box2'),
			pageH = $(document.body).height(),
			pageW = $(document.body).width(),
			winH = $(window).height(),
			maxH = Math.max(winH, pageH),
			floatingLayer2_x = (pageW - floatingLayer2.width()) / 2,
			floatingLayer2_y = (winH - floatingLayer2.height()) / 2;

		maskLayer2.show().width(pageW).height(maxH);
		floatingLayer2.show().css({ 'top': floatingLayer2_y + 'px', 'left': floatingLayer2_x + 'px' });
	}
</script>

