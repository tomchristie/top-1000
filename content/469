<script type="text/javascript">
	$(function() {
		var maskLayer = $('#maskLayer'),
			floatingLayer2 = $('#login_box'),
			pageH = $(document.body).height(),
			pageW = $(document.body).width(),
			winH = $(window).height(),
			maxH = Math.max(winH, pageH),
			floatingLayer2_x  = (pageW - floatingLayer2.width())/2,
			floatingLayer2_y  = (winH - floatingLayer2.height()) / 2;
		
		maskLayer.hide();
		floatingLayer2.hide();

		$('.loginclose').click(function(){
			floatingLayer2.hide();
			maskLayer.hide();
		});

		window.onresize = function () {
			dynAniAdapuive();
		};

		window.onscroll = function () {
			dynamicAnimation("dynamicAniRight", "left", "375");
			dynamicAnimation("dynamicAniLeft", "right", "425");
		};

	});

	function checkLogin()
	{
		if(!ret || !ret.loginFlag) {
			showLogin();
			return false;
		}
		return true;
	}

	function showLogin()
	{
		var maskLayer = $('#maskLayer'),
			floatingLayer2 = $('#login_box'),
			pageH = $(document.body).height(),
			pageW = $(document.body).width(),
			winH = $(window).height(),
			maxH = Math.max(winH, pageH),
			floatingLayer2_x  = (pageW - floatingLayer2.width())/2,
			floatingLayer2_y  = (winH - floatingLayer2.height()) / 2;
		
		maskLayer.show().width(pageW).height(maxH);
		floatingLayer2.show().css({'top': floatingLayer2_y + 'px','left': floatingLayer2_x + 'px'});
	}
	
	function changeLoginVerifyCode() {
		var obj = document.getElementById('imgLoginVerifyCode');
		if (obj) {
			obj.style.visibility = 'visible';
			obj.src = 'http://passport.9wee.com/common/verify_code?' + new Date().getTime();
		}
	}
	changeLoginVerifyCode();
</script>
