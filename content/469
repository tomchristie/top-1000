<script type="text/javascript">
	function changeLoginVerifyCode() {
		var obj = document.getElementById('imgLoginVerifyCode');
		var obj_tip = document.getElementById('imgLoginVerifyCode_tip')
		if (obj) {
			obj.style.visibility = 'visible';
			obj.src = 'http://passport.9wee.com/common/verify_code?' + new Date().getTime();
		}

		if (obj_tip) {
			obj_tip.style.visibility = 'visible';
			obj_tip.src = obj.src;
		}
	}
	changeLoginVerifyCode();
</script>
