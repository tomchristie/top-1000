function dopromoext()
{
    return;
if(!promoext) return;
var x=document.getElementById("promoext");
if(x)
{
if(didpromo++) return;
x.innerHTML="<div style=\"background:#eee;padding:10px;margin:10px auto;border-radius:15px;width:300px\"><center><a href=\"https://www.robtex.com/toolbar/\">Try our chrome extension!</a></center></div>";
}
}

function trypromo()
{
  if(!acceptedcookies) return;
if(window['chrome'])
if(window['chrome']['runtime'])
chrome.runtime.sendMessage(
'neagmbhdnabclgpgjkpepoahjkafipbn'
, { message: "version" },
    function (reply) {
        if (reply) {
        }
        else {
          promoext = 1;
            dopromoext();
        }
    });
}

function getcookie(n)
{
    var name=n+"=";
    var found=undefined;
    var ca = document.cookie.split(';');
    for(var i = 0; i <ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0)==' ') {
            c = c.substring(1);
        }
        if (c.indexOf(name) == 0) {
            found=c.substring(name.length,c.length);
        }
    }
    return found;
}

function setcookie(o)
{
var exdays=o.d;
var d = new Date();
d.setTime(d.getTime() + (exdays*24*60*60*1000));
var expires = "expires="+ d.toUTCString();
document.cookie =  o.n+"="+o.v + ";" + expires + ";path=/";

}

function acceptcookies(a)
{
acceptedcookies=a;
setcookie({'n':'acceptcookies','v':a,'d':30});
checkcookies();
return false;
}

function checkcookies()
{
document.getElementById('cookienotice').style.display=(acceptedcookies==1?'none':'block');
}

function dortpv()
{
  rtpv=getcookie('rtpv')||0;
  rtpv++;
//  console.log('rtpv',rtpv);
  setcookie({'n':'rtpv','v':rtpv,'d':365});
}


acceptedcookies=!!getcookie('acceptcookies');
dortpv();

</script>

