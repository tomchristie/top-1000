.covid-banner a, .covid-banner a:hover { color:white; text-decoration: none; }

.covid-banner span:nth-child(1) {
    border-right: 1px solid white;
    padding-right: 20px;
}

.covid-banner span:nth-child(2) {
    padding-left: 20px;
}
.covid-spacer, .covid-chev { display:none; }

@media (max-width: 768px) {
  .covid-banner { margin:15px auto; font-size:14px; line-height:16px; text-align:left; padding-left:36px; }
  .covid-banner span:nth-child(2) {
    padding-left: 0;
  }
  .covid-banner span:nth-child(1) {
    padding-right:0;
    border:none;
  }
  .covid-banner span { display:block; }
  
  .covid-spacer { border-bottom:1px solid white; width: 95%;
    display: block;
    margin: 10px 0; }
    
  .covid-banner span.covid-chev { display:inline; }
}
</style>
