    type: "relatedsearch",
    styleId : appvars.google_rs_style_id,
    linkTarget: '_blank',
    channel:appvars.google_channel,
  };
  
  if(kw_parm) pageOptions.kw = kw_parm;
  if(terms_parm) pageOptions.terms = terms_parm;
  if(style_parm) pageOptions.styleId = style_parm;


  var rs_ads_to_show = 5;
  if(typeof(appvars.rs_ads_to_show) != 'undefined') rs_ads_to_show = appvars.rs_ads_to_show;

  var rsblock1 = {
    'container' : 'google-rs-1',
    'type': 'relatedsearch',
    'number': rs_ads_to_show,
    'linkTarget':'_blank',


  };
  
  var rsblock2 = {
    'container' : 'google-rs-2',
    'type': 'relatedsearch',
