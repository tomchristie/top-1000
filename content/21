<script>

var settings = JSON.parse("{\"gapi\":7,\"default_o_code\":0,\"seo_o_code\":779176,\"default_google_client\":\"aj-cat\",\"default_google_afs_style_id\":6970689106,\"default_google_rs_style_id\":6970689106,\"default_google_channel\":0,\"origin_code\":{\"0\":{\"google_client\":\"aj-cat\",\"google_channel\":0,\"google_afs_style_id\":6970689106,\"google_rs_style_id\":6970689106,\"partner\":\"\"},\"779176\":{\"google_client\":\"aj-organic-seo\",\"google_channel\":779176,\"google_afs_style_id\":6970689106,\"google_rs_style_id\":6970689106,\"partner\":\"\"},\"740004\":{\"google_client\":\"aj-platinum-index\",\"google_channel\":740004,\"google_afs_style_id\":6970689106,\"google_rs_style_id\":6970689106,\"partner\":\"dexter\"}}}");

/* set iac specifics for datalayer */
try {
  var url = new URL(window.location.href);
  var utm_content = url.searchParams.get("utm_content");
  var embedded_qo = null;

  if(utm_content) {
    // look for utm_content override vars
    var tmp = utm_content.split(':');
    if(tmp[0] == 'params') {
      var var_list = tmp[1].split('&');
      var allowed_list = ['ad', 'an', 'ag', 'o', 'rch', 'qo'];
      var_list.forEach(function(a) { 
        tmp = a.split('=');
        allowed_list.forEach(function(b) {
          if(tmp[0] == b) {
            if(b == 'qo') {
              embedded_qo = tmp[1];
            } else {
              Cookies.set('iac_'+tmp[0], tmp[1]);
            }
          }
        })
      });
    }
  }
  
  var o_parm = url.searchParams.get("o");
  
  // 2021-06-21 - allow o codes not in the settings
  // if(typeof(settings.origin_code[o_parm]) == c'undefined') o_parm = settings.default_o_code;

  if(url.searchParams.get("ad")) Cookies.set('iac_ad', url.searchParams.get("ad"));
  if(url.searchParams.get("an")) Cookies.set('iac_an', url.searchParams.get("an"));
  if(o_parm) Cookies.set('iac_o', o_parm);
  if(url.searchParams.get("ag")) Cookies.set('iac_ag', url.searchParams.get("ag"));
  if(url.searchParams.get("rch")) Cookies.set('iac_rch', url.searchParams.get("rch"));

  var kw_parm = url.searchParams.get("kw");
  var terms_parm = url.searchParams.get("terms");
  var style_parm = url.searchParams.get("style"); 

  var appvars = {
    ad: url.searchParams.get("ad") || Cookies.get('iac_ad') || 'dirN',
    an: url.searchParams.get("an") || Cookies.get('iac_an'),
    ag: url.searchParams.get("ag") || Cookies.get('iac_ag'),
    o: o_parm || Cookies.get('iac_o') || settings.default_o_code,
    rch: url.searchParams.get("rch") || Cookies.get('iac_rch'),

    utm_medium: Cookies.get('utm_medium'),
    utm_campaign: Cookies.get('utm_campaign'),
    utm_subid: Cookies.get('utm_subid'),
    partner: '',
    qo: url.searchParams.get("qo") || embedded_qo || 'homepageSearchBox',
    gclid: url.searchParams.get("gclid")
  }
  
  var SEO_REFERERS = [
    "google.*",
    "search.yahoo.*",
    "bing.*",
    "baidu.*",
    "yandex.*",
    "search.aol.com"
  ];
  
  SEO_REFERERS.forEach(function(a) {
    var r = document.referrer.search(a);
    if(r > 0) {
      appvars.ad = 'SEO';
      appvars.o = settings.seo_o_code;
    }
  });

  if(typeof(settings.origin_code[appvars.o]) != 'undefined') {
    appvars.google_client = settings.origin_code[appvars.o].google_client;
    appvars.google_rs_style_id = settings.origin_code[appvars.o].google_rs_style_id;
    appvars.google_afs_style_id = settings.origin_code[appvars.o].google_afs_style_id;
    appvars.google_channel = settings.origin_code[appvars.o].google_channel;
    appvars.partner = settings.origin_code[appvars.o].partner;
    appvars.afs_ads_to_show = settings.origin_code[appvars.o].afs_ads_to_show;
    appvars.rs_ads_to_show = settings.origin_code[appvars.o].rs_ads_to_show;
  } else {
    appvars.google_client = settings.default_google_client;
    appvars.google_rs_style_id = settings.default_google_rs_style_id;
    appvars.google_afs_style_id = settings.default_google_afs_style_id;
    appvars.google_channel = settings.default_google_channel;
    appvars.afs_ads_to_show = settings.default_afs_ads_to_show;
    appvars.rs_ads_to_show = settings.default_rs_ads_to_show;
    appvars.partner = '';
  }
  
  if(appvars.rch) {
    appvars.google_channel = appvars.rch;
  }

  var rs_clk_payload_enc = clk_payload_enc = '';
  

  /* og data layer style from amg */
  dataLayer = [{
    'affiliateid': appvars.o,
    'acquisitiondivision': appvars.ad,
    'channel': 'content'
  }];



// RTK params
  window.rtkGPTSlotsTargeting = [[
    ['ad', appvars.ad],
    ['an', appvars.an],
    ['ag', appvars.ag],
    ['origin', appvars.o],
    ['channel', 'content'],
    ['pageType', ''],
    ['partner', appvars.partner]
  ]];
  
  var jita_tg_params = {};
  jita_tg_params['ad'] = appvars.ad;
  jita_tg_params['an'] = appvars.an;
  jita_tg_params['ag'] = appvars.ag;
  jita_tg_params['channel'] = 'content';
  jita_tg_params['pageType'] = '';
  jita_tg_params['partner'] = appvars.partner;
  jita_tg_params['origin'] = appvars.o;
  
  window.addEventListener("JITA_Ready", function(e) {
   JITA.setCustomParameters(window.jita_tg_params);
  });
  
  
  // global search pass params for serach page
  var ad = '', an = '', o = '', ag = '', qo = '', rch = '';
  if(appvars.ad) ad = '&ad='+appvars.ad;
  if(appvars.an) an = '&an='+appvars.an;
  if(appvars.o) o = '&o='+appvars.o;
  if(appvars.ag) ag = '&ag='+appvars.ag;
  if(appvars.qo) qo = '&qo='+appvars.qo;
  if(appvars.rch) rch = '&rch='+appvars.rch;

  // google rs initialization
  var rs_qo = '&qo=' + (url.searchParams.get("qo") || 'contentGoogleRelatedSearch');
  var pageOptions = {
    pubId : appvars.google_client,
    relatedSearchTargeting : 'content',
    resultsPageBaseUrl : 'https://www.ask.com/web?'+ad+an+o+ag+rs_qo+rch,
    resultsPageQueryParam : 'q',
    clicktrackUrl: "https://www.ask.com/_rclk" + rs_clk_payload_enc,
