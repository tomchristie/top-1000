            script.parentNode.insertBefore(tag, script);
        }(window, document);

        obApi('track', 'PAGE_VIEW');
    }

    window.__tcfapi('getTCData', 2, (tcData, success) => {
        // API already loaded & interaction is complete, load outbrain.js
        if (success && (tcData.eventStatus === 'tcloaded' || tcData.eventStatus === 'useractioncomplete')) {
            loadOutbrainScript();
            loadOutbrainTracking();
        } else {
            // wait for the API to be finished, load outbrain.js
            window.__tcfapi('addEventListener', 2, (tcEventData) => {
                if (tcEventData.eventStatus === 'tcloaded' || tcEventData.eventStatus === 'useractioncomplete') {
                    loadOutbrainScript();
                    loadOutbrainTracking();
                }
            });
        }
    });
