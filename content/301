  (function () {
    if (document.querySelector == undefined) {
      return;
    }
    var agent = navigator.userAgent, isTablet, isSMP, isAndroid, isAndroidMobile, isAndroidTablet, isIPad, isIphone, isWindowsPhone;
    isAndroid = agent.match(/Android/i) != null;
    isAndroidMobile = agent.match(/Android.*Mobile/i) != null;
    isAndroidTablet = !isAndroidMobile && isAndroid;
    isIPad = agent.match(/iPad/i) != null;
    isIphone = agent.match(/iPhone/i) != null;
    isWindowsPhone = agent.match(/Windows Phone/i) != null;
    isTablet = isIPad || isAndroidTablet;
    isSMP = isIphone || isAndroidMobile || isWindowsPhone;
    var m, h;
    h = document.querySelector('head');
    m = document.createElement('meta');
    m.name = 'viewport';
    if (isTablet || isSMP) {
      m.content = 'width=1054, user-scalable=no';
      document.documentElement.classList.add("tablet")
    }
    h.appendChild(m);
  })();
</script>

