<script type="text/javascript">
	// <[!CDATA[
	(function ($window) {
		$window.Nmacs = $window.Nmacs || {};

		$window.Nmacs.Authentification = (function () {
			/**
			 *
			 * @private
			 */
			var _getLoginForm = function () {
				var $metanav = $('#metanav');
				var $templates = $('div.templates[data-src="login"]');

				Nmacs.Session.verify(
					function (user) {
						var html = $('section[data-type="loggedIn"]', $templates).html();
						$metanav.append(
							html.replace('##loginname##', user.loginName)
						);

						$('.switch .username', $metanav).on('click', function(){
							$(this).toggleClass('active');
						});
						$('.menulogin .pictureblock').avatar();
					},
					function () {
						$metanav.append($('section[data-type="loggedOut"]', $templates).html());
					},
					false
				);
			};

			_getLoginForm();

			// Export öffentlicher Methoden.
			return {
				logout: function () {
					Nmacs.Session.logoutMethod();
					$('#login_form').submit();
				}
			}
		})();
	})(window);
	// ]]>
</script>
