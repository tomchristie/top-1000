				<script>
	document.addEventListener("DOMContentLoaded", function() {
		if(window.location.pathname.match(/\/suche\//)){
			var getUrlParameter = function getUrlParameter(sParam) {
				var sPageURL = window.location.search.substring(1),
					sURLVariables = sPageURL.split('&'),
					sParameterName,
					i;

				for (i = 0; i < sURLVariables.length; i++) {
					sParameterName = sURLVariables[i].split('=');

					if (sParameterName[0] === sParam) {
						return sParameterName[1] === undefined ? true : decodeURIComponent(sParameterName[1]);
					}
				}
			};
			$('#suche_text').val(getUrlParameter('q'));
			var base64 = "";
			$('#gsresults').append('<script class="cmp-template" data-cmp-type="googleCSE" data-cmp-purpose="search" type="text/html">'+base64+'</'+'script>');
			window._cmp_ && window._cmp_.render();
		}
	});
</script>

<form id="searchform" class="searchform">
    <input id="suche_text" type="text" placeholder="Suchbegriff eingeben" />
    <input id="cbsearch" type="submit" name="q" value="Suchen" onclick="$('#suche_text').val() && window.open('https://www.computerbild.de/suche/?q=' + encodeURI($('#suche_text').val()),'_self')"/>
    <div class="close"></div>
	<span class="delete" onclick="$('#searchform input#suche_text').val('');$('#searchform span.delete').hide();">&#215;</span>
</form>
<script>
    document.addEventListener("DOMContentLoaded", function() {
		$(document).ready(function() {
			$('#searchform input#suche_text').keyup(function(){
				if ($(this).val()) {
					$('#searchform span.delete').show();
				} else {
					$('#searchform span.delete').hide();
				}
			});
		});
		$(document).on('submit','#cbheader.active #searchform',function(e) {
			if (!$(this).hasClass("active")) {
				$(this).addClass("active");
				e.preventDefault();
			}
		});
		$(document).on('click', '#cbheader.active #searchform .close', function() {
			$("#searchform").removeClass("active");
		});
    });
</script>

