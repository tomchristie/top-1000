<script type="text/javascript">
let styleObject = document.getElementById('skyscrapersecondary'),
    scrollHeight = 2500,
    minHeight = 2500

const handleResize = () => {
    let scrollHeight = document.documentElement.scrollHeight
    styleObject.style.top = scrollHeight * 0.5 <= minHeight ? minHeight + "px" : scrollHeight * 0.5 + "px"
}

window.onload = () => {
    document.addEventListener('scroll', () => handleResize())
    window.addEventListener('resize', () => handleResize())
    handleResize()
}
