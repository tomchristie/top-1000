<script type="text/javascript">
	// <![CDATA[
	var adListenerRegistered = [];
	var noAdvertIdentifier = ["superbanner", "sky", "inpage"];
	var observeDOM = (function(obj, callback) {
		var MutationObserver = window.MutationObserver || window.WebKitMutationObserver,
			eventListenerSupported = window.addEventListener;

		if(!MutationObserver && ! eventListenerSupported){
			callback(function(){});
		}

		if(adListenerRegistered[obj.id]){
			return;
		}
		adListenerRegistered[obj.id] = true;

		if (MutationObserver) {
			var observer = new MutationObserver(function (mutations) {
				mutations.forEach(function (mutation) {
					if ("attributes" === mutation.type.toString()) {
						callback(function () {
							observer.disconnect();
						});
					}
				});
			});
			observer.observe(obj, {attributes: true});
		} else if (eventListenerSupported) {
			obj.addEventListener('DOMAttrModified', function () {
				callback(function() {
					obj.removeEventListener('DOMAttrModified');
				});
			}, false);
		}
	});

	var setAdMarker = function (adResponse, adContainer) {
		var adWrapper = adContainer.parentNode;
		if (!adWrapper.className.match(/sasloaded/)
			&& -1 === noAdvertIdentifier.indexOf(adContainer.id)
		) {
			adWrapper.className += " sasloaded";
		}
		if ("billboard" === adContainer.id) {
			adWrapper.style.marginBottom = '20px';
		}

		if ("wallpaper" === adResponse.tempName
			&& void 0 !== adResponse.crea3
			&& void 0 !== adResponse.crea3.width
		) {
			document.body.style.paddingLeft = adResponse.crea3.width + 'px';
		}
	};

	var setAdFallback = function (adContainer) {
		if (!adContainer){
			console.log('adContainer is empty in setAdFallback and will be skipped');
			return;
		}
		adContainer.className = adContainer.className.replace(/sasloaded/, '');
		if ("superbanner_btf" === adContainer.id) {
			adContainer.className += " googleloaded sasloaded";
			var i = document.createElement('iframe');
			i.id = "gadfallback";
			i.scrolling = 'no';
			i.frameBorder = 0;
			i.style.padding = 0;
			i.height = 88;
			i.width = 660;
			i.style.display = 'block';
			adContainer.innerHTML = "";
			adContainer.appendChild(i);
			adContainer.style.height = i.height + "px";
			i.doc = document.getElementById(i.id).contentDocument || document.frames[i.id].document;
			i.doc.write(
				"<!doc" + "type html><ht" + "ml><he" + "ad><ti" + "tle></ti" + "tle></he" + "ad><bo" + "dy style='margin:0;padding:0'>" +
				"<scr" + "ipt type='text/javascript'>var inDapIF=inDapMgrIf=true;</scr" + "ipt>" +
				"<scr"+"ipt>google_ad_channel = '1854044203';google_ad_client = 'ca-pub-4347119884249993';google_ad_type = 'text';google_color_bg = '#FFFFFF';google_color_link = '#3579A7';google_color_text = '#000000';google_color_url = '#3579A7';google_font_face = 'Open Sans, Arial';google_language = 'de';google_max_num_ads = '1';google_override_format = 'true';google_ad_format = '660x88';google_ad_height = '88';google_ad_width = '660';</scr"+"ipt><scr"+"ipt src=\"//pagead2.googlesyndication.com/pagead/show_ads.js\"></scr"+"ipt>" +
				"</bo" + "dy></ht" + "ml>"
			);
			if (navigator.userAgent.indexOf('MSIE') === -1
				&& navigator.userAgent.indexOf('Opera') === -1
			) {
				i.doc.close();
			}
		}
	};

	var destructors = [];

	var check_delivered_class = function(e) {
		return (0 <= e.className.indexOf('addelivered') || 0 <= e.className.indexOf('sas_delivered'));
	};

	sas_loadHandler = function (e) {
		var adContainer = document.getElementById(e.contId);
		if(!e.hasAd) {
			if(destructors[e.contId]) {
				destructors[e.contId]();
			}
			setAdFallback(adContainer);
			return;
		}
		if (check_delivered_class(adContainer)) {
			setAdMarker(e, adContainer);
		} else {
			observeDOM(adContainer, function (destructor) {
				destructors[e.contId] = destructor;
				if (check_delivered_class(adContainer)) {
					destructors[e.contId]();
					setAdMarker(e, adContainer);
				}
			});
		}
	};
	// ]]>
</script>
	
