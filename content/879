	<script type="text/javascript">
	var cookieStr = 'facebookOptOut';

	// Disable tracking if the opt-out cookie exists.
	if (document.cookie.indexOf(cookieStr + '=true') > -1) {
		window[cookieStr] = true;
	}

	// Opt-out function
	function facebookOptOut() {
		document.cookie = cookieStr + '=true; expires=Thu, 31 Dec 2099 23:59:59 UTC; path=/';
		window[cookieStr] = true;
		alert('Der Opt-Out-Cookie wurde gesetzt.')
	}
</script>
