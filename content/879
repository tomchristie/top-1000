	<script type="text/javascript">
		function mobileBrowsersFixed() {
			var mobileFixed = false;
			var isAndroidChrome = false;
			var isiDevice = /iphone|ipod/i.test(navigator.userAgent.toLowerCase());
			var isAndroid = /android.*mobile/i.test(navigator.userAgent.toLowerCase());
			var isChrome = /chrome/i.test(navigator.userAgent.toLowerCase());

			if (isAndroid && isChrome) {
				isAndroidChrome = true;
			}

			if (isiDevice || isAndroidChrome) {
				mobileFixed = true;
			}
			return mobileFixed;
		}
		document.addEventListener('DOMContentLoaded', function() {
			var myUrl = new URL(window.location.href);
			var setMobileCookie = myUrl.searchParams.get("noMobileCookie");
			if (setMobileCookie === 0) {
				$.cookie('no_mobile', 1);
			} else if (setMobileCookie === 1) {
				$.cookie.remove('no_mobile');
			}
			var render = 0;
			// mobile view available
			var mobileAvailable = 1;
			var mobileSwitchText = $("#mobileswitchtext");
			var mobileSwitchLink = $("#mobileswitchlink");
			var mobileSwitchLinkFix = $("#mobileswitchlinkfix");

			var switchToMobile = function(event) {
				$.cookie.remove('no_mobile');
				setTimeout(function() {document.location.href = event.target.href;}, 0);
				event.preventDefault();
			};

			if (mobileAvailable && typeof $.cookie('no_mobile') === 'undefined') {
				$('body').prepend('<link href="https://i.computer-bild.de/css/mobile/mobileswitch2.css?240" rel="stylesheet" type="text/css" />');
				mobileSwitchText.html('Diese Seite ist mobil nicht verf&uuml;gbar.');
				mobileSwitchLink.html('Zur mobilen Homepage');
				document.querySelector("#mobileswitchlinkfix").addEventListener("click", switchToMobile, false);
				document.querySelector("#mobileswitchlink").addEventListener("click", switchToMobile, false);
				mobileSwitchLink.attr('href', 'https://www.computerbild.de/?noMobileCookie=1');
				mobileSwitchLinkFix.attr('href', 'https://www.computerbild.de/?noMobileCookie=1');
				render = 1;
			} else if (mobileAvailable && $.cookie('no_mobile') == 1) {
				$('body').prepend('<link href="https://i.computer-bild.de/css/mobile/mobileswitch.css?240" rel="stylesheet" type="text/css" />');
				mobileSwitchText.html('Sie befinden sich in der Webansicht.');
				mobileSwitchLink.html('Zur Mobilansicht');
				document.querySelector("#mobileswitchlinkfix").addEventListener("click", switchToMobile, false);
				document.querySelector("#mobileswitchlink").addEventListener("click", switchToMobile, false);
				mobileSwitchLink.attr('href', '?noMobileCookie=1');
				mobileSwitchLinkFix.attr('href', '?noMobileCookie=1');
				render = 1;
			}
			if (render) {
				if (!mobileBrowsersFixed()) {
					$('main').prepend($('#mobileswitchTop').detach());
					$('#mobileswitch').remove();
				} else {
					$('body').prepend($('#mobileswitch').detach());
					$('#mobileswitchTop').remove();
				}
			}
		});
	</script>
</div>
