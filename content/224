<script type="text/javascript" src="default5/script/owl.carousel.min.js"></script>
<script type="text/javascript">
	document.domain="70yx.com";
</script>
<script type="text/javascript">
$(function(){
	(function(){
	var btn = document.getElementById('gameNames');
	var menu = document.getElementById('specific_name');
	btn.onmouseover = menu.onmouseover = function(){
		menu.style.display = 'block';
	}
	btn.onmouseout =menu.onmouseout = function(){
		menu.style.display = 'none';
	}
	})();
	
	
	//游戏左右切换
	$('#scroll').owlCarousel({
		items: 3,
		autoPlay: false,
		navigation: true,
		navigationText: ["",""],
		scrollPerPage: true
	});
	
	
	//游戏点击弹层
	var $dialog=$('.scroll-dialog');
	var $dialogBox=$('.scroll-dialog>div');
	$('.item').click(function(){
		var url=$(this).attr('data-load');
		openLoad(url);
	});
	 var timer1=0;
	//弹层内轮播
	function showImg(box,timer){
				console.log(box+','+timer);
					var $ul = box.find('ul');
					var $li = $ul.find('li');
					var $btn = box.find('.list-btn>span');
					var $w = $li.outerWidth(true);
					var num = 0;
					
					function animateImg(index){
						stopAuto();
						num=index;
						$ul.animate({marginLeft:-$w*index}, 500);
						$btn.removeClass('td-list-hover').eq(index).addClass('td-list-hover');
						starAuto();
						
					}
					function starAuto(){
						timer = setTimeout(function(){
							num+=1;
							if(num>$li.length-1){
								num=0;
							}
							animateImg(num);
						},5000);
					}
					function stopAuto(){
						clearTimeout(timer);
					}
					starAuto();
					$btn.on('mouseenter',function(){
						var a=$(this).index();
						animateImg(a);
						stopAuto();
					}).on('mouseleave',function(){
						starAuto();
					});
					$li.on('mouseenter',function(){
						stopAuto();
					}).on('mouseleave',function(){
						starAuto();
					});
	}
	
	//根据url加载展示内容
	function openLoad(url) {
	  var surl=url + "?" + new Date().getTime();
	   $(".scroll-dialog").load(surl,function(data){
			 $(".scroll-dialog").html(data);
				var scrollDialog=$('.show-img-box');	
				$('.scroll-dialog-close').on('click',function(){
					$dialog.fadeOut(300);
					clearTimeout(timer1);
				});
			  $(".scroll-dialog").fadeIn(314, function() {
					showImg(scrollDialog,timer1);
			  });
	   });
	}
});
</script>
