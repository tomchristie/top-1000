            try {
                var a = document.referrer;
                if (a && a.toLowerCase().indexOf('i.cnki.net') > -1) {
                    return
                }
                var d = navigator.userAgent;
                var b = d.toLowerCase();
                if (b.indexOf('android') > -1 || b.indexOf('iphone') > -1) {
                    location.href = 'http://i.cnki.net'
                }
            } catch (c) {
            }
        };
        CheckTerminalType();
