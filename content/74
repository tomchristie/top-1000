  (function() {
      var p;
          if (!window.Promise || !window.CustomEvent || !window.fetch || !Element.prototype.closest) { p = '/etc.clientlibs/hp-com/clientlibs/clientlib-public/resources/js/polyfills/ie11.4fb60a42.js'; }
          else if (!window.URLSearchParams || !window.IntersectionObserver || !window.customElements) { p = '/etc.clientlibs/hp-com/clientlibs/clientlib-public/resources/js/polyfills/edge-safari.4fb60a42.js'; }
      p && document.write('<script src="' + p + '">\x3C/script>');
