      <script>
        var variation = document.querySelector('.variation-search');
        var form = variation.querySelector('.variation-search-form');
        var btn = form.querySelector('.c-button');
        
        btn.addEventListener('click', submitForm);
        form.addEventListener('submit', submitForm);

        function submitForm (e) {
          e.preventDefault();
          var data = form.elements.search.value.trim();
          
          if (data) {
            window.location.href = getSearchUrl(data);
          }
        }
        function getSearchUrl(query) {
          //en-us
          //https://www8.hp.com/us/en/search/search-results.html?ajaxpage=1#/page=1&/qt=elite
          var lang = document.documentElement.lang.split('-');
          var cc = lang[1];
          var lc = lang[0];
          return 'https://www8.hp.com/' + cc + '/' + lc + '/search/search-results.html?ajaxpage=1#/page=1&/qt=' + query;
        }
      </script>
    </div>
