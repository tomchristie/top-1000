{{#if reservations.length}}
  <table class="table-basic" id="flightsList-table">
	<caption class="hidden-accessible">Your upcoming flights</caption>
	<thead>
		<tr>
        	<th scope="col">Depart date</th>
        	<th scope="col">Depart from</th>
        	<th scope="col">Reservation name</th>
        	<td></td>
    	</tr>
	</thead>
	<tbody>
	{{#visibleReservations}}
		<tr>
	    	<td>{{departureDate}}</td>
			<td>{{originAirportCode}}</td>
			<td>{{reservationName}}</td>
			<td class="text-center" scope="row">
	        	<a class="call-to-action" href="/reservation/selectReservationSubmit.do?fromHomePage=true&amp;recordLocator={{recordLocator}}">
					View/Change
					<span class="hidden-accessible">{{reservationName}}, {{departureDate}}</span>
				</a>
	        </td>
		</tr>
	{{/visibleReservations}}
	</tbody>
  </table>
  {{#aaCheck reservations.length ">" 5}}
  <a href="../aadvantage-program/profile/wallet" class="walletLinkText">View travel credits in your wallet<span aria-hidden="true" class="call-to-action"></span></a>
  <form id="viewall-flights" class="right">
	<button type="submit" formaction="/reservation/viewReservationsAccess.do" formmethod="post" id="viewAllBtn" class="btn">View all</button>
  </form>
  {{/aaCheck}}
{{else}}
  <p><span class="message-warning" aria-hidden="true"></span><span>You do not have any current reservations.</span></p>
{{/if}}
{{#aaCheck 6 ">" reservations.length}}
  {{#aaCheck 1 ">" reservations.length}}
    <hr />
  {{/aaCheck}}
  <a href="../aadvantage-program/profile/wallet" class="walletLinkText">View travel credits in your wallet<span aria-hidden="true" class="call-to-action"></span></a>
{{/aaCheck}}
