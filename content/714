<script type="text/javascript">
    function getCurrentSearch() {
        var currentSearch = {
            elementID: "",
            originAirport: $j('#reservationFlightSearchForm\\.originAirport').val(),
            originTravelDate: $j('#aa-leavingOn').val(),
            destinationAirport: $j('#reservationFlightSearchForm\\.destinationAirport').val(),
            destinationTravelDate: $j('#aa-returningFrom').val(),
            createDate: new Date(),
            tripType: getSelectedFindFlightType(),
            awardBooking: $j('#flightSearchForm\\.tripType\\.redeemMiles').prop('checked'),
            expired: false
        };
        return currentSearch;
    };

    function getSelectedFindFlightType() {
        if ($j('#flightSearchForm\\.tripType\\.oneWay').prop('checked')) {
            return "oneWay";
        }

        return "roundTrip";
    };
</script>















