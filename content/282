  <script>
    //메인비주얼
    var swiper1 = new Swiper('.main-visual', {
        loop: true,
        autoplay: true,
        delay: 3000,
        preventClicksPropagation: false,
        pagination: {
            el: '.swiper-pagination',
            clickable: true,
        },
    });

    //메인비주얼 페이지
    console.log("====================Test=================");
    var swipeBullet1Txt = $('.main-visual .showcase-text .showcase-txt-detail').eq(0).text();
    var swipeBullet2Txt = $('.main-visual .showcase-text .showcase-txt-detail').eq(1).text();
    var swipeBullet3Txt = $('.main-visual .showcase-text .showcase-txt-detail').eq(2).text();
    var swipeBullet4Txt = $('.main-visual .showcase-text .showcase-txt-detail').eq(3).text();
    var swipeBullet5Txt = $('.main-visual .showcase-text .showcase-txt-detail').eq(4).text();
    $('.main-visual .showcase-text').remove();

    var swipeBullet1 = $('.main-visual .swiper-pagination-bullet').eq(0);
    var swipeBullet2 = $('.main-visual .swiper-pagination-bullet').eq(1);
    var swipeBullet3 = $('.main-visual .swiper-pagination-bullet').eq(2);
    var swipeBullet4 = $('.main-visual .swiper-pagination-bullet').eq(3);
    var swipeBullet5 = $('.main-visual .swiper-pagination-bullet').eq(4);

    swipeBullet1.append('<span class="swiper-pagination-txt">' + swipeBullet1Txt + '</span>');
    swipeBullet2.append('<span class="swiper-pagination-txt">' + swipeBullet2Txt + '</span>');
    swipeBullet3.append('<span class="swiper-pagination-txt">' + swipeBullet3Txt + '</span>');
    swipeBullet4.append('<span class="swiper-pagination-txt">' + swipeBullet4Txt + '</span>');
    swipeBullet5.append('<span class="swiper-pagination-txt">' + swipeBullet5Txt + '</span>');

    //에디터스 픽
    var swiper2 = new Swiper('.mp__swiper', {
        centeredSlides: true,
        slidesPerView: 'auto',
        navigation: {
            nextEl: '.swiper2-button-next',
            prevEl: '.swiper2-button-prev',
        },
        loop: true,
        slidePrevClass: 'swiper-slide-prev',
        slideNextClass: 'swiper-slide-next',
       // simulateTouch: false,
        autoplay: {
            delay: 5000,
            disableOnInteraction: false
        },
    });

    // 주간차트 슬라이드
    var swiper3 = new Swiper('.mc__swiper', {
        //simulateTouch: false,
        preventClicksPropagation: false,
        navigation: {
            nextEl: '.mc__swiper-button-next',
            prevEl: '.mc__swiper-button-prev',
        },
        slidesPerView: 'auto',
        spaceBetween: 50,
        speed: 500,
        initialSlide: 0,
        slidesPerGroup: 5,
    });



    // 실시간 급상승
    $.fn.realtime = function () {
        var mainRT = this;
        var mrList = this.find('.mr__group');
        setInterval(function () {
            var top1El = mrList.find('.mr__item').eq(0);
            var top2El = mrList.find('.mr__item').eq(1);
            var top3El = mrList.find('.mr__item').eq(2);
            $('.mr__item').animate({
                'top': '-60px'
            }, 300);
            setTimeout(function () {
                mrList.append(top1El, top2El, top3El);
                $('.mr__item').css('top', 0);
            }, 400);
        }, 3000);
    }

    // dom 전부 로드 후 실행
    window.onload = function () {
        // 실시간 급상승 함수 실행
        $('.main-realtime').realtime();
    }


    // 추천기사
    // 현재 페이지 sns공유팝업에 url 자동 입력
    var copyUrl = document.querySelector('.js-copy-url');
    //copyUrl.value = thisHref;

    function copyToClipboard(val) {
        var t = document.createElement("textarea");
        document.body.appendChild(t);
        t.value = val;
        t.select();
        document.execCommand('copy');
        document.body.removeChild(t);
    }



    $('.js-copy-url').click(function (e) {
        //e.preventDefault(); // 스크롤유지
        var sBrowser, sUsrAg = navigator.userAgent;
        console.log(sUsrAg);

        var thisHref = location.href; // 현재 URL
              

        //----- 내/외부 기사 구분해서 copy link값 생성
        var srcLink = $('#artFrame').attr('src');   //내부링크용
        var outLink = $('#artFrame').contents().find('#outLinkFrame').attr('src'); 
      
        var outLink2 = $('#CopyLink').text();
        console.log('outLink2' + outLink2);



        console.log('artFrame: ' + srcLink);
        console.log('outLinkFrame: ' + outLink);

        if (outLink != undefined && outLink != '') {
            thisHref = outLink;
        } else
        {
            thisHref = thisHref + srcLink;
        }
        //-------------------

        copyToClipboard(thisHref);
        layerPop.openName('url-copy');
    });






    // 최신기사 팝업 열기
    $('.js-recommended-pop').on('click', function (e) {
        e.preventDefault();
        $('.rcmd-pop').removeClass('hidden').addClass('visible');
        $('html').css({
            'overflow': 'hidden'
        });
    });
    // 최신기사 팝업 닫기
    $('.js-rcmd-pop__close').on('click', function () {
        $('.rcmd-pop').addClass('hidden').removeClass('visible');
        $('html').css({
            'overflow': 'visible'
        });
    });
    //// 추천기사

  </script>

