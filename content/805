			// 食事あり,食事なし,食事・寝具あり,食事のみ,寝具のみ,食事・寝具なし,その他
			[ 5, 4, 0, 0, 0, 0, 0 ],	//9-11才
			[ 1, 1, 0, 0, 0, 0, 0 ],	//6-8才
			[ 0, 0, 5, 1, 5, 1, 0 ],	//3-5才
			[ 0, 0, 0, 0, 0, 0, 3 ]		//0-2才
		]
		,m_defPlaceOfStayDistrict:'10'		//宿泊地用(方面)※方面/地区/地域のどれか1パラメータのみ指定
		,m_defPlaceOfStayRegion:'502'		//宿泊地用(地区)
		,m_defPlaceOfStayArea:'525'			//宿泊地用(地域)
		,m_defCheckin:'2018/03/18'			//チェックイン用
		,m_defCheckout:'2018/03/25'			//チェックアウト用
		,m_defIsWithCar:'checked'			//レンタカーも合わせて予約チェック用
	}
	必要に応じてコメント外して使用してください→ */
});

$('#module-tabisaku').tourCommonModule('setTargetValue', {
	// AdobeTarget
	domtourDp : {
		m_defDepFlightAirportDep:undefined
	}
});

$('#module-tabisaku').domtopModuleDp('topModuleDisplay', {
});

