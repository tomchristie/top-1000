$(document).ready(function() {
  $('.subscribe-cta').click(function(e) {
    e.preventDefault();
    if ($(this).hasClass('cta')) {
      $('html, body').animate({ scrollTop: $('#the_latest').offset().top }, 'fast', function() {
        $('#first_form #subscriber_email').focus();
      });
    } else {
      $('html, body').animate({ scrollTop: $('#become_insider').offset().top }, 'fast', function() {
        $('#last_form #subscriber_email').focus();
      });
    }
    return false;
  })
  $('#subscribe_cta_header').click(function(e) {
    window.fathom.trackGoal('VFSISAIG', 0);
  })
  $('#subscribe_cta').click(function(e) {
    window.fathom.trackGoal('L3BNMMDO', 0);
  })
  var modal_shown = false;
  $(window).blur(function() {
    if (modal_shown == false){
      $('#homepage-modal').modal('show');
      modal_shown = true;
    }
  });
});
</script>
