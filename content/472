	<script type='text/javascript'>
	$("#dfp_gpt_1628511815_445348772").attr('debug','dfp_gpt_1628511815_445348772; /1015057/Fanpop_Network_300x250_ATF');
	if (typeof dfp_console !== "undefined") {
		console.log('$("#dfp_gpt_1628511815_445348772").attr(\'debug\',\'dfp_gpt_1628511815_445348772; /1015057/Fanpop_Network_300x250_ATF\');');
