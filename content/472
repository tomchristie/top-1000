<script type="text/javascript">
	
		$('header .club-header').on('click.joinClub', '.join-club.button2019', function() {
			if ($(this).hasClass('disabled')) {
				return;
			}
			$(this).addClass('disabled');
			$.ajax('/user/subscriberemote_2011/1737528?redirect_url=%2F&amp;cards_2019=true')
				.done(function(data) {
					$('header .club-header').off('click.joinClub');
					$('header .club-header .join-club.button2019').addClass('disabled').html('Joined');
					if (data.match(/window\.location/)) {
						$('header .club-header .join-club.button2019').append(data);
					}
				})
				.fail(function() {
					$('header .club-header .join-club.button2019').removeClass('disabled').html('Join');
				});
		});
	
	$('header .title').on('click.newPost', '.new-post.button2019', function() {
		console.log('click!');
		if ($(this).hasClass('on')) {
			$(this).removeClass('on');
			$(this).parents('.title').siblings('.add-content').addClass('hidden');
		} else {
			$(this).addClass('on');
			$(this).parents('.title').siblings('.add-content').removeClass('hidden');
		}
	});
	$('.add-content .content-choices').on('click.addContent', '.choice', function() {
		var url = $(this).attr('data-add-url');
		if (url) {
			window.location = url;
		}
	});
</script>

		
