<script>
	function showDropdownMenu(){
		$('#hiddenDropMenu').css('visibility', 'visible');		
	}

	$(document).ready(function() {
		var $window = $(window);
	
		function checkWidth() {
			var windowsize = $window.width();
			if (windowsize < 980) {
				//$('#banner-bar').css('visibility', 'hidden');
				//$('#notification-bar').css('visibility', 'hidden'); 
				//$('#mobile-notification-bar').css('visibility', 'visible');
				$('.desktop-only').css('visibility', 'hidden');
				$('.mobile-only').css('visibility', 'visible');
				
			} else {
				//$('#banner-bar').css('visibility', 'visible');
				//$('#notification-bar').css('visibility', 'visible');				
				//$('#mobile-notification-bar').css('visibility', 'hidden');
				$('.desktop-only').css('visibility', 'visible');
				$('.mobile-only').css('visibility', 'hidden');

			}
		}
		// Execute on load
		checkWidth();
		// Bind event listener
		$(window).resize(checkWidth);
		
		bind_actions_to_cards();

		
		add_infinite_scroll($(window), function() {if (typeof ga_event !== "undefined") { ga_event('infinite_scroll', 'undefined', '2'); }});
		if (typeof ga_event !== "undefined") {
			ga_event('infinite_scroll', 'undefined', 'initial_page_load (before scrolling)');
		}
	});

	function filter_button(elem, filterlaceh_type) {				
		//re-write the contents of the pagination link to reset the page (or pass additional param to reset db offset) and filter for content... This is only for SEO and bots since the div will be hidden on type switching; infinite scroll will not register these changes and needs to use the path: variable
		$('#page-nav').replaceWith('<nav id="page-nav"><a href="/clubs/black-widow-movie/page/2?show_only=%27+%2B+filter_type+%2B+%27">next page</a></nav>');
		
		$('.club-card-filter').css('opacity', '0.5');
		$('.club-card-filter').removeAttr('id');		
		$(elem).css('opacity', '1');
		$(elem).attr('id', 'selectedButton');
		$('.contentcard').remove();
		manual_card_load($(elem).attr('href'));		
		//load_cards_if_no_scrollbar();
	};
	
	function load_cards_if_no_scrollbar(){
		if(document.documentElement.scrollHeight == document.documentElement.clientHeight){
			// ****** Need ajax call here ****** 
			manual_card_load('//www.fanpop.com/?page=2');
		}
	}
	
	$(window).load(function () {

	});
	
</script>

<script type="text/javascript" language="javascript">
	function quizhighlight(num)
	{
		background_color = "#F0F8F0";
		if (document.getElementById("quiz_option_"+num).style.backgroundColor == "transparent")
		{
			document.getElementById("quiz_option_"+num).style.backgroundColor = background_color;
			//document.getElementById("quiz_answer_bubble_"+num).src = "/images/quiz/answered4.jpg";
		}
		
	}
	function quizunhighlight(num, option_counter)
	{
		document.getElementById("quiz_option_"+num).style.backgroundColor = "transparent";
		//document.getElementById("quiz_answer_bubble_"+num).src = "/images/quiz/answer"+(option_counter+1)+".jpg";
	}
	function submitvote(url, site_id)
	{
		document.getElementById("quizchoices" + site_id).innerHTML = "<div style='text-align:center;padding:12px 0 12px 0;font-size:18px;font-weight:bold'>Submitting...</div>";
		window.location=url;
	}
</script>

