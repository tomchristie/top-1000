						console.log('#dfp_gpt_1628511815_48472881 adloop: card ad found on lightbox');
					}	else if ($('#dfp_gpt_1628511815_48472881').parents('.contentcard').length == 0) {
						console.log('dfp_gpt_1628511815_48472881 adloop: non-card ad found - ad is visible now; load');
					}
				}
				googletag.cmd.push(function() {
					if (typeof dfp_console !== "undefined") { console.log("googletag.display('dfp_gpt_1628511815_48472881');"); }
					window.setTimeout(function() { 
							if (typeof dfp_console !== "undefined") { console.log("height check for dfp_gpt_1628511815_48472881:"+$('#dfp_gpt_1628511815_48472881').height()); }
							if ($('#dfp_gpt_1628511815_48472881').height() == 0) {
								if (typeof dfp_console !== "undefined") { console.log('displayed a second time for dfp_gpt_1628511815_48472881'); }
								googletag.display('dfp_gpt_1628511815_48472881');
							}
						}, 100);
					googletag.display('dfp_gpt_1628511815_48472881');
					});
				( function () {
						
						if (true && typeof CLARITY === "undefined" ) {
							if (typeof ga_event === "function") {
								ga_event('ad', 'dfp', 'mrec2', {'nonInteraction':1});
							}
						}
				})();
			} else {
				window.setTimeout(function() {adloop_1628511815_48472881(i-1);}, 200);
