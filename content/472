<script>
	window.fanpop = window.fanpop || {};
	window.fanpop.facebook = window.fanpop.facebook || {};
	window.fanpop.facebook.qlogin = function() {
		FB.login(function(response) {
				if (response.status === 'connected') {
					// Logged into your app and Facebook.
					$.ajax({url:'/login/noop?facebook=connected'}).done(function() {
							window.location.reload();
					});
				} else if (response.status === 'not_authorized') {
					// The person is logged into Facebook, but not your app.
					window.location="/logout";
				} else {
					// The person is not logged into Facebook, so we're not sure if
					// they are logged into this app or not.
					window.location="/logout";
				}
		}, {scope: 'public_profile,email,user_friends'});
	};
	window.fanpop.facebook.logout = function() {
		if (typeof FB !== "undefined") {
			FB.getLoginStatus(function(response) {
				if (response.status === 'connected') {
					FB.logout(function() {
							window.setTimeout( function() { window.location='/logout'; }, 100 );
					});
				} else {
					window.location='/logout';
				}
			 });
		} else {
			window.location='/logout';
		}
	};
	
  window.fbAsyncInit = function() {
    FB.init({
      appId      : '87685182958', // App ID
      version		 : 'v5.00',
      channelURL : '//www.fanpop.com/fbconnect/channel.html', // Channel File
      status     : true, // check login status
      cookie     : true, // enable cookies to allow the server to access the session
      oauth      : true, // enable OAuth 2.0
      xfbml      : true  // parse XFBML
    });

    // Additional initialization code here
    
		FB.getLoginStatus(function(response) {
			if (response.status === 'connected') {
				if ($('body').hasClass('logged-in') == false) {
					$.ajax({url:'/login/noop?facebook=connected'}).done(function() {
							window.location.reload();
					}).fail(function() {
						window.location="/login?redirect_url=%2F";
					});
				}
			}
		});
		FB.Event.subscribe('auth.login', function(r) {
				if ( r.status === 'connected' )
				{
					
						//setTimeout("window.location.reload()",100);
					
				}
			}
		);
		

		FB.Event.subscribe('auth.logout', function(r) {
				if ( r.status === 'connected' ) {
				} else {
					window.location="/logout";
				}
			}
		);

	};

  // Load the SDK Asynchronously
  (function(d){
     var js, id = 'facebook-jssdk'; if (d.getElementById(id)) {return;}
     js = d.createElement('script'); js.id = id; js.async = true;
     js.src = "//connect.facebook.net/en_US/sdk.js";
     d.getElementsByTagName('head')[0].appendChild(js);
   }(document));
</script>


	  
		

<script>
  function searchButtonHit() {
		var searchMenu = document.getElementById('search-slider');
		var searchMenuButton = document.getElementById('search-menu-btn');
		var navOpenButton = document.getElementById('nav-open-btn');

		if ($('body').hasClass('shownav')) {
			$('body').removeClass('shownav').addClass('hidenav');
			navOpenButton.style.backgroundColor = 'rgba(0,0,0,0)';	  
		}
		if (searchMenu.style.top == "-80px" || searchMenu.style.top == "" ){
			searchMenu.style.top = "40px";
			searchMenuButton.style.background = 'url(/images/2011/search-button.png)  0px no-repeat rgba(0,0,0,0.5)';
			
			searchMenuButton.style.backgroundSize = "100%";
			searchMenuButton.style.backgroundPosition = "center";
			ga_event('search_button_clicked_open', 'card_search', 'welcome');
		} else {
			searchMenu.style.top = "-80px";
			searchMenuButton.style.background = '';
			document.getElementById("search-menu-btn").className = document.getElementById("search-menu-btn").className.replace(/\bsearch-btn-on\b/,'');
			ga_event('search_button_clicked_close', 'card_search', 'welcome');
		}
  }  
</script>

