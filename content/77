var writePh=function(fFlg){
var PhData=new Array();
PhData[0]=new Array();
PhData[0][0]="210809496611";
PhData[0][1]="ワクチン2回接種 入所者感染(毎日新聞)";
PhData[0][2]="https://news.nifty.com/topics/mainichi/210809496611/";
PhData[0][3]='<img src="/cms_image/niftop/topicImage2/210809496611/thumb-12159-1189009-domestic.jpg" alt="ワクチン2回接種 入所者感染(毎日新聞)"  title="ワクチン2回接種 入所者感染(毎日新聞)"   />';
var ctlgId="";
ctlgId=loadCookie("phCtlgId");
var phHtml="";
for(var i=0;i<0;i++){
	if(ctlgId === PhData[i][0]){phHtml=phOutput(PhData[i+1]);break;}
}
if(fFlg!==1&&phHtml===""){phHtml=phOutput(PhData[0]);}
if(phHtml!==""){$("#PhotoNews").html(phHtml);}
}
