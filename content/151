  <script>
        function model_show() {
            //$("#edit_mask").removeClass("none");
            //$(".edit_popup").removeClass("none");
            
        }
        
        function model_hied() {
            $("#edit_mask").addClass("none");
            $(".edit_popup").addClass("none");
        }
        
        function newYearScolls_hied() {
            $(".new_year_scrolls").addClass("none");
        }
        
        function newYearScolls_show() {
            $(".new_year_scrolls").removeClass("none");
        }
        
        $(document).ready(function () {
            model_hied();
            var timestamp = new Date().getTime();
            timestamp = timestamp + 4 * 60 * 60 * 1000;
            //格式化时间获取年月日
            var dateAfter = new Date(timestamp);
            var COOKIE_NAME = "showbox";
            if ($.cookie(COOKIE_NAME)) {
                //newYearScolls_hied();
            } else {
                newYearScolls_show();
                $(".reg_btn").click(function () {
                    $.cookie(COOKIE_NAME, 'ishide', {
                        expires: dateAfter
                    }); //expires过期时间（单位为天！）
                })
                $(".close_btn").click(function () {
                    $.cookie(COOKIE_NAME, 'ishide', {
                        expires: dateAfter
                    }); //expires过期时间（单位为天！）
                })
                $(".colse").click(function () {
                    $.cookie(COOKIE_NAME, 'ishide', {
                        expires: dateAfter
                    }); //expires过期时间（单位为天！）
                })
                
            }
            $(".colse").on("click", function () {
                //newYearScolls_hied();
            });
            
        })
    </script> 
