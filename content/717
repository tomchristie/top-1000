$(window).scroll(function(){//开始监听滚动条
      var STop = 72-$(window).scrollTop();
      if (STop >= 0) {
        $(".bgmarket").css("top",STop);
      }
      if (STop < 0) {
        $(".bgmarket").css("top",0);
      }
      })
</script>
