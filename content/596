var handle = setInterval(getScheduleId, 10);

function getScheduleId(){
    var getScheduleId = document.getElementById('schedule');
    if(getScheduleId){
        var childrenGroup = document.getElementsByClassName('row mb-4 mb-md-0')[0];
        if(childrenGroup){
            if(childrenGroup.childNodes[2]){childrenGroup.childNodes[2].style.display = 'none';}
            if(childrenGroup.childNodes[4]){childrenGroup.childNodes[4].style.display = 'none';}
            if(childrenGroup.childNodes[5]){childrenGroup.childNodes[5].style.display = 'none';}
        }
    }
}
}
</script>
