<script type="text/javascript">
var storeJumioRibbonLastUrl = function()
{
     var loc= window.location.href;
     var d = new Date();
     var currentDomain = "";
     d.setTime(d.getTime() + (1*24*60*60*1000));
     var expires = 'expires='+ d.toUTCString();
	 if(window.clientConfig.m2Page !== undefined)
	 {
	 currentDomain = window.clientConfig.m2Page.domain;
	 }
	 else {
	 currentDomain = window.clientConfig.vnPage.domain;
	 }
     document.cookie = 'jumioRibbonPrevLoc= '+ loc + ';'+ expires +';path=/; domain='+ currentDomain;
     return true;
}
</script>
<script type="text/javascript" id="mm">
setTimeout(function appendTarget(){
var mmLinks = document.getElementsByClassName('dropdown nav-item');
          if(mmLinks.length>0){
                    menuToggle();
			}else{
                   setTimeout(appendTarget, 500)
              }
       }, 500)

setTimeout(function appendBm(){
bottomNavLinks = document.getElementsByTagName('vn-bottom-nav');
          if(bottomNavLinks.length>0){
                    bottomAttach();
			}else{
                   setTimeout(appendBm, 500)
              }
       }, 500)

function bottomAttach(){
document.querySelector('vn-bottom-nav').addEventListener('click', function(){menuToggle()})
}

function menuToggle(){
	if(clientConfig.vnDevice.isMobile || clientConfig.vnDevice.isTablet){
		var mobileMenulinks = document.getElementsByClassName('dropdown nav-item')
              for(i=0;i<mobileMenulinks.length;i++){mobileMenulinks[i].addEventListener('click', function(event){
			var myLinks = document.getElementsByClassName('dropdown nav-item')
				for(i=0;i<myLinks.length;i++){
					if(myLinks[i] != event.target.parentElement){
                     			myLinks[i].classList.remove('open');
					}
				}
				event.target.parentElement.classList.add('open');
		})}
	}
}
</script>
