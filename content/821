  <script type="text/javascript">
      $(function(){
          var rurl = 'http://passport.rayli.com.cn/index.php?c=member&a=register';
          $('#register').attr('href', rurl);
          if ($.cookie('pass_username') !=null ) {
              $('#login_status').show();
              var username = decodeURIComponent($.cookie('pass_username'));
              var uid = $.cookie('pass_uid');
              if(username.length>10){
                  username=username.substr(0, 6)+'……';
              }
              $('#uname').html(username);
              $('#rstart').attr('href', 'http://star.rayli.com.cn/u/'+uid+'/me');
          } else {
              $('#logout_status').show();
          }
      });
  </script>
