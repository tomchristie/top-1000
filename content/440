<script>
    $(document).ready( function () {
        var getFragConfig = function() {
            var urifrag = window.location.hash.replaceAll('#', '');
            var frags = urifrag.split("&");
            console.log("----- FRAGS -----", frags);
            var isOpenPopup = false;
            for (var i = 0; i < frags.length; i++) {
                if (frags[i].includes("openPopupVC")) {
                    isOpenPopup = true;
                }
            }
            return isOpenPopup;
        };

        var inputWizard = $('.tm-modal-overlay__overlay');
        if(inputWizard.length === 1 && getFragConfig()){
            window.ModaliPopupController.openById("mp-" + "-60807561");
        }else {
            console.warn("CI SONO PIÙ MODALI DI VERIFICA COPERTURA IN PAGINA")
        }
    });
</script>


