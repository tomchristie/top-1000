<script id="redational-config">
    const RedationalManager60807561 = function () {
        var config = {};
        config.general = {};
        config.comune = {};
        config.indirizzo = {};
        config.civico = {};
        config.prefisso = {};
        config.numero = {regexMessage: []};
        config.general.debounceTime = +"400";
        config.general.urlVetrinaFissoOffertePerTe = "";
        config.general.myTimBasePath = "";
        config.general.enableRedirectOffertePerTe = "true";
        config.general.disableSearchNotTrustedAddress = window.apiConfig["disableSearchNotTrustedAddress"]  ? window.apiConfig["disableSearchNotTrustedAddress"].toUpperCase() === "TRUE" : false;
        config.general.metaOffersId = "MO00000060";
        config.comune.minNumOfDigit = +"3";
        config.indirizzo.minNumOfDigit = +"2";
        config.indirizzo.certifiedWarningMessage = "";
        config.civico.certifiedWarningMessage = "";
        config.prefisso.maxLength = +"4";
        config.prefisso.validationRegex = "^0[0-9]{1,3}$";
        config.prefisso.validationMessage = "Si prega di inserire un prefisso da 2 a 4 cifre e che inizia con 0";
        config.numero.maxLength = +"8";


        var regex = "^[2-9]{1,1}";
        var message = "Il campo NUMERO non può iniziare per 0 o per 1"
        if(regex && message){
            config.numero.regexMessage.push({
                REGEX: regex,
                MESSAGE: message
            });
        }

        regex = undefined;
        message = undefined;
        regex = "[0-9]{4,}$";
        message = "Il campo NUMERO non può essere formato da meno di 4 cifre";
        if(regex && message){
            config.numero.regexMessage.push({
                REGEX: "[0-9]{4,}$",
                MESSAGE: "Il campo NUMERO non può essere formato da meno di 4 cifre"
            });
        }

        config.hide_myNumber = "false";
        config.hide_activateNewLine = "false";
        config.isMetaOpzione = "false";
        config.errorUrl = "";

        return {
            getConfig: function () {
                return config
            }
        };
    };
</script>


