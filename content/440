l:250,m:180,s:50},e=0;e<b.length;e++)$(b[e]).on("mouseover",function(){// evaluate current breakpoint
var a=window.innerWidth,b=a>=c.l?"l":a>=c.m?"m":"s";// positioning
if(this.classList.contains("-full-width")||"s"==b&&this.classList.contains("-full-width-s")||"m"==b&&this.classList.contains("-full-width-m")||"l"==b&&this.classList.contains("-full-width-l"))this.classList.remove("-centered"),this.classList.remove("-to-left");else{// this.classList.remove('-to-left'); // clean up any unnecessary classes
var e=this.getBoundingClientRect(),f=a>=c.l?d.l:a>=c.m?d.m:d.s;// tooltip current position and size
// evaluate margin depending on current breakpoint
a-parseInt(e.right)<f&&(this.classList.remove("-centered"),this.classList.add("-to-left"),console.log("aggiungo toleft")),e.x<f&&(this.classList.remove("-centered"),this.classList.remove("-to-left"))}})}}}();
(function () {
    if ( typeof window.CustomEvent === "function" ) return false;
    function CustomEvent ( event, params ) {
        params = params || { bubbles: false, cancelable: false, detail: null };
        var evt = document.createEvent( 'CustomEvent' );
        evt.initCustomEvent( event, params.bubbles, params.cancelable, params.detail );
        return evt;
    }
    window.CustomEvent = CustomEvent;
})();

</script>


    

    



    
