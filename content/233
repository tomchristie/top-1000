      function BSANativeCallback(a) {
        if (a.ads && (a.key == 'CEBI6KQN' || a.key == 'CEBI6KQU')) {
          var body = document.getElementsByTagName('body')[0];
          body.className += ' not-fixed';
          if (a.ads.length <= 0 && a.key == 'CEBI6KQN') {
            document.getElementById('bsa-zone_1593722135885-4_123456').removeAttribute('data-explicitly-hidden');
          }

          const mainContent = document.querySelector('.container-1400');
          const menu = document.querySelector('.not-fixed .primary-navigation');
          const flexbar = document.querySelector('#_flexbar_');

          try {
            menu.insertBefore(flexbar, document.querySelector('.not-fixed .primary-navigation .primary'));
            mainContent.style.marginTop = '215px';

            document.querySelectorAll('.flexbar-sticky-offset').forEach(el => {
              el.classList.add('top-56');
              el.classList.add('lg:top-56');
            });
            window.scrollTo(0, 0);
          } catch (error) {
            console.log('error adding flexbar');
          }
        }

        if (a.key == 'CK7ICKQE') {
          var element = document.querySelector('article.fp-vertical-story[data-id="742304"]');
          element.classList.remove('placeholder');
        }
        if (a.key == 'CE7DC2QI') {
          var element = document.querySelector('article.fp-vertical-story[data-id="1057851"]');
          element.classList.remove('placeholder');
        }
        if (a.key == 'CE7DC2JE') {
          var element = document.querySelector('article.fp-vertical-story[data-id="827995"]');
          element.classList.remove('placeholder');
        }
        if (a.key == 'CE7DC2JJ') {
          var element = document.querySelector('article.fp-vertical-story[data-id="838019"]');
          element.classList.remove('placeholder');
        }
      }
