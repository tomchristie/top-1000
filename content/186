<script type='text/javascript'>
        var pbjs = pbjs || {};
        pbjs.que = pbjs.que || [];

        var googletag = googletag || {};
        googletag.cmd = googletag.cmd || [];

        googletag.cmd.push(function() {
            googletag.pubads().disableInitialLoad();
        });
        var PREBID_TIMEOUT = 1500;
        var FAILSAFE_TIMEOUT = 2000;

        var PrebidadUnits = [{
            code: "sys-ac67",
            mediaTypes: {
                banner: {
                    sizes:[
                        [300, 250]
                    ]
                }
            },
            bids: [{
                bidder: "adgeneration",
                params: {
                    "id": "135350"
                }
            }]
        }];
        pbjs.que.push(function() {
            const customConfigObject = {
                "buckets": [{
                    "precision": 2,
                    "min": 0,
                    "max": 2000,
                    "increment": 5
                }]
            };

            pbjs.setConfig({
                priceGranularity: customConfigObject,
                currency: {
                    "adServerCurrency": "JPY",
                    "conversionRateFile": "https://currency.prebid.org/latest.json"
                },
                "bidderCurrencyDefault": {
                    "adgeneration": "JPY"
                },
                userSync:{
                    filterSettings:{
                        iframe:{
                            bidders:"*",
                            filter:"include"
                        }
                    },
                    syncDelay: 6000,
                }
            });

            pbjs.addAdUnits(PrebidadUnits);
            pbjs.requestBids({
                bidsBackHandler: sendAdserverRequest,
                timeout: PREBID_TIMEOUT
            });
        });

        function sendAdserverRequest() {
            if (pbjs.adserverRequestSent) return;
            pbjs.adserverRequestSent = true;
            googletag.cmd.push(function() {
                pbjs.que.push(function() {
                    pbjs.setTargetingForGPTAsync();
                });
                googletag.pubads().refresh([aa_pc_toppanel]);
            });
        };

        setTimeout(function() {
            sendAdserverRequest();
        }, FAILSAFE_TIMEOUT);

    var aa_pc_toppanel;

    googletag.cmd.push(function() {

        aa_pc_toppanel = googletag.defineSlot('/4872126/aa_pc_toppanel_allabout_67', [[1,1], [300, 250], [200, 200], [250, 250], [336, 280]], 'sys-ac67').addService(googletag.pubads());



        googletag.pubads().enableSingleRequest();
        googletag.pubads().collapseEmptyDivs();
        googletag.pubads().setTargeting("url", [aa.v.url]);
        googletag.pubads().setTargeting("sr", [aa.v.sr]);
        googletag.pubads().setTargeting('guidesite', [aa.v.ad_dir1]);
        googletag.pubads().setTargeting('page_type', [aa.v.page_type]);
        googletag.enableServices();
    });

</script>

