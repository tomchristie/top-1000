<script>
    function getUrlParameterForGrid(name) {
        name = name.replace(/[\[]/, '\\[').replace(/[\]]/, '\\]');
        var regex = new RegExp('[\\?&]' + name + '=([^&#]*)');
        var results = regex.exec(location.search);
        return results === null ? '' : decodeURIComponent(results[1].replace(/\+/g, ' '));
    };


    if(getUrlParameterForGrid('grid') == '1' || getUrlParameterForGrid('grid') == 1) {
        document.querySelector('.permagrid').classList.add('showgrid');
        document.querySelector('.page__main').classList.add('showgrid');
    }
</script>
