    window.addEventListener("onWcomSomInit", function (e) {
        var cmp = window.Wetter && window.Wetter.cmp ? window.Wetter.cmp : {};

        if (performance.mark) {
            performance.mark('insertAd');
        }

        console.log('CMP: SOM insert ads');

                somtag.cmd('insertAd', 'skyscraper1', {container: '#somtag-container-skyscraper1'}, function (error, ad) {
            if (ad && ad.data && ad.data.adConfig && ad.data.adConfig.productName) {
                document.body.classList.add('has-' + ad.data.adConfig.productName);
            }
                        
                    });
                somtag.cmd('insertAd', 'fullbanner2', {container: '#somtag-container-fullbanner2'}, function (error, ad) {
            if (ad && ad.data && ad.data.adConfig && ad.data.adConfig.productName) {
                document.body.classList.add('has-' + ad.data.adConfig.productName);
            }
                        if (ad.data && ad.data.options && ad.data.slotName === 'fullbanner2' && ad.data.adConfig && ad.data.options.container) {
                var iframe = document.querySelector(ad.data.options.container + ' iframe.somtag');
                if (iframe) {
                    iframe.addEventListener('load', function () {
                        var el = document.getElementById('ad-fullbanner2-outer');
                        el.style.height = Object(iframe).offsetHeight + 'px';
                    });
                }
            }
                        
                    });
                somtag.cmd('insertAd', 'rectangle1', {container: '#somtag-container-rectangle1'}, function (error, ad) {
            if (ad && ad.data && ad.data.adConfig && ad.data.adConfig.productName) {
                document.body.classList.add('has-' + ad.data.adConfig.productName);
            }
                                    if (ad.data && ad.data.slotName === 'rectangle1' && ad.data.options && ad.data.options.container) {
                var iframe = document.querySelector(ad.data.options.container + ' iframe.somtag');
                if (iframe) {
                    iframe.addEventListener('load', function () {
                        Object(iframe).offsetHeight >= 600 && document.body.classList.add('has-halfPage');
                        
                        var el = document.getElementById('somtag-container-rectangle1');
                        if (el.offsetHeight > 0) {
                            el.parentElement.style.height = 'initial';
                            el.parentElement.style.paddingBottom = '30px';
                        }
                    });
                }
            }
            
                    });
                somtag.cmd('insertAd', 'performanceFullbanner1', {container: '#somtag-container-performanceFullbanner1'}, function (error, ad) {
