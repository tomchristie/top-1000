<script>
	(function($) {
		$('#footer-main dt').click(function() {
			$parent = $(this).parent();
			if($parent.attr("id") === 'footer-service') {
				// "サーバーサービス"は処理対象外
				return;
			}
			$parent.toggleClass('expand');
		});
	})(jQuery);
</script>

