  <script>
    function formatDate (dateString) {
      var d = new Date(dateString)
      return [
        d.getFullYear(),
        d.getMonth() + 1,
        d.getDate()
      ].join('/')
    }
    $.ajax({
      url: 'https://www.sakura.ad.jp/information/wp-json/news/v1/articles/?tag=rentalserver&per_page=3',
      success: function (data) {
        $.each(data, function (index, entry) {
          var time = formatDate(entry.date)
          var url = entry.link
          var title = entry.title.rendered
          var dt = '<dt class="news-time">' + time + '</dt>'
          var dd = '<dd class="news-title"><a class="link" href="' + url + '">' + title + '</a></dd>'
          $('.information-rs .news-list').append(dt + dd)
        })
      }
    })
    $.ajax({
      url: 'https://www.sakura.ne.jp/maint/api/v1/feeds/?type=minorinfo',
      success: function (data) {
        if (data.results.length > 0) {
          $('.information-addfunction').append(
            '<div class="information-addfunction-body">' +
              '<h2 class="hl">機能追加情報</h2>' +
              '<div class="article-block"><dl class="news-list"></dl></div>' +
            '</div>')

          $.each(data.results, function (index, entry) {
            var time = formatDate(Number(entry.date)*1000)
            var url = entry.url
            var title = entry.title
            var dt = '<dt class="news-time">' + time + '</dt>'
            var dd = '<dd class="news-title"><a class="link" href="' + url + '" target="_blank">' + title + '</a></dd>'
            $('.information-addfunction .news-list').append(dt + dd)
          })
        }
      }
    })
  </script>
