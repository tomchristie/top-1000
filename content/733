<script>
    var global_navi_crp_url = location.href;
    var global_navi_crp_script = document.createElement("script");

    if(global_navi_crp_url.indexOf('/biz/') != -1){
        global_navi_crp_script.src = '/js/common/o2o/contents/global_navi_crp.js';
        document.getElementById('global_navi_crp_script_area').appendChild(global_navi_crp_script);
        var fn_global_navi_crp = function(){
            dcmoto.contents.GlobalNavi.render();
        };
        setTimeout(fn_global_navi_crp, 1000);
    }else{
        global_navi_crp_script.src = '/js_osp/mydm/global_navi_crp.js';
        document.getElementById('global_navi_crp_script_area').appendChild(global_navi_crp_script);
    }
</script>

