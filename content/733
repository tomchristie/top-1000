<div id="global_error_script_area"></div>
<script>
    var $navArea     = $('#dcmoto_global_navi').find('.area-o2o');
    var navAreaStyle = $navArea.css('display');
    if(navAreaStyle == 'none'){
        $('#dcmoto_global_error').hide();
    }else{
        var global_error_url = location.href;
        var global_error_script = document.createElement("script");
        if(global_error_url.indexOf('/biz/') != -1){
            global_error_script.src = '/js/common/o2o/contents/global_error.js';
            document.getElementById('global_error_script_area').appendChild(global_error_script);
            var fn_global_error = function(){
                dcmoto.contents.GlobalError.render();
            };
            setTimeout(fn_global_error, 1000);
        }else{
            global_error_script.src = '/js_osp/mydm/global_error.js';
            document.getElementById('global_error_script_area').appendChild(global_error_script);
        }
    }
</script>

