						autocomplete(locationSelector, response, false);
					} else {
						autocomplete(locationSelector, [], false);
					}
				},
				error: function (jqXHR, txtStatus, txtError) {
					console.log('inside error');
				},
			});
		} else {
			autocomplete(locationSelector, [], false);
		}
