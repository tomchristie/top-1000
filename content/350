    var rxAuthHeader;
    var clientDetails = {
        client_id: 'e4e3f73a-0ceb-4d37-939e-90ddb1238360',
        secret_key: '8720f366-3769-47e9-8156-187b9c6583b5'
    }
    require(['webmd.oauth/1/webmd.oauth'], function (oAuth) {
        rxAuthHeader = oAuth.generate(clientDetails);
    });

    function closeRXLists(elmnt) {
        console.log(elmnt);
        var x = document.getElementsByClassName("rx-autocomplete-items");
        for (var i = 0; i < x.length; i++) {
            if (elmnt != x[i]) {
                x[i].parentNode.removeChild(x[i]);
            }
        }
    }

    document.addEventListener("click", function (e) {
        closeRXLists(e.target);
    });

    function navigatetoFirstDrug() {
        var firstDrug = document.querySelector('#rxTypeaheadInputrx-autocomplete-list .rx-item.rx-hoverable a');
        if (firstDrug) {
            window.location.href = firstDrug.getAttribute('href');
        }
    }

    function logClick(n, l){
        window.wmdPageLink('rx-srch-btn_'+n);
        window.location.href = l;
    }

    function rxAutocomplete(inp, arr) {
        var currentFocus;
        var a, b, i, val = inp.value;
        closeRXLists();
        if (!val) {
            return false;
        }
        currentFocus = -1;
        a = document.createElement("DIV");
        a.setAttribute("id", inp.id + "rx-autocomplete-list");
        a.setAttribute("class", "rx-autocomplete-items");
        inp.parentNode.appendChild(a);
        for (i = 0; i < arr.length; i++) {
            var nullcheckHtml = '';
            var urlRedirect = getEnvironment();
            var goto = `${urlRedirect.redirectURL}/rx/drug-prices/${arr[i].drug_seo_name}`;

            if (arr[i].drug_name && arr[i].drug_seo_name) {
                nullcheckHtml = arr[i].drug_name;
            }
            var optionhtml = `<a class="rx-drug-link" href="${goto}" onclick='logClick(${i+1}, "${goto}")'>${nullcheckHtml}</a>`;
            b = document.createElement("DIV");
            b.setAttribute('class', 'rx-item rx-hoverable')
            b.innerHTML = optionhtml;
            b.addEventListener("click", function (e) {
                closeRXLists();
            });
            a.appendChild(b);
        }
    }


    function makerxapicall(query) {
        // eslint-disable-next-line
        const rxsearchapi = `${getEnvironment().servicedomain}/search/2/api/rx/find/${encodeURIComponent(query)}?count=6&app_id=web`;
        return rxsearchapi;
    }

    function getEnvironment(domain) {
        let servicedomain = 'https://www.webmd.com';
        let redirectURL = 'https://www.webmd.com';
        let host;
        if (typeof window !== 'undefined') {
            host = window.location.host.toLowerCase();
            servicedomain = 'https://www.webmd.com';
            if (host.indexOf('.staging') > 0) {
                servicedomain = 'https://www.webmd.com';
                redirectURL = 'https://www.staging.webmd.com';
            } else {
                servicedomain = 'https://www.webmd.com';
                redirectURL = 'https://www.webmd.com';
            }
        } else if (domain !== '') {
            host = domain.toLowerCase();
            servicedomain = 'https://www.webmd.com';
            if (host === 'staging') {
                servicedomain = 'https://www.webmd.com';
            } else {
                servicedomain = 'https://www.webmd.com';
            }
        } else {
            servicedomain = 'https://www.webmd.com';
        }
        return {
            servicedomain,
            redirectURL,
        };
    }



    rxInputSelector.addEventListener("keyup", function (e) {
        var queryUrl = makerxapicall(rxInputSelector.value);
        $.ajax({
            method: 'GET',
            url: queryUrl,
            headers: rxAuthHeader,
            dataType: "json",
            "async": true,
            success: function (data, textStatus, jqXHR) {
                if (data.data && data.data.rxsearch && data.data.rxsearch.data) {
                    var response = data.data.rxsearch.data;
                    rxAutocomplete(rxInputSelector, response);
                }
            },
            error: function (jqXHR, txtStatus, txtError) {
                console.log('rx typeahead error');
            },
        });
    });</script></div><div id="ContentPane31" class="pane" tabindex="-1"><div class="topSearchTerms module" id="lln-topsrch-hp" data-metrics-module="lln-topsrch-hp">
