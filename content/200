                    </hp-input>
                    <p v-if="passwordLink.text" aria-label="reg"><a v-text="passwordLink.text" tabindex="-1" href="https://passwort.web.de/" :href="passwordLink.href">Passwort vergessen?</a></p>
                </div>

                <hp-button inline-template="" data-importance="primary">
                    <button data-component="button" type="submit" data-size="l" data-importance="primary">
                        <slot>Login</slot>
                    </button>
                </hp-button>
            </form>

