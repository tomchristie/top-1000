            const duckMatch = ua.match(/DuckDuckGo\/(\d+)/i)
            const ucMatch = ua.match(/UCBrowser\/(\d+)/i)
            const facebookMatch = ua.match(/FBAV\/(\d+)/i)

            if (operaMatch != null) {
              return {
                name: operaMatch[1].replace('OPR', 'Opera'),
                version: operaMatch[2],
                isMobile: isMobile,
              }
            }

            if (duckMatch != null) {
              return {
                name: duckMatch[0].split('/')[0],
                version: duckMatch[1],
                isMobile: isMobile,
              }
            }

            if (ucMatch != null) {
              return {
                name: ucMatch[0].split('/')[0],
                version: ucMatch[1].split('.')[0],
                isMobile: isMobile,
              }
            }

            if (facebookMatch != null) {
              return {
