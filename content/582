    <script type="text/javascript">
    window.log = function() {
        log.history = log.history || [];
        log.history.push(arguments);
        if (this.console && window.location.search.indexOf("showlog") > -1) {
            console.log(Array.prototype.slice.call(arguments));
        }
    };
    /**
     * [Sym  An object has all generic helper functions that can be used across different applications]
     * @type {Object}
     */
    if (typeof Sym === "undefined") {
        var Sym = {};
    }
    /**
     * [Sym.Misc An Object that has a function that finds page load time]
     * @parent Sym
     * @type {object}
     */
    Sym.Misc = {
        //Timestamp that page started to load
        beforeLoad: (new Date()).getTime(),
        //Find page load time
        pageloadingtime: function() {
            try {
                var afterload = (new Date()).getTime();
                log("beforeload:", Sym.Misc.beforeLoad);
                log("afterload:", afterload);
                var seconds = Math.round(((afterload - Sym.Misc.beforeLoad) / 1000) * Math.pow(10, 3)) / Math.pow(10, 3);
                return seconds;
            } catch (e) {
                log("exception_pageloadingtime", e);
            }
        }
    };
    /**
     * [Sym.CookieSetter An Object that makes servelet call and cookies set in the response]
     * @parent Sym
     * @type {object}
     */
    Sym.CookieSetter = {
        url: "/service/norton/head",
        xhrTimeout: {},
        setCookies: function() {
            try {
                log("setCookies:");
                //set norton.com cookies
                if (window.XMLHttpRequest) {
                    // code for IE7+, Firefox, Chrome, Opera, Safari
                    xmlhttp = new XMLHttpRequest();
                } else {
                    // code for IE6, IE5
                    xmlhttp = new ActiveXObject("Microsoft.XMLHTTP");
                }
                //async = false so we know cookie values are ready
                //invokes com.norton.servlet.NortonCookieServlet
                xmlhttp.open("GET", this.url, false);
                xmlhttp.onreadystatechange = function() {
                    if (xmlhttp.readyState === 4 && xmlhttp.status === 200) {
                        clearTimeout(this.xhrTimeout);
                    }
                }
                xmlhttp.send();
                this.xhrTimeout = setTimeout(function() {
                    Sym.CookieSetter.ajaxTimeout(xmlhttp);
                }, 5000);
            } catch (e) {
                log("exception_setcookies", e);
            }
        },
        init: function(country, language) {
            this.url = this.url + "?ct=" + country + "&lg=" + language + "&ref=" + encodeURIComponent(document.referrer) + "&" + window.location.search.substr(1);
        },
        ajaxTimeout: function(xhr) {
            xhr.abort();
        }
    };
    /**
     * [Sym.CookieParse An Object that has functions that are used to read cookie,subcookies,decode,encode,set traffic source, getdays]
     * @parent Sym
     * @type {object}
     */
    Sym.CookieParse = {
        /**
         * [encodeToHex encodes a given ASCII string to hexadecimal string]
         * @param  str - The string to encode to hexadecimal
         * @return - Returns the hexadecimal value of the given ASCII string
         */
        encodeToHex: function(str) { //encodestohex
            try {
                log("encodeToHex:", arguments);
                var text2 = "";
                for (i = 0; i < str.length; i++) {
                    text2 += str.charCodeAt(i).toString(16);
                }
                return text2;
            } catch (e) {
                log("exception_encodetohex", e);
            }
        },
        /**
         * [setCookie sets a cookie with the given name,value,millsec]
         * @param  name - Name of the cookie
         * @param  value - Value of the cookie
         * @param  millisec - expiration time of the cookie in millisecs
         */
        setCookie: function(name, value, millisec) {
            try {
                log("setCookie:", arguments);
                value = this.encodeToHex(value);
                var expires = "";
                var path = "/";
                var domain = ".norton.com";
                if (millisec) {
                    var date = new Date();
                    date.setTime(date.getTime() + (millisec));
                    expires = "; expires=" + date.toGMTString();
                }
                var cookie_string = name + "=" + escape(value) + expires;
                if (path) {
                    cookie_string += "; path=" + escape(path);
                }
                if (domain) {
                    cookie_string += "; domain=" + escape(domain);
                }
                cookie_string += "; secure";
                log("cookie string:", cookie_string);
                document.cookie = cookie_string;
            } catch (e) {
                log("exception_setcookie", e);
            }
        },
        /**
         * TODO Rewrite with best practices in 6.5
         * @param name
         * @param value
         * @param millisec
         * @param domain
         */
        setCookieWithOutDomain: function(name, value, millisec) {
            try {
                log("setCookieWithOutDomain:", arguments);
                value = this.encodeToHex(value);
                var expires = "";
                var path = "/";
                if (millisec) {
                    var date = new Date();
                    date.setTime(date.getTime() + (millisec));
                    expires = "; expires=" + date.toGMTString();
                }
                var cookie_string = name + "=" + escape(value) + expires;
                if (path) {
                    cookie_string += "; path=" + escape(path);
                }
                cookie_string += "; secure";
                log("setCookieWithOutDomain string:", cookie_string);
                document.cookie = cookie_string;
            } catch (e) {
                log("exception_setCookieWithOutDomain", e);
            }
        },
        /**
         * [readSubCookie gets value of the given subcookie from given cookie]
         * @param cname - Name of the cookie
         * @param key   - Name of the subcookie
         * @return - Returns the given subcookie from given cookie
         */
        readSubCookie: function(cname, key) {
            try {
                log("readSubCookie:", arguments);
                if (typeof key !== "undefined" && typeof cname !== "undefined") {
                    var cookieValHEx = this.readCookie(cname);
                    var cookieVal = this.hexDecode(cookieValHEx);
                    log("cookieVal:", cookieVal);
                    var subCookie;
                    if (cookieVal) {
                        subCookie = cookieVal.split("|");
                    }
                    if (subCookie) {
                        for (var j = 0; j < subCookie.length; j++) {
                            var subCookieKeyValPair = subCookie[j];
                            var subCookieVal = subCookieKeyValPair.split("=");
                            if (subCookieVal[0].replace(/^\s+|\s+$/g, "") == key.replace(/^\s+|\s+$/g, "")) {
                                return unescape(subCookieVal[1]);
                            }
                        }
                    }
                }
            } catch (e) {
                log("exception_subcookie", e);
            }
        },
        /**
         * [hexDecode decodes a given hexadecimal string to ASCII string]
         * @param data  -  The string to decode to ASCII
         * @return - Returns the ASCII value of the given HexaDecimal value
         */
        hexDecode: function(data) {
            try {
                log("hexDecode", arguments);
                var b16_digits = "0123456789abcdef";
                var b16_map = new Array();
                for (var i = 0; i < 256; i++) {
                    b16_map[b16_digits.charAt(i >> 4) + b16_digits.charAt(i & 15)] = String.fromCharCode(i);
                }
                if (data) {
                    if (!data.match(/^[a-f0-9]*$/i)) return false;

                    if (data.length % 2) data = "0" + data;

                    var result = new Array();
                    var j = 0;
                    for (var i = 0; i < data.length; i += 2) {
                        result[j++] = b16_map[data.substr(i, 2)];
                    }
                    return result.join("");
                }
            } catch (e) {
                log("exception_hexDecode", e);
            }
        },
        /**
         * [readCookie reads the given cookie]
         * @param name - Name of the cookie
         * @return - Returns the value of the given cookie
         */
        readCookie: function(name) {
            try {
                log("readCookie", arguments);
                var name = name + "=";
                var ca = document.cookie.split(";");
                for (var i = 0; i < ca.length; i++) {
                    var c = ca[i];
                    while (c.charAt(0) == " ") {
                        c = c.substring(1, c.length);
                    }
                    if (c.indexOf(name) == 0) {
                        return unescape(c.substring(name.length, c.length));
                    }
                }
                return null;
            } catch (e) {
                log("exception_readCookie", e);
            }
        },
        /**
         * [formatDate formats the given date from MON-DD-YYYY to MON DD, YYYY ]
         * @param input  - Date in the format MON-DD-YYYY(e.g:Mar-27-2014 14:10:08)
         * @return returns the date in the format MON DD, YYYY(e.g.: Mar 27, 2014 14:10:08)
         */
        formatDate: function(input) {
            try {
                log("formatDate:", arguments);
                var dt = input.replace(
                    /^(\w+)-(\d+)/, "$2-$1"
                ).split("-");
                var jd = dt[1] + " " + dt[0] + ", " + dt[2];
                log("formatDate:", jd);
                return jd;
            } catch (e) {
                log("exception_formatDate", e);
            }
        },
        /**
         * [getDays gets the no of days between two given dates]
         * @param d1 Date in the format of MON DD, YYYY
         * @param d2 Date in the format of  MON DD, YYYY
         * @return  returns the no of days between two given dates
         */
        getDays: function(d1, d2) {
            try {
                log("getDays:", arguments);
                if (typeof(d1) !== "undefined" && typeof(d2) !== "undefined") {
                    var fdate = new Date(this.formatDate(d1));
                    log("fdate:", fdate);
                    var ldate = new Date(this.formatDate(d2));
                    log("ldate:", ldate);
                    return days = (ldate - fdate) / 1000 / 60 / 60 / 24;
                } else {
                    return null;
                }
            } catch (e) {
                log("exception_formatDate", e);
            }
        },
        /**
         * [setTRS returns the traffic source using tp,TRS cookie and Program Type cookies]
         */
        setTRS: function() {
            try {
                var programType = this.readCookie('PROGRAM_TYPE') ? this.readCookie('PROGRAM_TYPE').toLowerCase() : '';
                isProgramType = programType.indexOf('paidsearch') > -1,
                    isSeoCookie = this.readSubCookie('tp', 'TRS') ? this.readSubCookie('tp', 'TRS').indexOf('seo_norton') > -1 : false,
                    isAffiliates = (nortonAnalytics !== undefined && nortonAnalytics.site_section ? nortonAnalytics.site_section.indexOf('affiliates') > -1 : false) || (programType.indexOf('cj') > -1 || programType.indexOf('impactradius') > -1),
                    currentLocalStorage = localStorage.getItem('trfsrc');
                if (isSeoCookie) {
                    localStorage.setItem('trfsrc', 'seo_norton');
                    return 'seo_norton'
                } else if (isAffiliates) {
                    localStorage.setItem('trfsrc', 'affiliates');
                    return 'affiliates'
                } else if (isProgramType) {
                    localStorage.setItem('trfsrc', 'paidsearch');
                    return 'paidsearch';
                } else  if (currentLocalStorage) {
                    return localStorage.getItem('trfsrc');
                } else {
                    localStorage.removeItem('trfsrc');
                    return 'direct';
                }
            } catch (e) {
                log("exception_setTRS", e);
            }
        },
        /**
         * [isFirstTimeVisitor description]
         * @return {Boolean} [description]
         */
        isFirstTimeVisitor: function() {
            try {
                log("isFirstTimeVisitor:", arguments);
                if (typeof(this.readSubCookie("es", "FCD")) !== "undefined" && typeof(this.readSubCookie("es", "LCD")) !== "undefined") {
                    if (Sym.CookieParse.getDays(Sym.CookieParse.readSubCookie("es", "FCD"), Sym.CookieParse.readSubCookie("es", "LCD")) === 0) {
                        return "YES";
                    } else {
                        return "NO";
                    }
                }
            } catch (e) {
                log("exception_isfirsttimevisitor", e);
            }
        },
        /**
         * [setAndgetTCG sets or reads TCG sub cookie in ttControl cookie and returns it]
         */
        setAndgetTCG: function() {
            try {
                log("setTCG:", arguments);
                var tcgSubCookieExisting = this.readSubCookie("ttControl", "TCG");
                if(tcgSubCookieExisting){
                    return tcgSubCookieExisting;
                }
                else{
                    var randomGroup = Math.floor((Math.random() * 10) + 1);
                    var randProfile = "TCG=" + randomGroup;
                    this.setCookie("ttControl",randProfile,5184000000);
                    return randomGroup;
                }
            } catch (e) {
                log("exception_setTCG", e);
            }
        }
    };
    Sym.Utils = {

        getlastSegmentOfURL: function() {
            try {
                var URL = window.location.pathname;
                return URL.substring(URL.lastIndexOf('/') + 1, URL.lastIndexOf('.'));
            } catch (e) {
                log("exception_getlastSegmentOfURL", e);
            }
        },
        /**
         * [readQueryParam gets the value of the given query string parameter name ]
         * @param key - querystring paramater
         * @return- returns the value of query string parameter name key (1st arg)
         */
        readQueryParam: function(key) {
            try {
                log("readQueryParam:", arguments);
                return unescape(window.location.search.replace(new RegExp("^(?:.*[&\\?]" + escape(key).replace(/[\.\+\*]/g, "\\$&") + "(?:\\=([^&]*))?)?.*$", "i"), "$1"));
            } catch (e) {
                log("exception_readQueryParam", e);
            }
        },
        /**
         * [readEstoreQueryParam gets the value of given query string parameter name that exists in between two slashes. eStore now removes all query params, but you can still add variables between slashes e.g. /test=1/]
         * @param key - querystring paramater
         * @return- returns the value of query string parameter name
         */
        readEstoreQueryParam: function(key) {
            try {
                log("readEstoreQueryParam:", arguments);
                var u = window.location.pathname.split("/");
                //find entries in name value pair format
                for (var i = 0, l = u.length; i < l; i++) {
                    if (u[i].indexOf("=") > -1) {
                        var uKey = u[i].split("=")[0],
                            value = u[i].split("=")[1];
                        if (uKey === key) {
                            return value;
                        }
                    }
                }
            } catch (e) {
                log("exception_readEstoreQueryParam", e)
            }
        },
        /**
         * [getStringParamVal gets the value of the given querystring parameter name from any given URI string]
         * @param p - name of querystring parameter
         * @param q - any URI string
         * @return - returns the value of the given querystring parameter name from any given URI string
         */
        getStringParamVal: function(p, q) { //Returns value of a parameter name key (1st arg) of any URI-like string (2nd arg)
            try {
                log("getStringParamVal:", arguments);
                q = q || location.search;
                var re = new RegExp("&" + p + "=([^&]*)", "i");
                return (q = q.replace(/^\?/, "&").match(re)) ? q = q[1] : q = "";
            } catch (e) {
                log("exception_StringParam", e);
            }
        },
        /**
         * [removeParameter removes any given querystring parameter from the given url]
         * @param url - url from which the given parameter to be removed
         * @param parameter - querystring parameter that should be removed
         * @return - returns the url with out the given the parameter.
         */
        removeParameter: function(url, parameter) {
            try {
                log("removeParameter:", arguments);
                var fragment = url.split("#");
                var urlparts = fragment[0].split("?");

                if (urlparts.length >= 2) {
                    var urlBase = urlparts.shift(); //get first part, and remove from array
                    var queryString = urlparts.join("?"); //join it back up

                    var prefix = encodeURIComponent(parameter) + "=";
                    var pars = queryString.split(/[&;]/g);
                    for (var i = pars.length; i-- > 0;) { //reverse iteration
                        if (pars[i].lastIndexOf(prefix, 0) !== -1) { //Similar to string.startsWith
                            pars.splice(i, 1);
                        }
                    }
                    url = urlBase + "?" + pars.join("&");
                    if (fragment[1]) {
                        url += "#" + fragment[1];
                    }
                }
                return url;
            } catch (e) {
                log("exception_removeParameter", e);
            }
        },
        /**
         * [getCustomVariable Returns value of a given variable from url query param or ae or tp or es cookies ]
         * @param name Variable name
         * @return Returns value of a given variable from url query param or ae or tp or es cookies
         */
        getCustomVariable: function(name) {
            try {
                log("getCustomVariable:", arguments);
                var retVal = "";
                var incomingUrlCookie = (typeof(Sym.CookieParse.readCookie("ae")) !== "object") ? Sym.CookieParse.hexDecode(Sym.CookieParse.readCookie("ae")) : "missing";
                retVal = Sym.Utils.readQueryParam(name) || Sym.Utils.getStringParamVal(name, incomingUrlCookie) || Sym.CookieParse.readSubCookie("tp", name) || Sym.CookieParse.readSubCookie("es", name) || Sym.CookieParse.readCookie(name) || "missing";
                log("readQueryParam:", Sym.Utils.readQueryParam(name));
                log("getStringParamVal:", Sym.Utils.getStringParamVal(name, incomingUrlCookie));
                log("readsubcookiefrom tp:", Sym.CookieParse.readSubCookie("tp", name));
                log("readsubcookiefrom es:", Sym.CookieParse.readSubCookie("es", name));
                log("readcookie:", Sym.CookieParse.readCookie(name));
                return (retVal.toLowerCase());
            } catch (e) {
                log("exception_getcustomvariable", e);
            }
        },
        /**
         * [appendQueryParams returns url variable with all query string parameters from the current page URL appended to the variable.]
         * @param {Object} url
         */
        appendQueryParams: function(url) {
            try {
                log("appendQueryParams:", arguments);
                if (location.search.length > 0) {
                    if (url.indexOf('?') > -1) {
                        var newurl = url + location.search.replace('?', '&');
                    } else {
                        var newurl = url + location.search;
                    }
                    return newurl;
                }
                return url;
            } catch (e) {
                log("exception_appendqueryparams", e);
            }
        },
        /**
         * [loadOnReferrerOnly allows landing page to load if referring URL is in list of sites variables.  If not on the list, it will redirect back to the norton.com homepage]
         * @param {Object} sites
         */
        loadOnReferrerOnly: function(sites) {
            try {
                log("loadOnReferrerOnly:", arguments);
                var allow = false;
                for (var i = 0; i < arguments.length; i++) {
                    if (document.referrer.indexOf(arguments[i]) > -1) {
                        allow = true;
                    }
                }

                if (!allow) {
                    if (redirectURL) {
                        var redirectURL_enc = encodeURIComponent(redirectURL);
                        var redirectURL_dec = decodeURIComponent(redirectURL_enc);
                        window.location = redirectURL_dec;
                    } else {
                        //redirect to Norton.com home page
                        window.location = "http://www.norton.com";
                    }
                }

            } catch (e) {
                log("exception_loadonreferreronly", e);
            }

        },
        /**
         * [loadOnTrafficSourceOnly allows landing page to load if referring URL is in list of sites variables.  If not on the list, it will redirect back to the norton.com homepage]
         * @param {Object} sites
         */
        loadOnTrafficSourceOnly: function(trafficSource) {
            try {
                log("loadOnTrafficSourceOnly:", arguments);
                var allow = false;
                for (var i = 0; i < arguments.length; i++) {
                    if (nortonAnalytics.traffic_source.indexOf(arguments[i]) > -1) {
                        allow = true;
                    }
                }

                if (!allow) {
                    if (redirectURL) {
                        var redirectURL_enc = encodeURIComponent(redirectURL);
                        var redirectURL_dec = decodeURIComponent(redirectURL_enc);
                        window.location = redirectURL_dec;
                    } else {
                        //redirect to Norton.com home page
                        window.location = "http://www.norton.com";
                    }
                }
            } catch (e) {
                log("exception_loadOnTrafficSourceOnly", e);
            }
        },
        /**
         * [loadOnPartnerVerified allows landing page to load if timestamp and shared key are verified.  If not verified, it will redirect back to the norton.com homepage or the specified redirect URL]
         * @param name
         */
        loadOnPartnerVerified: function(partnerName) {
            try {
                log("loadOnPartnerVerified:", arguments);
                var allowAccess = false;
                if (partnerName == 'ee'){
                    var preSharedKey = 'a781dcec543f7d085146e60b1262bff2';
                    var partnerTimestamp = Sym.Utils.readQueryParam('ts');
                    var partnerKey = Sym.Utils.readQueryParam('key');
                    if (partnerKey == MD5(partnerTimestamp + preSharedKey)) {
                        // page request time in seconds
                        var pageRequestTime = Math.round((new Date()).getTime() / 1000);
                        var timeDifference = pageRequestTime - partnerTimestamp;
                        // 15 min in seconds: 900, difference less than 15 min
                        if (timeDifference < 900) {
                            allowAccess = true;
                        }
                    }
                    if (!allowAccess) {
                        if (redirectURL) {
                            var redirectURL_enc = encodeURIComponent(redirectURL);
                            var redirectURL_dec = decodeURIComponent(redirectURL_enc);
                            window.location = redirectURL_dec;
                        } else {
                            //redirect to Norton.com home page
                            window.location = 'http://www.norton.com';
                        }
                    }
                }
            } catch (e) {
                log('exception_loadonpartnerverified', e);
            }
        }
    };
    Sym.FeatureFlags = {
        isLive: function(featureName) {
            if (typeof globalVariables !== "undefined") {
                //feature flags function
                if (globalVariables.getFeatureFlags() != null && (globalVariables.getFeatureFlags().indexOf(featureName) > -1)) {
                    return true;
                } else {
                    return false;
                }
            }
        }
    };
</script>


    

    



    
