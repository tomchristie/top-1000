    var globalVariables = {
        FEATURE_FLAGS : 'OPS-318143,OPS-320873,OPS-330047,OPS-329505,OPS-327957,OPS-337362,CLP-OPS-338000',
        getFeatureFlags : function(){
            return this.FEATURE_FLAGS;
        }
    }
</script>

    






