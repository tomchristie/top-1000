<script data-pagespeed-no-defer pagespeed_no_defer>
        window.favoritesReady = new Promise(function (resolve) {
            document.addEventListener('FAVORITES:READY', function (event) {
                if (event.type === 'FAVORITES:READY') {
                    resolve();
                };
            });
            var script = document.createElement('script');
            var ref = document.getElementsByTagName('script')[0];
            // loads async by default - https://developer.mozilla.org/en-US/docs/Web/HTML/Element/script#Attributes
            script.src = "/assets/watchlist/watchlist-client-javascripts/1f1c6b097c14ba54475a295fa94a7ad1-watchlist-client-global.min.js";
            ref.parentNode.insertBefore(script, ref);
        });
        // backwards compatibility alias
        window.watchlistReady = window.favoritesReady
</script>
