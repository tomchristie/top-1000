<script type="text/javascript" src="/jp/ja/commonY20/js_sites/slider_control.js?js_param=u20113"></script>
<script type="text/javascript">
(function(){

  $('#JS_bottomBanner').find('.JS_grounding')[0].style.display='none';
  $('#JS_bottomBanner').find('.JS_rtoaster')[0].style.display='none';

	JLJS_RecommendControl.addListCallbacks('BOTTOM_BANNER',function(){
		var $buttomBanner = $('#JS_bottomBanner');
					var isRTContents = false;
					$buttomBanner.find('.JS_rtContents').each( function( index, element ) {
							 element.parentNode.style.display='block';
							 isRTContents  = true;
					});
					if(isRTContents){
								 $buttomBanner.find('.JS_grounding')[0].style.display='none';
								 $buttomBanner.find('.JS_rtoaster')[0].style.display='block';
					}else{
								 $buttomBanner.find('.JS_grounding')[0].style.display='block';
								 $buttomBanner.find('.JS_rtoaster')[0].style.display='none';
		}
	});
})();
$(function(){
	$('.link-txt').on('click', function(e){
		e.stopPropagation();
	});
});
//--></script>

</div>

<!-- /freehtml end -->


    
</div>


            
    
