    var ebates = ebates || {}; ebates.onboard_screen_name = 'Welcome Onboarding';
    document.cookie = "onboard_screen_name=Welcome Onboarding";
    window.addEventListener('DOMContentLoaded', function () {
        $('.bottomCta').on('click', function () {
            window.scroll(0,0);
        });
        var trafficSource = '';
        if (ebates.param.get('utm_medium') === 'video' || ebates.param.get('utm_source') === 'video') {
            trafficSource = 'Paid';
        } else if (ebates.param.get('utm_medium') === 'sem' || ebates.param.get('utm_channel') === 'sem') {
            trafficSource = 'SEM Branded';
        } else if (!ebates.param.get('utm_medium')) {
            trafficSource = 'Direct';
        }
        document.dispatchEvent(new CustomEvent('stats-event', {detail: {map: {amp: {
                        eventType: 'Visit Onboarding',
                        eventProps: { screen_name: 'Web Holiday SU Onboarding Homepage', traffic: trafficSource }
                    }}}}));
        $(document).on('home-inlinesurecaptcha-cb',function () {
            $('.welcome').addClass('with-captcha');
        });
        $('.user.logged-out .btn-join .show-join-overlay').attr('data-href','/ajax/su-revamp.htm');
        $('.user.logged-out .btn-sign-in .show-join-overlay').attr('data-href','/ajax/si-revamp.htm');
    });
