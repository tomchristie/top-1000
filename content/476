        <script type="text/javascript" async>
            function matchTarget(array, layout, str, exceptions) {
                if(layout){
                    return array.reduce(function(previous, item) {
                        var regexp = new RegExp(item, 'i');
                        //console.log("reg exp ?",regexp instanceof RegExp);
                        if(exceptions.find(function(elem){ return layout === elem; })){
                            previous = str + "article2";
                        }else{
                            if (layout.match(regexp)) {
                                previous = str + item.toLowerCase();
                            } else if (!previous) {
                                previous = str  + layout.slice(0, -5).toLowerCase();
                            }
                        }
                        if(sousType === 'media'){
                            previous = str + "articlemedia";
                        }
                        return previous;
                    }, '');
                }else{
                    return layout =  str + 'default';
                }
            }
            var gabaritPage = matchTarget(["Article", "Live"], currentLayout, "page=", ["ArticleComparatif.twig", "Articlelive.twig", "ArticleTest.twig"]);

            var sas = sas || {};
            sas.cmd = sas.cmd || [];

            // You initialized the Smart setup:
            sas.cmd.push(function() {
                // We MUST use theese settings.
                // renderMode: 2 will fail
                sas.setup({ networkid: 13, domain: 'https://ww13.smartadserver.com', async: true, insequence:true, renderMode:2  });
            });



            var smart_stringIds = '95,98,289,291,379,1972,2649,2650,2651,2652,6457,6458,6459,7330,16079,16232,25486,25487,28022,29974,29992,29993,29995,29996,29997,30385,30386,33361,33362,33363,33364,33365,34288,34289,35537,38373,41801,41802,41803,44563,44564,44565,44566';
            var smart_tabIds =  smart_stringIds.split(',');

            var smart_tabIdsObject = [];
            for (i=0; i<smart_tabIds.length;i++){
                var object = new Object();
                object.id = smart_tabIds[i];
                smart_tabIdsObject.push(object);
            }

            // Set a "onecall" call:
            sas.cmd.push(function() {
                // This command will called on every sas.render(id).
               sas.call("onecall", {
                            siteId:7270,
                            pageId:45414,
                            formats: smart_tabIdsObject,
                            target:'' + gabaritPage + ';'
               });
               sas.render();
            });

        </script>
                    

<script type="text/javascript">
        var isMobile = {
        Android: function() {
            return navigator.userAgent.match(/Android/i);
        },
        BlackBerry: function() {
            return navigator.userAgent.match(/BlackBerry/i);
        },
        iPhone: function() {
            return navigator.userAgent.match(/iPhone|iPod/i);
        },
        iOS: function() {
            return navigator.userAgent.match(/iPhone|iPad|iPod/i);
        },
        Opera: function() {
            return navigator.userAgent.match(/Opera Mini/i);
        },
        Windows: function() {
            return navigator.userAgent.match(/IEMobile/i);
        },
        any: function() {
            return (isMobile.Android() || isMobile.BlackBerry() || isMobile.iOS() || isMobile.Opera() || isMobile.Windows());
        }
    };

    
    
    if (!isMobile.any() ){
        $(document).ready(function(){

            var flag="true";
            // full responsive if mediaplayer
                        // full responsive if mediaplayer 01net
            if( $("#mediaplayer.site01net").length > 0 || $("body#mediaplayer").length > 0){
                flag = "false";
            }
            if( flag == "true"){
                // Delete responsive functions
                // We remove all Col-xx and replace by col-xs
                for (i = 1 ; i <13; i++){
                    // $(".col-sm-" + i).removeClass("col-sm-" + i);
                    // $(".col-xs-" + i).removeClass("col-xs-" + i);
                    // $(".col-md-" + i).addClass("col-xs-" + i).removeClass("col-md-" + i);

                    // modification de la regle pour qu'elle ne s'applique pas au nouveau <header>
                    $("#wrap .col-sm-" + i).removeClass("col-sm-" + i);
                    $("#wrap .col-xs-" + i).removeClass("col-xs-" + i);
                    $("#wrap .col-md-" + i).addClass("col-xs-" + i).removeClass("col-md-" + i);
                }

                // We delete responsive CSS
                $('#bfm-responsive').remove();

                css_link = '<link rel="stylesheet" type="text/css" href="https://static.bfmtv.com/ressources/css/bfm-no-responsiveness.css?v=2018-9-24-13-47" />';
                $('head').append(css_link);
            }
        });
    }
    
</script>

