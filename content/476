    <script type='didomi/javascript' data-vendor='c:chartbeat'>

                $charbeatdomain = '01net.com';
        
        $chartbeatSection = 'site01net::accueil';

        if($chartbeatSection  == 'ACCUEIL' || $chartbeatSection  == 'bfmbusiness::accueil' || $chartbeatSection  == 'rmc::accueil' || $chartbeatSection  == 'rmcsport::accueil' || $chartbeatSection  == 'site01net::accueil'){
            $chartbeatCategory = 'Home';
        }else if($chartbeatSection  == 'bfmtv::accueil-4::villes::paris'){
            $chartbeatCategory = 'Home BFM Paris';
        }else{
            $chartbeatPosition = $chartbeatSection.indexOf("::");
            $chartbeatBrand = $chartbeatSection.replace($chartbeatSection.substr(0, $chartbeatPosition)+'::', '');

            if($chartbeatBrand.includes('::')){
                $chartbeatPosition = $chartbeatBrand.indexOf("::");
                $chartbeatCategory = $chartbeatBrand.substr(0, $chartbeatPosition);

                            }else{
                if($chartbeatSection  == 'people::accueil'){
                    $chartbeatCategory =   'home people';
                }else if($chartbeatSection  == 'auto::accueil') {
                    $chartbeatCategory = 'home auto';
                }else {
                                        $chartbeatCategory =   'home ' + $chartbeatBrand;
                                    }
            }
        }



        (function() {
            /** CONFIGURATION START **/
            var _sf_async_config = window._sf_async_config = (window._sf_async_config || {});

            _sf_async_config.uid = 121042;
            _sf_async_config.domain = $charbeatdomain;
            _sf_async_config.flickerControl = false;
            _sf_async_config.useCanonical = true;
            _sf_async_config.useCanonicalDomain = true;
            _sf_async_config.sections = $chartbeatCategory;
            _sf_async_config.authors = '';

            /** CONFIGURATION END **/
            function loadChartbeat() {
                var e = document.createElement('script');
                var n = document.getElementsByTagName('script')[0];
                e.type = 'text/javascript';
                e.async = true;
                e.src = '//static.chartbeat.com/js/chartbeat.js';
                n.parentNode.insertBefore(e, n);
            }
            loadChartbeat();
        })();
    </script>
    

