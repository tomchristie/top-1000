    <script>

	function test_do_track_me_mail(){
		var l=document.getElementById('hd_mail_ico');
		if(document.body.className.indexOf("hp_mobile")>-1)l.target="_self";
		var toto=false;
		var tgt=0;
		if(typeof(l.target)!='undefined')if(l.target=='_blank')tgt=1;
		function do_load(){
			if(toto)clearTimeout(toto);
			var oldh=l.onclick;
			l.onclick="";
			l.click();
			setTimeout(function(){l.onclick=oldh},1000);
		}
		if(l){
			var u="https://evnt.iol.it/v1?&ck=wwwtest.virgilio.it%2Chp%2C%2C%2Cmail.virgilio.it_%2Chp%2Cheader%2Cautenticazione%2Cmail%2Cmail.virgilio.it%2Chttp%253A//mail.virgilio.it/login/%253Fa2_chId%253DLVHPTOP%2CA%2Cmail.virgilio.it_%2C1000%2C%2C8141&nc=";
			var c=l.className;
			if (c.indexOf('logd')>=0) u="https://evnt.iol.it/v1?&ck=wwwtest.virgilio.it%2Chp%2C%2C%2Cwebmail.virgilio.it_%2Chp%2Cheader%2Cautenticazione%2Cmail%2Cwebmail.virgilio.it%2Chttp%253A//mail.virgilio.it/login/%253Fa2_chId%253DLVHPTOP%2CA%2Cwebmail.virgilio.it_%2C1000%2C%2C8141&nc=";
			var i=new Image();
			if (tgt==0){
				i.onload=do_load;
				i.onerror=do_load;
				toto=setTimeout(do_load,5000);
			}
			i.src=u+new Date().getTime();
			if(tgt==1)do_load();
			return false
		}
	}
	function logError(a,b){}

	function test_do_track_me_search(){
		var l=document.body;
		var toto=false;
		var tgt=0;
		if(typeof(document.getElementById('search').target)!='undefined')if(document.getElementById('search').target=='_blank')tgt=1;
		function do_load(){
			if(toto)clearTimeout(toto);
			var oldh=document.getElementById('search').onsubmit;
			document.getElementById('search').onsubmit="";
			document.getElementById('search').submit();
			setTimeout(function(){document.getElementById('search').onsubmit=oldh;document.getElementById('ricerca').value='';document.getElementById('hd_sugg').className=''},1000);
		}
		if(l){
			var u="https://evnt.iol.it/v1?&ck=wwwtest.virgilio.it%2Chp%2C%2C%2C%2Chp%2Cheader%2Csearch%2Ccerca%2C%2C%2CA%2C%2C1000%2C%2C8858&nc=";
			var c=l.className;
			if (c.indexOf('header_fixed')>=0) u="https://evnt.iol.it/v1?&ck=wwwtest.virgilio.it%2Chp%2C%2C%2C%2Chp%2Cheader%2Csearch%2Ccerca%2C%2C%2CA%2C%2C1000%2C%2C46281&nc=";
			var i=new Image();
			if (tgt==0){
				i.onload=do_load;
				i.onerror=do_load;
				toto=setTimeout(do_load,5000);
			}
			i.src=u+new Date().getTime();
			if(tgt==1)do_load();
			return false;
		}
	}

	function sugg01_submit(i,s){
		test_do_track_me_search();
		return false;
	}

	function lens_click(){
		window.scrollTo(0,0);
		document.getElementById('ricerca').focus();
	}
</script>
