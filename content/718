      <script type="text/javascript" xml:space="preserve">
        //<![CDATA[
        var oSearchForm = document.getElementById('searchform');
        var oSearchImg = document.getElementById('searchimg');
        var oSearchTxt = document.getElementById('searchtxt');
        oSearchImg.onclick = function (){
          if(oSearchTxt.value != ''){


            if($('.select_showbox')[0].innerHTML == 'Site Search'){
              oSearchForm.action = 'http://newssearch.chinadaily.com.cn/en/search?query='+oSearchTxt.value;
              oSearchForm.target = '_blank';
              oSearchForm.submit();
            }
            else if($('.select_showbox')[0].innerHTML == 'Sogou Search'){
              oSearchForm.action = 'https://english.sogou.com/english?query=' + oSearchTxt.value + '&amp;b_o_e=1&amp;ie=utf8 ';
              oSearchForm.target = '_blank';
              oSearchForm.submit();
            }
          }
          else{
            alert('请输入搜索内容')

          }

        };
        document.onkeydown = function (event) {
          var e = event || window.event || arguments.callee.caller.arguments[0];
          if (e && e.keyCode == 13) {
            if(oSearchTxt.value != ''){


              if($('.select_showbox')[0].innerHTML == 'Site Search'){
                oSearchForm.action = 'http://newssearch.chinadaily.com.cn/en/search?query='+oSearchTxt.value;
                oSearchForm.target = '_blank';
                oSearchForm.submit();
              }
              else if($('.select_showbox')[0].innerHTML == 'Sogou Search'){
                oSearchForm.action = 'https://english.sogou.com/english?query=' + oSearchTxt.value + '&amp;b_o_e=1&amp;ie=utf8 ';
                oSearchForm.target = '_blank';
                oSearchForm.submit();
              }
            }
            else{
              alert('Please input your words')

            }
          }
        };

        $jq = jQuery.noConflict(true);
        //]]>
      </script>
