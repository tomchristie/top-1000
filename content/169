    <script type="text/javascript">
        (function(){
            //监控上报
            var jsErrorData = [],
            jsLogerTimeout = null;
            window.addEventListener("error", function(e) {
                var errorData = {
                        msg: e.message,
                        url: e.filename,
                        crUrl : location.href,
                        lineNo: e.lineno,
                        columnNo: e.colno,
                        error: e.error,
                        ua: navigator.userAgent,
                        kugouid : window.kugou_id || 'null'
                    };
                    jsErrorData.push(errorData);
                    clearTimeout(jsLogerTimeout);
                    jsLogerTimeout = setTimeout(function(){
                        if(jsErrorData.length){
                            $.post('https://statwww2.kugou.com/node/weblog/jsloger',{"key":"home_jserror","data":jsErrorData});
                            jsErrorData = [];//3.5秒内只发送一次
                        }
                    },8000);
            }, true)
        })();

