// remove refresh_ce from history current url if refresh url is not present
try {
  if (document.cookie.indexOf('KW_REFRESH') === -1 && window.location.search.indexOf("refresh_ce") > -1 && history.replaceState) {
    var newUrl;

    if (typeof URLSearchParams !== 'undefined') {
      newUrl = new URL(window.location.href);
      newUrl.searchParams.forEach(function (value, key) {
        if (key === "refresh_ce") {
          newUrl.searchParams.delete(key);
        }
      });
      window.history.replaceState({path: newUrl.toString()},'', newUrl.toString());

    }
    else {
      if (window.location.search.indexOf("?refresh_ce") > -1) {
        newUrl = window.location.href.replace("?refresh_ce", "");
      }
      else {
        newUrl = window.location.href.replace("refresh_ce", "");

      }
    }
    window.history.replaceState({path: newUrl.toString()},'', newUrl.toString());
  }
}
catch (e) {
  console.log('%c[head.js] %c ERROR while trying to remove "refresh_ce" param from history url', 'padding: 2px; background: yellow; color: #000', 'padding: 2px; color: #00F');
}

