  $('.close-modal').click(function(e) {
    e.preventDefault();
    $('#yt-player').children('iframe').attr('src', '');
  });

  $('#modalTrigger').click(function(e) {
    e.preventDefault();
    $('#yt-player').children('iframe').attr('src', 'https://www.youtube.com/embed/TXeYWEzNiCE');
  });
