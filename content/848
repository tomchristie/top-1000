        // shows the bar
        $('.search-show-ada').on('click', function () {
            //$(this).closest('.site-search').addClass('searchopen');
            //$(this).hide();
            $('.search-close-ada').removeClass('d-none');
            if ($(window).width() < 992) {
                //$('.site-search').show();
                $('form[name="simpleSearch-ada"]').removeClass('d-none');
            } else {
                $('form[name="simpleSearch-ada"]').removeClass('d-none');
            }
        });
        $('.search-show-ada').on('click', function () {
            $(".search-field").focus();
        });
        $('.search-close-ada').on('click', function () {
            if ($(this).closest('.search-ada .site-search').hasClass('searchopen')) {
                $(this).closest('.search-ada .site-search').removeClass('searchopen');
            }
            $('.search-show-ada').show();
            $('.search-close-ada').addClass('d-none');
            if ($(window).width() < 992) {
                $('.search-mobile .search-ada .site-search').hide();
                $('form[name="simpleSearch-ada"]').addClass('d-none');
            } else {
                $('form[name="simpleSearch-ada"]').addClass('d-none');
            }
        });
        // for the back-to-top icon tooltip
        $('.back-to-top').tooltip();
    </script>
