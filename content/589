<script type="text/javascript">
function waitForElement(className, callBack){
    window.setTimeout(function(){
        var element = document.querySelector(className);
        if(element) {
            callBack(className, element);
        } else {
            waitForElement(className, callBack);
        }
    },1000)
};

let observer = new MutationObserver(function(mutations) {
    for (let i = 0; i < mutations.length; i++) {
        if (mutations[i].oldValue === "ej-button rounded-corners arrow-button search-submit holiday-search ng-hide") {
            console.log("Holidays button displayed...");
            window.gtmDataLayer.push({
                "event": "ejOptExpImpression"
            });
        };
    };
});

waitForElement(`[search-model="SearchPodModel"]`,function(){
    let target = document.querySelector('[search-model="SearchPodModel"]');
    observer.observe(target, { attributes : true, attributeOldValue: true, subtree: true});
});
</script>








<script type="text/javascript">
(function()
{
	if(AngularEj.Shared.Services.Funnel.Payment.AdyenThreeDS2Provider.prototype.PerformChallenge) {
		AngularEj.Shared.Services.Funnel.Payment.AdyenThreeDS2Provider.prototype.PerformChallenge = function (threeDS2PaymentParameters) {
            var challengeWindowSize = this._threeDS2Utils.config.validateChallengeWindowSize(this.GetWindowSize());
            var iframeSizesArr = this._threeDS2Utils.config.getChallengeWindowSize(challengeWindowSize);
            var cReqData = {
                threeDSServerTransID: threeDS2PaymentParameters.DataItems.threeDSServerTransID,
                acsTransID: threeDS2PaymentParameters.DataItems.acsTransID,
                messageVersion: threeDS2PaymentParameters.DataItems.messageVersion,
                messageType: "CReq",
                challengeWindowSize: challengeWindowSize
            };
            var base64URLEncodedData = this._threeDS2Utils.base64Url.encode(JSON.stringify(cReqData));
            var IFRAME_NAME = "threeDSChallengeIframe";
            var threeDSContainer = document.getElementById("three-ds2-container");

            // HOTFIX
            var voucherContainer = angular.element(".payment-voucher");

            var paymentContainer = document.getElementById("payment-options");
            var threeDSChallengePlaceholder = document.getElementById("three-ds2-challenge");
            threeDSContainer.style.display = "block";

            // HOTFIX
            voucherContainer.hide();

            paymentContainer.style.display = "none";
            this._threeDS2Utils.createIframe(threeDSChallengePlaceholder, IFRAME_NAME, iframeSizesArr[0], iframeSizesArr[1]);
            var form = this._threeDS2Utils.createForm("cReqForm", threeDS2PaymentParameters.DataItems.acsURL, IFRAME_NAME, "creq", base64URLEncodedData);
            threeDSChallengePlaceholder.appendChild(form);
            form.submit();
            this.AdjustScrollForMobileAndTablet();
        };
	}

	if(AngularEj.Shared.Services.Funnel.Payment.AdyenThreeDS2Provider.prototype.ClearUI) {
		AngularEj.Shared.Services.Funnel.Payment.AdyenThreeDS2Provider.prototype.ClearUI = function () {
			var threeDSContainer = document.getElementById("three-ds2-container");
			var threeDSChallengePlaceholder = document.getElementById("three-ds2-challenge");

			// HOTFIX
			var voucherContainer = angular.element(".payment-voucher");

			var paymentContainer = document.getElementById("payment-options");

			// HOTFIX
			voucherContainer.show();

			paymentContainer.style.display = "block";
			threeDSContainer.style.display = "none";
			while (threeDSChallengePlaceholder.firstChild) {
				threeDSChallengePlaceholder.removeChild(threeDSChallengePlaceholder.firstChild);
			}
		};
	}
})();
</script>

<script type="text/javascript">
(function()
{
	if(AngularEj.Services.Inspiration.InspireMeDataService.prototype.OnFilterChanged) {
		AngularEj.Services.Inspiration.InspireMeDataService.prototype.OnFilterChanged = function(filters) {
			var _this = this;
			if (!!this._currentlyAppliedFilters && !this.ShouldMakeNewRouteDataRequest(filters)) {
				this.ApplyLatestFilters(filters);
			}
			else {
				var outboundDate = this._inspireMeDateSearchQuery.GetCustomStartDate(filters.DateSearch);
				var returnDate = this._inspireMeDateSearchQuery.GetCustomEndDate(filters.DateSearch);
				this._api.GetRouteData(filters.OriginIata, outboundDate, returnDate, this._priceService.GetCurrentCurrency().CurrencyId, function (data) {

					// HOTFIX WEB-18776 BEGIN
					if (true) {
						var notQuarantinefree = function (flag) {
							return flag.code !== "Quarantinefree";
						};

						var notWhitelisted = function (iata) {
							return [
								"BHX", "BOH", "BRS", "LPL", "LGW", "LTN", "SEN", "STN", "MAN", "NCL", "NQY", "SOU"
							].indexOf(iata) == -1;
						};

						if (notWhitelisted(data.OriginAirportCode)) {
							for (var _i = 0, _a = data.Airports; _i < _a.length; _i++) {
								var a = _a[_i];
								a.Flags = a.Flags.filter(notQuarantinefree);
							}
						}
					}
					// HOTFIX END

					_this._currentData = data;
					_this._allRoutes = _this._currentData.Routes.filter(function (all) { return !!all.flights && all.flights.length > 0; });
					_this.ApplyLatestFilters(filters);
				}, function () {
					_this._currentData = null;
					_this.ApplyLatestFilters(filters);
				});
			}
		};
	}
})();
</script>

	<!-- START WEB-15879-Remove-Maximyser-Cookies-IE11 Fix -->
	<script type = "text/javascript">
	(function()
		{
			let pairs = document.cookie.split(";");
			let cookies = [];
			for (var i = 0; i < pairs.length; i++)
			{
				var pair = pairs[i].split("=");
				cookies.push(pair[0].trim());
			}
		
			for(var i = 0; i < cookies.length; i++){
				if (cookies[i].indexOf('mm-if-site-persist') == 0 || cookies[i].indexOf('mmapi') == 0 || cookies[i] === 'mmauthid')
					{	
						window.MambaCookies.setCookie(cookies[i], '', {domain: '.easyjet.com', expires : -1});
						window.MambaCookies.setCookie(cookies[i], '', {domain: document.domain, expires : -1});
					}
			}
		})();
	</script>
	<!-- END WEB-15879-Remove-Maximyser-Cookies-IE11 Fix -->

<!-- START: WEB-18547 Worldwide Tiles Iata Hot Fix -->
<script type="text/javascript">
(function()
{
	if(AngularEj.Controllers.Controls.FlightGrid.prototype.IsAWorldwideConfiguredRoute) {
		AngularEj.Controllers.Controls.FlightGrid.prototype.IsAWorldwideConfiguredRoute = function (configuredWorldWideRoutes) {
		    var currentRoute = this._scope.Route;
		    var matchRouteByOrigin = function (iata) { return iata === currentRoute.OriginIata; };
		    var matchRouteByDestination = function (iata) { return iata === currentRoute.DestinationIata; };
		    var validByOrigin = configuredWorldWideRoutes[currentRoute.OriginIata]
		        && configuredWorldWideRoutes[currentRoute.OriginIata].length
		        && configuredWorldWideRoutes[currentRoute.OriginIata][0].split("|").some(matchRouteByDestination);
		    var validByDestintation = configuredWorldWideRoutes[currentRoute.DestinationIata]
		        && configuredWorldWideRoutes[currentRoute.DestinationIata].length
		        && configuredWorldWideRoutes[currentRoute.DestinationIata][0].split("|").some(matchRouteByOrigin);
		    return validByOrigin || validByDestintation;
		};
	}
})();
</script>
<!-- END: WEB-18547 Worldwide Tiles Iata Hot Fix -->


