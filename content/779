      <script src="/news-catalog/public/js/merged/main-with-jquery-3.merged.js?i=2"></script>
    <script>
    (function() {
        /*
         * Скрываем теги <time> with datetime attribute
         */
        var style = document.createElement('style');
            style.innerHTML = 'time[datetime] { visibility: hidden; };';
        document.head.append(style);
    })();

      $(document).ready(function() {
          var i, j, more_cnt, w = 0, first_load = true, menu = [];

          // Show/Hide menu for mobile devices
          $('#menu-toggle').on('click', function() {
              $('body').toggleClass('displacement');
              $('#nav').toggleClass('displacement');
          });

          $('html').on('click', function() {
              $('#menu-more').addClass('hidden');
          });

          $('#search-toggle').on('click', function() {
              $(this).toggleClass('pressed');
              $('#header').toggleClass('search-pressed');
          });

          // Trigger resize for initialization
          $(window).trigger('resize');

          $('time[datetime]').
            each(function(k, v) {
                try {
                    if (!$(v).attr('format') || !$(v).attr('datetime'))
                        return true;
                    var f = $(v).attr('format'),
                        d = $(v).attr('datetime'),
                        d_ = new Date(Date.parse(d.replace(' ', 'T') + '+03:00'));

                    if (d_ == 'Invalid Date')
                        return true;

                    var dd = d_.getDate() > 9 ? d_.getDate() : '0'+ d_.getDate(),
                        mm = Number(d_.getMonth()+1) > 9 ? ''+Number(d_.getMonth()+1) : '0'+ Number(d_.getMonth()+1),
                        yyyy = d_.getFullYear();

                    var HH = d_.getHours() > 9 ? d_.getHours() : '0'+ d_.getHours(),
                        MM = d_.getMinutes() > 9 ? d_.getMinutes() : '0'+ d_.getMinutes();

                    f = f.replace('dd', dd);
                    f = f.replace('mm', mm);
                    f = f.replace('yyyy', yyyy);
                    f = f.replace('HH', HH);
                    f = f.replace('MM', MM);

                    $(v).html(f);
                } catch (e) { console.log(e); }
            }).
            css('visibility', 'visible')
        ;
      });
  </script>

  
