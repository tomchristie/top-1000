    var newsType = ["\u516c\u544a","\u6d3b\u52a8","\u65b0\u95fb","\u4e13\u9898"];
    var $aTab    = $(".newsTab h3");
    var $newsUl  = $(".newsTab ul");

    $aTab.delegate('a', 'click', function() {
		var $newsUl  = $(".newsTab ul");
        if ($(this).hasClass("newsOn")) {
            return;
        } else {
            var index=$(this).index();
            $(this).addClass("newsOn").siblings('a').removeClass("newsOn");
			$newsUl.eq(index).show().siblings('ul').hide();
        }
    });

    $('.newsTab').bind('update', function(evt, gid){
        var $this = $(this);
        if (gid == 'ALL') {
            $this.trigger('render', newslist_allgame);
			$newsUl.hide();
            $newsUl.eq(0).show();
        } else {
            $.ajax({
                type: 'GET',
                url: '/websiteAjax/op/getNewsList/gid/' + gid + '/',
                dataType: 'json',
                success: function(result){
                    if (result.status) {
                        $this.trigger('render', result.data);
                    } else {
                        $this.trigger('render', {});
                    }
                }
            });
        }
        return false;
    }).bind('render', function(evt, newslist){
        var m,mlen,i,len,data,tplArr = [],
        newsArr = ['ALL', 'NEWS', 'AFFICHE', 'ACTIVITY'],
        tplArr = [];
        for (m=0,mlen=newsArr.length; m<mlen; m++) {
            tplArr = [];
            if (newslist && newslist[newsArr[m]]) {
                data = newslist[newsArr[m]];
                var j =35;
                for (i=0,len=data.length; i<len; i++) {
					if(typeof(gameIdCodeName[data[i]['gameid']]) != 'undefined') {
						tplArr.push('<li>');
						tplArr.push('    <a href="' + data[i]['path'] + '" target="_blank" title="' + data[i]['subject'] +'" >' + data[i]['title'] + '</a>');
						tplArr.push('    <em>' + data[i]['date'].substring(0, 10) + '</em>');
						tplArr.push('</li>');
					}
                    j++;
                } 
            } else {
                tplArr.push('<li>暂无记录</li>');
            }
            $newsUl.eq(m).html(tplArr.join(''));
        }
        return false;
    });

    $('.newsTab .news_gids').bind('click', function(){
        var gid = $(this).attr('data-id');
		var names = $(this).html();
        $('.newsTab').trigger('update', gid);
		$('.newsTab .dragDiv').hide();
		$('.newsTab .dragDiv').prev().html(names);
        return false;
    });

	$('.newsTab').trigger('update', 'ALL');
});
</script>
