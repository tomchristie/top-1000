    <script type="text/javascript" src="//script.game2.com/jquery/jquery.cookie.js"></script>
	<script>
		//首页幻灯
		var showTime=5000;//大图停留时间
		var changeTime=3000;//小图切换时间
		var setItTime=null;
		var move=true;
		var $small=$(".small");
		var $big=$(".big");
		var $swfBox=$(".swfBox");
		var $swfDiv=$(".swfDiv");
		var $len=$small.children("a").length;
		var $now=0;
		$(".small a:first").show();//默认让第一张小图显示
		
		//显示大图
		function openIt(mc) 
		{	
			$(".small").slideUp(500);
			$(".smallHar").hide();
			$swfDiv.eq(mc).slideDown(500,function(){
				if($(".itBtn").is(":hidden")) {
					$(".itBtn").fadeIn();
				} else if($(".itBtn a").hasClass("rep")) {
					$(".itBtn a").removeClass("rep");
				};
			});
			if($len>1) {
				$("a.swfLeft").css('left', '10px');
				$("a.swfRight").css('right', '10px');
			}
			clearTimeout($small.timer);
			if($big.setItTime){clearTimeout($big.setItTime);}
			$big.setItTime = setTimeout(closeIt,showTime);
			
			$big.hover(
				function(){
					clearTimeout($big.setItTime);
					clearTimeout($small.timer);
					move=false;
					/*if($len>1) {
						$("a.swfLeft").stop(true).animate({"left":0});
						$("a.swfRight").stop(true).animate({"right":0});
					}*/
				},
				function(){
					if($big.setItTime) {
						clearTimeout($big.setItTime);
					};
					$big.setItTime = setTimeout(closeIt,showTime);move=true;
					//$("a.swfLeft").stop(true).animate({"left":"-27px"});
					//$("a.swfRight").stop(true).animate({"right":"-27px"});
					
				});
		}
		
		//关闭大图显示小图
		function closeIt()
		{
			$swfDiv.eq($now).slideUp(500,function(){
				$(".smallHar").show();
				$(".small").slideDown(500,function(){
					$(".itBtn a").addClass("rep");
					if(move) {
						clearTimeout($small.timer);
						$small.timer=setInterval(changeImg,changeTime);
					};
				});
			});
		}
		
		//打开网页1500ms后开始显示大图
        $small.timer=setInterval(changeImg,changeTime);

		//点击小图或关闭重播按钮时切换小图和大图
		$(".itBtn,.small").click(function(){	
			
			if($(".itBtn a").hasClass("rep")) {
				openIt($now);
			} else {
				closeIt();
			}
		})

		//小图切换
		function changeImg(x)
		{
			$(".small a").hide();
			if(!x) {
				if($now<$len-1) {
					$now++;
				} else if($now>=$len-1) {
					$now=0;
				}
			} else {
				if($now <= 0) {
					$now=$len-1;
				} else if($now > 0) {
					$now--;
				}
			}
			
			$(".small a").eq($now).fadeIn();
			$(".smallHar span").eq($now).addClass("on").siblings(this).removeClass("on");
		};

        function removeTsSwf()
        {
          $("#tsSwf").hide().html('')
          $("html,body").css({"overflow":"auto","overflow-x":"hidden"});
        };
        function closeSwf()
        {
          $("#tsSwf").hide().html('')
          $("html,body").css({"overflow":"auto","overflow-x":"hidden"});
        }
        function showTsSwf()
        {
            var show_mir_flash = '1489161600';
            var mir_url = '/server/code/gcd/mflash/1/';
            if (mir_url) {
                $(window).scrollTop(0);
                $("html,body").css({"overflow":"hidden"});
                var expiresDate= new Date();
                expiresDate.setTime(expiresDate.getTime() + (864000 * 1000));
                $.cookie('SHOW_GCD_FLASH', "1", { path: "/", domain: ".game2.cn", expires:expiresDate });
                setTimeout(function(){
                   var swfHtml = '<object width="100%" height="100%" align="middle" codebase="http://fpdownload.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=8,0,0,0" classid="clsid:d27cdb6e-ae6d-11cf-96b8-444553540000"><param name="allowScriptAccess" value="always"><param name="movie" value="/images/swf/swf.swf"><param name="quality" value="high"><param name="bgcolor" value="#FFFFFF"><param name="wmode" value="transparent"><param name="menu" value="false"><param value="URL=' + mir_url + '" name="FlashVars"><embed width="100%" height="100%" align="middle" src="/images/swf/swf.swf" quality="high" menu="false" allowscriptaccess="always" wmode="transparent" type="application/x-shockwave-flash" pluginspage="http://www.macromedia.com/go/getflashplayer" flashvars="URL=' + mir_url + '"></object>';
                   var winW = $(window).width();
                   var winH=$(window).height();
                   $('#tsSwf').css({width:winW+"px", height:winH+'px'}).html(swfHtml).show();
                }, 100);
            }
        };
        $(document).ready(function() {

            var swfHtmlStr = '<div id="tsSwf" class="tsSwf" style="position:absolute; left:0;top:0; height:100%;width:100%; background-color: #fff; z-index:9999;display:none;"></div>'
                           + '<div class="lin" style="display:none;"><a href="javascript:void(0)">所有传奇都在攻沙！</a></div>'
                           + '<div class="screenClick" style="display:none;"><a href="javascript:void(0)"></a></div>';
            $('body').append(swfHtmlStr);

            $(window).bind("resize scroll",function(){
                 var winW=$(window).width();
                 var winH=$(window).height();
                 $('#tsSwf').css({"width":winW,"height":winH});
            });
            $(".lin a").click(function(){
               showTsSwf();
               return false;
            });

            if ((parseInt((new Date()).getTime()/1000) >= 1499910900 && parseInt((new Date()).getTime()/1000) < 1500048000) || $.cookie('SHOW_GCD_FLASH2') == "1") {
                $(".lin").show();
            }

            if (false && $.cookie('SHOW_GCD_FLASH') != "1" && parseInt((new Date()).getTime()/1000) < 1496764800) {
                var expiresDate= new Date();
                expiresDate.setTime(expiresDate.getTime() + (864000 * 1000));
                $.cookie('SHOW_GCD_FLASH', "1", { path: "/", domain: ".game2.cn", expires:expiresDate });
                $(".screenClick a").click(function(){
                    showTsSwf();
                    $(this).parent().remove();
                    return false;
                });

                 var winW=$(window).width();
                 var winH=$(window).height();
                 var docH = $(document).height();
                $(".screenClick").css({"width":winW+"px","height":docH+"px"}).show();
            } else {
                $('.showBox .itBtn .rep:first').trigger('click');
            }

			$(".smallHar span").mousemove(function(){
				var tep=$(this).index();
				$(".small a").eq(tep).fadeIn().siblings("a").hide();
				$(".smallHar span").eq(tep).addClass("on").siblings(this).removeClass("on");
				$now = tep;	
			});
			
			$(".small").hover(
				function(){clearTimeout($small.timer);move=false},
				function(){$small.timer=setInterval(changeImg,changeTime); move=true});
			
			$(".swfRight").click(function(){
				changeImg();
				$(".smallHar span").eq($now).addClass("on").siblings(this).removeClass("on");
				$swfDiv.eq($now).fadeIn().siblings().hide();	
			});

			$(".swfLeft").click(function(){
				changeImg(-1);
				$(".smallHar span").eq($now).addClass("on").siblings(this).removeClass("on");
				$swfDiv.eq($now).fadeIn().siblings().hide();	
			});

			//重点推荐
			$(".hotCon").hover(
				function(){$(this).find(".hotIntro").stop(true,true).animate({"top":"0"})},
				function(){$(this).find(".hotIntro").stop(true,true).animate({"top":"290px"})}
			);

			//大家都在玩
			$(".Gbox").hover(
				function(){$(this).find(".onBorder").stop(true,true).show()},
				function(){$(this).find(".onBorder").stop(true,true).hide()}
			);

			//首页左侧活动
			 $(".actImg").hover(
				function(){$(this).find("img").stop().animate({"opacity":0.8})},
				function(){$(this).find("img").stop().animate({"opacity":1})}
			 );

			 //客服ico
			$(".csDiv a").hover(
				function(){$(this).find("img").stop(true,true).animate({"left":"20px"})},
				function(){$(this).find("img").stop(true,true).animate({"left":"15px"})}
			);

			if($(".hotLink").length) {
				$(".hotLink a.aIn").hover(
					function(){$(this).siblings("span").find("strong").addClass("str2")},
					function(){$(this).siblings("span").find("strong").removeClass("str2")}
				)
			}
			var timestamp=new Date().getTime();
			if(timestamp>=1579676400000 && timestamp<=1580522400000) {
				$.getScript("/script/snow.js",function(){
				});
			}
		});
	</script>

