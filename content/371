<script type="text/javascript">
(function() {
	$search = window.location.search;
	if ($search.lastIndexOf('?') == 0) {
		var str = $search.substring($search.lastIndexOf('?') + 1, $search.length);
		var arr = str.split('&');
		var parames = [];
		for (var i = 0; i < arr.length; i++) {
			$split = arr[i].split('=');
			if(typeof($split[1]) != 'undefined') {
				parames[$split[0]] = $split[1];
			}
		}

		if(typeof(parames['trackid']) != 'undefined') {
			var trackid = $.cookie('UserTrackid');
			if(trackid != parames['trackid'] && parames['trackid'] != '') {
				$.cookie('UserTrackid', parames['trackid'], { path: "/", domain: ".game2.cn", expires:1});
			}
		}
		if(typeof(parames['bpcode']) != 'undefined') {
			//$.getScript('http://tg.game2.cn/brandjs/brandLinkConfig.js?v=' + Math.random()).done(function () {});
		}
	} 
})();
</script>

