<script type="text/javascript">

$(document).ready(function(){
    
    $(".modal-legales").click(function() {
        var path = $(this).attr("data-path");
        $.ajax({
            type: 'GET',
            url: path,
            success: function (response) {
                $('.open-modal').html(response).addClass('modalLegalesOpen');
                window.scrollTo(0, 0);
                $('html').addClass('overflow');
            }
        })
        return false;
    });
});
