	CBSNEWS.features.queue.push(function () {
		var heroDupe = document.getElementById('component-top-stories-fd-test');
		var hero = document.getElementById('component-top-stories');
		if (heroDupe) {
			heroDupe.style.display = 'none';
			var ad = document.querySelector('#component-top-stories-fd-test ~ .ad-wrapper');
			if (ad) {
				ad.style.display = 'none';
			}
			CBSNEWS.features.executeIfEnabled(['fd-hero-shelf-test'], function () {
				if (CBSNEWS.device == 'desktop') {
					// unhide dupe
					heroDupe.style.display = 'block';
					// scoot it before actual hero, then remove
					hero.parentElement.insertBefore(heroDupe, hero);
					hero.remove();
					// remove 'has--video-shelf-with-hero-first' HTML class
					document.documentElement.classList.remove('has--video-shelf-with-hero-first')
