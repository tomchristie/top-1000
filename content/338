  <script>
  document.domain = '21cn.com'
  window.addEventListener('load', function() {
  
    // 导航栏样式判断
    var domain = document.domain.split('.')[0]
    $('#nav-' + domain).addClass('header_select')
  
    // 登录功能
    window.loginSuccess = function() {
      $("#Dialog").hide();
      $("#Dialog").css("display", "none");
      window.location.reload();
    }
    $('.login_new').click(function() {
      $("#Dialog").css("display", "block");
      var ifrElem = '';
      if (location.href.indexOf('https') > -1) {
        ifrElem =
          ' <iframe width="312px" height="316px" id="udburl" frameborder="0" src="//passport.21cn.com/api/new/loginUrl.do?httpsOn=1"></iframe>'
      } else {
        ifrElem =
          ' <iframe width="312px" height="316px" id="udburl" frameborder="0" src="//passport.21cn.com/api/new/loginUrl.do"></iframe>'
      }
  
      $('#main1').append(ifrElem)
      $('#udburl').attr('src', $('#udburl').attr('src'));
    });
    $('#login_tab').find('.close').click(function() {
      $("#Dialog").hide()
  
      $('#udburl').remove();
    });
  
    if (Cookie.get("SSON") && Cookie.get("SSONKEY") != 'null') {
      $.ajax({
        url: 'https://passport.21cn.com/api/getUser.do?jsoncallback=?',
        dataType: 'json',
        timeout: 10000,
        error: function() {
          return false;
        },
        success: function(data) {
          $('.login_new').hide();
          $('.exit_login').show();
  
          $('#exit_login').attr("href", 'https://passport.21cn.com/api/logout.do?returnUrl=' + window.location.href + '')
          $('.already_login').html('欢迎您，' + '<a style="float:none; display:inline;" href="https://e.189.cn">' + data.account.screenName + '</a>');
          if ((/^(13|15|18|17)+[0-9]{9}$/.exec(data.account.userName)) ||
            (/^(13|15|18|17)+[0-9]{9}@189\.cn$/.exec(data.account.userName))) {
            $('#free_mail').attr('href', 'https://mail.189.cn/webmail/jsp/189misc/fromCloudL.jsp');
          } else {
            $('#free_mail').attr('href', 'https://mail.21cn.com');
          }
        }
      });
    };
  
    // app下载选择
    $(function() {
        var dom_21cnNew = $("#21cnNew")
        var dom_21cnNewPic = $("#21NewPic")
  
        var dom_cn21cn = $("#CN-21cn")
        var dom_21cn = $("#21cn")
  
        var dom_tianyiyun = $("#tianyiyun")
        var dom_productsText = $('#products-text')
        var dom_189 = $("#189")
        var dom_jiatingyun = $("#jiatingyun")
        var dom_products = $("#products")
        var dom_jutousu = $("#jutousu")
        var dom_appContainer = $(".appContainer")
  
        dom_21cnNewPic.css("z-index", "9");
        
        dom_products.on("mouseenter", "#21cnNew", function() {
          dom_21cnNewPic.css("z-index", "2");
          dom_21cnNewPic.siblings("img").css("z-index", "1");
          dom_21cnNew.css("background-color", "#eee").css("color", "#096DD9");
          dom_productsText.text('扫码关注')
        });
        dom_products.on("mouseenter", "#CN-21cn", function() {
          dom_21cn.css("z-index", "2");
          dom_21cn.siblings("img").css("z-index", "1");
          dom_21cnNew.css("background-color", "#fff").css("color", "black");
          dom_productsText.text('扫码关注')
        });
        dom_products.on("mouseenter", "li.tianyiyun", function() {
          dom_tianyiyun.css("z-index", "2");
          dom_tianyiyun.siblings("img").css("z-index", "1");
          dom_21cnNew.css("background-color", "#fff").css("color", "black");
          dom_productsText.text('扫码下载')
        });
        dom_products.on("mouseenter", "li.189", function() {
          dom_189.css("z-index", "2");
          dom_189.siblings("img").css("z-index", "1");
          dom_21cnNew.css("background-color", "#fff").css("color", "black");
          dom_productsText.text('扫码下载')
        });
        
        dom_products.on("mouseenter", "li.jutousu", function() {
          dom_jutousu.css("z-index", "2");
          dom_jutousu.siblings("img").css("z-index", "1");
          dom_21cnNew.css("background-color", "#fff").css("color", "black");
          dom_productsText.text('扫码关注')
        });
        dom_products.on("mouseenter", "li.jiatingyun", function() {
          dom_jiatingyun.css("z-index", "2");
          dom_jiatingyun.siblings("img").css("z-index", "1");
          dom_21cnNew.css("background-color", "#fff").css("color", "black");
          dom_productsText.text('扫码下载')
        });
        dom_appContainer.mouseleave(function() {
          dom_21cnNewPic.css("z-index", "9");
          dom_21cnNew.css("background-color", "#eee").css("color", "#096DD9");
        })
      });
  })
  </script>
