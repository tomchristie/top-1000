    <script>
      window.addEventListener('load', function() {
        var height = $(window).height()
          $(window).scroll(function() {
              // 返回顶部显示
              if ($(window).scrollTop() > height)
                  $(".newsAppBox-bd").show()
              else
                  $(".newsAppBox-bd").hide()
          });
      })
      function scollTop() {
          document.getElementsByTagName('html')[0].scrollIntoView({ behavior: 'smooth' })
      }
    </script>
