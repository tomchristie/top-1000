var allList = $('.all-sort-list');
var allListH = allList.height();
allList.find('.item').hover(function () {
    var $this = $(this);
    $this.addClass('hover');
    // 获取距离父级距离
    var posT = $this.position().top;
    var itemList = $this.find('.item-list');
    itemList.show();
    var itemH = itemList.height();
    // 判断是否超过父级总高度
    if (itemH + posT > allListH) {
        itemList.css({
            top:'auto',
            bottom: -2
        });
    }else {
        itemList.css('top',posT);
    }
},function () {
    var $this = $(this);
    $this.removeClass('hover');
    var itemList = $this.find('.item-list');
    itemList.hide();
}),
