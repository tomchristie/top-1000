<script type="text/javascript">
	var business = "";
	_ub.city = '全国';//必写，中文
	_ub.request('vwg.business,vwn.position,vwn.subway,vwn.belongschool,vwn.avgprice,vwn.dwellingtype,vwn.fixstatus,vwn.feature', function(){ //（直接把需要的往后添就行）
	        _ub.load(2); //选取时间最近的值    
	        //以下添加业务逻辑
	       business=_ub['vwg.business'];
	       
	});

    $('.dkhA').on('click', function(){
        var bid = $(this).attr('data-bid');
        var bname = $(this).attr('data-bname');
        _ub.collect(808, {
            'vwg.page': 'sy_sy_pc',
            'vwg.agency': encodeURIComponent(bname + '_' + bid),
        });

        var originalUrl = $(this).attr('data-href');
        window.open(originalUrl,'_blank');
        return false;
    });


    $('.dkhfyA').on('click', function(){
        var bid = $(this).parents('ul').attr('data-bid');
        var bname = $(this).parents('ul').attr('data-bname');
        var houseid = $(this).attr('data-houseid');
        _ub.collect(808, {
            'vwg.page': 'sy_sy_pc',
            'vwg.agency': encodeURIComponent(bname + '_' + bid + '_' + houseid +'_155_sy_app'),
        });

        var originalUrl = $(this).attr('data-href');
        window.open(originalUrl,'_blank');
        return false;
    });

    $('.esftitle').on('mouseover', function () {
        if(!$(this).hasClass('s1')){
            $('.esftitle').attr('class', 's2 esftitle');
            $(this).attr('class', 's1 esftitle');
            $('.esflp').hide();
            var index = $('.esftitle').index(this);
            $('.esflp').eq(index).show();
            if(index > 0){
                var source = '';
                var bid = $('.esflp').eq(index).find('ul').attr('data-bid');
                var bname = $('.esflp').eq(index).find('ul').attr('data-bname');
                $('.esflp').eq(index).find('li').each(function () {
                    source = source + '{"houseid":"' + $(this).attr('data-houseid') + '","housetype":"' + $(this).attr('data-houseytpe') + '","buserid":"'+bid+'","companyname":"'+bname+'"},';
                });
                source = source.substring(0, source.length-1);
                var cityName = $('#cityName');
                if(cityName === null || cityName === undefined || cityName.length ===0){
                    cityName = $('#bigcityName').val();
                }else{
                    cityName = cityName.val();
                }
                var bgcity = $URL.encode(cityName);
                var bgcity = $URL.encode(cityName);
                var bgjson = '{"city":"' + bgcity + '","source":"pc","channel":"home","module":"sy_syesf_pc","pageid":"sy_syesf_pc", "bglist":['+ source +']}';
                jQuery.ajax({
                    url: "//esfbg.3g.fang.com/qyk.html",
                    type: "post",
                    data: bgjson,
                    success: function () {
                        //alert(111);
                    }
                });
            }
        }
    });
</script>
<script type="text/javascript" src="//img.soufunimg.com/newhousedata/images/support/FQA.js"></script>

<script>
	(function(){
		//id1是tab的开头,id2是content的开头,index是当前的位置,count是总的tab个数,classname没选中tab的样式中的样式类
		///交换样式
	
		window.show_menuc = function(id1, id2, index, count, classcurrent, classname) {
			for (var i = 0; i < count; i++) {
				 document.getElementById(id2.toString() + i).style.display = "none";
				 document.getElementById(id1.toString() + i).className = classname;
			}
			 document.getElementById(id2.toString() + index).style.display = "block";
			 document.getElementById(id1.toString() + index).className = classcurrent;
		}
	})();
</script>
