      'layoutclass': window.initialData.serviceResponse && window.initialData.serviceResponse._params && window.initialData.serviceResponse._params.layoutclass,
      'consentlevel': window.initialData.serviceResponse && window.initialData.serviceResponse._params && window.initialData.serviceResponse._params['prf[consentlevel]'],
      'weather_condition': window.initialData.serviceResponse && window.initialData.serviceResponse._params && window.initialData.serviceResponse._params['prf[weather_condition]'],
