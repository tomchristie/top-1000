    const paramsObject = {
      'wi': window.initialData.serviceResponse && window.initialData.serviceResponse._params && window.initialData.serviceResponse._params.wi,
      'section': window.initialData.serviceResponse && window.initialData.serviceResponse._params && window.initialData.serviceResponse._params.section,
