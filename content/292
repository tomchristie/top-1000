<script type="text/javascript" async=true>
function loadMasTags(useGooglePersonalization){
  let script_a = document.createElement('script');
      script_a.src = "https://www.googletagmanager.com/gtag/js?id=UA-5877893-4";
      script_a.async = true;
  document.body.append(script_a);
  window.dataLayer = window.dataLayer || [];
  function gtag(){dataLayer.push(arguments);}
  gtag('js', new Date());
  if(useGooglePersonalization) {
    gtag('config', 'UA-5877893-4')
  } else {
    gtag('config', 'UA-5877893-4', { 'anonymize_ip': true })
  }
  
}
</script>
