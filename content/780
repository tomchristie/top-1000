    //页面拉到底时加载更多
    
    $(window, document, "body").scroll(function(event) {
        $('.bdsharebuttonbox').hide()
        // var wScrollY = window.screenY || window.screenTop||$("html,body").scrollTop(); // 当前滚动条位置 
        var wScrollY = $("html,body").scrollTop();
        var newwScrollY = document.body.scrollTop
        if (newwScrollY != 0) {
            wScrollY = newwScrollY
        }
        var wInnerH = window.innerHeight; // 设备窗口的高度（不会变）  
        var newwInnerH = document.documentElement.clientWidth
        var bScrollH = document.body.scrollHeight - 837; // 滚动条总高度
        // console.log(wScrollY)
        // console.log(newwScrollY)
        // // console.log(wInnerH)      
        // console.log(bScrollH)
        // console.log(newwInnerH)
        if (wScrollY + newwInnerH >= bScrollH) {
            console.log('flag')

            // setTimeout(function() {
            if (flag) {
                if (page != 1) {
                    console.log("触发底部");
                    flag = false;
                    hotlist()
                    LockMore = true;
                }

            }
            // }, 1000)

            if (LockMore) {
                return false;
            }
        }
    });

    function waterFlow() {
        // console.log(j)
        var arr = []; //定义一个数组
        var imgWidth = $(".hotlist").innerWidth();
        for (var j = 0; j < $(".hotlist").length; j++) {
            if (j < num) { //一行显示num个div   ,将第一行的每个div高度写入数组
                arr[j] = $(".hotlist").eq(j).innerHeight()
                // console.log(arr[j])
            } else {
                // console.log(j)
                // console.log(arr)
                var minImgHeight = Math.min.apply(null, arr); //取得数组中最小高度的div
                var index = getMinIndex(arr, minImgHeight); //取得数组中最小高度的div的索引
                $(".hotlist").eq(j).css({
                    "position": "absolute",
                    "top": minImgHeight + "px",
                    // "left": $(".hotlist").eq(index).offset().left - 63 + "px"
                    "left": imgWidth * index + "px",
                }); //瀑布流布局
                arr[index] += $(".hotlist").eq(j).innerHeight(); //将布局好的该div的高度和该div上面的div高度相加，重新放入数组
            }
        }
        // console.log('i')
        // console.log(j)
        var maxHeight = Math.max.apply(null, arr);
        // console.log('maxHeight')
        // console.log(maxHeight)
        $("#waterfall-container").css({
            "height": maxHeight + 30 + "px"
        })
    }

    function getMinIndex(arr, min) { //取得数组中最小高度的div的索引
        for (var k in arr) {
            if (arr[k] == min) {
                return k;
            }
        }
    }
    
    //信息流控制
    $(document).ready(function() {})
    //今日热点
    function hotlist() {
        console.log("开始异步")
        // console.log(window.screenY)
        // console.log(window.screenTop)
        // console.log($("html,body").scrollTop())
        // console.log("ajax")
        if (page < 6) {
            $.ajax({
                type: "get",
                // url: "http://www.qianlong.com/backend/api/yxsd_list.php",
                url: 'http://www.qianlong.com/qianlong-new/api/hot_list.php',
                data: {
                    page: page,
                },
                dataType: "json",
                success: function(res) {
                    console.log(res)
                    if (res.code == 0) {
                        page = page + 1;
                        // hot_list = hot_list.concat(res.data);
                        // console.log(hot_list)
                        for (var i in res.data) {
                            if (typeof(res.data[i].tags[0]) == 'undefined') {
                                res.data[i].tags[0] = '';
                            }
                            if (res.data[i].description != '') {

                                a = res.data[i].description;

                                var kan = res.data[i].description.split('');
                                // console.log(kan.length)
                                if (kan.length >= 59) {
                                    kan = kan.slice(0, 60)

                                    var a = kan.join("")
                                    a = a + "..."
                                }
                            }

                            var demo = ''
                            var demo1 = ''
                            demo += "<div class='col-xs-12 col-lg-4 hotlist' style='display:none;'>"
                            demo += "<div class='row'>"
                            demo += "<div class='pin_demo_out'>"
                            demo += "<div class='pin'>"

                            demo += "<div class='pin_demo'>"
                            if (res.data[i].modelid == 4) {
                                demo += "<div style='overflow:hidden;position: relative;'><img class='hot_player' src='http://img.qianlong.com/ql2019/index/images/player.png'><a href=" + res.data[i].url + " target='_blank'><img class='img-responsive fd' src=" + res.data[i].thumb + " /></a></div>"
                            } else {
                                demo += "<div style='overflow:hidden;position: relative;'><a href=" + res.data[i].url + " target='_blank'><img class='img-responsive fd' src=" + res.data[i].thumb + " /></a></div>"
                            }
                            demo += "<a href=" + res.data[i].url + "  target='_blank'><div class='pin_tit'>" + res.data[i].title + "</div></a>"
                            if (res.data[i].description != "") {
                                demo += "<div class='pin_tips'>" + a + "<div class='look'><a href=" + res.data[i].url + " target='_blank'>阅读全文 ></a></div></div>"
                            }

                            if (res.data[i].relation != []) {
                                // console.log(res.data[i].relation)
                                var relation = ''
                                for (var j in res.data[i].relation) {
                                    relation += "<a href=" + res.data[i].relation[j].url + " target='_blank'><span></span>" + res.data[i].relation[j].title + "</a>"
                                }
                            }
                            if (relation != '') {
                                demo += "<div class='pin_tips' style='margin-top:15px'>" + relation + "</div>"
                            }
                            // if (res.data[i].cat_name != "" || res.data[i].tags[0]) {
                            demo += "<div class='pin_from'>"
                            if (res.data[i].cat_name == "千龙2019") {
                                demo += "<div class='pin_channel'>北京</div>"
                            } else {
                                demo += "<div class='pin_channel'>" + res.data[i].cat_name + "</div>"
                            }

                            // demo += "<div class='pin_flag'  data-id=" + res.data[i].url + " data-title=" + res.data[i].title + "><div>" + res.data[i].tags[0] + "</div></div>"
                            demo += "<div class='pin_flag'  data-id=" + res.data[i].url + " data-title=" + res.data[i].title + "><div><img src='http://img.qianlong.com/ql2019/list/images/fenxiang.png'></div></div>"
                            demo += "</div>"
                            // }
                            demo += "<div class='pin_line'></div>"
                            demo += "</div>"

                            demo += "</div>"
                            demo += "</div>"
                            demo += "</div>"
                            demo += "</div>"
                            if (res.data[i].title == "广告") {
                                demo = ''
                                demo1 = ''
                                demo += "<div class='col-xs-12 col-lg-4 hotlist' style='display:none;'>"
                                demo += "<div class='row'>"
                                demo += "<div class='pin_demo_out'>"
                                demo += "<div class='pin'>"
                                demo += "<div class='pin_demo think_img'>"
                                demo += "<div style='overflow:hidden;'><a href=" + res.data[i].url + " target='_blank'><img class='img-responsive' src=" + res.data[i].thumb + " /></a></div>"
                                demo += "<div class='pin_line'></div>"
                                demo += "</div>"
                                demo += "</div>"
                                demo += "</div>"
                                demo += "</div>"
                                demo += "</div>"

                                demo1 += ''
                                demo1 += "<div class='pin'style='margin-top:18px'>"
                                demo1 += "<div class='pin_demo'>"
                                demo1 += "<a href=" + res.data[i].url + " target='_blank'><img class='img-responsive' src=" + res.data[i].thumb + " /></a>"
                                // demo1 += "<a href=" + res.data[i].url + "  target='_blank'><div class='pin_tit'>" + res.data[i].title + "</div></a>"
                                // if (res.data[i].description != "") {
                                //     demo1 += "<div class='pin_tips'>" + res.data[i].description + "</div>"
                                // }
                                // if (res.data[i].cat_name != "" || res.data[i].tags[0]) {
                                //     demo1 += "<div class='pin_from'>"
                                //     demo1 += "<div class='pin_channel'>" + res.data[i].cat_name + "</div>"
                                //     demo1 += "<div class='pin_flag'  data-id=" + res.data[i].url + " data-title=" + res.data[i].title + "><div>" + res.data[i].tags[0] + "</div></div>"
                                //     demo1 += "</div>"
                                // }
                                demo1 += "<div class='pin_line'></div>"
                                demo1 += "</div>"

                                demo1 += "</div>"
                            } else {

                                demo1 += "<div class='pin clearfix' style='margin-top:18px'>"
                                demo1 += "<div class='m_hot_demo col-xs-12'>"

                                demo1 += "<div class='col-xs-8' style='padding-left:0px;padding-right:20px;position: relative;'><div style='overflow:hidden;'><a href=" + res.data[i].url + "  target='_blank'><div class='pin_tit'>" + res.data[i].title + "</div></a></div>"
                                // if (res.data[i].description != "") {
                                //     demo1 += "<div class='pin_tips'>" + res.data[i].description + "</div>"
                                // }
                                // if (res.data[i].cat_name != "") {
                                demo1 += "<div class='pin_from'>"
                                if (res.data[i].cat_name == "千龙2019") {
                                    demo1 += "<div class='pin_channel'>北京</div>"
                                } else {
                                    demo1 += "<div class='pin_channel'>" + res.data[i].cat_name + "</div>"
                                }
                                // demo1 += "<div class='pin_flag'  data-id=" + res.data[i].url + " data-title=" + res.data[i].title + "><div>" + res.data[i].tags[0] + "</div></div>"
                                demo1 += "<div class='pin_flag'  data-id=" + res.data[i].url + " data-title=" + res.data[i].title + "><div><img src='http://img.qianlong.com/ql2019/list/images/fenxiang.png'></div></div>"
                                demo1 += "</div>"
                                // }



                                demo1 += "</div>"
                                demo1 += "<a class='col-xs-4' href=" + res.data[i].url + " target='_blank'><img class='img-responsive m_hot_img' src=" + res.data[i].thumb + " /></a>"
                                demo1 += "</div>"
                                demo1 += "<div class='col-xs-12 pin_line'></div>"
                                demo1 += "</div>"
                            }




