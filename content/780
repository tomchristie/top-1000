<script>
    var page = 1;
    var hot_list = [];
    var num = 3;
    var LockMore = false; //锁定
    var flag = false;

    function headfize() {
        var head = $('#pc_head').height();
        var thisEle = $("#pc_head").css("font-size");
        var textFontSize = parseFloat(thisEle, 10);
        var unit = thisEle.slice(-2); //获取单位
        // console.log(textFontSize)
        if (head > 60) {
            textFontSize = textFontSize - 2;
            var size = textFontSize + unit;
            $("#pc_head").css("font-size", size);
            var newhead = $('#pc_head').height();
            if (newhead > 60) {
                headfize()
            }

        }
    };

    function headfize1() {
        var head1 = $('#pc_head_s').height();
        var thisEle1 = $("#pc_head_s").css("font-size");
        var textFontSize1 = parseFloat(thisEle1, 10);
        var unit1 = thisEle1.slice(-2); //获取单位
        // console.log(textFontSize1)
        console.log(head1)
        if (head1 > 60) {
            textFontSize1 = textFontSize1 - 2;
            var size1 = textFontSize1 + unit1;
            console.log("size1")
            console.log(size1)
            $("#pc_head_s").css("font-size", size1);
            var newhead1 = $('#pc_head_s').height();
            if (newhead1 > 60) {
                headfize1()
            }

        }
    };

    function headfize2() {
        var head1 = $('#pc_head1').height();
        var thisEle1 = $("#pc_head1").css("font-size");
        var textFontSize1 = parseFloat(thisEle1, 10);
        var unit1 = thisEle1.slice(-2); //获取单位
        // console.log(textFontSize1)
        console.log(head1)
        if (head1 > 60) {
            textFontSize1 = textFontSize1 - 2;
            var size1 = textFontSize1 + unit1;
            console.log("size1")
            console.log(size1)
            $("#pc_head1").css("font-size", size1);
            var newhead1 = $('#pc_head1').height();
            if (newhead1 > 60) {
                headfize2()
            }

        }
    };
    // headfize();
