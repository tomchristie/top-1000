		// Detect when ad wasn't filled and remove ad slot wrapper
        // Doesn't apply to the takeover slot.
		googletag.pubads().addEventListener('slotRenderEnded', function(event){
            var slotId = event.slot.getSlotElementId();
            if (slotId == "dfp-takeover") return;
            if (slotId == "dfp-ci-bottom-fixed") return;
            if (slotId == "dfp-ci-after-game") return;

            var slotElement = document.getElementById(event.slot.getSlotElementId());
            if (!slotElement) return;

            var parentElm = slotElement.parentElement;
            if (!parentElm) return;

            if (parentElm == document.body) return;
            parentElm.style.display = event.isEmpty ? "none" : "block";
        });
        
        googletag.pubads().setRequestNonPersonalizedAds(1);

        
        googletag.enableServices();
	});
