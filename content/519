    <div id="landing-video-container"></div>
    <script id="landing-video-js">
        IMVU.loadLandingVideo = function() {
            if (!Date.now) {
                Date.now = function now() {
                    return new Date().getTime();
                };
            }
            var container = document.querySelector('#landing-video-container');
            container.innerHTML = '<video id="landing-video" autoplay loop preload="none" muted></video>';
            var video = document.querySelector('#landing-video');
            var webmSource = document.createElement('source');
            var mp4Source = document.createElement('source');
            video.appendChild(webmSource);
            video.appendChild(mp4Source);
            webmSource.setAttribute('type', 'video/webm');
            webmSource.setAttribute('src', 'https://static-akm.imvu.com/imvufiles/ftux-frontpage-20180319.webm');
            mp4Source.setAttribute('type', 'video/mp4');
            mp4Source.setAttribute('src', 'https://static-akm.imvu.com/imvufiles/ftux-frontpage-20180319.mp4');
            window.landingVideoInfo = {
                container: container,
                video: video,
            };
            video.addEventListener('loadeddata', function() {
                window.landingVideoInfo.isAlreadyLoaded = true;
                video.classList.add('is-refreshed');
                if (!video.classList.contains('is-full-height') && video.offsetHeight < container.offsetHeight) {
                    video.classList.add('is-full-height');
                }
                if (video.classList.contains('is-full-height') && video.offsetWidth < container.offsetWidth) {
                    video.classList.remove('is-full-height');
                }
            });
        };
        IMVU.removeLandingVideo = function() {
            let video = document.querySelector('#landing-video');
            if (video) {
                video.remove();
            }
            window.landingVideoInfo = {};
        };
        IMVU.loadLandingVideo();
    </script>
