  <script>
    function fullCss(u, m) {
      var l = document.createElement('link');
      l.rel = 'stylesheet';
      l.type = 'text/css';
      l.href = 'https://cdn.woopic.com/18d8339538654b1dbf96a30e92745731/css/hp-prospect.6733ad9994fcb2f9bdd6.css';
      l.media = 'all';
      document.getElementsByTagName('head')[0].appendChild(l);
    }
    if (window.addEventListener) {
      window.addEventListener('DOMContentLoaded', fullCss, false);
    } else {
      window.onload = fullCss;
    }
  </script>

      <script>
    var searchContainer = document.querySelector('#searchForm');

    var togglePlaceholder = function() {
      var searchInputHp = document.querySelector('#o-search-input-hp');
      var searchLabelHp = document.querySelector('.o-search-label');
      if (searchInputHp.value !== '') {
        searchLabelHp.style.opacity = 0;
        searchLabelHp.style.cssText = 'display:none !important';
      } else {
        searchLabelHp.style.opacity = 1;
        searchLabelHp.style.cssText = 'display:block !important';
      }
    }

    var callback = function() {
      var searchInputHp = document.querySelector('#o-search-input-hp');
      searchInputHp.addEventListener('input', togglePlaceholder, false);
      searchInputHp.addEventListener('change', togglePlaceholder, false);
      searchInputHp.addEventListener('keydown', togglePlaceholder, false);
      searchInputHp.addEventListener('focus', togglePlaceholder, false);
      searchInputHp.focus();
      observer.disconnect();
    }

    var observer = new MutationObserver(callback);
    var options = {subtree: true, childList: true};

    if (searchContainer !== null) {
      observer.observe(searchContainer, options);
    }
  </script>

