                                <script>
                                    var captchOk = false;
                                    var policyOk = false;
                                    var emailOk = false;

                                    function validateEmail() {
                                        var inputEmail = document.querySelector('#email');
                                        var email = inputEmail.value || '';
                                        console.log(email);
                                        var re = /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
                                        emailOk = re.test(email.toLowerCase());
                                        checkStateButton();
                                    }

                                    var verifyPolicy = function () {
                                        var inputPolicy = document.querySelector('#subscribe-informed');
                                        policyOk = inputPolicy.checked == 1;
                                        checkStateButton();
                                    }
                                    var verifyCallback = function (response) {
                                        console.log(verifyPolicy);
                                        captchOk = response.length > 0;
                                        checkStateButton();
                                    };

                                    function checkStateButton() {
                                        var button = document.querySelector('.js-submit');
                                        console.log("Policy : " + policyOk);
                                        console.log("Captcha : " + captchOk);
                                        console.log("Email : " + emailOk);
                                        if (captchOk && policyOk && emailOk) {
                                            button.removeAttribute('disabled')
                                            button.classList.add('on')
                                        }
                                        else {
                                            button.setAttribute('disabled', 'disabled')
                                        }
                                    }

                                    var myCallBack = function () {
                                        if ($('.captcha div#gcaptcha').length > 0) {
                                            var captcha = grecaptcha.render('gcaptcha', {
                                                'sitekey': '6Lc8mHQUAAAAAP8U58My2TKKpLBWSL1Ecqe2NVMN',
                                                'theme': 'light',
                                                'callback': verifyCallback,
                                            });
                                        }
                                    };
                                </script>

                                <script type='text/javascript'
                                        src='https://my.sendinblue.com/public/theme/version4/assets/js/src/subscribe-validate.js?v=1526904693'></script>
                                <!-- End : SendinBlue Signup Form HTML Code -->
                            </div>
