        trackType: '2',
        elementsStr: '',
        paramsRank: ["VerType", "webId", "version", "logTime", "ip", "guid", "domain", "pageCode", "eventType","traceName", "cusParam"],
        params: {
            VerType: "81",
            webId: "2",
            version: "1",
            domain: 'i.51job.com',
