      $('.footerTopics a').click(function (e) {
        e.preventDefault();
        var href = $(this).attr('href').replace(/&_ga=[0-9\.]*/i, '');
        var target = $(this).attr('target');
        if (target == '_blank') {
          window.open(href);
        } else {
          location.href = href;
        }
      });
    });
