function f0(d){return TAD.ld[d.getUTCDay()]+', '+d.getUTCDate()+' '+TAD.lm[d.getUTCMonth()]+' '+d.getUTCFullYear()+', '+p2(d.getUTCHours())+':'+p2(d.getUTCMinutes())+':'+p2(d.getUTCSeconds());}
function f1(d){return p2(d.getUTCHours())+':'+p2(d.getUTCMinutes());}
function f2(d){return ''+p2(d.getUTCSeconds());}
function f3(d){return '';}
function f4(d){return TAD.ld[d.getUTCDay()];}
function f5(d){return d.getUTCDate()+' '+TAD.sm[d.getUTCMonth()]+' '+d.getUTCFullYear();}
function f6(d){return ''+TAD.sm[d.getUTCMonth()];}
function f7(d){return ''+d.getUTCFullYear();}
