<script>
var uuidRun = "";                                    
if (getCookie("nct_uuid") == "" || getCookie("nct_uuid") == null || getCookie("nct_uuid") == "null") {
    var name = $.browser.name;
    var version = $.browser.version;
    var platformName = $.browser.platform;
    var platformVersion = window.navigator.platform;
    var data = {
        browserName: name,
        browserVersion: version,
        platformName: platformName,
        platformVersion: platformVersion,
        t: (new Date()).getTime()
    };
    $.getJSON(NCTLoginJWT.get_uuid, data, function (result) {
        if (result.data != "") {
            uuidRun = result.data;
            setCookie("nct_uuid", result.data, 30);
        }
    });
}                           
</script>

