    function smartyOnscrollCustom() {
        try {
            console.log(NCTInfo.trackingUsername);
            props = {};
            props.user_id = (NCTInfo.trackingUsername != "") ? NCTInfo.trackingUsername : "null";            
            props.url = window.location.href;                
            props.param = "null";
            var item1 = document.getElementsByClassName("ngheGiHomNay")[0];
            if (shouldSendItemView(item1)) {
                props.id = 'ngheGiHomNay';
                props.displayed_items = ngheGiHomNay;
                Countly.q.push(['minty_track_itemview', item1, props]);
            }
            var item2 = document.getElementsByClassName("albumHot")[0];
            if (shouldSendItemView(item2)) {
                props.id = 'albumHot';
                props.displayed_items = albumHot;
                Countly.q.push(['minty_track_itemview', item2, props]);
            }
            var item3 = document.getElementsByClassName("mvhot")[0];
            if (shouldSendItemView(item3)) {
                props.id = 'mvhot';
                props.displayed_items = mvhot;
                Countly.q.push(['minty_track_itemview', item3, props]);
            }
            var item4 = document.getElementsByClassName("listSong")[0];
            if (shouldSendItemView(item4)) {
                props.id = 'listSong';
                props.displayed_items = listSong;
                Countly.q.push(['minty_track_itemview', item4, props]);
            }            
        } catch(ex) {
            console.log(ex);
        }
    }
    $(document).scroll(function(){smartyOnscrollCustom()});
</script>

