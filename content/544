<script>
    $(document).ready(function(){
        
        $("#right_support_online").show();
        
        NCTAdv.initButtonOffAdv();
        var queryPopup = getQueryVariable(window.location.href, "popup");
        if (queryPopup == "dang-nhap") {
            NCTLoginJWT.showBoxLogin();
        } else if (queryPopup == "dang-ky") {
            NCTLoginJWT.showPopupRegisterNew();
        } else if (queryPopup == "quen-mat-khau") {
            NCTLoginJWT.showPopupFogotPass();
        } else if (queryPopup == "dang-nhap-facebook") {
            NCTOAuth.fbLogin();
        } else if(queryPopup == "dang-nhap-google") {
             NCTOAuth.googleLogin();
        }
    });                                
</script>






