<script>
    var trafficing_exclu_300mtf = false;

    var trafficing_exclu_all = (
        window.unify_dataSlayer[0].page.subSection === 'régimes'                                                // 2021-05-02
        || window.unify_dataSlayer[0].page.subSecLvl5 === 'lesrumdenuitaurtinolpur-revitaliftlaserdeloralparis' // 2021-05-04
        || window.unify_dataSlayer[0].page.subSecLvl5 === 'soinjouranti-agerevitaliftfillerdeloralparis'        // 2021-05-08
        || window.unify_dataSlayer[0].page.subSecLvl3 === 'lethermomtreaxillaireconnecte-torm'                  // 2021-05-16
        || window.unify_dataSlayer[0].page.subSection === 'vitamine'                                           // 2021-06-12
    );

    var trafficing_exclu_full = (
        window.unify_dataSlayer[0].page.level_1 === 'equipe'
        || window.unify_dataSlayer[0].page.subSecLvl3 === 'test-qi'
        || window.unify_dataSlayer[0].page.subSecLvl4 === 'panier-100-pourcent-sante-bien-choisir-aide-auditives' // 2021-04-30
    );

    // Remove dyninfeed for pages in exclu pub
    window.addEventListener('DOMContentLoaded', function() {
        if (trafficing_exclu_all || trafficing_exclu_full) {
            document.querySelectorAll('.doc-block-pub-dyn-infeed').forEach(function(e) {
                e.remove();
            });
        }
        if (trafficing_exclu_full && document.querySelector('#taboola-below-article-thumbnails')) {
            document.querySelector('#taboola-below-article-thumbnails').remove();
        }
    });
</script>



