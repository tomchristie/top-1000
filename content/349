			function checkSlide() {
				if (~this.wrapperOuter.context.className.indexOf('owl-slider-four')) {
					var addr = ["https://uid.me/pavel_kudinov", "https://uid.me/cloud_richard", "https://uid.me/carlos_tamaoki", "https://uid.me/alexmoloney", "https://uid.me/dikaya"];
					$("#carousel-personal-page .address-bar a").text(addr[this.owl.currentItem]).attr("href", addr[this.owl.currentItem]);
					$("#carousel-personal-page .item a").attr("href", addr[this.owl.currentItem]);
				}
			}
		});

