    <script>
  		(function (doc, win) {
		    var docEl = doc.documentElement,
	        recalc = function () {
            var clientWidth = docEl.clientWidth;
            if (clientWidth > 1280) {
            	clientWidth = 1280;
            }
            if (!clientWidth) return;
            var rate = clientWidth / 1280
            var oMeta = document.getElementsByTagName('meta')[1];
            oMeta.content = "width=device-width, initial-scale=" + rate + ", maximum-scale=1, user-scalable=no";
	        }
		    doc.addEventListener('DOMContentLoaded', recalc, false);
			})(document, window);
	  </script>
