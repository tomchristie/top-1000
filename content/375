<script>
  //advagg-ignore
  var toggleInstantLoader = function(state, callback) {
    if (state && window.matchMedia && window.matchMedia(DEVICE_QUERY).matches) {
      document.body.classList.add('instant-loader');
    } else {
      document.body.classList.remove('instant-loader');
    }
    if(callback) callback();
  };
  if(document.location.search && document.location.search.match(/play=instant/g)) {
    toggleInstantLoader(true);
  }
</script>



