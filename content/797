$(document).ready(function() {
	var itemSlider = $('#item_photo').lightSlider({
		item:4,
		loop:false,
		slideMove:4,
		//easing: 'cubic-bezier(0.25, 0, 0.25, 1)',
		speed:600,
		slideMargin:0,
		controls: false,
		pager: false
	});
	$('.main_photo .btn_prev').on('click', function () {
		itemSlider.goToPrevSlide();
	});
	$('.main_photo .btn_next').on('click', function () {
		itemSlider.goToNextSlide();
	});
});

/* 셀렉트박스 */
$(document).ready(function() {
	//$(".dropdown img.flag").addClass("flagvisibility");

	$(".dropdown dt a").click(function() {
		$(".dropdown dd").toggle();
		$(this).toggleClass("active");
	});

	$(".dropdown dd ul li a").click(function() {
		var text = $(this).html();
		$(".dropdown dt a span").html(text);
		$(".dropdown dd").hide();
		$(".dropdown dt a").toggleClass("active");
	});

	function getSelectedValue(id) {
		return $("#" + id).find("dt a span.value").html();
	}

	/* side_remote 활성화, 비활성화 제어 */
	$(".side_remote a.link_top").click(function() {
		$("a.link_bottom").addClass("btn_show");
		$("span.link_bottom").removeClass("btn_show");
	});

	$(".side_remote a.link_bottom").click(function() {
		$(this).removeClass("btn_show");
		$("span.link_bottom").addClass("btn_show");
	});
});

AOS.init({
  duration: 1200,
});
