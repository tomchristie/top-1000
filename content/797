if(page_name!='sitemap' && page_name!='error_page'){
	jQuery(function($) {
		$('#sokboPlayer').ulslide({
			 axis: 'y'				// x, y
			,affect: 'slide'		// slide, fade
			,slidetag: 'li'
			,duration: 500
			,width: 300
			,height: 34
			,autoslide: 4000
			,mouseover: true
		});
	});
}


////////////////////////////////////
// 검색
////////////////////////////////////
jQuery(function($) {
	$('#search1').on('focus', function(){
		if($('#search1').val()=='뉴스검색'){
			$('#search1').val('');
		}
	});

	$('#search1').on('blur', function(){
		$('#query_result1').hide();
		if($('#search1').val()==''){
			$('#search1').val('뉴스검색');
			$('.header_sear').hide();
			$('.btn_searchon').attr('class', 'btn_search');
		}
	});
});

function checkSearchSubmit(obj)
{
	if( obj.sn.value && obj.sn.value != '뉴스검색' )
	{
		return true;
	}
	else
	{
		alert('검색어를 입력하세요.');
		return false;
	}
}
