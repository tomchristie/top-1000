	<script type="text/javascript">
	function footerNav()
	{
		if($('.f_event_nav_all').css('display') == 'none')
		{
			$('.f_event_nav_all').show();
			$('.menu_btn a').removeClass('open');
			$('.menu_btn a').addClass('close');
			$('.menu_btn a').html('전체 서비스닫기<em></em>');
		}
		else
		{
			$('.f_event_nav_all').hide();
			$('.menu_btn a').removeClass('close');
			$('.menu_btn a').addClass('open');
			$('.menu_btn a').html('전체 서비스보기<em></em>');
		}
	}

jQuery(function($) {
	$('#eventPlayer').ulslide({
		 axis: 'y'				// x, y
		,affect: 'slide'		// slide, fade
		,slidetag: 'li'
		,duration: 500
		,width: 230
		,height: 61
		,autoslide: 4000
		,mouseover: true
	});
});
	</script>
