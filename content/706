<script>

    function expand_banner(){try{var player;if(navigator.appName.indexOf('Microsoft')!=-1){player=document.getElementById('izlesene_player');}else{player=document['izlesene_player'];}player.pause()}catch(e){};document.getElementById('VIRGUL_EXPANDABLE_CONTAINER').style.clip='rect(0px,'+970+'px,'+500+'px,0px)';};function collapse_banner(){try{var player;if(navigator.appName.indexOf('Microsoft')!=-1){player=document.getElementById('izlesene_player');}else{player=document['izlesene_player'];}player.resume();}catch(e){}document.getElementById('VIRGUL_EXPANDABLE_CONTAINER').style.clip='rect(0px,'+970+'px,250px,'+0+'px)';};function close_banner(){try{var player;if(navigator.appName.indexOf('Microsoft')!=-1){player=document.getElementById('izlesene_player');}else{player=document['izlesene_player'];}player.unmute();}catch(e){};closeRoadBlockStatic()};

    var refUrl = '';
    var isFirst = true;
    var virgulContent = '';
    var virgulPlayerAds = '';
    var seeding = false;

    window.openRoadBlock = function (type) {
        $('body').css('overflow-x','hidden');
        $('#roadblock').remove();
        $('#adContainer').append('<div id="roadblock" class="roadblock" style="position:relative">'+virgulContent.zones[52119].script+'</div>');

        if ( typeof virgulContent.zones[52119].leftBanner != 'undefined' ) {
            $('#roadblock').append('<div style="position: absolute;top: 0; left: -170px;">'+virgulContent.zones[52119].leftBanner+'<\/div>');
        }

        if ( typeof virgulContent.zones[52119].rightBanner != 'undefined') {
            $('#roadblock').append('<div style="position: absolute;top: 0; right: -170px;">'+virgulContent.zones[52119].rightBanner+'<\/div>');
        }

        $('.nb_default').hide();
        $('#roadblock_show').remove();
        $('#showad').hide();
        setCookie('roadblock_closed', 0);
    }

    window.openRoadBlockStatic = function () {
        var content = '<div id="vrgdiv53565f146e12927b78814446" style="width: 970px; height: 250px; margin: 0px auto;"><div style="position: relative;"><div id="VIRGUL_EXPANDABLE_CONTAINER" style="position: absolute; overflow: visible; top: 0px; width: 970px; height: 500px; z-index: 199; clip: rect(0px 970px 250px 0px);"><object width="970" height="500" id="banner_expand" classid="clsid:D27CDB6E-AE6D-11cf-96B8-444553540000" codebase="http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=9,0,45,0"><param name="allowfullscreen" value="true"><param name="allowscriptaccess" value="always"><param name="wmode" value="transparent"><param name="movie" value="https://www.izlesene.com/player_others/vodafone_live.swf&amp;clicktag=&amp;clickTAG=&amp;"><param name="flashvars" value=""><embed name="banner_expand" src="//www.izlesene.com/player_others/vodafone_live.swf?clickTag=&amp;clicktag=&amp;clickTAG=" wmode="transparent" allowfullscreen="true" allowscriptaccess="always" menu="false" type="application/x-shockwave-flash" width="970" height="500" flashvars=""></object></div></div></div>';

        $("body").css('overflow-x','hidden');
        $('#adContainer').html('<div id="roadblock" class="roadblock" style="position:relative">'+content+'</div>');
        $(".nb_default").hide();
        $("#roadblock_show").remove();
        $("#showad").hide();
        setCookie('roadblock_closed', 0);
    }

    window.closeRoadBlock = function (type) {
        try{collapse_banner();}catch(err){}

        if ( type !== "undefined" && type == "gm" ) {
            $('#adContainer').html('');
        } else {
            $('#roadblock').remove();
            setCookie('roadblock_closed', 1);
        }
    }

    window.closeRoadBlockStatic = function () {
        try{collapse_banner();}catch(err){}
        $('#roadblock').remove();
        setCookie('roadblock_closed', 1);
    }

    window.closeRoadBlockCookie = function () {
        $('#roadblock').remove();
        setCookie('roadblock_closed', 1);
    }

    window.closeRoadBlockCookieStatic = function () {
        $('#roadblock').remove();
        setCookie('roadblock_closed', 1);
    }

    window.close3DCubeAd = function () {
        $('#3DCubeAd').remove();
    }

    window.expandMobileExpandable = function () {
        var height = window.innerWidth * 300 / 414;
        $('#mobile_expandable').css({'height': height + 'px'});
        // $('#mobile_expandable').animate({'height': height + 'px'}, 1000);
    }

    window.collapseMobileExpandable = function () {
        var height = window.innerWidth * 50 / 414;
        $('#mobile_expandable').css({'height': height + 'px'});
        // $('#mobile_expandable').animate({'height': height + 'px'}, 1000);
    }

    window.displayPageskin = function(ad){

        window.pageskinAd = true;
        $('#right-ad,#left-ad').remove();

        if(typeof ad.removeSelector !== 'undefined' && ad.removeSelector !== ''){
            $(ad.removeSelector).remove();
        }

        $('body').append('<a id="newAdSkinClickUrl" target="_blank"></a>')

        $('#newAdSkinClickUrl').attr('href', ad.click);

        if(typeof ad.view !== 'undefined'){
            $('#newAdSkinClickUrl').append(ad.view);
        }

        if(ad.pageskinType === 'video'){

            $('#newAdSkinClickUrl').append('<video id="pageskin"><source type="video/mp4" src="'+ad.background+'"></video>').css({'color': '#fff'});

            
            if(ad.large){
                var top = '164px';
                $('.wrapper').css({
                    'margin-top': ($('header').height() + 361) + 'px',
                    'border': 'none',
                    'pointer-events': 'none'
                });
                $('.container').css({'pointer-events': 'all'});
                $('#topAdContainer').next().css({'background': '#fff'}); 
                $('#ngVirgulPageskin').css({'top': '-360px'});
            }

            $('#pageskin').css({
                'position': 'absolute',
                'top': top,
                'left': '50%',
                'z-index': '-100',
                'transform': 'translate(-50%, -50%)',
                'cursor': 'pointer',
                'z-index': '-2'
            }).attr('data-id', ad.adId).attr('data-resource', ad.resource).addClass('virgul-ad');
            $('body').css({'z-index': '-3'});   
            

            var pageskinVideo = $('#pageskin').get(0);
            pageskin.muted = true;

            if(ad.loop){
                pageskinVideo.loop = true;    
            }

            pageskinVideo.play();

            $('body').append('<div id="videoPageskinSound"><img id="videoPageskinSoundOn" src="https://static.virgul.com/theme/mockups/icons/speaker-on.png"><img id="videoPageskinSoundOff" src="https://static.virgul.com/theme/mockups/icons/speaker-off.png" style="display:none"></div>');

            $('#videoPageskinSound > img').css({'width': '100%'});
            $('#videoPageskinSound').css({
                'position': 'fixed',
                'top':  '135px',
                'left': '6px',
                'width': '60px',
                'cursor': 'pointer'
            }).click(function(){
                if(pageskinVideo.muted){
                    $('#videoPageskinSoundOn').hide();
                    $('#videoPageskinSoundOff').show();
                    pageskinVideo.muted = false;
                }
                else{
                    $('#videoPageskinSoundOff').hide();
                    $('#videoPageskinSoundOn').show();
                    pageskinVideo.muted = true;
                }
            });
        }

        if(typeof window.pageskinAdView !== 'undefined'){
            $('#newAdSkinClickUrl').append(window.pageskinAdView);
            window.pageskinAdView = undefined;
        }
    }

    window.assignAds = function (response) {
        virgulContent = response;
        virgulPlayerAds = response;

        $('#left-ad,#right-ad').remove();

        //extra calls
        if ( typeof response.extraCalls != 'undefined' && response.extraCalls != null ) {
            $('#rekTracker').append(response.extraCalls);
        }

        //roadblock
        if ( typeof response.zones[52119] != 'undefined' ) {
            openRoadBlock('first');
        }

        if(typeof response.zones[152647] !== 'undefined' && typeof window.pageskinAd === 'undefined'){
            $('.wrapper').append('<div id="right-ad">'+response.zones[152647].script+'</div>');
            $('#right-ad').css({
                'position': 'absolute',
                'top': 0,
                'left': $('.container').offset().left + $('.container').outerWidth() + 5
            });

            $(window).scroll(function(){
                var rightAdTop = $('.wrapper').get(0).getBoundingClientRect().top;
                if(rightAdTop < $('.navbar').height()){
                    $('#right-ad').css({'position': 'fixed', 'top': $('.navbar').height()});
                }
                else{
                    $('#right-ad').css({'position': 'absolute', 'top': 0});
                }
            });
        }

        if(typeof response.zones[152785] !== 'undefined' && typeof window.pageskinAd === 'undefined'){
            $('.wrapper').append('<div id="left-ad">'+response.zones[152785].script+'</div>');
            $('#left-ad').css({
                'position': 'absolute',
                'top': 0,
                'left': $('.container').offset().left - 165
            });
        }

        $(window).resize(function(){
            $('#right-ad').css({
               'left': $('.container').offset().left + $('.container').outerWidth() + 5
            });
            $('#left-ad').css({
                'left': $('.container').offset().left - 165
            });
        });

        if ( isFirst ) {

            if(typeof response.zones[153004] !== 'undefined' && typeof response.zones[153006] === 'undefined'){
                displayPageskin(response.zones[153004]);
            }

            if(typeof response.zones[152863] !== 'undefined'){
                $('.home-sidebar.left > h3').append($(response.zones[152863].script).css({'margin-left': '10px', 'vertical-align': 'baseline', 'display': 'inline'}));
            }

            /*960x60 banner*/
            if ( typeof response.zones[52157] != 'undefined' ) {
                $('#display960').show();
                $('#display960').html(response.zones[52157].script);
            }

            /*anasayfa takeover*/
            if ( typeof response.zones[2] != 'undefined' ) {
                $('#takeoverAd').show();
                $('#takeoverAdScript').html(response.zones[2].script);
            }
            /*splash*/
            if ( typeof response.zones[9] != 'undefined' ) {
                $('#splashAd').show();
                $('#splashAdScript').html(response.zones[9].script);
            }

            if ( typeof response.zones[52485] != 'undefined' && typeof response.zones[153006] === 'undefined'){
                $('#roadblock').hide();
                var div = document.createElement('div');
                div.innerHTML = '';
                div.setAttribute('id', 'fullscreenAd');
                div.style.cssText = 'position: fixed;width: 100%;height: 100%;z-index: 2147483647;top: 0;left: 0;margin: 0;padding: 0;';
                document.body.appendChild(div);
                $(div).append(response.zones[52485].script);
            }

            if ( typeof response.zones[152602] !== 'undefined'){
                $('#adContainer').prepend('<div id="topBanner">'+response.zones[152602].script+'</div>').removeClass('hide');
                $('#topBanner').css({'height': '100px', 'margin-bottom': '10px', 'margin-left': 'auto', 'margin-right': 'auto'});
            }


            if(typeof response.zones[153006] !== 'undefined'){

                var wrapperMarginTop = $('.wrapper').css('margin-top');
                var wrapperBG = $('.wrapper').css('background-color');
                $('#footerContainer').css({'margin-top': '-10px'});
                $('.wrapper').css({'background': '#fff', 'transition': '3s', 'margin-top': window.innerHeight + 'px'});
                $('.navbar').css({'transition': '3s', 'opacity': '0'});
                setTimeout(function(){
                    $('.navbar').hide();
                }, 3000);
                
                $('body').prepend('<div id="gravityAd" style="position:fixed;z-index:-2;width: 100%;height: 100%;top: -100%;left: 0;margin: 0;padding: 0;transition: 3s">'+response.zones[153006].script+'</div>');
                $('#fullscreenAd').css({'z-index': '-5'});

                setTimeout(function(){
                    $('#gravityAd').css({'top': '0%'});    
                }, 10);
                

                window.closeSlidingFullscreen = function(){
                    $('#footerContainer').css({'margin-top': 'initial'});
                    $('.navbar').show();
                    $('.navbar').css({'opacity': '1'});
                    $('.wrapper').css({'margin-top': wrapperMarginTop, 'background-color': wrapperBG});
                    $('#gravityAd').on('transitionend', function(){
                        $('#gravityAd').remove();
                    }).css({'opacity': '0', 'top': '-100%'});
                }
            }

            isFirst = false;
        }

        /*header ad*/
        if ( typeof response.zones[52164] != 'undefined' ) {
            $('#headerAd').show();
            $('#headerAd').html(response.zones[52164].script);
        }

        if ( typeof response.zones[152537] != 'undefined' ) {
            window._nativeAdRandomPosition = Math.round(Math.random()) % 2;
            if ( $('#nativeAd').length > 0 ) {
                $('#nativeAd').html(response.zones[152537].script);
            } else {
                window._beforeNativeAdHTML = $('<div />').append($($('.videos-vertical > a')[_nativeAdRandomPosition]).clone()).html();
                $($('.videos-vertical > a')[_nativeAdRandomPosition]).replaceWith('<div class="card vertical-card no-padding" id="nativeAd">' + response.zones[152537].script + '</div>');
                setTimeout(function () {
                    if ( $('#nativeAd > div > div > div').height() === 0 ) {
                        $('#nativeAd').replaceWith(window._beforeNativeAdHTML);
                    }
                }, 3000);
            }
        }

        /* Video Wall */
        if ( typeof response.zones[52240] !== 'undefined' && typeof response.zones[52241] !== 'undefined' && typeof response.zones[52242] !== 'undefined' && typeof response.zones[52243] !== 'undefined' ) {
            var _videoWallAutoplaySeeding = false;
            if ( typeof response.zones[52251] !== 'undefined' ) {
                _videoWallAutoplaySeeding = true;
            }
            if ( ! _videoWallAutoplaySeeding || typeof response.zones[52251] !== 'undefined' ) {
                var _videoWallLeftFirst = '<li class="big_four">' + response.zones[52240].script + '</li>';
                var _videoWallLeftSecond = '<li class="big_four">' + response.zones[52241].script + '</li>';
                if ( _videoWallAutoplaySeeding ) {
                    var _videoWallMiddle = '<li class="big_image" id="videoWallSeedingVideo"></li>';
                } else {
                    var _videoWallMiddle = '<li class="big_image">' + response.zones[52244].script + '</li>';
                }
                var _videoWallRightFirst = '<li class="big_four">' + response.zones[52242].script + '</li>';
                var _videoWallRightSecond = '<li class="big_four">' + response.zones[52243].script + '</li>';
                var _videoWall = '';
                _videoWall += '<section class="featured_area"><div class="inner" id="featured_1"><div class="featured_videos"><ul class="column_left">' + _videoWallLeftFirst + _videoWallLeftSecond + '</ul>';
                _videoWall += '<ul class="big_image ml_20">' + _videoWallMiddle + '</ul>';
                _videoWall += '<ul class="column_right ml_20">' + _videoWallRightFirst + _videoWallRightSecond + '</ul>';
                _videoWall += '</div></div></section><div class="clear"></div>';
                $('#adContainer').html(_videoWall);
            }
        }

        /* Video Wall Autoplay Seeding */
        if ( _videoWallAutoplaySeeding ) {
            var videoid = response.zones[52251].video_id;
            var link = response.zones[52251].link;
            link = encodeURIComponent(link);
            $.ajax({
                type: 'GET',
                url: '/ajax/index/seeding',
                data: 'videoid=' + videoid + '&link=' + link+"&from=index",
                success: function (seedResponse) {
                    if ( seedResponse ) {
                        $('#videoWallSeedingVideo').html(seedResponse);
                        $('#videoWallSeedingVideo div').css('margin', 0);
                    }
                }
            });
        }

        /*autoplay seeding*/
        if ( typeof response.zones[52146] != 'undefined' && typeof response.zones[52270] == 'undefined' ) {
            var videoid = response.zones[52146].video_id;
            var link = response.zones[52146].link;
            link = encodeURIComponent(link);
            $.ajax({
                type: 'GET',
                url: '/ajax/index/seeding',
                data: 'videoid=' + videoid + '&link=' + link + "&from=index",
                success: function (seedResponse) {
                    if ( seedResponse ) {
                        $('#video_wall_seeding').html(seedResponse);
                        $('#video_wall_seeding div:first').css('margin', '0');
                        if ( response.zones[52146].view ) {
                            $('#rekTracker').append(response.zones[52146].view);
                        }
                    }
                }
            });
        } else if ( typeof response.zones[52270] != 'undefined' ) {
            var code = response.zones[52270].script;
            $('#video_wall_seeding').html(code);
            $('#video_wall_seeding div:first').css('margin', '0');
        }

        /*scroll ad*/
        if ( typeof response.zones[11] != 'undefined' ) {
            setTimeout('getAnswer()', 1000);
            setTimeout(function () {
                $('#scrollAdScript').html(response.zones[11].script);
            }, 1000);
        }

        /*300x250 anasayfa test*/
        if ( typeof response.zones[1039] != 'undefined' ) {
            $('#rek_display_1').show();
            $('#rek_display_1').html(response.zones[1039].script);
        }

        /*anasayfa 300x500*/
        if ( typeof response.zones[1916] != 'undefined' ) {
            $('#rek_display_1').show();
            $('#rek_display_1').html(response.zones[1916].script);
        }

        /*5 li seeding*/
        if ( typeof response.zones[52213] != 'undefined' && typeof response.zones[52214] != 'undefined' && typeof response.zones[52215] != 'undefined' && typeof response.zones[52216] != 'undefined' && typeof response.zones[52217] != 'undefined') {

            var seedZones = [52213, 52214, 52215, 52216, 52217];
            var seed5Html = '';

            seedZones.shuffleArray();

            seed5Html += '<div class="featured_videos col_5"><ul>';
            for ( var fCnt = 0; fCnt < 5; fCnt++ ) {
                response.zones[seedZones[fCnt]].thumbnail = response.zones[seedZones[fCnt]].thumbnail.replace("160x100", "176x110");
                seed5Html += (fCnt == 4) ? '<li style="margin-right: 0">' : '<li>';
                seed5Html += '<a onclick="track_event(\'new_index\',\'sponsor_5_'+ fCnt +'\');" href="'+response.zones[seedZones[fCnt]].link+'" title="'+response.zones[seedZones[fCnt]].title+'">';
                seed5Html += '<span class="play_icon sp_new"></span>';
                seed5Html += '<img class="lazy" src="'+response.zones[seedZones[fCnt]].thumbnail+'" alt="'+response.zones[seedZones[fCnt]].title+'" width="176" height="110">';
                seed5Html += '<span class="bg"></span><h4>'+response.zones[seedZones[fCnt]].title+'</h4></a></li>';
            }
            seed5Html += '</ul><div class="clear"></div></div>';

            $('#bottom_featured_v2').append(seed5Html);
        }

        /*seeding-1*/
        if ( typeof response.zones[2112] != 'undefined' ) {
            var seedHtml = '';
            seedHtml += '<a href="'+response.zones[2112].link+'" title="'+response.zones[2112].title.replace("<![CDATA[", "").replace("]]>", "")+'" onclick="track_event(\'new_index\',\'sponsor_0\');"><div>';
            seedHtml += '<div class="card"><div class="card-thumbnail">';
            seedHtml += '<img src="'+response.zones[2112].thumbnail.replace("100x62", "280x158")+'" alt="'+response.zones[2112].title.replace("<![CDATA[", "").replace("]]>", "")+'" height="158" width="280">';
            seedHtml += '<span class="card-thumbnail-overlay"></span><span class="card-thumbnail-play"><span class="icon icon-thumbnail-play"></span></span>';
            seedHtml += '<span class="card-thumbnail-duration">' + response.zones[2112].duration + '</span>';
            seedHtml += '</div><div class="card-content">';
            seedHtml += '<span class="card-content-title" title="'+response.zones[2112].title.replace("<![CDATA[", "").replace("]]>", "")+'">'+response.zones[2112].title.replace("<![CDATA[", "").replace("]]>", "")+'</span>';
            seedHtml += '<span class="card-content-channel" title="'+response.zones[2112].account.replace("<![CDATA[", "").replace("]]>", "")+'">'+response.zones[2112].account.replace("<![CDATA[", "").replace("]]>", "")+'</span>';
            seedHtml += '<span class="card-content-views">' + response.zones[2112].count + ' izlenme</span>';
            seedHtml += '</div></div>';

            $( "#sponsored1" ).replaceWith(seedHtml );

            if ( response.zones[2112].view ) {
                $('#rekTracker').append(response.zones[2112].view);
            }
        } else {
            $('.mini_title').hide();
        }


        /*seeding-2*/
        if ( typeof response.zones[2113] != 'undefined' ) {
            var seedHtml = '';
            seedHtml += '<li>';
            seedHtml += '<a href="'+response.zones[2113].link+'" title="'+response.zones[2113].title.replace("<![CDATA[", "").replace("]]>", "")+'" onclick="track_event(\'new_index\',\'sponsor_1\');">';
            seedHtml += '<img src="'+response.zones[2113].thumbnail+'" alt="'+response.zones[2113].title.replace("<![CDATA[", "").replace("]]>", "")+'" width="100" height="62">';
            seedHtml += '<h4>'+response.zones[2113].title.replace("<![CDATA[", "").replace("]]>", "")+'</h4>';
            seedHtml += '<span class="view">'+response.zones[2113].count+' izlenme</span>';
            seedHtml += '</a>';
            seedHtml += '</li>';
            $('#sponsored2').html(seedHtml);
            if ( response.zones[2113].view ) {
                $('#rekTracker').append(response.zones[2113].view);
            }
        } else {
            $('.mini_title').hide();
        }

        /*seeding-3*/
        if ( typeof response.zones[52130] != 'undefined' && seeding == false ) {
            seeding = true;
            var liid = "#bigthumb_3";
            var seedHtml = '';
            seedHtml += '<a onclick="track_event(\'new_index\',\'first_three_seeding\');" href="'+response.zones[52130].link+'" title="'+response.zones[52130].title.replace("<![CDATA[", "").replace("]]>", "")+'" onclick="track_event(\'new_index\',\'sponsor_1\');">';
            seedHtml += '<span class="play_icon sp_new"></span>';
            seedHtml += '<img src="'+response.zones[52130].thumbnail.replace("160x120","225x141").replace("100x62","225x141")+'" alt="'+response.zones[52130].title.replace("<![CDATA[", "").replace("]]>", "")+'" width="225" height="141">';
            seedHtml += '<span class="bg"></span>';
            seedHtml += '<h4>'+response.zones[52130].title.replace("<![CDATA[", "").replace("]]>", "")+'</h4>';
            seedHtml += '<p>'+response.zones[52130].count.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ".")+' izlenme </p>';
            seedHtml += '</a>';
            $(liid).html(seedHtml);
            if ( response.zones[52130].view ) {
                $('#rekTracker').append(response.zones[52130].view);
            }
        }
    }

    window.assignAdsMobile = function (response) {
        virgulContent = response;
        virgulPlayerAds = response;

        //extra calls
        if (typeof response.extraCalls != 'undefined' && response.extraCalls != null ) {
            $('#rekTracker').append(response.extraCalls);
        }

        if(isFirst){
            /*flying carpet top banner*/
            if(typeof response.zones[152613] !== 'undefined'){
                // var height = window.innerWidth * 100 / 320;

                var height = 180; // changed for tadim live concert
                $('.wrapper').prepend('<div id="flying_carpet_banner"></div>');
                $('#flying_carpet_banner').css({'height': height + 'px', 'padding': '0px', 'z-index':'2'}).append(response.zones[152613].script).show();
                $('#flying_carpet_banner > div').css({'z-index': '2'});
            }
            /* actual flying carpet */
            if(typeof response.zones[152612] !== 'undefined'){
                var ad = response.zones[152612];

                var abs1Height = $('.videos-vertical.left > a')[0].getBoundingClientRect().top + $('.videos-vertical.left > a').outerHeight(true);
                $('body').append('<div id="absCarpet1" style="position:absolute;width:100%;height:'+abs1Height+'px;z-index:1;background-color:#F6F6F6;top:-60px;"></div>');

                $('<a style="height:400px;margin-bottom:10px;" href="'+ad.click+'" target="_blank"></a>').insertAfter($($('.videos-vertical.left > a')[0]));
                var abs2Top = abs1Height + 400 - 60;
                var abs2Height = $('body').get(0).scrollHeight - abs2Top;

                $('body').append('<img src="'+ad.background+'" style="position:fixed;top: 0;z-index: 0;width:100%">');
                $('body').append('<div id="absCarpet2" style="position:absolute;width:100%;height:'+abs2Height+'px;top:'+abs2Top+'px;z-index:1;background-color:#F6F6F6"></div>');
                $('.wrapper > .container, .wrapper > .videos-horizontal').css({'position': 'relative', 'z-index': '2'});
                $('#footerContainer').css({'position':'relative', 'z-index':'2'});
                $('#rekTracker').append(ad.view);

                setTimeout(function(){
                    if($('#ad_300_250').html() !== ''){
                        var abs2Height = $('body').get(0).scrollHeight - abs2Top - 30;
                        $('#absCarpet2').css({'height': abs2Height.toString() + 'px'});
                    }
                    $('#ad_300_250').outerHeight(true);
                }, 3000);
            }

            if(typeof response.zones[152623] !== 'undefined'){
                var height = window.innerWidth * 50 / 414;
                $('.wrapper').prepend('<div id="mobile_expandable">'+response.zones[152623].script+'</div>');
                $('#mobile_expandable').css({'width': '100%', 'height': height + 'px', 'overflow': 'hidden', 'position':'relative', 'transition': 'height 1s', 'transition-timing-function': 'ease-in'});
                // $('#mobile_expandable').css({'width': '100%', 'height': height + 'px', 'overflow': 'hidden', 'position':'relative'});
            }

            if(typeof response.zones[152617] !== 'undefined'){
                $('body').append('<div id="3DCubeAd" style="width:100%;height:100%;position:fixed;bottom:0;z-index:2147483647">'+virgulContent.zones[152617].script+'</div>');
            }

            if(typeof response.zones[152732] !== 'undefined'){
                if(!fullscreenAdStarted){
                    $('#fullscreenAd').remove();
                    $('body').append('<div id="fullscreenAd" style="width:100%;height:100%;position:fixed;bottom:0;z-index:2147483647">'+virgulContent.zones[152732].script+'</div>');
                }
            }

            if(typeof response.zones[153019] !== 'undefined'){
                $("#splashAd").append(response.zones[153019].script).show();
            }

            isFirst = false;
        }

        /*320x50*/
        if (typeof response.zones[152792] != 'undefined') {
            $("#ad_320_50_below_navigation").show();
            $("#ad_320_50_below_navigation").html(response.zones[152792].script);
        }

        // 300x250 below featured(top 5) videos
        if( typeof response.zones[52428] !== 'undefined' ){
            $('#ad_300_250_below_featured').html(response.zones[52428].script);
        }

        if( typeof response.zones[153153] !== 'undefined' ){
            $('#ad_320_50_first').html(response.zones[153153].script);
        }

        if( typeof response.zones[153154] !== 'undefined' ){
            $('#ad_320_50_second').html(response.zones[153154].script);
        }

        /*320x100*/
        if (typeof response.zones[52348] != 'undefined') {
            $("#ad_320_100_container").show();
            $("#ad_320_100_container").html(response.zones[52348].script);
        }

        /*320x100*/
        if (typeof response.zones[152791] != 'undefined') {
            $("#ad_320_100_below_featured").show();
            $("#ad_320_100_below_featured").html(response.zones[152791].script);
        }

        /*300x250*/
        if (typeof response.zones[52480] != 'undefined') {
            $("#ad_300_250_container").show();
            $("#ad_300_250_container").html(response.zones[52480].script);
        }

        /*336x280*/
        if (typeof response.zones[52482] != 'undefined') {
            $("#ad_336_280_container").show();
            $("#ad_336_280_container").html(response.zones[52482].script);
        }

        /*320x600*/
        if (typeof response.zones[52481] != 'undefined') {
            $("#ad_320_600_container").show();
            $("#ad_320_600_container").html(response.zones[52481].script);
        }

        /*336x600*/
        if (typeof response.zones[52483] != 'undefined') {
            $("#ad_336_600_container").show();
            $("#ad_336_600_container").html(response.zones[52483].script);
        }

        /*300x250*/
        if (typeof response.zones[52483] != "undefined") {
            $('#ad_300_250').html(response.zones[52483].script);
            $("#ad_300_250").css({"display" : "block"});
            $("#ad_300_250").show();
        } else {
            $('#ad_300_250').html('<a title="Müzik Dinle" href="https://www.izlesene.com/muzik" onclick="track_event(\'new_index\',\'banner_300\');"><img src="//c1.imgiz.com/html5/images/izlesene-muzik-banner.png" alt="Müzik Dinle" width="300" height="250" border="0"/></a>');
        }

        if ( typeof response.zones[152583] !== 'undefined' ){
            $('#ad_sticky').html(response.zones[152583].script).show();
        }

        /*masthead */
        if (typeof response.zones[52345] != 'undefined') {
            var mastheadHtml = "";
            var thumbnailUrl = response.zones[52345].thumbnail.replace('_100x62.', '_125x75.');
            var title =  response.zones[52345].title;
            var account =  response.zones[52345].account;
            var duration =  response.zones[52345].duration;
            var viewCount =  response.zones[52345].count;
            var hashtag =  response.zones[52345].hashtag;
            var click =  response.zones[52345].click;

            $('#ad_masthead_a').attr("href", response.zones[52345].link);
            $('#ad_masthead_a').attr("title", title);

            mastheadHtml += '<div class="card-thumbnail">';
            mastheadHtml += '<img src="' + thumbnailUrl + '" alt="' + title + '" height="158" width="280" style="display: inline-block;">';
            mastheadHtml += '<span class="card-thumbnail-overlay"></span>';
            mastheadHtml += '<span class="card-thumbnail-duration">' + duration + '</span>';
            mastheadHtml += '</div>';
            mastheadHtml += '<div class="card-content" style="max-height: 88px !important;">';
            mastheadHtml += '<span class="card-content-title" title="' + title + '">' + title + '</span>';
            mastheadHtml += '<span class="card-content-views">' + viewCount + ' izlenme</span>';
            mastheadHtml += '<div class="card-content-views" style="background-color:#F6D658;width:auto;display:inline;padding:0px 4px;font-size:11px;color:#FFF;">Reklam</div>';
            mastheadHtml += '<a href="' + click + '" target="_blank">' + hashtag + ' <img src="//www.izlesene.com/html5/images/m/ad-link.png"></a>';
            mastheadHtml += '</div>';
            mastheadHtml += '<div id="impression" style="display:none;">' + response.zones[52345].view + "</div>";

            $("#ad_masthead_container").show();
            $("#ad_masthead").html(mastheadHtml);
            $("#ad_masthead").show();
        }

        if (typeof response.zones[152610] !== 'undefined'){
            // var height = window.innerWidth * 50 / 414;
            // $('body').append('<div id="api_bar"></div>');
            // $('#api_bar').append(response.zones[152610].script).css({'height': height + 'px', 'padding': '0px', 'position':'fixed', 'z-index':'2147483646', 'width': '100%', 'bottom': '0'}).show();

            $('#sticky_bottom').remove();
            $('body').append('<div id="sticky_bottom"></div>');
            $('#sticky_bottom').append(response.zones[152610].script).css({'max-height': '175px', 'padding': '0px', 'position':'fixed', 'z-index':'2147483646', 'width': '100%', 'bottom': '0'}).show();
        }
    }

    window.virgulCatId = "izlesene_anasayfa";
    window.virgulSiteId = 'izlesene';
    window.virgulPageId = 'anasayfa';
    window.virgulCallbackFunction = assignAds;

    if (typeof window.innerWidth !== 'undefined' && window.innerWidth < 768) {
        window.virgulSiteId = 'mobil_alanlar';
        window.virgulCatId = virgulCatId.replace('izlesene', 'mobil_alanlar');
        window.virgulCallbackFunction = assignAdsMobile;
    }

    window.addEventListener('load', function () {
        izlesene.virgulAd();
    });

