<div id="fb-root"></div>

<script type="text/javascript">
    var fbloaded = false;
    var faceBookAppID = 300689731773330;
    window.fbAsyncInit = function ()
    {
        FB.init({ appId: faceBookAppID, status: true, cookie: 'fixya.com' ,xfbml:false });
    };

    function fblogin(elem)
    {
        head.load(document.location.protocol + '//connect.facebook.net/en_US/all.js', function(){
            var cb = function (response)
            {
                if(response.status && response.status=='connected' ){
                    onLogin(response);
                }
            };
    		
            fbAsyncInit();
            FB.login(cb, { scope: 'email' });
        });

        return false;
    }
    function onLogin(response) {
        
        var form = $("#fbConnectForm");
        if (form.length > 0) {
            var input = form.find("input[name=fbat]");
            if (input.length > 0) {
                input.val(response.authResponse.accessToken);
            }
            input = form.find("input[name=fbid]");
            if (input.length > 0) {
                input.val(response.authResponse.userID);
                form.submit();
            }
            
        }
    }
</script>



