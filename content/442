<script>
    function refreshAdSlots(google_slots_array)
    {
        googletag.cmd.push(function() {
            if (window.deployads) {
                deployads.push(function() {
                    deployads.gpt.pubadsRefresh(google_slots_array); 
                });
            } else {
                googletag.pubads().refresh(google_slots_array);
            }
        });
    }
</script>


	<script type="text/javascript">
		
        
            function AddFloatListener(isFloat, elementId)
            {
                if (isFloat)
                {
                    var floatListener = function () {
                        var headerTop = 78;
                        var elemTop = $(elementId).parent().offset().top;
                        if($(window).scrollTop() < elemTop - headerTop)
                        {
                            $(elementId).css('position', '');
                            $(elementId).css('top', '');
                            $(elementId).css('justify-content', 'start');
                        }
                        else
                        {
                            $(elementId).parent().css('height',$(elementId).height() + "px");
                            $(elementId).css('position', 'fixed');

                            var footerTop = $("footer").offset().top;
                            
                            var x = $(window).scrollTop() + $(elementId).parent().height()
                            if(footerTop - headerTop > x)
                            {
                                $(elementId).css('top', headerTop + "px");
                            }
                            else
                            {
                                $(elementId).css('top', (footerTop - x).toString() + "px");
                            }
                        }
                    };
                    window.addEventListener('scroll', floatListener);
                }
            }
        

    
	</script>
	
        
        
    


