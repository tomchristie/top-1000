	<script type="text/javascript">
		
        var func = function() {

            var events = [];
            var len = events.length;
            if (len > 0) 
            {
                for (var i = 0;  i<len; i++ ) {

                    var evnt = events[i];
                    
                    if (evnt.eventType == "Click" ||evnt.eventType==0) {
                        logEvent(evnt.eventId);
                    } 
                    else if (evnt.eventType == "PageView" ||evnt.eventType==1)
                    {
                        logEventPtype(1, evnt.eventId);
                    } 
                    else 
                    {
                        if (evnt.eventName.length > 0 && evnt.eventPage.length > 0) {
                            _gaq.push(['_trackEvent', evnt.eventPage, evnt.eventName, window.location.pathname, undefined, true]);
                        }
                    }
                        
                }
            }


        };
        head.ready(function () {
        	func();
        });

	</script>
	
