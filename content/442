<script>
    function navUrl(ele) {
        if ($("#" + ele).length > 0) {
            $("#" + ele).trigger('click');
        } else {
            $("[data-ref='" + ele + "']").trigger('click');
        }
    }
</script>

    
    <script type="text/javascript">
        $(function () {
            $('a').on('click',
                function (event) {
                    event.preventDefault();
                    var target = $(this).context;
                    var hash = target.hash;
                    var href = target.href;//.toLowerCase();

                    var currentLoc = $(window).scrollTop();
                    //Current page remove #
                    var currentUrl = location.href.split("#")[0];

                    var onlyHash = target.href.replace(currentUrl, '');

                    if ($(target).data("reload")) {
                        window.location = currentUrl;
                    }

                    if ((hash === "" && target.href === location.href) ||
                        (hash === "" && href === "") ||
                        onlyHash === "#" ||
                        onlyHash.indexOf("javascript:") > -1) {
                        $('html, body').animate({ scrollTop: currentLoc });
                        return;
                    }


                    if (hash !== "") {
                        try {
                            $('html, body').animate({
                                    scrollTop: ($(hash).offset().top - 120)
                                },
                                800,
                                function () {
                                    // Add hash to the end of url
                                    window.location.hash = hash;
                                });
                        } catch (err) {}
                    } else {
                        window.location = href;
                    }

                });

        });

    </script>
