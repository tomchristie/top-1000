    <script>
        function changeBtn() {
            window.timer && clearInterval(window.timer)
            window.timer = setInterval(function(){
                if($('#big-event-tab-btn-0').length){
                    $('#big-event-tab-btn-0').html('评审入口')
                    $('#big-event-tab-btn-0').attr('mx-click', '')
                    $('#big-event-tab-btn-0').attr('href', 'https://www.alimama.com/index.htm#!/big-event/event-detail?key=mAwards&id=99999&tab=0')
                    clearInterval(window.timer)
                }
            }, 500)
        
    }

    function isJudge() {
        if(window.isJdg) {
            changeBtn()
            return
        }
        const isPre = false;
        const host = isPre ? "pre-fether.m.taobao.com" : "fether.m.taobao.com";
        const params = {
            method: "GET",
            headers: {
            "X-Requested-With": "XMLHttpRequest",
            "Content-Type": "application/json",
            Accept: "application/json"
            },
            credentials: "include",
        }
        window.fetch(`https://${host}/mm_awards_pro/get_member_info?_f_needLogin=1${ isPre ? "&_f_storage=test&token=x" : ""}`, params).then(function(response){
            response.json().then(function(res){
                if(res && res.result && res.result.status == 0){
                    changeBtn()
                    window.isJdg = true;
                } else {
                }
            })
        });
    }
    let isAward2021 = function(test){
        return test.indexOf('key=mAwards&id=10012')>=0 || test.indexOf('key=mAwards&id=10013')>=0
    } 
    if(isAward2021(location.href)){
        isJudge()
    }
    window.addEventListener('hashchange',function(event){
     if(isAward2021(location.href)){
        isJudge()
    }
    })
    </script>
