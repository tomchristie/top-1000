<script>
    function toggleNavCheckboxes() {
        var checkboxes = document.querySelectorAll('nav input[type=checkbox]');
        for(var i = 0; i < checkboxes.length; i++) {
            var checkbox = checkboxes[i];
            if(checkbox && checkbox.checked) {
                checkbox.value = false;
            }
        }
    }

    (function(window, document){
        var mobileMenuToggle = document.querySelector('#mobile-menu-toggle > input[type=checkbox]');
        if(mobileMenuToggle) {
            mobileMenuToggle.addEventListener('change', function(e) {
                var target = e.currentTarget || e.target;
                var isChecked = target ? target.checked : false;
                if(!isChecked) {
                    toggleNavCheckboxes();
                }
            });
        }

        if (document.readyState === 'loading') {
            window.addEventListener('DOMContentLoaded', function() {
                toggleNavCheckboxes();
            });
        } else {
            toggleNavCheckboxes();
        }
    })(window, document)
</script>

