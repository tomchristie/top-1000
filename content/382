function SetStayNow() {
/* デフォルト表示年月 : クイック検索は本日＋<diffday=14>日 */
var diffday = 1;
/* 現在日は、日次JOB作成の日付から取得 (PC時間依存対策) */
var year  = getNowYear();   //年(４桁)
var month = getNowMonth() - 1;  //月（0〜11）
