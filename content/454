                $(this).addClass("active").siblings().removeClass("active");
                var i = $(this).index() == 0 ? 0 : ($(this).index() == 2 ? 1 : 2);
                //alert(i);
                $(".today").eq(i).show().siblings(".today").hide();
            });

            $(".cont-tab2>span").hover(function () {
                $(this).addClass("active").siblings().removeClass("active");
                var i = $(this).index() == 0 ? 0 : 1;
                $(".host-list").eq(i).show().siblings(".host-list").hide();
            });
            $('.rankList li').hover(function () {//读书板块切换
                var idx = $(this).index();
                $('.leftPP div').eq(idx).stop().show().siblings().hide();
            })
            var swiper = new Swiper('.focus-scroll', {
                loop: true,
                autoplay: {
                    delay: 3000,
                    stopOnLastSlide: false,
                    disableOnInteraction: false
                },
                navigation: {
                    nextEl: '.swiper-button-next',
                    prevEl: '.swiper-button-prev',
                },
                on: {
                    init: function () {
                        $(".focus-scroll-num").eq(0).text('1/' + ($(".focus-scroll li").length - 2));
                    },
                    slideChangeTransitionEnd: function () {
                        var i = this.activeIndex > $(".focus-scroll li").length - 2 ? 1 : this.activeIndex < 1 ? $(".focus-scroll li").length - 2 : this.activeIndex;
                        $(".focus-scroll-num").eq(0).text(i + '/' + ($(".focus-scroll li").length - 2));
                    }
                }

            });






            var r_t_signTop = $(".r-t-sign").offset().top;

            $(window).on("scroll", setLeftNavPos);   //r-t-sign
            function setLeftNavPos() {
                var infoFlowTop = $(".info-flow").offset().top;
                var wstop = $(window).scrollTop();
                if (wstop >= infoFlowTop) {
                    $(".info-flow-nav").css({ "position": "fixed", "top": "0" });
                    $(".info-flow-list").css({ "margin-left": "160px" });
                }
                else {
                    $(".info-flow-nav").css({ "position": "static", "top": "0" });
                    $(".info-flow-list").css({ "margin-left": "40px" });
                }

                //右侧的

                if (wstop >= r_t_signTop) {
                    $(".r-t-sign").css({ "position": "fixed", "top": "0" });
                } else {
                    $(".r-t-sign").css({ "position": "static", "top": "0" });

                }
            }

        })


        var stype = parseInt($(".info-flow-nav .active").index());
        var isFirst = false;
        //li切换内容
        $(".info-flow-nav li").click(function () {
            $(this).addClass("active").siblings().removeClass("active");
            $(".info-flow-cont").eq($(this).index()).show().siblings(".info-flow-cont").hide();
            stype = stype = parseInt($(".info-flow-nav .active").index());
            isFirst = flowInfo['type' + stype] == 2 ? true : false;

            $(window).off("scroll", getFlowCont);
            var topLen = $(".info-flow-list").offset().top;
            if (flowInfo['type' + stype] == 2) {
                $(window).scrollTop(topLen);
            }
            setTimeout(function () {
                $(window).on("scroll", getFlowCont);
            }, 100);
        })

        var isGet = true;
        //每页加载信息
        var flowInfo = {
            type0: 2,
            type1: 2,
            type2: 2,
            type3: 2,
            type4: 2
        }
        //点击加载

        $(".more-feed").click(function () {
            var _this = this;
            var url = "/xmlhttp/GetDefaultv3JsonData.aspx?op=" + (stype + 1) + "&pageindex=" + flowInfo['type' + stype];
            if (flowInfo['type' + stype] >= 8) { $(this).html("<span>已经全部加载</span>"); return false; }
            getAjaxCont(url, function (data) {
                flowInfo['type' + stype]++;
                $(_this).before(data);
            })
        });

        //列表
        //var scrollFnList = [getFlowJs, getFlowGj, getFlowLs, getFlowSh, getFlowMs];


        $(window).on("scroll", getFlowCont);
        function getFlowCont() {
            if (flowInfo['type' + stype] >= 8) { $(".info-flow-cont").eq(stype).children(".more-feed").html("<span>已经全部加载</span>"); return false; }
            if (flowInfo['type' + stype] % 2 == 0 && flowInfo['type' + stype] != 2) { return false }

            var wtop = $(window).scrollTop(), wh = $(window).height();
            if (wtop + wh + 200 > $(document).height()) {

                var url = "/xmlhttp/GetDefaultv3JsonData.aspx?op=" + (stype + 1) + "&pageindex=" + flowInfo['type' + stype];
                getAjaxCont(url, function (data) {
                    flowInfo['type' + stype]++;
                    $(".info-flow-cont").eq(stype).children(".more-feed").before(data)
                })
            }

        }

        function getAjaxCont(url, callback) {
            if (isGet) {
                isGet = false;
                $.ajax({
                    url: url, async: false, cache: false,
                    success: function (data) {
                        isGet = true;
                        callback(data);
                    },
                    error: function () {
                        console.log("请求失败");
                    }
                });
            }
        }
        !function () {


        }();


    </script>
    
