<script type="text/javascript">
if (window.active_tests["BatchSDK14c8300b58"]) {
if (window.active_tests["BatchSDK14c8300b58"].buckets.indexOf(RobinSettings.earlyTestBucket) > -1 &&
window.active_tests["BatchSDK14c8300b58"].sites.indexOf(RobinSettings.earlyDomain) > -1 ) {
pushNotificationsSetup();
}
}
function pushNotificationsSetup(){
if('serviceWorker' in navigator){
navigator.serviceWorker.register('/batchsdk-worker-loader.js').then(function(registration){
batchSetup();
}, function(err) {
console.error(err);
return;
})
} else {
return;
}
function batchSetup() {
/* Load remote Batch SDK JavaScript code */
(function(b,a,t,c,h,e,r){h='batchSDK';b[h]=b[h]||function() {
(b[h].q=b[h].q||[]).push(arguments)};e=a.createElement(t),r=a.getElementsByTagName(t)[0];
e.async=1;e.src=c;r.parentNode.insertBefore(e,r);})(window,document,'script','https://via.batch.com/v2/bootstrap.min.js');
/* Initiate Batch SDK opt-in UI configuration (native prompt) */
var batchSDKUIConfig = {
native: {
autoShow: false
}, alert: {
autoShow: false
}
};
var batchKeys = {
subdomain: 'zimbio',
dev: window.Settings.env === 'dev' ? true : false,
initialized: true
}
batchKeys.apiKey = '50B1D2ED835C4BA79B79087E80445CA0';
batchKeys.authKey = '2.iow0p9PRDnro3KkmwSFKBuzmErDFE+iKJZAo1vPFOYU=';
batchKeys.vapidPublicKey = 'BCGutMOIzPSCaKW3g7irDNNAiX6QyNQ3eXkykpAe1Mv1g2FTlxhAeEk5b8AkOTvBUeDc/nGrN11FWSVJaM25DGQ=';
batchSDKUIConfig.alert.icon = 'https://www.zimbio.com/images/zimbio_logo_square_100.png';
batchKeys.defaultIcon = 'https://www.zimbio.com/images/zimbio_logo_square_100.png';
batchKeys.smallIcon = 'https://www.zimbio.com/images/zimbio_logo_square_100.png';
batchKeys.initialized = true;
if(!batchKeys.initialized){
console.error('No api keys provided to BatchSDK');
return;
}
try {
batchSDK('setup', {
apiKey: batchKeys.apiKey,
subdomain: batchKeys.subdomain,
authKey: batchKeys.authKey,
dev: batchKeys.dev,
vapidPublicKey: batchKeys.vapidPublicKey,
ui: batchSDKUIConfig,
defaultIcon: batchKeys.defaultIcon,
smallIcon: batchKeys.smallIcon
});
} catch(error) {
console.error(error);
return;
}
function setBatchCookies(batchState){
var expireDate = new Date(Date.now() + (1000 * 60 * 60 * 24 * 365 * 2));
expireDate = expireDate.toUTCString();
document.cookie=
'batch_permission=' + batchState.permission +
';expires=' + expireDate;
document.cookie=
'batch_subscribed=' + batchState.subscribed +
';expires=' + expireDate;
if(localStorage.getItem("com.batch.displayed_onboard_notif") === "1" ||
document.cookie.includes('batch_displayed_onboard=true')){
document.cookie='batch_displayed_onboard=true;expires=' + expireDate;
}
}
// Decides whether to show native or Batch-specific ui
var isFirefox = navigator.userAgent.indexOf('Firefox') !== -1;
window.batchSDK(function (api) {
api.getSubscriptionState().then(function(response) {
setBatchCookies(response);
if(response.subscribed === false){
api.ui.show(isFirefox ? "alert" : "native", true)
}
})
});
window.batchSDK(function (api) {
api.on("subscriptionChanged", function (api, s) {
setBatchCookies(s)
if (localStorage.getItem("com.batch.displayed_onboard_notif") !== "1" &&
s.subscribed === true) {
var expireDate = new Date(Date.now() + (1000 * 60 * 60 * 24 * 365 * 2));
expireDate = expireDate.toUTCString();
document.cookie='batch_displayed_onboard=true;expires=' + expireDate;
welcomeJourney();
}
})
});
function welcomeJourney() {
localStorage.setItem("com.batch.displayed_onboard_notif", "1");
navigator.serviceWorker.ready.then(function (reg) {
var deeplink = "https://www.livinglymedia.com/push-notifications-information";
reg.showNotification("Thank you for subscribing 🤗", {
body: "Click to learn more about push notifications 👉",
data: {
title:" ",
alert:" ",
"com.batch": {
da: {
a: "batch.deeplink",
args: { l:deeplink, newTab:true }
}
}
}
});
});
}
}
}
</script>
