var jydate = {
y: new Date().getFullYear(),
m: new Date().getMonth() + 1,
d: new Date().getDate(),
min: 18,
max: 99
};
function insertYear(){
var html = '';
for(var i = 9; i > 2; i--){
html += '<li><span>';
html += i;
html += '0\u540e\uff1a'; //'0后'
html += '</span>';
for(var j = 0; j < 10; j++){
var temp = 1900 + i * 10 + j;
if(jydate.y - temp < jydate.min || jydate.y - temp > jydate.max){
continue;
}	
html += '<a href="">';
html += temp;
html += '</a>';
}
html += '</li>';
}
return html;
}
function insertMD(arg){
var html = '<li>';
for (var i = 1; i <= arg; i++ ) {
html += '<a href="">';
html += i;
html += '</a>';
} 
html += '</li>';
return html;
}
