(function(){
//根据IP获取当前地区
var cur_loc = get_loc();
/*注册初始化*/
//jQuery("input[name=sex]").val();								//性别
//jQuery("input[name=year]").val(DEFAULT_REG_YEAR);				//出生年
//jQuery("input[name=month]").val(DEFAULT_REG_MONTH);				//出生月
