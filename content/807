  <!--뉴스레터 프로모션-->
  <div id="newsletterPop" style="width:300px;height:300px;position:fixed;left:20px;bottom:30px;z-index:500;display:none;">
    <img src="//img.seoul.co.kr/img/21_letter_popup2.png" usemap="#Map">
    <map name="Map" id="Map">
      <area shape="rect" coords="61,203,240,241" href="/news/newsletter.php" />
    </map>

    <a href="javascript:newsletterPop_close();"><img src="//img.seoul.co.kr/img/newsletter/21_letter_popup_closei.png" style="position:absolute; top:8px; left:277px;"></a>
  </div>

  <script>
  $(window).on("load resize scroll", function(event) {	
    var cHeight=window.innerHeight;  //브라우져 height
    var sHeight=$(window).scrollTop();
    //var topArea=$(".W20_onlineS").offset().top;
    var topArea=$(".W20_onlineS").offset().top-$(".W20_onlineS").height();
    //var limitTop=$("#contentsBodyDiv").offset().top+$("#contentsBodyDiv").height();    

    var letterPopClose= getCookie('letterPopClose');

    if(letterPopClose!="done"){
      if( topArea > sHeight){
        $("#newsletterPop").slideUp("slow");				
      }else{
        $("#newsletterPop").slideDown("slow");
      }
    }
  });    

  var letterPop_live=true;
  function newsletterPop_close(){
    $("#newsletterPop").slideUp("fast");
    document.cookie = "letterPopClose=" + escape("done") + "; path=/; max-age=10800";  //3시간
  }
  </script>
  <!-- end :: 뉴스레터 프로모션-->



