   <script> 
    var speType="Normal";
    
	var effect="{specialTopArticle[0]['effect_speTopPhotoGroup']";
	var speed_v=500;
	var rtime="1";


	if(rtime==""){
		var rtime_v=3000;	
	}else{
		var rtime_v=Number(rtime)*1000;		
	}

	if(effect!="slide"){
		var fade_v=true;
		if(effect=="none"){
			speed_v=0;		
		}else{
			speed_v=700;				
		}
	}else{
		var fade_v=false;	
	}



	var showCnt=speType=="3ColumnMulti"?2:4;

    $('#speTopImgGroup').slick({	
       arrows:false,   
       slidesToShow:1,
       slidesToScroll:1,   
       autoplay: true,
       pauseOnFocus:false,
       speed: speed_v,
	   fade : fade_v,
	   autoplaySpeed: rtime_v
    });

    $('#slick_spePlist').slick({
       arrows:false,
       slidesToShow:showCnt,
       slidesToScroll:showCnt,
       infinite:true,
       autoplay: true,
       autoplaySpeed:3000
    });

	if($('#slick_spePlist li').length>showCnt){
		$('#spePlistDiv .arrow').show();		
	}

    $("#spePlist_prev").click(function(){
      $('#slick_spePlist').slick("slickPrev");
    });

    $("#spePlist_next").click(function(){
      $('#slick_spePlist').slick("slickNext");
    });
    </script>

