	<script>
	$(window).on("load",function(){
		setPosition();
	});		

	function loadNeoInteractiveAd(){
		$("#neoInteractiveIframe").attr("src","https://nscreen.neoebiz.co.kr/xJqScreenIframe.php?data=seoul");
		$("#neoInteractiveDiv").css({"display":"block"});
	}

	function closeNeoInteractiveAd(){
		$("#neoInteractiveIframe").attr("src","");
		$("#neoInteractiveDiv").css({"display":"none"});
	}

	window.addEventListener('message', function(e){
		var result_data = decodeURIComponent(e.data);
		if(result_data=="neoSV_CB_CLOSE") { closeNeoInteractiveAd(); }
	});
	</script>
  

  </body>
</html>