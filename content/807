		  //$(".S20_HBsection ul li").eq(0).addClass("on");
		  //$(".S20_headlineBS ul").eq(0).css({"display":"block"});

		  var menuCnt="10";
		  var random_num = Math.floor(Math.random() * Number(menuCnt));
		  $(".S20_HBsection ul li").eq(random_num).addClass("on");
		  $(".S20_headlineBS ul").eq(random_num).css({"display":"block"});	  

		  $(".S20_HBsection ul li").mouseover(function(){
			  var overIndex=$(this).index();
			  $(".S20_HBsection ul li").removeClass();
			  $(".S20_headlineBS ul").css({"display":"none"});
			  
			  $(".S20_HBsection ul li").eq(overIndex).addClass("on");
			  $(".S20_headlineBS ul").eq(overIndex).css({"display":"block"});
		  });
		  </script>


