	  //var status = $('.pageNum');
	  var slickElement = $('#topRelnews');

	  var relLength=$("#topRelnews li").length;

	  if(relLength>3){

		  $("#topRelnewsPaging").css({"display":"block"});

		  slickElement.on('init reInit afterChange', function (event, slick, currentSlide, nextSlide){
			  var i = (currentSlide ? currentSlide : 0) + 1;
			  $('.pageNum').text(i + '/' + slick.slideCount);
		  });

		  $('#topRelnews ul').slick({	
			 arrows:false,   
			 rows:3,
			 slidesToScroll:1,   
			 fade:true,
			 //autoplay: true,
			 pauseOnFocus:false
			 //autoplaySpeed:3000
		  });

		  $("#topRel_prev").click(function(){
			  $('#topRelnews ul').slick("slickPrev");
		  });

		  $("#topRel_next").click(function(){
			  $('#topRelnews ul').slick("slickNext");
		  });
		
	  }

	  
