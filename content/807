	  <script>
	  $('#slick_editorial').slick({	
        arrows:false,   
        slidesToShow:1,
        slidesToScroll:1,   
        autoplay: true,
        pauseOnFocus:false,
        autoplaySpeed:3000,
      });

	  $("#slick_editorial").on('afterChange', function(event, slick, currentSlide){
	    $(".paging ul li").removeClass("on");   
	    $(".paging > ul li:eq("+currentSlide+")").addClass("on");
	  });

	  $(".paging_dots li").click(function(){
		  slideIndex=$(this).index();
		  $('#slick_editorial').slick('slickGoTo', parseInt(slideIndex));
      });
