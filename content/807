<script>
	function getTopDate(){
		$.ajax({
			type: "GET",
			url: "/api/getServerTimeAjax.php?dateFormat=Y-m-d-w",
			dataType: "jsonp",
			success:function(result){
				if(result!=""){
					text_set=new Array('일','월','화','수','목','금','토');
					day_array=result.split("-");
					$("#topDate").text(day_array[0]+"."+day_array[1]+"."+day_array[2]+" ("+text_set[day_array[3]]+")");
				}			
			},
			error:function(){
			}
		});		
	}

	//setInterval(getTopDate, 6000);
	getTopDate();


	$('.S20_Tweather ul').slick({	
			 arrows:false,   
			 slidesToScroll:1,   
			 fade:true,
			 autoplay: true,
			 pauseOnFocus:false,
			 autoplaySpeed:3000
	});
</script>

          
