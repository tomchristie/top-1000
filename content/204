function dlc_switch(sect, el)
{
	$('.cldl_sect').fadeOut(100);
	$('#cldl_'+sect).fadeIn(200);
	$('div.sect').removeClass("bcol-blue").removeClass("motdown").removeClass("motdown1-blue");
