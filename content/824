var convertDates = function convertDates(config) {
  var _ref = (config === null || config === void 0 ? void 0 : config.defaultData) || {},
      calendar = _ref.calendar;

  if (calendar) {
    if (calendar.outbound) calendar.outbound = new Date(calendar.outbound);
    if (calendar.inbound) calendar.inbound = new Date(calendar.inbound);
  }

  return config;
};

