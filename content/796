<script type="text/javascript">
    window.onload = function() {
        /*li hover事件追加*/
        $(".timeline p").hover(
            function(){
                $(this).addClass("hover");
            },
            function(){
                $(this).removeClass("hover");
            }
        );
    };
</script>
<script>
    $(window).scroll(function() {
        var nav = $('.nav-wrapper');
        //判断向下滑动的距离
        if($(window).scrollTop()>=100){
            //将顶部导航栏固定
            //需要执行显示导航动画操作
            nav.addClass('nav-active animated fadeInDown');
        } else {
            //需要执行隐藏导航的操作
            nav.removeClass('nav-active animated fadeInDown');
        }
    });
</script>
