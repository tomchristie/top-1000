						if ( _window.obApi ) {
							return;
						}
						var api = _window.obApi = function() {
							api.dispatch ? api.dispatch.apply( api, arguments ) : api.queue.push( arguments );
						};
						api.version = '1.0';
						api.loaded = true;
						api.marketerId = OB_ADV_ID;
						api.queue = [];
