    ;(function(window){
            var encode = encodeURIComponent,
            decode = decodeURIComponent,
            cookie = {set: function (saveKey, saveValue, passDays, needURIEncode, secure, domain) {
            var date = new Date();
            passDays = passDays || -1;
            date.setDate(date.getDate() + passDays);
            return(document.cookie = [encode(saveKey), "=", needURIEncode ? encode(saveValue.toString()) : saveValue.toString(), "; expires=" + date.toUTCString(), "; path=/", "; domain=." + (domain ? domain : "7k7k.com"), secure ? "; secure" : ""].join(""))
            }, get: function (saveKey, needURIDecode) {
            var value;
            return(value = new RegExp("(?:^|; )" + encode(saveKey) + "=([^;]*)").exec(document.cookie)) ? (needURIDecode ? decode(value[1]) : value[1]) : null
            }};

            /*判断是否手机登录*/
            var isAndroid,
            ua = window.navigator.userAgent;
            ua = ua ? ua.toLowerCase().replace(/-/g, "") : "";
            isAndroid = ua.indexOf("android") !== -1;
            if(location.hash === "#m_1") {
            cookie.set("fromMobile", "1", 30, false, false, "7k7k.com");
            }else if(isAndroid) {
                var isMobile = cookie.get("fromMobile");
                if(isMobile !== "1") {
                    location.href = "http://m.7k7k.com";
                }
            }

    })(window);
</script>
