<script>
    function init() {
        //adding the click event to the button
        document.addEventListener('click', function(e){        
            if(e.target.id === "bannerTrigger" || e.target.parentElement.id === "bannerTrigger") {
                e.preventDefault();
                var targetElem = document.querySelector('#bannerTrigger').getAttribute('aria-controls');
                //if extra content is not visible
                if(document.querySelector('#'+targetElem).hasAttribute('hidden')) {
                    //show extra content and set sessionStorage to true
                    document.querySelector('#'+targetElem).removeAttribute('hidden');
                    document.querySelector('#bannerTrigger').setAttribute('aria-expanded', true);
                } else {
                    //hide extra content and set sessionStorage to false
                    document.querySelector('#'+targetElem).setAttribute('hidden', true);
                    document.querySelector('#bannerTrigger').setAttribute('aria-expanded', false);
                }
            }
        }); 
    }
    document.addEventListener('DOMContentLoaded', init)
     
</script>


