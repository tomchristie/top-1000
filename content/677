<script>
// JavaScript Document
//$(function() {
	/* 快速連結
	var $win = $(window),
		$ad = $('#flutter'),
		_moveSpeed = 1000;// 移動的速度
	if ( $win.width() > 1050){
	$ad.css({
		left: ($win.width()+1000)/2 +10|0,
		top: 117,
		opacity: 1
	}).show();
	}
	$win.bind('scroll resize', function(){
		var $this = $(this);
		if ($(document).scrollTop() >= 117 ){
			$ad.stop().animate({
				top: $this.scrollTop() + 10,
				left: ($win.width()+1000)/2 +10|0
			}, _moveSpeed, 'easeOutQuint');
		} else {
			$ad.stop().animate({
				top: $this.scrollTop() + 117,
				left: ($win.width()+1000)/2 +10|0
			}, _moveSpeed, 'easeOutQuint');
		}
	}).scroll();
	 */
	/* 服務總覽 */
	$('#overview').click(function(){
		var $body = (window.opera) ? (document.compatMode == "CSS1Compat" ? $('html') : $('body')) : $('html,body');
		$body.animate({
			scrollTop: $('#footer').offset().top
		}, _moveSpeed, 'easeOutQuint');
		return false;
	});
	/* top */
	$('#gotop').click(function(){
		var $body = (window.opera) ? (document.compatMode == "CSS1Compat" ? $('html') : $('body')) : $('html,body');
		$body.animate({
			scrollTop: 0
		}, _moveSpeed, 'easeOutQuint');
		return false;
	});
	/* 常用服務 */
	$('#ser_open').click(function(){
		$('#servset').toggleClass("open");
		return false;
	});
	var $block = $('.s_list_box'),
	$li = $block.find('li'),
	$img = $li.find('img'),
	_width = $img.width(),
	_height = $img.height(),
	_ratio = 1.2,
	_speed = 200;
$li.hover(function(){
	var $this = $(this);
	$this.css('z-index', 1).find('.s_hover').stop().animate({
		top: -3
	}, _speed).find('img').attr('src',function(i,e){
			return e.replace("_o","_h");
	}).stop().animate({
		width: _width * _ratio,
		height: _height * _ratio
	}, _speed);
}, function(){
	var $this = $(this);
	$this.css('z-index', 0).find('.s_hover').stop().animate({
		top: 0
	}, _speed).find('img').stop().animate({
		width: _width,
		height: _height
	}, _speed).attr('src',function(i,e){
			return e.replace("_h","_o");
	});
});
	/* 熱門主打 */
	$('#mainnav li').hover(function(){
		$(this).find('.submenu').show();
		return false;
	}, function() {
		$(this).find('.submenu').hide();
		return false;
	});
	$("#mainnav li").click(function(){
		$(this).find('.submenu').toggle();
	});
	/* 圖片效果 */
	$('.pic_hover').hover( function(){
		$(this).find('img').animate({opacity: ".7"}, 300);
		$(this).find('.caption').animate({top:"-85px"}, 300);
		return false;
		}, function(){
		$(this).find('img').animate({opacity: "1.0"}, 300);
		$(this).find('.caption').animate({top:"85px"}, 100);
		return false;
	});
	/* 瀏覽器說明 */
	$("a.browser-open").click(function(){
		$(".browser-info").toggle();
		$(this).blur(); return false;
	});
	$("a.browser-close").click(function(){
		$(".browser-info").hide();
		$(this).blur(); return false;
	});
	/* live輪播 */
	$('#picList').carouFredSel({
		prev: '.gleft',
		next: '.gright',
		swipe: {
			onMouse: true,
			onTouch: true
		},
		auto : {
			        easing      : "elastic",
			        duration    : 4500,
			        pauseOnHover: true
			    }
	});
	/* 氣象/財經 */
	$('.weather .bt').click(function(){
		$( '#tab1 .com' ).show();
		$( '#tab2 .com' ).hide();
		$( '#tab1' ).switchClass( "close", "open", 400, "easeInOutQuad" );
		$( '#tab2' ).switchClass( "open", "close", 400, "easeInOutQuad" );
		return false;
	});
	$('.stocks .bt').click(function(){
		$( '#tab2 .com' ).show();
		$( '#tab1 .com' ).hide();
		$( '#tab2' ).switchClass( "close", "open", 400, "easeInOutQuad" );
		$( '#tab1' ).switchClass( "open", "close", 400, "easeInOutQuad" );
		return false;
	});
	/* 相簿 */
	for (i=1; i<=6; i=i+1){
		$( '#bb'+i+'_1' ).bookblock();
		$( '#bb'+i+'_2' ).bookblock();
		$( '#bb'+i+'_3' ).bookblock();
		$( '#bb'+i+'_4' ).bookblock();
		$( '#bb'+i+'_5' ).bookblock();
		$( '#bb'+i+'_6' ).bookblock();
    }
//});
/****************************************************************************/
/* 天氣/加權指數自動切換 */
var tabTimer;
function changeTab1() {
	$('#tab1 .com').show();
	$('#tab2 .com').hide();
	$('#tab1').switchClass("close", "open", 400, "easeInOutQuad");
	$('#tab2').switchClass("open", "close", 400, "easeInOutQuad");
	clearInterval(tabTimer);
	tabTimer = setInterval(changeTab2, 20000);
}
function changeTab2() {
	$('#tab2 .com').show();
	$('#tab1 .com').hide();
	$('#tab2').switchClass("close", "open", 400, "easeInOutQuad");
	$('#tab1').switchClass("open", "close", 400, "easeInOutQuad");
	clearInterval(tabTimer);
	tabTimer = setInterval(changeTab1, 20000);
}

/** 新聞區選單 **/
function changeNews(block, cssClass) {
	for (var i = 1; i < 6; i++) {
		$('#newsList' + i).hide();
		$('#newsImg' + i).hide();
		if (i == 5) {
			$('#newsTitle' + i).removeClass().addClass("t_item" + " last");
		} else {
			$('#newsTitle' + i).removeClass().addClass("t_item");
		}
	}

	$('#newsList' + block).show();
	$('#newsImg' + block).show();
	$('#newsTitle' + block).removeClass().addClass(cssClass + " current");
}
/** 加到我的最愛 **/
function addFavorite() {
	if (document.all){
        document.body.style.behavior='url(#default#homepage)';
          document.body.setHomePage(window.location.href);
    }else if (window.sidebar){
        if(window.netscape){
            try{
                netscape.security.PrivilegeManager.enablePrivilege("UniversalXPConnect");
            }catch (e){
                alert( "該操作被瀏覽器拒絕，如果想啟用該功能，請在地址欄內輸入 about:config,然後將 signed.applets.codebase_principal_support 值設為true" );
            }
        }
        var prefs = Components.classes['@mozilla.org/preferences-service;1'].getService(Components. interfaces.nsIPrefBranch);
        prefs.setCharPref('browser.startup.homepage',window.location.href);
    }else{
        alert('您的瀏覽器不支持自動設置為首頁, 請使用瀏覽器手動設置!');
    }
}
/** 加到我的最愛 **/

/** 常用服務 **/
function get_cookies_array(id) {

	var commonServices = $.cookie('commonServices_'+id);
	if(commonServices==null){
		$.cookie("commonServices_" + id, "1", {expires: 365});
	}else{
		$.cookie("commonServices_" + id, parseInt(decodeURIComponent(commonServices)) + 1, {expires: 365});
	}

}
/** 常用服務 **/
/* 天氣 */
function setCity(k) {
	$.cookie('ucity', k, {expires: 365});
	setCityWeather(k);
}
function setCityWeather(k) {
	try {
		$('#othercity').hide();
		$('#city').html(forecast[k].name);
		$("#forecastImg").attr("src", "resources/images/" + forecast[k].img);
		$("#forecastImg2").attr("src", "resources/images/" + forecast[k].img);
		$('#forecast').html(forecast[k].rain + " / " + forecast[k].temp);
	} catch (exception) {
	}
}
/* 搜尋 */
var Search = {
		searchTypeCookieName: "searchType",
        init: function(){
			var parent = this;
			{
				var searchType = $.cookie(parent.searchTypeCookieName);
				if (searchType !== null) {
					$("#searchType").text(decodeURIComponent(searchType));
				} else {
					$("#searchType").text("網頁");
				}
				$('#searchBar').click(function() {
					//$('#searchType').hide();
					if ($('#search_select').css('display') == 'none') {
						$("#searchType").text("網頁");
						$.cookie(parent.searchTypeCookieName, encodeURIComponent("網頁"), {expires: 365});
						$('#search_select').show();
					} else {
						$('#search_select').hide();
					}
					return false;
				});
				$('#search_select li a').click(function() {
					$("#searchType").text($(this).text());
					$.cookie(parent.searchTypeCookieName, encodeURIComponent($(this).text()), {expires: 365});
					$('#search_select').hide();
					return false;
				});
			}
        },
		searchByType: function() {
			var k = $('#k').val();
			this.doSearch(k, $("#searchType").text());
		},
		searchService: function(id) {
			var k = $('#'+id).val();
			if ('本站搜尋' === k) {
				alert('請輸入搜尋關鍵字');
				return;
			}
			this.doSearch(k, '本站');
		},
		doSearch: function(k, searchType){
			//var k = $("#k").val();
			var ke = encodeURIComponent(k);
			var url;
			switch (searchType) {
				case "網頁":
					url = Config.hisearchUrl+"?k=" + ke + "&t=wps";
					url = Redir.getUrl('searchWeb' , url);
					break;
				case "圖片":
					url = Config.hisearchUrl+"?k=" + ke + "&t=images";
					url = Redir.getUrl('searchImg' , url);
					break;
				case "新聞":
					url = "http://times.hinet.net/search?keyword="+ ke;
					url = Redir.getUrl('searchNews' , url);
					break;
				case "影片":
					url = Config.hisearchUrl+"?k=" + ke + "&t=video";
					url = Redir.getUrl('searchVideo' , url);
					break;
				case "店家":
					url = "https://www.iyp.com.tw/search.php?a_id=0&k=" + ke;
					url = Redir.getUrl('searchStore' , url);
					break;
				case "購物":
					url = "http://mall.cheerspoint.com.tw/quidditch/cheerspoint?keywords=" + ke;
					url = Redir.getUrl('searchShopping' , url);
					break;
				case "本站":
					if (k !== '') {
						url = "http://search.cht.com.tw/Search?DataSource=Hinet_Main&searchTemplate=18&Keyword="+ ke;
						url = Redir.getUrl('searchService' , url);
					} else {
						url = Config.hisearchUrl;
						url = Redir.getUrl('searchService' , url);
					}
					break;
			}
			window.open(url);
		}
};
Search.init();

/* AD */
function showAllAD() {
	showAD("portalgoldentxt1");
	showAD("portalgoldentxt2");
	showAD("portal160600");
	showAD("portalt1");
	showAD("portalt2");
	showAD("portal300250");
	showAD("portal300100");
	showAD("portallife10090");
}
function showAD(id) {
	$id_h = $('#' + id + '_h');
	$id_h.find('script').remove();
	var html = jQuery.trim($id_h.html());
	if (html !== '') {
		$id_h.html('');
		$('#' + id).html(html);
		$('#' + id).show();
	}
}
function changeAlbums(count) { //change menu
	for (var i = 1; i < 6; i++) {
		$('#albumsClass' + i).removeClass().addClass("t_item");
		$('#albums' + i).hide();
	}
	$('#albumsClass' + count).removeClass().addClass("t_item current");
	$('#albums' + count).show();
}
/** 影音區 **/
function setMedia(index, varIndex, type, h264, dataType, link, channel, content) {
	$('#mediaUl > li').removeClass().addClass("t_item");
	$("#media" + index).removeClass().addClass("t_item current");
	var img = $("#mediaImg" + index).val();
	var title = $("#mediaTitle" + index).val();
//	var link = encodeURIComponent($("#mediaLink" + index).val());
	var link = $("#mediaLink" + index).val();
	var mediaH264 = $("#mediaH264" + index).val();
	var target = $("#mediaTarget" + index).val();
	if (type == '1') {
		if(h264!=''){
			$("#mediaPic").html("<a id='httpstreaming' style='display:block;width:370px;height:235px;'></a>");
			flowplayer("httpstreaming", "resources/swf/flowplayer.commercial-3.2.16.swf", {
				plugins: {
					f4m: {url: "resources/swf/flowplayer.f4m-3.2.9.swf"},
					httpstreaming: {url: "resources/swf/flowplayer.httpstreaming-3.2.10.swf"},
					// the "tube" skin
					controls: {url: "resources/swf/flowplayer.controls-3.2.15.swf"}
				},
				key: '#$9a415ad7bb3e2216302',
				clip: {
					scaling: 'fit',
					url: h264,
					autoPlay: false,
					autoBuffering: true,
					urlResolvers: ['f4m'],
					provider: 'httpstreaming'
				}
			});
		}else{
			link = Redir.getUrl('media' , link);
			$("#shareIt").html("<a target=_blank href="+link+">更多影片>></a>");
			if(img.indexOf(".")!='-1'){
				$("#mediaPic").html("<a title='" + escapeHtml(content) + "' href='" + link + "' target='" + target + "'><img src='" + img + "' onerror='this.src=\"resources/images/340x210.jpg\"' width='370' height='235' /></a>");
			}else{
				$("#mediaPic").html("<a title='" + escapeHtml(content) + "' href='" + link + "' target='" + target + "'><img src='resources/images/340x210.jpg' width='370' height='235' /></a>");
			}
		}
		$("#mediaName").html("<a title='" + escapeHtml(title) + "' href='" + link + "' target='" + target + "' class='p_link'>"+title+"</a>");
		$("#playPic").html("<a title='" + escapeHtml(content) + "' href='" + link + "' target='" + target + "' class='p_link'></a>");
	} else {
		if (dataType == '1') {
			$("#shareIt").html("<a target=_blank href="+$("#mediaLink" + index).val().split(",")[1]+">更多影片>></a>");
			$("#mediaPic").html($("#mediaLink" + index).val().split(",")[0]);
			$("#mediaName").html("<a title='" + escapeHtml(title) + "' href='" + $("#mediaLink" + index).val().split(",")[1] + "' target='" + target + "' class='p_link'>"+title+"</a>");
		}else if (dataType == '2') {
			$("#mediaPic").html("<a id='httpstreaming' style='display:block;width:370px;height:210px;'></a>");
			flowplayer("httpstreaming", "resources/swf/flowplayer.commercial-3.2.16.swf", {
				plugins: {
					f4m: {url: "resources/swf/flowplayer.f4m-3.2.9.swf"},
					httpstreaming: {url: "resources/swf/flowplayer.httpstreaming-3.2.10.swf"},
					// the "tube" skin b
					controls: {url: "resources/swf/flowplayer.controls-3.2.15.swf"}
				},
				key: '#$9a415ad7bb3e2216302',
				clip: {
					url: mediaH264,
					autoPlay: false,
					autoBuffering: true,
					urlResolvers: ['f4m'],
					provider: 'httpstreaming'
				}
			});
			link = Redir.getUrl('media' , link);
			$("#mediaName").html("<a title='" + escapeHtml(title) + "' href='" + link + "' target='" + target + "' class='p_link'>"+title+"</a>");
			$("#playPic").html("<a title='" + escapeHtml(content) + "' href='" + link + "' target='" + target + "' class='p_link'></a>");
		} else {
			link = Redir.getUrl('media' , link);
			$("#shareIt").html("<a target=_blank href="+link+">更多影片>></a>");
			$("#mediaPic").html("<a title='" + escapeHtml(content) + "' href='" + link + "' target='" + target + "'><img src='" + img + "' width='370' height='235' /></a>");
			$("#mediaName").html("<a title='" + escapeHtml(title) + "' href='" + link + "' target='" + target + "' class='p_link'>"+title+"</a>");
			$("#playPic").html("<a title='" + escapeHtml(content) + "' href='" + link + "' target='" + target + "' class='p_link'></a>");
		}

	}

	$("#channelId").html(channel);
	$("#mediaUl").trigger("pause");
}

