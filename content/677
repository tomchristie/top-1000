/* vlog */
function setVlog(type,link,duration,img,title){
	var div = "<a href="+link+" target='_target'><div class='play'></div></a>"+
	"<div class='time'>"+duration+"</div>"+
	"<a title="+escapeHtml(title)+" href="+link+" target='_target'>"+
	"<img src="+img+" width='306' height='180' onerror='this.src=\x22resources/images/306x180.jpg\x22'/></a>";
	$("#vlogId").html(div);
	if(type == '0'){
		$('#vlogCss1').removeClass().addClass("info_item current");
		$('#vlogCss2').removeClass().addClass("info_item");
	}else{
		$('#vlogCss1').removeClass().addClass("info_item");
		$('#vlogCss2').removeClass().addClass("info_item current");
	}
}

function getAllcookies() {
	var cookies = {};
	if (document.cookie && document.cookie != '') {
		var split = document.cookie.split(';');
		for (var i = 0; i < split.length; i++) {
			var name_value = split[i].split("=");
			name_value[0] = name_value[0].replace(/^ /, '');
			if(cookies.length == undefined){
				cookies =name_value[0]+"_"+name_value[1];
			}else{
				cookies = cookies+","+name_value[0]+"_"+name_value[1];
			}
		}
	}
	return cookies;
}

function getCommonServices(cookie) {
    var formData = "cookie="+cookie;
    $.ajax({
        type: "POST",
        url: apServiceUrl+'getCommonServices',
        data: formData,
        dataType: 'jsonp',
        jsonp: 'callback',
        jsonpCallback: 'commonCallback',
        success: function( commData, textStatus, jqXHR) {
        	$("#servsetData").html(commData.countryInfo.content);
        	setBlock();
//        	console.log(data.countryInfo.content);
        },
        error: function(jqXHR, textStatus, errorThrown){
//        	 console.log(textStatus);
        }
    });
}

function setBlock(){
	var $block = $('.s_list_box'),
	$li = $block.find('li'),
	$img = $li.find('img'),
	_width = $img.width(),
	_height = $img.height(),
	_ratio = 1.2,
	_speed = 200;
	$li.hover(function(){
		var $this = $(this);
		$this.css('z-index', 1).find('.s_hover').stop().animate({
			top: -3
		}, _speed).find('img').attr('src',function(i,e){
				return e.replace("_o","_h");
		}).stop().animate({
			width: _width * _ratio,
			height: _height * _ratio
		}, _speed);
	}, function(){
		var $this = $(this);
		$this.css('z-index', 0).find('.s_hover').stop().animate({
			top: 0
		}, _speed).find('img').stop().animate({
			width: _width,
			height: _height
		}, _speed).attr('src',function(i,e){
				return e.replace("_h","_o");
		});
	});
}

function mbr_init()
{
	var otpw = $.cookie('otpw');
	if( otpw === null || otpw === "" )
	{
		$('#memberLogin').show();
		$('#memberLogout').hide();
	} else {
		$('#memberLogin').hide();
		$('#memberLogout').show();
	}
	setTimeout("mbr_init()",2000);
}
if($.browser.msie && $.browser.version < 7) {
	if(confirm("親愛的網友，因為您的瀏覽器版本較舊，請您以IE6 以上的版本瀏覽本網站，您可前往 http://windows.microsoft.com/zh-TW/internet-explorer/products/ie/home 更新新版IE瀏覽器。謝謝您的愛用！前往更新?")) {
        window.location='http://windows.microsoft.com/zh-TW/internet-explorer/products/ie/home';
	}
}

function checkIpad(){
	 if ((navigator.userAgent.toLowerCase().match(/(iPhone|iPod|Android|ios|iPad)/i)))
	 {
	  return true;
	 }
	 else{
	  return false;
	 }
}

function escapeHtml(unsafe) {
    return unsafe
         .replace(/&/g, "&amp;")
         .replace(/</g, "&lt;")
         .replace(/>/g, "&gt;")
         .replace(/"/g, "&quot;")
         .replace(/'/g, "&#039;")
		 .replace(/ /g, "&nbsp;");
}
function switchScreenRightAd() {
	 var width = (window.innerWidth > 0) ? window.innerWidth : screen.width;
	 var height = (window.innerHeight > 0) ? window.innerHeight : screen.height;
	 if (width > height) {
		$('#screenRightAd').show();
	 } else {
		$('#screenRightAd').hide();
	 }
}
function lbs() {
	try {	/* LBS */
		var city = $.cookie('city');
		if (city === null) {
			try {
				var url = apServiceUrl + 'lbs';
				$.ajax({
					type: 'GET',
					url: url,
					jsonpCallback: 'jsonCallback',
					contentType: "application/json",
					dataType: 'jsonp',
					success: function(json) {
						city = json['city'];
						$.cookie('city', city, {expires: 1, path: '/', domain: cookieDomain});
						var ucity = $.cookie('ucity');
						if (ucity === null) {
							setCityWeather(city);
						}
					},
					error: function(e) {
						//console.log(e.message);
					}
				});
			} catch (exception) {}
		}
	} catch (exception) {}
}
function cityWeather() {
	try {/* 天氣 */
		var city = ($.cookie('ucity') !== null) ? $.cookie('ucity') : $.cookie('city');
		if (city !== null) {
			setCityWeather(city);
		} else {
			setCityWeather('Taipei');	//default Taipei
		}
	} catch (exception) {}
}
