            <script>
            		function showHinetTimes(id){
            			$("#lifeset_tabs").removeClass().addClass("t0"+id);
            			$("#hinetTimes1").hide();
            			$("#hinetTimes2").hide();
            			$("#hinetTimes3").hide();
            			$("#hinetTimes4").hide();
            			$("#hinetTimes5").hide();
            			$("#hinetTimes"+id).show();
            			var image = $("#img_"+id).val();
            			var link = $("#link_"+id).val();
            			$("#imgDiv_"+id).html("<div class='pic' id='img'><span></span><a href='"+link+"' target='_target'><img src='"+image+"' /></a></div>");
            		}
            		showHinetTimes("1");
            </script>
