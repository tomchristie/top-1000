<script>
var Req = {
	init: function(){
		if('' == location.search) return;
		var req = location.search;
		req = req.substring(1,req.length);
		req = req.split("&");
		for(var i in req){
			var sArr = req[i].split("=");
			if (null !== sArr[0]
				&& null !== sArr[1]
				&& "" !== sArr[0]
				&& "" !== sArr[1] )
				this[sArr[0]] = sArr[1];
		}
    },
	get: function(key){
		if(undefined === this[key]) return null;
			return this[key];
		}
};
Req.init();
</script>
<script>
var isPhone = {
	Android: function(ua) {
		return (ua.match(/Android/i) && ua.match(/Mobile/i));
    },
    iOS: function(ua) {
        return ua.match(/iPhone/i);
    },
    Others: function(ua) {
        return ua.match(/Phone/i);
    },
    any: function(ua) {
        return (isPhone.Android(ua) || isPhone.iOS(ua) || isPhone.Others(ua));
    }
};
(function(){
	var pc = Req.get("pc");
	if (pc === null) {
		var ua = navigator.userAgent;
		if (isPhone.any(ua)) {
			var mobileUrl = 'mobile.html';
			
			location.href = mobileUrl;
		}
	}
})();
</script>
