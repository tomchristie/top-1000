          <script>
          function changeArticle(count) {
        	  	$('#article').removeClass().addClass("t0"+count);
        	  	for (var i = 1; i < 4; i++) {
        			$('#totday' + i).hide();
        		}
        		$('#totday' + count).show();
        	}
