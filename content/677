	function getAd(url,divId) {
    var formData = "";
	$.ajax({
		type: "POST",
        url: url,
        data: formData,
        dataType: 'jsonp',
        jsonp: 'callback',
       // jsonpCallback: 'moneyCallback',
        success: function( data, textStatus, jqXHR) {
        	if(data.TextContent!=undefined)
        	$("#"+divId).html("<a href='"+data.NavigateUrl+"' target='_target'>"+data.TextContent+"</a>");
        },
        error: function(jqXHR, textStatus, errorThrown){
//        	 console.log(textStatus);
        }
    });
}
getAd('https://ssp.hinet.net/api/web/request/json?c=2017033&s=201702000025','ad1');
