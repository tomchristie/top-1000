	<script>
	  var lang = "en";
	  var dropdown = document.getElementById('selectlanguage');
	  var options = dropdown.options;
	  for( var i = 0; i < dropdown.options.length; i++ ) {
	    if( dropdown.options[ i ].value == lang ) {
	      dropdown.selectedIndex = i;
	      break;
	    }
	  }
	</script>
