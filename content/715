				titCell: ".qdbtab li",
				mainCell: ".con",
				trigger: "click"
			});
			jQuery(".multipleColumn").slide({
				titCell: ".title ul",
				mainCell: ".bd .rdbox",
				autoPage: true,
				effect: "leftLoop",
				autoPlay: true,
				vis: 1,
				interTime: 3000
			});
			jQuery(".focusBox").slide({
				titCell: ".hd ul",
				mainCell: ".bd ul",
				effect: "left",
				autoPlay: true,
				autoPage: true,
				switchLoad: "_src",
				interTime: 5000
			});
			$(document).ready(function() {
				$(".gamebox").mouseenter(function() {
					$(this).addClass("on").siblings().removeClass("on");
				});
				$(".meiti-list li").hover(function() {
					$(this).find(".mt-fc").toggleClass("active");
				})
		
			})
			</script> </body> </html>