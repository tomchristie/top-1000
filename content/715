															var currentTime = Date.parse(new Date());
															var dateTime = time;
															var ts = timeSelector;
															var d_day = Date.parse(new Date(dateTime));
															var day = Math.abs(parseInt((d_day - currentTime) / 1000 / 3600 / 24)); //计算日期
															var hour = Math.abs(parseInt((d_day - currentTime) / 1000 / 3600)); //计算小时
															var minutes = Math.abs(parseInt((d_day - currentTime) / 1000 / 60)); //计算分钟
															var seconds = Math.abs(parseInt((d_day - currentTime) / 1000)); //计算秒
															if (day >= 7) {
																return dateTime;
															} else if (day >= 2 && day < 7) {
																ts.text(parseInt(day) + "天前").toString();
															} else if (day > 0 && day < 2) {
																ts.text("昨天").toString();
															} else if (hour > 0 && hour < 24) {
																ts.text(parseInt(hour) + "小时前").toString();
															} else if (minutes > 0 && minutes < 60) {
																ts.text(parseInt(minutes) + "分钟前").toString();
															} else if (seconds > 0 && seconds < 60) {
																ts.text(parseInt(seconds) + "秒前").toString();
															}
														}
														$(".newsbox").each(function() {
															var t_time = $(this).find(".time").text();
															var timeSelector = $(this).find(".time");
															setTime(t_time, timeSelector);
														})
													}
												})


											})
										})
									}
									ajax();
								</script> </div> </div> </div> <div class="w400 r"> <div class="adbox2"> <script>
								UUU9_CREATIVE(84)
							</script> <i></i> </div> <div class="row mt25"> <div class="title clear"> <h2>热门礼包</h2> <em><a href="http://ka.uuu9.com/" target="_blank">更多</a></em> </div> <div class="lbpic"> <dl class="clear"> <dt><img src="http://image5.uuu9.com/news/portal/20210804/610a069b6aabb.jpg" alt="领地人生"></dt> <dd class="lbtext"> <h4>领地人生</h4> <p>信仰礼包</p> </dd> <dd class="lbbut"><a href="http://ka.uuu9.com/intro/18689.html" title="领地人生" target="_blank">领取</a></dd> </dl><dl class="clear"> <dt><img src="http://image5.uuu9.com/news/portal/20210730/610367a9d5c47.jpg" alt="剑网3怀旧服"></dt> <dd class="lbtext"> <h4>剑网3怀旧服</h4> <p>豪侠礼包</p> </dd> <dd class="lbbut"><a href="http://ka.uuu9.com/intro/18686" title="剑网3怀旧服" target="_blank">领取</a></dd> </dl><dl class="clear"> <dt><img src="http://image5.uuu9.com/news/portal/20210716/60f1096e9fee9.jpg" alt="剑侠世界3"></dt> <dd class="lbtext"> <h4>剑侠世界3</h4> <p>首测礼包</p> </dd> <dd class="lbbut"><a href="http://ka.uuu9.com/intro/18677" title="剑侠世界3" target="_blank">领取</a></dd> </dl> </div> <div class="lblist mt15"> <dl class="clear"><dt>全民奇迹2</dt> <dd class="lktext">媒体专属奇迹礼包</dd> <dd class="lkbut"><a href="http://ka.uuu9.com/intro/18617.html" title="全民奇迹2" target="_blank">领卡</a></dd> </dl><dl class="clear"><dt>刀剑online</dt> <dd class="lktext">阳春三月礼包</dd> <dd class="lkbut"><a href="http://ka.uuu9.com/intro/18608.html" title="刀剑online" target="_blank">领卡</a></dd> </dl><dl class="clear"><dt>EVE Online</dt> <dd class="lktext">旭日级赤武对决涂装</dd> <dd class="lkbut"><a href="http://ka.uuu9.com/intro/18596.html" title="EVE Online" target="_blank">领卡</a></dd> </dl><dl class="clear"><dt>机动都市阿尔法</dt> <dd class="lktext">中级补给包</dd> <dd class="lkbut"><a href="http://ka.uuu9.com/intro/18588.html" title="机动都市阿尔法" target="_blank">领卡</a></dd> </dl><dl class="clear"><dt>生死狙击2</dt> <dd class="lktext">劫掠测试礼包</dd> <dd class="lkbut"><a href="http://ka.uuu9.com/intro/18582.html" title="生死狙击2" target="_blank">领卡</a></dd> </dl> </div> </div> <div class="row mt25"> <div class="title cle