								function ajax() {
										$(function() {
											$.getJSON("http://www.uuu9.com/comm/tnews.json?v=" + Math.random() + "", function(data) {
												var data = data
												var nums = 17;
												var pages = Math.ceil(data.length / nums);
												var thisDate = function(curr) {
													var str = '',
														last = curr * nums - 1;
													last = last >= data.length ? (data.length - 1) : last;
													for (var i = (curr * nums - nums); i <= last; i++) {
														if(data[i].ucenter_id==0){
															authorHtml = "<span class='author'>" + data[i].post_writer + "</span>";
														}else{
															authorHtml = "<span class='author'><a href='http://id.uuu9.com?uid="+data[i].ucenter_id+"' target='_blank'>" + data[i].post_writer + "</a></span>";
														}
														str += '<div class="newsbox"><h4><b><a href="' + data[i].term_path + '" target="_blank">[ ' + data[i]
															.ttname + ' ]</a></b><a href="' + data[i].url + '" target="_blank">' + data[i].post_title +
															'</a></h4><dl class="clear"> <dt><a href="' + data[i].url + '" target="_blank"><img src="' + data[i]
															.pic_path + '"  class="scale_img"></a></dt><dd><p>' + data[i].post_excerpt +'...</p><div class="tags clear">'+authorHtml+'<span class="time">' + data[i].post_date +'</span> </div></dd></dl></div>';}
													return str;
												};
												laypage({
													cont: 'pagination',
													pages: pages,
													first: false,
