							function getEsList(id) {
								var gameId = id;
								jQuery.getJSON("http://api.uuu9.com/es/fight/u9lists?gameId=" + gameId, function(result) {
									var defaultIndex = 0;
									for (i in result.data) {
										if (defaultIndex == 0) {
											if (result.data[i].flag == 1 || result.data[i].flag == 4) {
												defaultIndex = i
											}
										}
									}
									var endgames = template.render('es_list', {
										list: result.data
									});
									document.getElementById("endgames").innerHTML = endgames;
									var swiper = new Swiper('.swiper-container', {
									  slidesPerView: 4,
									  spaceBetween: 30,
									  slidesPerGroup: 1,
									  initialSlide :defaultIndex,
									  loop: false,
									  pagination: {
									    el: '.swiper-pagination',
									    clickable: true,
									  },
									  navigation: {
									    nextEl: '.swiper-button-next',
									    prevEl: '.swiper-button-prev',
									  },
									});
								});
							}
