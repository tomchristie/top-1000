    var pbjs = pbjs || {};
    pbjs.que = pbjs.que || [];

</script>
<!-- /FLUX head -->
<script>
    // set failsafe timeout
    setTimeout(function() {
        requestAdServer();
    }, failSafeTimeout);

</script>
