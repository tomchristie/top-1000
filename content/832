<div class="over-ray-ads" style="display:none;">
  <script>
      var hidePath = ["/special/","/culture/education/kyotsu-exam/"];
      var setFlag = true;
      for(var i = 0; i < hidePath.length; i++){
          if(location.pathname.indexOf(hidePath[i]) == 0){
            setFlag = false;
            break;
          }
      }
      var screen_w = window.parent.screen.width;
      var tag = "";
      var style = "";
      if(screen_w >= 768 && setFlag == true) {
          console.log("pc over-ray");
          if (location.pathname == "/") {
              //<!-- /193632318/LMC/kyodo/47NEWS/PC_TOP/overlay -->
              tag = 'div-gpt-ad-1612486995174-0';
          } else {
              //<!-- /193632318/LMC/kyodo/47NEWS/PC_inside/overlay -->
              tag = 'div-gpt-ad-1612487094094-0';
          }
          var overrayads = document.getElementsByClassName('over-ray-ads');
          var div = document.createElement('div');
          div.setAttribute('id', tag);
          if (!overrayads == false) {
              overrayads[0].appendChild(div);
              googletag.cmd.push(function() { googletag.display(tag); });
          }
      }else if(screen_w < 768 && setFlag == true){
          console.log("sp over-ray");
          if (location.pathname == "/") {
              //<!-- /193632318/LMC/kyodo/47NEWS/SP_TOP/overlay -->
              tag = 'div-gpt-ad-1612487151343-0';
              style = 'min-width: 320px; min-height: 50px;';
          } else {
              //<!-- /193632318/LMC/kyodo/47NEWS/SP_inside/overlay -->
              tag = 'div-gpt-ad-1612487209987-0';
              style = 'min-width: 320px; min-height: 50px;';
          }
          var overrayads = document.getElementsByClassName('over-ray-ads');
          var div = document.createElement('div');
          div.setAttribute('id', tag);
          if (style != "") { div.setAttribute('style', style); }
          if (!overrayads == false) {
              overrayads[0].appendChild(div);
              googletag.cmd.push(function() { googletag.display(tag); });
          }
      }
  </script>
</div>
