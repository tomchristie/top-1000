<script src="/wp-content/themes/informer/swiper-bundle.min.js"></script>

<script>
    var swiper = undefined;

    function sliderInit()
    {
            swiper = new Swiper('.swiper-container', {
                slidesPerView: 1,
                slidesPerGroup: 1,
                spaceBetween: 0,
                loop: true,
                //noSwiping: true,
                grabCursor: true,
                navigation: {
                    nextEl: '.go_next',
                    prevEl: '.go_prev'
                },
                breakpoints: {
                    640: {
                        slidesPerView: 2,
                        slidesPerGroup: 2,
                        //noSwiping: false,
                        spaceBetween: 10
                    }
                },
                navigation: {
                    nextEl: '.go_next',
                    prevEl: '.go_prev'
                }
            });
    }

    $(document).ready(function()
    {
                check_slider($(window).width());
                $(window).on('resize', function(){
                    check_slider($(this).width());
                });
            });

            function check_slider(w)
            {
                    console.log(w);
                    console.log($('.swiper-container').hasClass("disabled"));
                    if (w < 920)
                    {
                        if ($('.swiper-container').hasClass("disabled"))
                        {
                            $('.swiper-container').removeClass("disabled");
                            if (swiper==undefined)
                                sliderInit();
                        }
                    }
                    else
                    {
                        if (!$('.swiper-container').hasClass("disabled"))
                        {
                            $('.swiper-container').addClass("disabled");
                        }
                        if (swiper!=undefined)
                        {
                            swiper.destroy();
                            swiper = undefined;
                        }
                    }
    }
</script>

