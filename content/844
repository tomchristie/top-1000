<script type="text/javascript">
var bgURL1280, bgURL1366, bgURL1600, back_color_1280, back_color_1366, back_color_1600, brand_exist = false, brand_href = '', crash=1;
function brandsActivion(){
brand_href = document.getElementById('google_ads_iframe_/17774365/ukr.net_brend_resize_0__container__').querySelector('iframe').contentWindow.brand_href;
if (brand_href) {
bgURL1280 = document.getElementById('google_ads_iframe_/17774365/ukr.net_brend_resize_0__container__').querySelector('iframe').contentWindow.bgURL1280;
bgURL1366 = document.getElementById('google_ads_iframe_/17774365/ukr.net_brend_resize_0__container__').querySelector('iframe').contentWindow.bgURL1366;
bgURL1600 = document.getElementById('google_ads_iframe_/17774365/ukr.net_brend_resize_0__container__').querySelector('iframe').contentWindow.bgURL1600;
back_color_1280 = document.getElementById('google_ads_iframe_/17774365/ukr.net_brend_resize_0__container__').querySelector('iframe').contentWindow.back_color_1280;
back_color_1366 = document.getElementById('google_ads_iframe_/17774365/ukr.net_brend_resize_0__container__').querySelector('iframe').contentWindow.back_color_1366;
back_color_1600 = document.getElementById('google_ads_iframe_/17774365/ukr.net_brend_resize_0__container__').querySelector('iframe').contentWindow.back_color_1600;
if(brandsActiv && (screen.width >= 1280)){var param = brandsParameters(bgURL1280, bgURL1366, bgURL1600, back_color_1280, back_color_1366, back_color_1600);
$('body').append('<div id="brands"></div>');
var brands = $('#brands');
brands.css({'display':'block', 'height':'2000px','background':'url(' + param[0] + ') no-repeat center top'});
$('body').css('background',param[1]);
$('.main').css('z-index','10');var link_DFP = document.createElement('a');
link_DFP.setAttribute('href', brand_href);
link_DFP.setAttribute('id', 'takeover_Skin');
link_DFP.setAttribute('target', '_blank');
link_DFP.setAttribute('style', 'position: fixed; top:0px; left:0px; bottom:0px; height:100%; width: 100%; z-index:1;')
document.body.appendChild(link_DFP);window.onresize = function(){
param = brandsParameters(bgURL1280, bgURL1366, bgURL1600, back_color_1280, back_color_1366, back_color_1600);
brands.css('background' , 'url(' + param[0] + ') no-repeat center top');
$('body').css('background',param[1]);
}
};} else {
setTimeout(function(){
if (crash > 50) return;
crash++;
brandsActivion();
}, 100);
}
}
function brandsParameters(bgURL1280, bgURL1366, bgURL1600, back_color_1280, back_color_1366, back_color_1600){
var ww = window.innerWidth;
var result = [];
if(ww < 1366){
result = [bgURL1280, back_color_1280];
}else if(ww >= 1366 && ww < 1600){
result = [(bgURL1366 ? bgURL1366 : bgURL1280), back_color_1366];
}else if(ww >= 1600){
result = [bgURL1600 ? bgURL1600 : (bgURL1366 ? bgURL1366 : bgURL1280), back_color_1600];
}
return result;
}function setbrandsActivion () {
brandsActiv = true;
if (!brand_exist) {
brand_exist = true;
setTimeout(brandsActivion, 750);
}
};
</script>
