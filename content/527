    <!-- 20181119 통합 트래픽 GA코드 추가  -->    <script>
        var gnbbx_top = 200;
        $(document).ready(function () {
            gnbbx_top = $('#gnbbx').offset().top + $('#gnbbx').height();
        });
        $(document).on('click', function (e) {
            if ($(e.target).closest("#gnb_move").length === 0) {
                $('#move_gnb_total_list').hide();
                $('#ic_ham_btn').addClass('ic_ham_m');
                $('#ic_ham_btn').removeClass('ic_close_m');
            }
        });
        $(window).scroll(function () {
            if ($(this).scrollTop() < gnbbx_top) {
                $('#gnb_move').fadeOut();
            } else {
                $('#gnb_move').fadeIn();
            }
        });

        var openMoveNavAll = function () {
            if($('#move_gnb_total_list').css('display') == 'none') {
                $('#move_gnb_total_list').show();
                $('#ic_ham_btn').removeClass('ic_ham_m');
                $('#ic_ham_btn').addClass('ic_close_m');
            } else {
                $('#move_gnb_total_list').hide();
                $('#ic_ham_btn').addClass('ic_ham_m');
                $('#ic_ham_btn').removeClass('ic_close_m');
            }
        }
        var openSearchBoxMove = function () {
            $('#search_box_move').show();
        };
        var closeSearchBoxMove = function () {
            $('#search_box_move').hide();
        };
    </script>
