</div><script type='text/javascript'>
function displayOperationText(){
	var dateTime = new Date();
	var nY = dateTime.getFullYear();	//YYYY
	var nM = dateTime.getMonth() + 1;	//0~11
	var nD = dateTime.getDate();		//1~31
	var nH = dateTime.getHours();		//0~23
	var nMs = dateTime.getMinutes();	//0~59
	var nS = dateTime.getSeconds();		//0~59
	var nowDates = nY+''+((nM.toString().length == 1) ? '0'+nM : nM)+''+((nD.toString().length == 1) ? '0'+nD : nD);
	var nowTimes = nH+''+((nMs.toString().length == 1) ? '0'+nMs : nMs);
	var displayOperation;
	// 예외 처리
var holydays = [
	"20190101",		//설날(신정)
	"20190102",		//개장일
	"20190204",		//설날(구정) 1일차
	"20190205",		//설날(구정) 2일차
	"20190206",		//설날(구정) 3일차
	"20190301",		//삼일절
	"20190501",		//근로자의날
	"20190506",		//어린이날 대체일
	"20190606",		//현충일
    "20190815",		//광복절
    "20190912",     //추석 1일차
    "20190913",     //추석 2일차
    "20191003",		//개천절
    "20191009",		//한글날
    "20191225",		//성탄절
    "20191231",		//폐장일
    "20200101",     // 2020년 설날(신정)
];

if(holydays.indexOf(nowDates) !== -1){
	displayOperation = '장종료';
	displayO = 0;
}
else if(nowDates == '20181115'){
	if(nowTimes >= 1000 && nowTimes <= 1630){
		displayOperation = '장중';
		displayO = 2;
	}
	else if(nowTimes > 1630 || nowTimes < 900){
		displayOperation = '장종료';
		displayO = 0;
	}
	else{
		displayOperation = '개장전';
		displayO = 1;
	}
}
// 평시
else{
	if(nowTimes >= 900 && nowTimes <= 1530){
		displayOperation = '장중';
		displayO = 2;
	}
	else if(nowTimes > 1530 || nowTimes < 700){
		displayOperation = '장종료';
		displayO = 0;
	}
	else{
		displayOperation = '개장전';
		displayO = 1;
	}
}	document.getElementById('nowStockTime').innerHTML = displayOperation;
}

displayOperationText();

function changeExrateTxt() {
	var exrateTxt = document.getElementById('exrateTxt');
	if(document.getElementById( 'chart4' ).style.display == "none") {
		exrateTxt.innerHTML = "환율";
	} else {
		exrateTxt.innerHTML = "원달러";
	}
}
</script>
