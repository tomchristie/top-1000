	var mk_tmp = Math.floor(Math.random()*10);// 0~10
	if(mk_tmp<6) mk_idx = 0; 
	else mk_idx = 1;
	for( i=0; i<5+1; i ++ ) {
	if (document.getElementById('right_top_gnb_div'+i) != null) {
		if( i == mk_idx ) document.getElementById( 'right_top_gnb_div'+i ).style.display = 'block';
		else document.getElementById( 'right_top_gnb_div'+i ).style.display = 'none';
	}
}

right_top_gnb_cur = mk_idx;
right_top_gnb_max = 6;
if( typeof(moveIssueDiv)!='undefined' ) {
	if(right_top_gnb_max>0) setInterval(function(){moveIssueDiv('right_top_gnb_div', 'right_top_gnb_cur', right_top_gnb_max, 1, 'next');}, 3000);
}
