<script>
window.addEventListener('load', function () {
    document.getElementById("close-ads").addEventListener("click", function(){ (elemRemove=document.getElementById("action-quick")).parentNode.removeChild(elemRemove);  });
   //console.log(" window Height :" + window.innerHeight);
    window.addEventListener("scroll",function () {
        //console.log( window.scrollY );
        if(window.scrollY  >= window.innerHeight){
            window.removeEventListener('scroll', arguments.callee, false);
            document.getElementById('action-quick').style.transform="translate(-50%, 0)"
            //console.log( 'Show Ad 728');
            //Render Ad 
            if (typeof zmt_mtag !='undefined' && typeof zmt_mtag.zmt_render_placement !='undefined') {
            zmt_mtag.zmt_render_placement(p926766_4);
            }
        }
    }, false);
}, false );       
</script>

    
