<script>
window.addEventListener('DOMContentLoaded', function(){
	/*
	 * インデックスページ	紙面一面
	 */
	var COKKIE_LIFE = 365;
	var DistrictDetail = ["", "東京版", "北海道版", "名古屋版", "大阪版", "西部版"];
	/*
	 * クラス NsPaper
	 */
	var NsPaper = function(){
		this.data = [// "地域"自体を選択した時 undefined を吐くので東京版を入れておいてごまかす
		'<img src="https://cache2.nipc.jp/mod2015/img/common/blank.gif" style="background-image: url(/mod2015/img/today_paper/tokyo.jpg); background-position: center; background-size: 130px auto;" alt="東京版『本日の一面』の写真" width="130" height="167">',
		'<img src="https://cache2.nipc.jp/mod2015/img/common/blank.gif" style="background-image: url(/mod2015/img/today_paper/tokyo.jpg); background-position: center; background-size: 130px auto;" alt="東京版『本日の一面』の写真" width="130" height="167">',
		'<img src="https://cache2.nipc.jp/mod2015/img/common/blank.gif" style="background-image: url(/mod2015/img/today_paper/hokkaido.jpg); background-position: center; background-size: 130px auto;" alt="北海道版『本日の一面』の写真" width="130" height="167">',
		'<img src="https://cache2.nipc.jp/mod2015/img/common/blank.gif" style="background-image: url(/mod2015/img/today_paper/nagoya.jpg); background-position: center; background-size: 130px auto;" alt="名古屋版『本日の一面』の写真" width="130" height="167">',
		'<img src="https://cache2.nipc.jp/mod2015/img/common/blank.gif" style="background-image: url(/mod2015/img/today_paper/osaka.jpg); background-position: center; background-size: 130px auto;" alt="大阪版『本日の一面』の写真" width="130" height="167">',
		'<img src="https://cache2.nipc.jp/mod2015/img/common/blank.gif" style="background-image: url(/mod2015/img/today_paper/seibu.jpg); background-position: center; background-size: 130px auto;" alt="西部版『本日の一面』の写真" width="130" height="167">'
		];
	};
	// セレクトした時の動作
	NsPaper.prototype.selectDistrict = function(value) {
		this.showImage(value);
	}
	
	// 紙面の画像を表示
	NsPaper.prototype.showImage = function(value) {
		if (!value) {
			return;
		}
		var message = this.data[value];
		$('#paperImage').html(message);
	}
	//	main
	var nsPaper = new NsPaper;
	var value = nsGetCookie("nsPaperDistrict");
//console.log(value);

	if (value != null && value != undefined) {
		nsPaper.selectDistrict(value);
		$("#paperSelect").val(value);
	} else {
		nsPaper.selectDistrict(0);
	}
	// 設定ボタンを押したときのイベント設定
	$("#paperSet").click(function() {
		var value = $("#paperSelect option:selected").val();
		nsSetCookie("nsPaperDistrict", value, COKKIE_LIFE);
		nsPaper.selectDistrict(value);
		return false;
	});

});
</script>


