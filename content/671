    </div><script type="text/javascript">
          (function(){
          var btt=document.getElementById("btt");
          var ie6=!-[1,] && !window.XMLHttpRequest;
          var clientHeight=getClientHeight();
          var scrollTop;

          function getClientHeight()//获取视口高度
          {
          var clientHeight;
          if(document.compatMode=="CSS1Compat")
          {
          clientHeight=document.documentElement.clientHeight;
          }
          else //compatMode=="BackCompat"
          {
          clientHeight=document.body.clientHeight;
          }
          return clientHeight;
          }

          window.onscroll=function()
          {
          scrollTop=document.documentElement.scrollTop || document.body.scrollTop;
          if(scrollTop>=10)
          {
          if(ie6)
          {
          btt.style.top=(scrollTop+clientHeight-74).toString()+"px";
          }
          btt.style.display="block";
          }
          else
          {
          btt.style.display="none";
          }
          }

          if(ie6)
          {
          btt.style.position="absolute";
          window.onresize=function()//当拉动窗口的大小得时候scrollTop是不会改变的
          {
          if(clientHeight!=getClientHeight())
          {
          clientHeight=getClientHeight();
          btt.style.top=(scrollTop+clientHeight-74).toString()+"px";
          }
          }
          }

          btt.onclick=function()
          {
          backToTop();
          }
          function backToTop()
          {
          if(scrollTop!=0)
          {
          if(document.documentElement.scrollTop==0)
          {
          //document.body.scrollTop=scrollTop-100;
          document.body.scrollTop=0;
          }
          else
          {
          //document.documentElement.scrollTop=scrollTop-100;
          document.documentElement.scrollTop=0;
          }
          //setTimeout(arguments.callee,0);
          }
          }
          })()
        </script></body>
</html>