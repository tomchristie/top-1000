                  "contentUrl": "https://ovp.tmz.com/video/49/o/2021/08/06/495fed5513854f5ebceb53ec56cf30be.mp4",
                          "embedUrl": "https://share.tmz.com/videos/2021-08-06-080621-racquel-bailey-1220521/",
                "author": {
          "@type": "Person",
          "name": "TMZ Staff"
        },
                "publisher": {
          "@type": "Organization",
          "name": "TMZ",
          "logo": {
            "@type": "ImageObject",
            "url": "https://static.tmz.com/tmz-web/img/tmz-logo-red-147x60-v1.png",
            "height": 60,
