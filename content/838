    <script id="commerce-preload-script" data-commercepreloaddelayseconds="20" data-src="//commerce.adobe.com/checkout/iframe/uc-preload.js">

        const currentScript = document.getElementById('commerce-preload-script');
        const commercePreloadDelaySeconds = currentScript.getAttribute('data-commercePreloadDelaySeconds');

        setTimeout(function() {
            const ucPreloadScript = document.createElement('script');
            const src = currentScript.getAttribute('data-src');
            ucPreloadScript.setAttribute('id', 'uc-preload-script');
            ucPreloadScript.setAttribute('src', src);
            document.body.appendChild(ucPreloadScript);
        }, commercePreloadDelaySeconds * 1000);
    </script>
    



            
            
