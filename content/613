	function buildAcceptedVendorList(iab_vendors_Json) {
		var iabVendors;
		const customVendors = ["Across Srl","Adservice A/S","AWIN","Microsoft Advertising","Facebook, Inc.","Google Adwords","Google Analytics","Google Optimize","iProspect GmbH","M, P, Newmedia, GmbH","Phonemotion GmbH","Pinterest Inc.","Tisoomi GmbH","PopAds","TikTok","Reddit, Inc.","SmartResponse","Media Force, Ltd."];

		if (iab_vendors_Json) {
			iabVendors = iab_vendors_Json.vendors;
		}
		var acceptedVendorList = '';

		if (customVendors && customVendors.length > 0) {
			for (var i = 0; i < customVendors.length; i++) {
				var vendor = customVendors[i];
				if (SDG.Publisher.getCustomVendorConsent(vendor)) {
					acceptedVendorList += '*' + vendor;
				}
			}
		}
		if (iabVendors) {
			var processedIabVendors = 0;
			var iabVendorsSize = getVendorSize(iabVendors);

			for (var iabVendorsKey in iabVendors) {
				if(iabVendors.hasOwnProperty(iabVendorsKey)) {
					var vendorId = iabVendors[iabVendorsKey]["id"];
					SDG.Publisher.getIABVendorConsent(vendorId, function (returnValue) {
						if (returnValue === true) {
							if(vendorId !== 848) {
								acceptedVendorList += '*' + vendorId;
							}
						}
						processedIabVendors++;
						if (iabVendorsSize === processedIabVendors) {
							if (acceptedVendorList.length > 0) {
								acceptedVendorList += '*';
								window.dataLayer.push({ 'acceptedVendorList': acceptedVendorList });
							}
							window.dataLayer.push({ 'event': 'vendor list loaded' });
						}
					});
				}
			}
		}
	}

	function getVendorSize(vendors) {
		var result = 0;
		for (var prop in vendors) {
			result++;
		}

		return result;
	}

	window.addEventListener('metaTagSystemCmpConsentAvailable', trackAcceptedVendorList);
</script>
