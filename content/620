function getCookieValue(name) {
  var b = document.cookie.match('(^|[^;]+)\s*' + name + '\s*=\s*([^;]+)');
  return b ? b.pop() : '';
}

function getWebtrackingLoginStatus(cookiePrefix) {
  var webtrackingSoft = getCookieValue(cookiePrefix + "-slstat");
  var webtrackingHard = getCookieValue(cookiePrefix + "-hlstat");

  if (webtrackingSoft && webtrackingHard) {
    return 'hslogin';
  } else {
    if (webtrackingSoft) {
      return 'slogin';
    }
    if (webtrackingHard) {
      return 'hlogin';
    }
  }

  return 'logout';
}

window.bahn = window.bahn || {};
window.bahn.loginStatus = getWebtrackingLoginStatus('DB4-pb');
</script>

