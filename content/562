<script id="wdg_set" data-props="{&quot;testgroup&quot;:&quot;default&quot;,&quot;testvariant&quot;:&quot;default&quot;,&quot;params&quot;:[],&quot;params_ttl&quot;:30000}">
SpilGames(['Portal', 'PageTracker', 'EventTracker', 'ThumbnailTracker'], function (Portal, PT, ET, TT) {
    'use strict';

    Portal
        .set('portalversion',   'widgets-teens')
        .set('devicetype',      'unknown')
        .set('pagetype',        'home')
        .set('pagetypedetail',  'index')
        .set('pageid',          '' || null)
        .set('requestid',       '0_0118C4B8')
        .set('hasgdprconsent',  'false')

        

        

        
        .set('testgroup',       'default')
        .set('testvariant',     'default')
        
    ;

    
    PT.track();
    
    ET.init();

    TT.init();

    // Create the Performance Observer instance.
    const observer = new PerformanceObserver((list) => {
        for (const entry of list.getEntries()) {
            const fid = entry.processingStart - entry.startTime;
            SpilGames('tracker.event.express', {
                eventCategory: 'page',
                eventAction: 'firstInputDelay',
                properties: {
                    pageType: 'home',
                    inputDelay: fid
                }
            });
        }
    });

    // Start observing first-input entries.
    observer.observe({
        type: 'first-input',
        buffered: true,
    });
});
</script>




        
            </div>
        </div>
        

