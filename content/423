<script>
	(function() {
		var defaultAspectRatio = '16\x3A9';
		var forcedVideoPlayer = AufVideo.Utils.getCookieValue('video_player_techno');
		var dailymotionId = "x837f58";
		var allTechnos = {"videojs":{"id":"videojs","name":"Video JS"},"jwplayer":{"id":"jwplayer","name":"JWPlayer"},"dailymotion":{"id":"dailymotion","name":"Dailymotion"}};
		var allAdsTechnos = {"videojs":{"id":"ima","name":"IMA DFP"},"jwplayer":{"id":"jwplayer-ads","name":"JWPlayer"},"dailymotion":{"id":"dmp-ads","name":"Dailymotion"}};
		var techno = forcedVideoPlayer ? allTechnos[forcedVideoPlayer] : {"id":"dailymotion","name":"Dailymotion"};
		var adTechno = forcedVideoPlayer ? allAdsTechnos[forcedVideoPlayer] : {"id":"dmp-ads","name":"Dailymotion"};
		var isDailymotion = techno.id === VideoPlayers.DAILYMOTION;

		var isComScoreTrackingEnabled = Boolean(1);
		var isAdDisabled = Boolean();

		if (isDailymotion && !dailymotionId) {
			// Fallback to jwplayer if no dailymotion id
			var fallback = VideoPlayers.VIDEOJS;
			techno = allTechnos[fallback];
			adTechno = allAdsTechnos[fallback];
		}

		var settings = {
			// Players
			techno: {
				id: techno['id'],
				name: techno['name']
			},
			playerId: 'af_player_6110fd918fbdf0_46136351',
			videoId: '645800',
			context: 'homepage',
			position: 'ATF',
			aspectRatio: '16\x3A9',

			// Ads
			adUnit: '\x2F229683329\x2Faufeminin\x2Fvideo\x2Fpreroll_ATF',
			adTechno: {
				id: adTechno['id'],
				name: adTechno['name']
			},
			isAdDisabled: isAdDisabled
		};

		

		function injectScript(d, src, cb, params) {
			var script = d.createElement('script');
			script.type = 'text/javascript';
			script.async = true;
			script.onload = function() {
				// remote script has loaded
				if (cb && typeof cb === 'function') {
					cb(params);
				}
			};
			script.src = src;
			d.getElementsByTagName('head')[0].appendChild(script);
		}

		function buildVideoLogger(props) {
			var videoLoggerSettings = {
				context: {
					adPlayer: (props.adTechno && props.adTechno.name) ? {
						name: props.adTechno.name
					} : undefined,
					player: {
						version: undefined,
						name: props.techno ? props.techno.name : 'undefined',
						videoId: props.videoId,
						context: props.context,
						position: props.position
					}
				}
			};

			return new VideoLogger(videoLoggerSettings);
		}

		function buildVideoPlayer(props) {
			var isVideoJs = props.techno.id === VideoPlayers.VIDEOJS;
			var isJwPlayer = props.techno.id === VideoPlayers.JWPLAYER;
			var isDailymotion = props.techno.id === VideoPlayers.DAILYMOTION;
			var playerConstructor = isDailymotion ? AufVideo.Dailymotion : isJwPlayer ? AufVideo.JWPlayer : isVideoJs ? AufVideo.VideoJS : undefined;

			var playerSettings = {
				aspectRatio: props.aspectRatio,
				fluid: true,
				height: 480,
				id: props.playerId,
				muted: false,
				playlist: isJwPlayer ? {
					sources: [{"file":"https:\/\/assets.afcdn.com\/video1\/20210806\/v_645800.m3u8","type":"application\/x-mpegURL"},{"file":"https:\/\/assets.afcdn.com\/video1\/20210806\/v_645800_HD.mp4","type":"video\/mp4","label":"HD"},{"file":"https:\/\/assets.afcdn.com\/video1\/20210806\/v_645800_sd.mp4","type":"video\/mp4","label":"SD"}],
					image: 'https://assets.afcdn.com/video1/20210806/cover645800_w854h480c1.png'
				} : undefined,
				title: 'Horoscope\x20de\x20la\x20semaine\x20du\x209\x20au\x2015\x20ao\u00FBt\x202021\x20\x3A\x20de\x20bonnes\x20nouvelles\x20pour\x20les\x20signes\x20de\x20Terre',
				videoId: isDailymotion ? dailymotionId : props.videoId,
				isAdDisabled: isAdDisabled
			};

			var player = playerConstructor ? new playerConstructor(playerSettings) : undefined;

			return player
		}

		function getUnifyVideoPlacement(props) {
			return [{
				placementId: 'preroll_atf',
				videoId: props.videoId,
				context: props.context,
				adUnit: props.adUnit,
				playerName: props.techno.id,
				adPlayerName: props.adTechno.id,
				playerId: props.playerId
			}];
		}

		function getAdTargetingString(props) {
			var adTarget = '';

			var newAdTargeting = ';' + [
				adTarget,
				'player=' + settings.techno.id,
				'adplayer=' + settings.adTechno.id,
				'context=' + settings.context,
				'video=' + settings.videoId + 'v'
			].join(';');

			return newAdTargeting;
		}

		
		/**
		 * Configure ad video player
		 */
		function buildAdPlayer(props, adTargeting, player) {
			var AdPlayers = Object.freeze({
				IMA: 'ima',
				JWPLAYER_ADS: 'jwplayer-ads',
				DAILYMOTION: 'dmp-ads'
			});

			var isIma = props.adTechno.id === AdPlayers.IMA;
			var isJwPlayerAds = props.adTechno.id === AdPlayers.JWPLAYER_ADS;
			var isDailymotionAds = props.adTechno.id === AdPlayers.DAILYMOTION;
			var adsPlayerConstructor = isIma ? AufVideo.IMA : isJwPlayerAds ? AufVideo.JWPlayerAds : isDailymotionAds ? AufVideo.DMPlayerAds : undefined;

			// Global Settings
			var adsSettings = {
				id: props.playerId,
				player: player,
				adUnit: props.adUnit,
				adTargeting: adTargeting
			};

			// VideoJS (IMA) & JwPlayer
			if (isIma || isJwPlayerAds) {
				adsSettings.allowMultipleTinyAds = true;
				adsSettings.siteId = 1;
				adsSettings.position = 'ATF';
				adsSettings.adScheduleId = '';
				adsSettings.hbVideoActive = false;
			}

			// Only VideoJS (IMA)
			if (isIma) {
				adsSettings.adLabel = 'Publicité - Votre vidéo dans';
				adsSettings.language = 'FR';
			}

			// Only JwPlayer Ads
			if (isJwPlayerAds) {
				adsSettings.isCM = false;
				adsSettings.midrollActive = false;
				adsSettings.minTimeMidroll = false;
				adsSettings.jwpAdvertisingParameters = true;
				adsSettings.needPegasus = true;
				adsSettings.adRule = true;
			}

			return adsPlayerConstructor ? new adsPlayerConstructor(adsSettings) : undefined;
		}

		function addStoreInstance(logger, player, adPlayer) {
			var store = AufVideo.Store.get();
			var instanceProps = {
				logger: logger,
				player: player,
				adPlayer: adPlayer,
				context: 'homepage',
				position: 'ATF',
				autoplay: {
					startOnSight: false,
				},
				sticky: {
					stickTo: '',
					blockedCookieTtl: '',
					scrollUp: false,
					fullMobile: false,
					showBackToOriginalPositionButton: true				}
			};

			store.addInstance('af_player_6110fd918fbdf0_46136351', instanceProps);
		}

		function initAll(settings) {
			/* * * * * * * * * * * * * * * * * *
			 *          Init trackers          *
			 * * * * * * * * * * * * * * * * * */
			var gaTracker = AufVideo.Tracker.GA.get(); // Note: gaTracker is not used afterwhile
			if (isComScoreTrackingEnabled) {
				AufVideo.Tracker.Comscore.init();
			}

			/* * * * * * * * * * * * * * * * * *
			 *    Instantiate Video Objects    *
			 * * * * * * * * * * * * * * * * * */
			var logger = buildVideoLogger(settings);
			var player = buildVideoPlayer(settings);

			if (!isAdDisabled) {
				window.unfyVideoPlacement = getUnifyVideoPlacement(settings);

				var adPlayer = buildAdPlayer(settings, getAdTargetingString(settings), player);
			}

			var manager = AufVideo.Manager.get({
				labels: {
					nextVideo: 'Vidéo suivante dans #SECONDS#'
				}
			});

			addStoreInstance(logger, player, adPlayer);
		}

		if (forcedVideoPlayer && forcedVideoPlayer === 'dailymotion') {
			dailymotionId = dailymotionId || 'x81ppg8';
			techno = allTechnos['dailymotion'];
			adTechno = allAdsTechnos['dailymotion'];
			
			settings.techno = {
				id: techno.id,
				name: techno.name
			};

			settings.adTechno = {
				id: adTechno.id,
				name: adTechno.name
			};

			injectScript(document, 'https://api.dmcdn.net/all.js', function() {
				injectScript(document, '/reloaded/static/Front/Component/videoPlayer/adPlayers/dailymotion.js', function() {
					injectScript(document, '/reloaded/static/Front/Component/videoPlayer/players/dailymotion.js', initAll, settings);
				}, settings);
			});
		} else {
			initAll(settings);
		}
	})();
</script>

          </div>
        </div>
      </div>
