          (function () {
            var TCF_VERSION = 2;
            var TCF_DIALOG_ID = '168480';
            var COOKIE_TIMEOUT_MS = 150;

            // Expose tcfConsentString with opt=out, which disables personalized adCalls in videoplayer by default
            window.tcfConsentString = 'opt=out';

            /**
             * Expose showPrivacyManager() which allows us to manually open the privacy manager modal
