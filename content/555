    function getQueryStringValue (key) {
      return decodeURIComponent(window.location.search.replace(new RegExp("^(?:.*[&\?]" + encodeURIComponent(key).replace(/[.+*]/g, "\$&") + "(?:\=([^&]*))?)?.*$", "i"), "$1"));
    }

    const outputType = getQueryStringValue('outputType');

    // handle nonav output type
    if (dataLayer && outputType === 'nonav') {
      const queryStringDevice = getQueryStringValue('device').toLowerCase() || 'desktop';

      // use the existing agof offer in
      let agof_offer = 'rtl';

      switch(queryStringDevice) {
        case 'ios':
          agof_offer = 'apprtlin';
          break;
        case 'android':
          agof_offer = 'aadrtlin';
          break;
      }

      dataLayer.push({
        'env_device': 'smartphone',
        'env_OS': queryStringDevice,
        'agof_offer': agof_offer,
      });
    } else if (dataLayer
      && window.localStorage
      && window.localStorage.getItem('userCountry')
      && window.localStorage.getItem('envDevice')
    ) {
      const cc = window.localStorage.getItem('userCountry');
      const xdevice = window.localStorage.getItem('envDevice');
      dataLayer.push({
        'user_country': cc,
        'env_device': xdevice,
      });
    } else if (dataLayer) {
      const client = new XMLHttpRequest();
      client.open('GET', '/', true);
      client.send();
      client.onreadystatechange = function() {
        if (this.readyState == this.HEADERS_RECEIVED) {

          //get the header variables
          const cc = client.getResponseHeader('arc-country');
          const xdevice = client.getResponseHeader('x-device');

          // set a country code and x-device cookie
          window.localStorage.setItem('userCountry', cc);
          window.localStorage.setItem('envDevice', xdevice);

          // only add env_device when we're not using the nonav output type
          dataLayer.push({
            'user_country': cc || 'DE',
            'env_device': xdevice,
          });
        }
      }
    }
