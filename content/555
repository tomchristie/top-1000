            window.__tcfapi('addEventListener', TCF_VERSION, function(tcData, success) {
              if (tcData.eventStatus === 'tcloaded' || tcData.eventStatus === 'useractioncomplete') {
                if (success && tcData) {
                  // Override opt=out when a valid tcf string has been received from the tcf api
                  // (@see components/utilities/Videoplayer/tracking.js)
                  window.tcfConsentString = 'consent=' + tcData.tcString;
                }

                window.__tcfapi('getCustomVendorConsents', TCF_VERSION, function(vendorConsents, success) {
                  // Expose tcfGrants map to make it accessible in TCF.getConsentForVendor()
