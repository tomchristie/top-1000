            window.showPrivacyManager = function() {
              if (window._sp_ && window._sp_.loadPrivacyManagerModal) {
                window._sp_.loadPrivacyManagerModal(TCF_DIALOG_ID);
              }
            };

            /**
             * @see https://documentation.sourcepoint.com/web-implementation/sourcepoint-gdpr-and-tcf-v2-support/using-__tcfapi-addeventlistener-and-removeeventlistener-commands
