          if(dataObj.isClosedPeriod == "Y"){
          			$(".sp-zb-tf").hide();
          		} 
          		else {
          			$(".sp-zb-tf").show().html("今日有直播");
          			var nowTime = dataObj.date;
          			var weekNums = dataObj.date.split(" ");
                  var timestamp = weekNums[1].split(":");
                  var hour = parseInt(timestamp[0]);
                  var minute = parseInt(timestamp[1]);
                  var second = parseInt(timestamp[2]);
                  if((hour == 20 && minute >=45) || (hour == 21 && minute >=0 && minute < 15)){
                                                                                       $(".sp-zb-tf").html("即将直播").css("letter-spacing","3px");
                                                                                       }
                                                                                       if ("Monday,Wednesday,Friday,Tuesday,Thursday,Sunday".indexOf(weekNums[0]) > -1) {
          if (hour == 21 && minute >= 15 && minute < 31) {
                                                        $(".sp-zb-tf").html("正在直播").css("letter-spacing","3px");
                                                        }
                                                        if((hour == 21 && minute > 31) || hour >= 22){
          $(".sp-zb-tf").hide();
          }
          } else if ("Saturday".indexOf(weekNums[0]) > -1) {
          if (hour == 21 && minute >= 15 && minute < 28) {
                                                        $(".sp-zb-tf").html("正在直播").css("letter-spacing","3px");
                                                        }
                                                        if((hour == 21 && minute > 28) || hour >= 22){
          $(".sp-zb-tf").hide();
          }
          }
          }
          }

          });

          </script>

