<script>
	var top_slot;
	var top_home_slot;
	var top_mthai_trend_slot;
	var top_mthai_tell_slot;
	var top_mthai_chilltalk_slot;
	var top_mthai_fanghuwaitalk_slot;
	var top_mthai_backtotalk_slot;
	var rec_slot1;
	var rec_slot2;
	var rec_trend_slot1;
	var rec_trend_slot2;
	var rec_tell_slot1;
	var rec_tell_slot2;
	var rec_chilltalk_slot1;
	var rec_chilltalk_slot2;
	var rec_fanghuwaitalk_slot1;
	var rec_fanghuwaitalk_slot2;
	var rec_backtotalk_slot1;
	var rec_backtotalk_slot2;
	var rec_between_slot = [];
	var billboard_slot;
	var billboard_home_slot;
	var billboard_home_slot2;
	var billboard_trend_slot;
	var billboard_tell_slot;
	var billboard_chilltalk_slot;
	var billboard_fanghuwaitalk_slot;
	var billboard_backtotalk_slot;
	var gallery_slot;
	var coverpage_slot;
	var adhesion_slot;
	var top_mthai_76354_slot;
	var rec_76354_slot1;
	
	googletag.cmd.push(function() {
		var top_sizeMapping = googletag.sizeMapping().
		addSize([1000, 0], [[970, 250], [980, 90], [970, 90], [728,250], [728, 90]]).
		addSize([320, 0], [[320, 150], [320, 100]]).
		build();
		var top_defineSlot = [[970, 250], [970, 90], [980, 90], [728,250], [728, 90], [320, 150], [320, 100]];

		var billboard_sizeMapping = googletag.sizeMapping().
		addSize([1000, 0], [[970, 250], [728, 250]]).
		addSize([320, 0], [[320, 150]]).
		build();
		var billboard_defineSlot = [[970, 250], [728, 250], [320, 150]];

		var rectangle_sizeMapping = googletag.sizeMapping().
		addSize([320, 0], [[1, 1], [200, 200], [300, 250], [250, 250]]).
		build();
		var rectangle_defineSlot = [[1, 1], [200, 200], [300, 250], [250, 250]];
					top_home_slot = googletag.defineSlot('/10983686/MThai/MThai_Homepage_2/MT_HP_Top', top_defineSlot, 'dfp-home-topbanner').
				defineSizeMapping(top_sizeMapping).
				addService(googletag.pubads());

			billboard_home_slot = googletag.defineSlot('/10983686/MThai/MThai_Homepage_2/MT_HP_Billboard', billboard_defineSlot, 'dfp-home-billboard').
				defineSizeMapping(billboard_sizeMapping).
				addService(googletag.pubads());

			billboard_home_slot2 = googletag.defineSlot('/10983686/MThai/MThai_Homepage_2/MT_HP_Billboard_2', billboard_defineSlot, 'dfp-home-billboard2').
				defineSizeMapping(billboard_sizeMapping).
				addService(googletag.pubads());	
		
			
		
		// adhesion
		if(  window.matchMedia("(max-width: 640px)").matches ) {
							var map_adhesion = googletag.sizeMapping().
					addSize([640, 0], []).
					addSize([0, 0], [320, 50]).
					build();
				adhesion_slot = googletag.defineSlot('/10983686/MThai/MThai_News/MT_News_AdhesionMB', [320, 50], 'dfp-adhesion').
					defineSizeMapping(map_adhesion).
					addService(googletag.pubads());
				googletag.pubads().refresh([adhesion_slot]);
					}

		// A) Enable with defaults.
		googletag.pubads().enableLazyLoad();
		// B) Enable without lazy fetching. Additional calls override previous
		// ones.
		googletag.pubads().enableLazyLoad({fetchMarginPercent: -1});
		// C) Enable lazy loading with...
		googletag.pubads().enableLazyLoad({
			// Fetch slots within 5 viewports.
			fetchMarginPercent: 500,
			// Render slots within 2 viewports.
			renderMarginPercent: 200,
			// Double the above values on mobile, where viewports are smaller
			// and users tend to scroll faster.
			mobileScaling: 2.0
		});

		googletag.pubads()
							.setTargeting('pages', ['frontpage'])
						.setTargeting('mthai_section', ['mthai']);
		googletag.enableServices();
		
		document.querySelectorAll(".gallery figure a").forEach(function (qSelect) {
			qSelect.addEventListener( "click", function() {
				googletag.pubads().refresh([gallery_slot]);
				/*
				var refresh_ads_gallery = setInterval(function() { googletag.pubads().refresh([gallery_slot]); }, 5000);
				window.addEventListener( "scroll", function() {
					clearInterval(refresh_ads_gallery);
				});
				*/
			});
		});
	});
</script>

		     
