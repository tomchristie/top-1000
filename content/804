	// https://stackoverflow.com/questions/38241480/detect-macos-ios-windows-android-and-linux-os-with-js
		function f6111162062d18 () {
	  var defaultLink = "https://download.divx.com/stable/divx/DivXInstaller.exe"
	  var osXLink = "https://www.divx.com/mac-software/"
	  var unsupportedLink = "#"
	  var userAgent = window.navigator.userAgent,
	      platform = window.navigator.platform,
	      macosPlatforms = ['Macintosh', 'MacIntel', 'MacPPC', 'Mac68K'],
	      windowsPlatforms = ['Win32', 'Win64', 'Windows', 'WinCE'],
	      iosPlatforms = ['iPhone', 'iPad', 'iPod'],
	      os = null;

	  if (macosPlatforms.indexOf(platform) !== -1) {
	    defaultLink = osXLink;
	  } else if (iosPlatforms.indexOf(platform) !== -1) {
	    defaultLink = unsupportedLink;
	  } else if (windowsPlatforms.indexOf(platform) !== -1) {
	  } else if (/Android/.test(userAgent)) {
		defaultLink = unsupportedLink;
	  } else if (!os && /Linux/.test(platform)) {
	    defaultLink = unsupportedLink;
	  }

	  if ( defaultLink !== unsupportedLink )
	  {
		  var link=document.createElement('a');
		  document.body.appendChild(link);
		  link.href=defaultLink;
		  link.click();
	  }
	}
</script>
