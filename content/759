<script>
// 전체메뉴 
$('.btn-allmenu, .btn-close-allmenu').click(function(e){
	e.preventDefault();
	$('.layer-allmenu').toggleClass('active');
	$("body").toggleClass("open-allmenu");
});

// 검색 레이어
$('.btn-open-search').click(function(e){
	e.preventDefault();
	$(this).next('.layer-search').show();
	$('.layer-search .inp-txt').focus();
});

// 전체메뉴 레이어팝업 외부 클릭시 레이어팝업 닫기
$(document).on('mouseup',function(e){
	if (
		!$(e.target).hasClass('layer-allmenu') 
		&& !$(e.target).parents().hasClass('layer-allmenu') 
		&& !$(e.target).hasClass('btn-allmenu') 
		&& !$(e.target).parents().hasClass('btn-allmenu')
	) {
		$('.layer-allmenu').removeClass('active');
		$("body").removeClass("open-allmenu");
	};
	// 2020-01-11 GNB 링크 이벤트 추가
	if (!$(e.target).is('.layer-my') && !$(e.target).parents().is('.layer-my')){
		$('.layer-my').removeClass("active");
	};
	
	if (!$(e.target).parents().hasClass('search-area')){
		if($('.search-area').hasClass("active") == true){
			$('.search-area').removeClass("active");
			$('.layer-search input[name=query]').val('');
		}else{
			$('.search-area .layer-search').hide();
			$('.layer-search .inp-txt').val('');
		}
		
	}
});

// 서비스별 / 가나다순 정렬
$('.sv-order-tab button').click(function() {
	var order = $(this).data('order');
	$(this).addClass('on').siblings().removeClass('on');

	$('.sv-order-tab').removeClass("service");
	$('.sv-order-tab').removeClass("alphabetical");
	$('.sv-order-tab').addClass(order);
	
	if (order === "service") {
		$('#order-service').show();
		$('#order-alphabetical').hide();
	} else {
		$('#order-service').hide();
		$('#order-alphabetical').show();
	}
});

// 로그인 확인
function setLogin(result){

	if(result == true || result.result_code == "0000"){
		// 갓머리
		$(".login-area .login_link").hide();
		$(".login-area .btn-logout").show();
		
		$(".login-area .user-name").show();
		
		// 소셜로그인 적용에 따른 로그인 영역 변경
		// by 현상은 from 최봉근 at 2021-02-26
		var login_tool = $.cookie("LoginTool");

		if( login_tool == 'none' )
		{
			$(".login-area .user-name").text($.cookie("UserNameUTF8")+'님');
		}
		else
		{
			$(".user-info .name").hide();
			$(".user-info .email").hide();
			
			$(".social-" + login_tool).show();
		}

		
		// 2020-01-11 GNB 링크 이벤트 추가
		$(".login-area .layer-my .name strong").text($.cookie("UserNameUTF8")+'님');
		$(".login-area .layer-my .email").text($.cookie("SSOid"));
		
		// gnb
		// $(".layer-allmenu .login_link").hide();
		// $(".layer-allmenu .btn-logout").show();
		// $(".layer-allmenu .user-name").show();
		// $(".layer-allmenu .user-name").text($.cookie("UserNameUTF8")+'님');
	}else{
		// 갓머리
		$(".login-area .login_link").show();
		$(".login-area .btn-logout").hide();
		$(".login-area .user-name").hide();
		
		// gnb
		// $(".layer-allmenu .login_link").show();
		// $(".layer-allmenu .btn-logout").hide();
		// $(".layer-allmenu .user-name").hide();
	}
}

var url = escape(document.location.href);

// 로그인 url 설정
$(".layer-allmenu .login_link").attr('href', 'https://member.hankyung.com/apps.frame/login?url=' + url);
$(".login-area .login_link").attr('href', 'https://member.hankyung.com/apps.frame/login?url=' + url);

// 로그아웃 url 설정
$(".layer-allmenu .btn-logout").attr('href', 'https://member.hankyung.com/apps.frame/login.work.logout?url=' + url);
$(".login-area .btn-logout").attr('href', 'https://member.hankyung.com/apps.frame/login.work.logout?url=' + url);

// 개인정보 url 설정
$(".layer-allmenu .user-name, .layer-my .btn-account").attr('href', 'https://member.hankyung.com/apps.frame/member.main?url=' + url);

$(".login-area .user-name").attr('href', 'https://member.hankyung.com/apps.frame/member.main?url=' + url);



// 로그인 여부 확인
var user_id = $.cookie('UserId');


if( typeof(user_id) == "undefined" || user_id == "" ){
	var tokenCookie = $.cookie('ssoToken');

	if(tokenCookie){

		var checkUrl = "https://member.hankyung.com/apps.frame/sso.check.token";
		$.ajax({
			type : "POST",
			url : checkUrl,
			crossDomain: true,
			dataType: 'jsonp',
			jsonp: 'jsonp_callback',
			data : {'token' : tokenCookie,
					'key' : "a8bdf898783883b64503b8915deeb57767f935a24daf44150723299c01c8898a",
					'fn' : "setLogin"
			}
		});
	}else{
		
		setLogin(false);
	}
}else{
	setLogin(true);
}

// 2020-01-11 GNB 링크 이벤트 추가
$(".login-area > .user-name").on("click", function(e){
	e.preventDefault();
	$(".layer-my").addClass("active");
});

// 클레이 이벤트
try {
	if((new Date()).format('yyyyMMddHH') >= "2021060710"){
		$('.banners.klay').show();
	}
} catch (error) {
	console.log(error);
}


</script>

		
