<script>
var type = "kospi";
var dayMax, ticMax, ticMin, tDay; 
var isChecked = false;
var item;

var isMin = true;

getChartData(type);

// 개장일 및 수능일
var test_date = '20200102';
var test_endTime = 1530;

var is_today = true;

function getTodayDate(){
	today = new Date();
	
	m = today.getMonth()+1;
	d = today.getDate();
	
	if(m < 10) m = '0'+m;
	if(d < 10) d = '0'+d;
	
	return m+'.'+d;
}

// 차트 초기화 및 그리기
function initChart(data){
	data = data.korea[type];
	
	dayMax = data.dayMax;
	ticMax = data.ticMax;
	ticMin = data.ticMin;
	tDay = data.tradeDay;
	
	// 기준일 설정
	tDate = data.tradeDate;
	
	if(tDate != getTodayDate()){
		is_today = false;
	}
	
	if(is_today == true){
		$(".finance-chart .date span").text(tDate);
		$(".finance-chart .date").show();
	}
	
	item = makeData(data);
	
	groupingUnits = (
		item.isDay == 'Y'
		? [['minute', [1]], ['day', [1]]]
		: [['day', [1]]]
	);
	
	var event = {
        afterSetExtremes: function (e) {
            if (e.rangeSelectorButton == undefined) return;
			
            function getIndex() {
                if (chart.rangeSelector.buttons.length > 0) {
                    for (var i = 0; i < chart.rangeSelector.buttons.length; i++) {
                        if (chart.rangeSelector.buttons[i].state == 2) {
                            return i;
                        }
                    }
                }
                return -1;
            }

            clickItem = getIndex();
			
            if (e.trigger == undefined) {
            }
            else {
				if (e.rangeSelectorButton.text != "1일") {
                    setTimeout(function () {
                        window.chart.xAxis[0].setExtremes(e.min, dayMax);
                        if (!isChecked) {
                            isChecked = true;
                            chart.rangeSelector.clickButton(clickItem, false);
                        }
                    }, 100);
					
					isMin = false;
					
					$(".finance-chart .date").hide();
                }else if (e.rangeSelectorButton.text == "1일") {
                    setTimeout(function () {
                        window.chart.xAxis[0].setExtremes(ticMin, ticMax);
                        if (!isChecked) {
                            isChecked = true;
                            chart.rangeSelector.clickButton(clickItem, true);
                        }
                    }, 100);
					
					isMin = true;
					
					$(".finance-chart .date").show();
                }
            }
        }
    };
	
	chartOption = {
		plotOptions: {
            series: {
                fillOpacity: 0.1
            }
        },
		exporting: { 
			enabled: false
		},
        // 차트가 그려지는 위치 설정
        chart: {
            renderTo: 'finance_chart'
        },
        // 범위선택 버튼 설정
        rangeSelector: {
            enabled: true,              // 사용
            inputEnabled: false,    // 입력 기능 미사용
            selected: 0,                    // 초기 선택 버튼(1일)
            buttonTheme: {
				width: 38
				// , height: 25
				, padding: 6
				, r: 5
                , states: {
                    hover: {},
                    select: {
                        fill: '#efefef'
                    }
                }
            },
            buttons: [
				{
					type: 'day'
					, count: 1
					, text: '1일'
				}
				, {
					type: 'week'
					, count: 1
					, text: '5일'
				}
				, {
					type: 'month'
					, count: 1
					, text: '1개월'
				}
			]
        },
        // 차트 아래 네비게이터
        navigator: {
            series: {
                color: '#777777',
                fillOpacity: 0.05,
                lineColor: '#777777',
                lineWidth: 1
            },
			enabled: false	// 네비게이터 on/off
        },
        scrollbar: {
            enabled: false
        },
        xAxis: {
            gridLineWidth: null
            , type: 'datetime'
            , dateTimeLabelFormats: {hour: "%H:%M", day: '%m.%d', week: '%Y.%m.%d', month: '%Y.%m.%d'}
            , events: event
        },
        yAxis: [{
            gridLineWidth: null
			, padding: 0.5
            , labels: {
                //align: 'left',
                formatter: function () {
                    return Highcharts.numberFormat(this.value, 2, '.', ',');
                }
            }
            , plotLines: [{
                value: item.pcp,    // 종가
                color: '#999999',
                width: 1
            }]
        }],
        tooltip: {
            valueDecimals: 2,
            type: 'datetime',
            useHTML: true,
            formatter: function () {
                var topDateFm = Highcharts.dateFormat('%Y.%m.%d', this.x);
				
                if(isMin == true) {
                    var tmp = Highcharts.dateFormat('%Y.%m.%d %H:%M', this.x);
                    if (tmp != "00:00") {
                        topDateFm = Highcharts.dateFormat('%Y.%m.%d %H:%M', this.x);
                    }
                }
				
                var d = new Date(this.x);
				
                var s = '';
                // s += '<b>' + topDateFm + '</b>';
				
                $.each(this.points, function (i, point) {
					s += '<b>'+Highcharts.numberFormat(point.y, 2, '.', ',')+'</b><br/>'+'<span>' + topDateFm + '</span>';
					// s += '<br/><span style="color: ' + point.series.color + ';">' + '</span>' + Highcharts.numberFormat(point.y, 2, '.', ',');
                });
                return s;
            },
            shared: true
        },
        series: [
			{
				type: "area",
				id: "tic",
				// name: "종가",
				valueDecimals: 2,
				data: item.chart,
				threshold: item.pcp,
				color: 'rgb(247, 111, 111)',
				negativeColor: '#3c74ff',
				dataGrouping: {
					units: groupingUnits
				}
			}
		]
	};
	
	window.chart = new Highcharts.StockChart(chartOption);
}

function makeData(data){
	var chartItem = [],
		chartItemEmpty = [], realChart = [],
		dataLength = data.chart == null ? 0 : data.chart.length;
		
	chartItem = [];
	chartItemEmpty = [];

	// 금일 데이터
	for (var i = 0; i < dataLength; i++) {
		if (data.chart[i][4] === undefined) {
			chartItemEmpty.push([
				data.chart[i][0], // the date
				null
			]);
		}
		else {
			chartItem.push([
				data.chart[i][0], // the date
				data.chart[i][4] === undefined ? null : data.chart[i][4]
			]);
		}
	}

	return {
		chart: realChart.concat(chartItem, chartItemEmpty),
		empty: chartItemEmpty,
		real: chartItem,
		pcp: data.pcp,
		isDay: data.isDay,
		tradeDay: data.tradeDay
	}
}

// 차트 데이터 호출
function getChartData(val){
	$(".stock-tab li").removeClass("on");
	$(".stock-tab li."+val).addClass("on");
	
	type = val;
	
	isMin = true;

	$.ajax({
		url: 'https://www.hankyung.com/data/datafile/stock/json/koreaChartData.json'
		, cache: false
		, type: 'POST'
		, dataType: "jsonp"
		, timeout: 200
		, crossDomain: true
		, jsonp: 'jsonp_callback'
		, success: function (data, status) {
			// console.debug(data);
		}
		, error: function (xOptions, textStatus) {
			// console.debug(xOptions);
		}
	});
}
</script>
