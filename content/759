			<script>
			if($.trim($(".major-thumb-news .layout-inner ul").html()) == ''){
				$(".major-thumb-news .layout-inner ul").remove();
			}
			
			if($.trim($(".major-thumb-news .layout-inner").html()) == ''){
				$(".major-thumb-news .layout-inner").remove();
			}
			
			if($.trim($(".major-thumb-news").html()) == ''){
				$(".major-thumb-news").remove();
			}
			
			$(".main-top-wrap [data-text=keyword]").each(function(){
				if($.trim($(this).html()) == ''){
					$(this).parents(".news-subject").remove();
				}
			});
			
			// 라인별 키워드 없을때 영역 삭제
			$(".main-top-wrap .major-news").each(function(idx, line){
				layer = $(line).find(".layout-inner");
			
				$.each($(line).find(".layout-inner"), function(i, layer){
					if($.trim($(layer).html()) == ''){
						$(this).remove();
						return;
					}
					
					is_subject = false;
					
					$(layer).find(".news-subject").each(function(i, subject){
						if($.trim($(subject).html()) != ''){
							is_subject = true;
							
							return;
						}
					});
					
					if(is_subject == false){
						$(line).find(".news-subject").remove();
					}
					
					$(layer).find("article[data-additional-index]").each(function(i, addition){
						if($(addition).is(":visible") == false){
							$(addition).remove();
						}
					});
				});
				
				if($.trim($(this).html()) == ''){
					$(this).remove();
					return;
				}
			});
			</script>
			<section data-module="sctColumnOpinion" data-use-disable="true" class="main-component opinion" data-name="sctColumnOpinion" data-area-description="[편집] 오피니언 편집">
        <div class="layout-inner">
            <h3 class="main-tit"><a href="https://www.hankyung.com/opinion">오피니언</a></h3>
            <article class="news-cont">
                <ul class="opinion-list">
                    <li class="opinion-item-1" data-index="0">
                        <div class="news-item">
                            <div class="thumb">
                                <a data-href="url" href="https://www.hankyung.com/opinion/article/2021080921291"><img data-src="pageImg01" data-alt="title" src="https://img.hankyung.com/photo/202108/07.20124489.3.jpg" alt="퇴직연금 '방치'하면서 노후대비?"></a>
                            </div>
                            <strong class="news-subject"><a data-href="keywordLink" data-text="keyword" href="javascript:alert('링크없음');">박성완 칼럼</a></strong>
                            <h2 class="news-tit"><a data-href="url" data-text="title" href="https://www.hankyung.com/opinion/article/2021080921291">퇴직연금 '방치'하면서 노후대비?</a></h2>
