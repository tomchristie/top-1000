<script>
var is_masquee = true;

// 실시간 데이터 세팅
function initRealitme(data){
	$.each(data.realtime, function(key, val){
		area = $(".stock-item-list li."+key);
		
		if(area.length > 0){
			area.find(".status").removeClass("up").removeClass("down").removeClass("hold");
			
			if(val[2] == "+"){
				area.find(".status").addClass("up");
			}else if(val[2] == "-"){
				area.find(".status").addClass("down");
			}else{
				area.find(".status").addClass("hold");
			}
			
			area.find(".status .num").text(number_format(val[1]));
			area.find(".status .rate em").text(val[2]+val[3]+"%");
			
		}
		
		// 증권 에어리어
		financeArea = $(".finance-chart li."+key);
		
		if(financeArea.length > 0){
			financeArea.find(".status").removeClass("up").removeClass("down").removeClass("hold");
			
			if(val[2] == "+"){
				financeArea.find(".status").addClass("up");
			}else if(val[2] == "-"){
				financeArea.find(".status").addClass("down");
			}else{
				financeArea.find(".status").addClass("hold");
			}
			
			financeArea.find(".status .num").text(number_format(val[1]));
			financeArea.find(".status .rate").html(val[4]+"<em>"+val[2]+val[3]+"%</em>");
		}
	});
	
	if(is_masquee == true){
		$(".stock-marquee .marquee").show();
		
		$('.stock-item-list').infiniteslide({
			speed: 50
		});
		
		is_masquee = false;
	}
}

// 증권 실시간 데이터 호출
function getRealTimeStockData(){
	$.ajax({
		url: '/data/datafile/stock/json/chartData.json'
		, cache: false
		, type: 'POST'
		, dataType: "jsonp"
		, async: false
		, timeout: 200
		, crossDomain: true
		, jsonp: 'jsonp_callback'
		, success: function (data, status) {
			// console.debug(data);
		}
		, error: function (xOptions, textStatus) {
			// console.debug(xOptions);
		}
	});
	
	setTimeout(function() {
        getRealTimeStockData();
    }, 1000 * 60);
}

// 차트 옵션 설정
Highcharts.setOptions({
	global: {
		useUTC: false
	},
	credits: {
		enabled: false
	}
});

$(document).ready(function(){
	// 증권 데이터 호출
	getRealTimeStockData();
	
	// 광고 닫기
	$(".btn-close-ad").on("click", function(){
		$(this).parents(".ad-top.close-ad").slideUp();
	});
	
	// EVENT
	$('.event-slide').slick({
		variableWidth: true,
		slidesToShow: 2
	});
	// 알림
	$('.notice-slide').slick({
		vertical: true,
		autoplay: true,
		autoplaySpeed: 2500,
		arrows: false
	});
	
	// 배너 롤링(1개 이상인 경우에만)
	if($('.header-banner .item').length > 1){
		$('.header-banner').slick({
			autoplay: true
			, autoplaySpeed: 3000
			, arrows: false
			, infinite: true
			, fade: true
			, cssEase: 'linear'
		});
	}
	
	// 헤드라인 및 주요뉴스 아이콘 처리
	$('[id-switch-ico-video]').each(function(){
		// 비디오
		if($(this).text() == '5'){
			$(this).parent().find('.thumb-icon.thumb-vod').show();
		}
		// 오디오
		else if($(this).text() == 'a'){
			$(this).parent().find('.thumb-icon.thumb-audio').show();
		}
		// 그래프
		else if($(this).text() == '1'){
			$(this).parent().find('.thumb-icon.thumb-graph').show();
		}
		// 포토
		else if($(this).text() == '0'){
			$(this).parent().find('.thumb-icon.thumb-photo').show();
		}
		// 슬라이드
		else if($(this).text() == '3'){
			$(this).parent().find('.thumb-icon.thumb-slide').show();
		}
	});
	
	// keyword 링크 없으면 url로 지정
	$('[data-href=keywordLink]').each(function(){
		if($(this).attr('href') == '' || $(this).attr('href') == 'javascript:alert(\'링크없음\');'){
			$(this).attr('href',$(this).closest('[data-index]').find('[data-href=url]:eq(0)').attr('href'));
		}
	});
});

function number_format(str){
	return str.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
}
</script>
