  console.log('CheckDepsAndCont',typeof T$,typeof D);  
  if ( typeof T$ !== 'undefined' && typeof D !== 'undefined' && T$('#asyncUnkritJS').length == 0 ) {
    var x=D.createElement('script');
    x.async=true;
    x.setAttribute( 'id', 'asyncUnkritJS' );
    x.onload=AsyncFinalize();

/* 20 minute caching */
    var customTS = Math.round(new Date().getTime()/1000/60/20);

    if (T.PRE===1)
      x.src='https://cmspreview.stroeerdp.de/rl13-home-async-unkrit-js/id_84796970/tid_js/index_ts'+customTS+'.js';
    else
      x.src='https://stats.t-online.de/rl13-home-async-unkrit-js/id_84796970/tid_js/index_ts'+customTS+'.js';
    var ank=T$('#TScriptAnker')[0];
    ank.parentNode.insertBefore(x,ank);
  }
};
