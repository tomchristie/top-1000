    var st=TckWS(a<=2); if(n===0) st.clear(); else {st.removeItem(n); st.removeItem(n+'_jsn'); st.removeItem(n+'_exp')} return 1;
  }
  else if(a&1) {TdC(n); return -1}  //Cookie
  return null;
}
/* TckWS(): Hilfsfunktion "checkWebStorage" */
function TckWS(x) {
  return typeof(x)==U ? W.Storage?1:0 : x ? localStorage : sessionStorage;
}


