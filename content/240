          if(!T$(this).hasClass('Tisright')) {
            T$('.TslideTo').addClass('Tisright');
            T$('.Tnextnav1').addClass('Tismoving');
            T$('.Tnextnav1').animate({
              'left': '-' + T$('.Tnextnav1').attr('data-offset') + 'px'
            }, 500, 'swing', function() {
              T$('.Tnextnav1').removeClass('Tismoving')
            });
          } else {
            T$('.TslideTo').removeClass('Tisright');
            T$('.Tnextnav1').addClass('Tismoving');
            T$('.Tnextnav1').animate({
              'left': '-' + navSpace + 'px'
            }, 500, 'swing', function() {
              T$('.Tnextnav1').removeClass('Tismoving')
            });
          }
        });
        if(T$('.Tnextnav1 li.Tnnak').length && (T$('.Tnextnav1 li.Tnnak').position().left + T$('.Tnextnav1 li.Tnnak').width()) > T$('.Tnextnav1o').width()) {
          T$('.TslideTo').addClass('Tisright');
          T$('.Tnextnav1').addClass('Tismoving');
          T$('.Tnextnav1').animate({
            'left': '-' + T$('.Tnextnav1').attr('data-offset') + 'px'
          }, 500, 'swing', function() {
            T$('.Tnextnav1').removeClass('Tismoving')
          });
        }
      } else {
        T$('.TslideTo').remove();
      }
      T$('#Tburger').on('click', function() {
        T$('body').toggleClass('Tshowsticky');
      })
      if(!T$('.Tnextnav2 li').length) T$('.Tnextnav2').remove();

      if(T.DCL == 1) {
        //Sticky Navigation
        var navOt = T$('.Tnextnav').offset().top - T$('#Tnnav').height();
        var globalHeaderOt = 10;
        
        T$(W).on('scroll', false, function() {
          var scrollTop = document.documentElement.scrollTop || document.body.scrollTop;
          var fixednav = T$('#Tfixdh');
          if(scrollTop > globalHeaderOt && !W._ghisHidden) {
            W._ghisHidden = true;
            T$('body').addClass('TghIsH');
            if ( (typeof T.stickyNaviGASent !== 'boolean' || T.stickyNaviGASent === false) && typeof dataLayer !== 'undefined' ) {
              dataLayer.push({'event':'gtmEvent','eventCategory':'sticky-header','eventAction':'view','eventLabel':''});
              T.stickyNaviGASent = true;
            }
            if(fixednav.length && T.DCL == 1) {
              fixednav.addClass('Tfixdh--TghIsH');
            }
          } else if(scrollTop < globalHeaderOt && W._ghisHidden) {
            W._ghisHidden = false;
            T$('body').removeClass('TghIsH');
            if(fixednav.length && T.DCL == 1) {
              fixednav.removeClass('Tfixdh--TghIsH');            }
          }
          if((scrollTop > navOt) && !W._nisScrolled) {
            W._nisScrolled = true;
            T$('body').addClass('TnIsH');
          } else if(scrollTop <= navOt && W._nisScrolled) {
            W._nisScrolled = false;
            T$('body').removeClass('TnIsH').removeClass('Tshowsticky');
          };
        });
      };
      //ClickTracking  Navigation initialisieren (ID: 80330474)
      if(W.initNaviagtionClick && typeof dataLayer != 'undefined') initNaviagtionClick();
      T$(D).trigger('scroll');
      //iPad Mehr-Dropdown
      if(C && C.isTouch) {
        T$('#Tnnav1 li span').click(function() {
          T$(this).next('dl').toggle().parent().toggleClass('Tnmna');
        });
      }
      T$('.CDB-ribbon-more-button').on('click', function() {
        if(!T$(this).hasClass('CDB-item-closed')) {
          T$('body').addClass('TCDB-big');
        } else {
          T$('body').removeClass('TCDB-big');
        }
      });
    }
  }
};
var Tliq = {
  setBut: function() {},
  init: function() {}
};
function hideC1TeaserTxt() {
  if(T$('body').hasClass('Tblock1')) {
    T$('.Tmc1.Tltb .Ttst .Ttsmore').each(function() {
      if(T$(this).text().trim() == 'mehr') {
        T$(this).remove();
      } else {
        T$(this).closest('a').append(T$(this));
      };
    });
  }
}

