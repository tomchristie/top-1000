function getTestVariation(cKey) {
   var testVarKey = (typeof cKey == "string") ? cKey :
  (typeof cKey == "boolean" && cKey) ? "CMS" : "";
   var name = "testVariation" + testVarKey + "=";
 
   var ca = document.cookie.split(';');
   for(var i = 0; i < ca.length; i++) {
      var c = ca[i];
      while(c.charAt(0) == ' ') {
         c = c.substring(1);
      }
      if(c.indexOf(name) == 0) {
         return c.substring(name.length, c.length);
      }
   }
   return "";
}
