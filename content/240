/* Tinc(css_js [,attributes [,parent]]): Include von CSS / CSS-Datei / JS-Datei
*   css_js: CSS '.class1{color:#0f0} .class2{color:#eee}' / CSS-Datei.css '.../datei.css' o. '.../tid_css/...' / JS-Datei.js analog '.js' o. 'tid_js'
*   attributes: Tag-Attribute 'media=screen,charset=utf-8,id=ID1'; optional, default: type=text/css bzw. text/javascript
*   parent: Elternelement, in den das Include eingehängt wird; optional, default: <head>
*   Return: erzeugtes/inkludiertes Element (= globale Var. Tinc_el)
*/
function Tinc(css_js,attributes,parent) {
  function cE(tag,typ) {var e=document.createElement(tag); if(typ) e.type=typ; return e}
  var a=arguments, l=a.length, i, x, el, par, att, cj=css_js;
  if(l>1&&typeof(x=a[l-1])=='object') {par=x; l--} else par=H;
  if(l>1&&typeof(x=a[l-1])=='string') att=x; else att='';
  if(cj.match('.js')||cj.match('/tid_js/')) {(el=cE('script','text/javascript')).src=cj}
  else if(cj.match('.css')||cj.match('/tid_css/')) {(el=cE('link','text/css')).href=cj;  el.rel='stylesheet'}
  else {el=cE('style','text/css');  if(el.styleSheet) el.styleSheet.cssText=cj; else el.appendChild(D.createTextNode(cj))}
  if(att) {a=att.replace(/ *, */g,',').replace(/ *= */g,'=').split(',');  for(i=0,l=a.length;i<l;i++) {x=a[i].split('='); el[x[0]]=x[1]}}
  par.appendChild(el);
  return Tinc_el=el;
}
