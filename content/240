function TgEC(e,t,b,s) {  //e=Element-ClassName [array von objects]
  var a=arguments,l=a.length,i,j,x,r=eval('/\\b'+e+'\\b/'),c=[];
  if(l==1) {s=b=0,t='*'}
  else switch(typeof a[1]) {case'number':s=t,b=0,t='*';break;  case'object':s=b,b=t,t='*'}
