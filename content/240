/* Set/Reset ATI-Variablen "xt..." (werden alle in Strings eingesetzt, aber inkonsistenterweise teils als String, teils als Zahl übergeben...)
*  xtsite = [...][] = ATI-Workaround Enhanced (T.DCL): [0=Default/Standard, 1=Desktop, 2=Tablet, 3=Smartphone]
*/
var xtnv=D, xt_form='', xtsd='https://logs1204', xtLogDom='.xiti.com/hit.xiti', xtsite=[463005, 463005, 550391, 550370][T.DCL||0], xtn2='', xtpage='', xtdi='', xtparam='', xt_multc='', xt_an='', xt_ac='', xtati='', xtergo='0';
if(T.PRE) {xtsd='https://stats'; xtLogDom='.toi.de/prev.xiti'}  //fuer CMS-Vorschau
  
/* ATI-Tracking (Klick- & Teasertracking) */
var TSob = {
  enableTeasers: 1,
  enableClicks: 1,
  numTIs: 80,
  tsrs: {},
  rxOF: /(A\("|A\(')(.+)("\)|'\))/,
  rxCLS: /\$/,
  rxClean: /[^a-z0-9 äöüß_.-]/gi,
  rxBlank: /\s+/g,
  rxUrl: /^(http:\/\/logc[0-9]+.xiti.com\/(go|gopc).url|http:\/\/count.shopping.t-online.de\/RE)/,
  rxBracket: /[\[\]]|%5b|%5d/gi,
  rxMM_: /\bTmm/,          //Teaser-Zaehlung Basis
  rxMM:  /\bTmm\b/,        //Teaser zaehlen
  rxMME: /Tmme-?(\w*)\b/,  //Einzelelement (im Teaser) zaehlen
  rxMMB: /Tmmb-?(\w*)\b/,  //Bereich (im Teaser) zaehlen
  rxMMNO: /\bTmmno\b/,     //Teaser- oder Klick-Element NICHT zaehlen
  txMM:  '.Tmm',           //jQ-"Klasse" fuer Teaser-Zaehlung
  txMMT: '.Tmmt',          //jQ-"Klasse" fuer Statistik-BestimmungsText in Einzelelement oder Bereich
  
  //Suche alle sichtbaren Teaser 
  gti: function() {
    //Vorbereitung fuer Messung von nur sichtbaren Teasern; 
    //( nachtraegliches Filtern ist schneller als Direktselektor ".Tmm:visible"(!) )
  //  var b = [], o, ts = this, s = '', a = T$(ts.txMM).filter(':visible');  //a) nur sichtbare
/*      var b = [], o, ts = this, s = '', a = T$(ts.txMM);  //b) alle
    if(ts.enableTeasers && TisjQ) a.each(function() {
      o = ts.pt(this);
      if(o.id) {
        s = 'PID-[' + o.id + ']-[' + (o.mp || '') + ']';
        if(!ts.tsrs[s]) { b.push(s); ts.tsrs[s] = 1; }
      }
    });
    return b;
*/
  },
  
  //es wurden moeglicherweise neue Teaser eingeblendet => pruefen und TIs senden
  reti: function() {
/*
    //nochmal alle sichtbaren Teaser parsen und schauen welche wir noch nicht gesendet haben
    var a = this.gti();
    //neuen TI-Hit senden
    if(a.length > 0) (new Image).src = xtsd + xtLogDom + '?s=' + xtsite + '&ati=' + a.join(',');
*/
  },
  
  //parst einen Teaser-DIV nach id und allen weiteren Attributen und liefert diese als Objekt
  pt: function(e) {
/*
    var i, a, b, d, o = { id: (e.id || 'T-0').substr(2) }, j = T$(e), of = j.attr('onfocus'), dm = j.attr('data-mm');
    //Parameter entweder aus onfocus, data-mm, oder 'nichts'
    if(of) {
      a = (of + '').match(this.rxOF);
      if(a && a.length >= 3) d = a[2];
    }
    d = d || dm || '';
    //Parameter zerlegen und hinzufuegen
    a = d.split(';');
    for(i=0; i<a.length; i++) { b = a[i].split('='); if (b.length >= 2) o[b[0]] = b[1]; };
    return o;
*/
  },
  
  //spezielle Serialisierung Objekt {p1:v1,p2:v2,...} => String {%22p1%22%3A%22v1%22,%22p2%22:%%3A22v2%22,...} (= {"p1":"v1","p2":"v2",...})
  // für Uebertragung z.B. von xtcustom
  obj2str: function(obj) {
/*
    var p, a=[];
    for(p in obj) a.push(encodeURIComponent('"'+p+'":' + JSON.stringify(obj[p])));
    return '{'+String(a)+'}'
*/
  },
  
  //checkClick: Pruefung Mausklick (rechte Maustaste: event.which = 3; relevant bei Firefox!); ev=Event, el=Element
  //Bedienung:
  //  rechte Maustaste: return null
  //    = nichts tun, kein Link, kein ATI-Pixel anfordern;
  //  SHIFT + CTRL/STRG + rechte Maustaste: return {}
  //    = ATI-Pixel mit Null-Objekt {} anfordern (Korrektur auf altes T-Verhalten).
  //  URL+'?ATI=ati': return el
  //    = el-Durchleitung, keine Fkt.wirkung => ATI-Pixel mit Element el anfordern (Original-ATI-Verhalten ohne Korrektur);
  //Kontrolle Rueckgabewert: TSob.chClck_R
  chClck: function (ev,el) {
/*    if((ev||{}).which==3)  //if rechte Maustaste; (ev||{}) gegen JS-Fehler bei IE<8
      if(ev.shiftKey && ev.ctrlKey) el={};  //if r.M. & SHIFT + CTRL/STRG
      else el=null;  //r.M. allein
    else if(L.search.indexOf('ATI=ati')==-1) el={};  //andere Maustasten, vorlaeufig: if not URL+'?ATI=ati'
    return this.chClck_R = el;
*/
  },
  
  /* Hier waren urspruenglich die ATI-ID- u. -Class-Listen (TSob.idtab, .cltab, .idnot, .clnot) eingebettet
  *   (CMS-Include (rl13-)include-Statistik-ATI-ID-und-Class-Listen-js, ID 70588628);
  *  der Listen-Aufruf erfolgt nun per Objekt TSob_tabs... in ID 77215584 separat NACH dieser Datei (rl13-)include-Statistik-ATI-js hier!
  *  Grund: Zentralisierung u. Entkopplung.
  */
  
  //neues Klicktracking ATI; lt (linktyp: 0=bild/imagemap, 1=headline, 2=mehrlink)
  //Kontrolle Rueckgabewert: TSob.clck_R
  clck: function (ev) {
/*    if(!TstatOK) return;
    var a, i, id, p, o={}, ts=TSob, el=ev.target, el0=el, di=el, ep=el.parentNode, x=el.tagName.toUpperCase(), lt= (x=='IMG' || x=='AREA' || x=='PATH') ? 0 : (x=='H5' || (ep && ep.tagName=='H5') ? 1 : 2), txt, cl, clk, svg=null, mmb, ret=0;
  
    //SVG-Umgebung ermitteln (default:HTML)
    // [tagName] = HTML:uppercase (z.B. DIV - A - IMG oder DIV - MAP - AREA) / SVG:lowercase (z.B. DIV - svg - a - path)
    // [className] = HTML:string / SVG:object SVGAnimatedString={baseVal:string,animVal:string}
    // [href] = HTML:string / SVG:object SVGAnimatedString={baseVal:string,animVal:string}
    x = el;  while(x) if(x.tagName!='svg') x = x.parentNode; else {svg = x; break}
  
    //Dieser Teil ist nur Klicktracking. Ermittelt anhand ID/Klasse des geklickten Elements bzw. seiner Elternelemente, welcher Klick ausgefuehrt wurde
    // (= "allgemeines Klicktracking" / "Klicktracking für NICHT-Teaser"; = Standard-ATI-Verfahren mit Klick-Baum)
    if(ts.enableClicks) {
      x = T$(el);
      mmb = x.attr('data-mmt');  //'data-mmt'-Inhalt am geklickten Element
      txt = mmb || x.text() || x.attr('alt') || T$(ep).text() || '';  //UebertragungsText
      while(di && di.tagName != 'A' && di.tagName != 'AREA' && di.tagName != 'DIV') di = di.parentNode;
      while(di && di!=D) {
        //'data-mmt'-Inhalt an uebergeordneten Elementen pruefen
        if(!mmb && (x=T$(di).attr('data-mmt'))) txt = mmb = x;  //ggf. UebertragungsText
        //ID pruefen
        id = di.id;
        if(id && ts.idnot.indexOf(id)!=-1) break;  //Klicktracking-while() abbrechen
        //Class pruefen (muss beruecksichtigen, dass es mehr als eine Klasse geben kann)
        cl = di.className;
        if(cl) {
          if(cl.match(ts.rxMMNO)) return;  //(Klick-)Tracking ganz abbrechen
          a = di.classList || cl.replace(ts.rxBlank,' ').split(' ');
          for(i=a.length; i; ) if((x=a[--i]) && ts.clnot.indexOf(x)!=-1) {i=-1; break}  if(i==-1) break;  //Klicktracking-while() abbrechen
        }
        //Einzeleintrag im Attribut <tag data-mmtab="abc::def::ghi" ...> pruefen
        if(x=di.getAttribute('data-mmtab')) clk = x;
        //ID suchen
        if(id && !clk) {x = ts.idtab[id]; if(x) clk = x}
        //Class suchen (muss beruecksichtigen, dass es mehr als eine Klasse geben kann)
        if(cl && !clk) {
          for(i=a.length; i; ) {x = ts.cltab[a[--i]]; if(x) {clk = x; break} }
        }
        //Falls was vorhanden => Formatieren, absenden und Ende
        if(clk) {
          //Fix fuer doppelte Navinamen
          i = txt.length >> 1;
          if(txt.substr(0,i)==txt.substr(i)) txt = txt.substr(i);
          //Rest fixen & senden
          txt = T$.trim(txt.replace(ts.rxClean,' ').replace(ts.rxBlank,' '));
          if(txt.length > 40) txt = 'unbekannt';  //Fix fuer spezielle Browser, die komplette Navi als Eintrag erzeugen
          clk = clk.replace(ts.rxCLS, '::' + txt) + Txt_click_(clk);
          //if(W.xt_click) xt_click({}, 'X', xtn2, clk, 'A');  //FIX 31.10.2011 xt_click mit einem nullobj versorgen, sonst macht es einen loc.ref=target(!)
          //FIX 09.09.2014 rechte Maustaste FF: xt_click mit el (oder einem nullobj {}) versorgen oder abbrechen (sonst macht FF einen loc.ref=target(!))
          if(W.xt_click&&(x=ts.chClck(ev,el))) ret = xt_click(x, 'X', xtn2, clk, 'A');
          break;
        }
        //Weiter suchen
        di = di.parentNode;
      }  //Ende "while(di)..."
    }  //Ende "if(ts.enableClicks)..."
  
    //Dieser Teil ist nur Teasertracking (= "Teaser-Klicktracking")
    if(ts.enableTeasers && !clk) {
      try {
        //Einzelelement und Bereich erfassen (gilt nur in HTML)
        if(svg) a = mmb = 0;  //SVG
        else {  //HTML
          x = el; while(x && !x.className.match(ts.rxMM_)) x = x.parentNode;  mmb = (x && x.className.match(ts.rxMMB)) ? x : null;
          a = el.className.match(ts.rxMME) || mmb;
        }
        //ausloesenden Link (el=A/AREA) ermitteln (falls in HTML nicht ausgenommen)
        if(!a) while(el && (x=el.tagName.toUpperCase()) != 'A' && x != 'AREA') el = el.parentNode;
        if(el&&(svg||!el.className.match(ts.rxMMNO))) {  //Teasertracking ggf. hier nicht ausführen, d.h. abbrechen
          //Teaser-Container (DIV oder inzwischen auch anderes Element) suchen
          di = (svg||el).parentNode;  //ab 1 Ebene darueber (u.a. wg. Ausschluss SVG-Abfrage className)
          while(di && !di.className.match(ts.rxMM)) di = di.parentNode;
          //Daten aus di extrahieren und ggfs um weitere aus el erweitern (ohne id)
          if(di) {
            o = ts.pt(di);
            p = ts.pt(el);  delete p.id;
            T$.extend(o, p);
            o.hr = (x=el.href) ? x.baseVal || x : '';  o.lt = lt;  o.mp = o.mp || '';  o.ec = o.ec || '';
            //Statistik-BestimmungsText (aus 1.) Einzelbereich oder 2.) Teaser)
            di = T$(di);  x = mmb ? T$(mmb) : di;
            //UebertragungsText
            txt = x.attr('data-mmt') || T$(x.find(ts.txMMT)[0]).text() || T$(di.find('H5')[0]).text() || T$(di.find('H6')[0]).text() || 
                  ((x=T$(el).text().replace(ts.rxBlank,' ')) != ' ' ? x : '') || T$(el0).attr('alt') || '';
            o.tt = T$.trim(txt.replace(ts.rxBlank,' '));
            if(ts.rxUrl.test(o.hr)) {
              x = o.hr.split('&');
              for(i=0; i<x.length; i++) if(x[i].toLowerCase().indexOf('url=') == 0) {o.hr = unescape(x[i].substr(4)); break}
            }
            o.hr = o.hr.replace(ts.rxBracket, '');
          }
        }
      } catch(ex){ }
      //teaser senden
      if(o.id) {
        clk = 1;
        x = [o.id, o.hr, o.tt, o.ec, o.mp, o.lt];
        for(i=0; i<x.length; i++) x[i] = encodeURIComponent(x[i]);
        p = 'PID-[' + x.join(']-[') + ']';
        //Linklisten: Textlinks-Liste (1), ImageMap (2), SVG-Map a+path (3);  Parameter: [P.1:visibility]=[P.2:display]=leer, [P.3:Text]
        x = el.parentNode.tagName=='LI' ? 1 : el.tagName=='AREA' ? 2 : (TgEfC(el)||{}).tagName=='path' ? 3 : 0;
        a = x ? '-[]-[]-['+encodeURIComponent(T$.trim(x==1||x==3 ? T$(el).text() : el.alt))+']' : '';
        //Einbau von design_id (siehe auch bei TSATI(), Seitenende)
        if(W.xtcustom) a += '&stc=' + ts.obj2str({design_id:(xtcustom.design_id||0)});
        //if(W.xt_ad) xt_ad(p+a);     //laut ATI, 16.07.14, fehlerhaft
        //if(W.xt_adc) xt_adc({},p+a);  //laut ATI, 16.07.14, korrekt (aber nur mit nullobj-FIX fuer FF wie oben bei xt_click: {} statt el !!!)
        if(W.xt_adc&&(x=ts.chClck(ev,el))) ret = xt_adc(x,p+a);  //FIX 09.09.2014 (chClck() analog zu Klicktracking, s. oben)
      }
    }  //Ende "if(ts.enableTeasers)..."
  
    ts.clck_R = ret;  //Kontrolle ATI-Funktions-Rueckgabewert
    //Test: if URL+'?ATI=ati': Klickhandler-Rueckgabewert = Original-ATI-Funktions-Rueckgabewert
    if(L.search.indexOf('ATI=ati')!=-1) return ret;
  */
  }  //Ende "clck: function (ev)..."
}; //Ende "var TSob =..."
