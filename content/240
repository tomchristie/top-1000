/*
* Lazy Load - jQuery plugin for lazy loading images
*
* Copyright (c) 2007-2013 Mika Tuupola 
*
* Licensed under the MIT license: 
*   http://www.opensource.org/licenses/mit-license.php 
* 
* Project home: 
*   http://www.appelsiini.net/projects/lazyload 
* 
* Version:  1.8.4 
* 
*/
(function(a,b,c,d){var e=a(b);a.fn.lazyload=function(c){function i(){var b=0;f.each(function(){var c=a(this);if(h.skip_invisible&&!c.is(":visible"))return;if(!a.abovethetop(this,h)&&!a.leftofbegin(this,h))if(!a.belowthefold(this,h)&&!a.rightoffold(this,h))c.trigger("appear"),b=0;else if(++b>h.failure_limit)return!1})}var f=this,g,h={threshold:0,failure_limit:0,event:"scroll",effect:"show",container:b,data_attribute:"original",skip_invisible:!0,appear:null,load:null};return c&&(d!==c.failurelimit&&(c.failure_limit=c.failurelimit,delete c.failurelimit),d!==c.effectspeed&&(c.effect_speed=c.effectspeed,delete c.effectspeed),a.extend(h,c)),g=h.container===d||h.container===b?e:a(h.container),0===h.event.indexOf("scroll")&&g.bind(h.event,function(a){return i()}),this.each(function(){var b=this,c=a(b);b.loaded=!1,c.one("appear",function(){if(!this.loaded){if(h.appear){var d=f.length;h.appear.call(b,d,h)}a("<img />").bind("load",function(){c.hide().attr("src",c.data(h.data_attribute))[h.effect](h.effect_speed),b.loaded=!0;var d=a.grep(f,function(a){return!a.loaded});f=a(d);if(h.load){var e=f.length;h.load.call(b,e,h)}}).attr("src",c.data(h.data_attribute))}}),0!==h.event.indexOf("scroll")&&c.bind(h.event,function(a){b.loaded||c.trigger("appear")})}),e.bind("resize",function(a){i()}),/iphone|ipod|ipad.*os 5/gi.test(navigator.appVersion)&&e.bind("pageshow",function(b){b.originalEvent.persisted&&f.each(function(){a(this).trigger("appear")})}),a(b).load(function(){i()}),this},a.belowthefold=function(c,f){var g;return f.container===d||f.container===b?g=e.height()+e.scrollTop():g=a(f.container).offset().top+a(f.container).height(),g<=a(c).offset().top-f.threshold},a.rightoffold=function(c,f){var g;return f.container===d||f.container===b?g=e.width()+e.scrollLeft():g=a(f.container).offset().left+a(f.container).width(),g<=a(c).offset().left-f.threshold},a.abovethetop=function(c,f){var g;return f.container===d||f.container===b?g=e.scrollTop():g=a(f.container).offset().top,g>=a(c).offset().top+f.threshold+a(c).height()},a.leftofbegin=function(c,f){var g;return f.container===d||f.container===b?g=e.scrollLeft():g=a(f.container).offset().left,g>=a(c).offset().left+f.threshold+a(c).width()},a.inviewport=function(b,c){return!a.rightoffold(b,c)&&!a.leftofbegin(b,c)&&!a.belowthefold(b,c)&&!a.abovethetop(b,c)},a.extend(a.expr[":"],{"below-the-fold":function(b){return a.belowthefold(b,{threshold:0})},"above-the-top":function(b){return!a.belowthefold(b,{threshold:0})},"right-of-screen":function(b){return a.rightoffold(b,{threshold:0})},"left-of-screen":function(b){return!a.rightoffold(b,{threshold:0})},"in-viewport":function(b){return a.inviewport(b,{threshold:0})},"above-the-fold":function(b){return!a.belowthefold(b,{threshold:0})},"right-of-fold":function(b){return a.rightoffold(b,{threshold:0})},"left-of-fold":function(b){return!a.rightoffold(b,{threshold:0})}})})(jQuery,window,document)

/*
 * Lazyloading JS
 */
jsLazyLoadInit = function() {
  T.JSLL = {
     jslist : [],
     observer : new IntersectionObserver( function(e,o) {
                    T.JSLL.intersectCB(e,o); 
                  }, {
                    rootMargin: '100px'
                  }),
     isInitialized:false,
     init : function(options) {
       if ( T.JSLL.isInitialized ) return;
       T.JSLL.jslist.forEach( function(o,i) {
         T.JSLL.tObserve( o );
       });
       T.JSLL.isInitialized=true;
     },
     tObserve : function( o ) {
       var trg=document.querySelector(o.sel);
       trg.setAttribute('data-jsllid',o.num)
       T.JSLL.observer.observe(trg);
     },
     tUnobserve : function( o ) {
       var trg=document.querySelector(o.sel);
       trg.setAttribute('data-jsll-loaded',true)
       T.JSLL.observer.unobserve(trg);
     },
     addJS : function( selector, scriptsrc, callback ) {
       var jsline={sel:selector,src:scriptsrc,cb:callback,num:this.jslist.length};
       T.JSLL.jslist.push(jsline);
       if ( T.JSLL.isInitalized ) {
         T.JSLL.tObserve(jsline);
       }
     },
     intersectCB : function( e, obs ) {
       for ( i=0; i<e.length; i++ ) {
        var o= e[i];
        if ( o.isIntersecting ) {
          var ind = parseInt(o.target.dataset.jsllid);
          var llentry = T.JSLL.jslist[ind];
          var script = document.createElement("script")
          script.type = "text/javascript";
          script.onload = function() {
            if ( typeof llentry.cb === 'function' ) llentry.cb();
          };
          script.src = llentry.src;
          document.getElementsByTagName("head")[0].appendChild(script);
          T.JSLL.tUnobserve(llentry)
        }
       }
     }
   };
};


domLazyLoadInit = function() {
 /* T-DOMLL 86357186 */



/*
 * Lazyloading DOM nodes
 *
 * to make a DOM node lazyloadable you need at least two things:
 *  - an anchor in the page, can be some container, just need to be catchable by a css-selector
 *  - a HTML document url to lazyload, should return pure HTML code
 *  - optional: a callback function which is executed, when the js is ready
 * Usage: you can add as much DOM nodes to lazyload as you want using the addNode function, 
 *   then call the init function when the containers you used are present in the DOM, 
 *   usually DOMContentLoaded
 *   from the time of initialization the added containers will be observed and loaded, 
 *   if/when visible
 * Trivia:
 *   - you can add DOM after initialization, the observation will start immediately
 *   - you can call init more than one time ( but you needn't ), it will not harm
 * Functions:
 *   - init() : has no options, starts observing container ID's
 *   - addNode( CSS-SELECTOR, URL, [CALLBACK] ) : add a DOM Node to be lazyloaded
 *      + CSS-SELECTOR : a valid css selector that has to deliver a DOM node when init() is called
 *      + URL : an url which when called delivers valid DOM node code which is inserted 
 *         into the container
 *      + CALLBACK : an optional function which is executed after the DOM node is loaded
 */

  T.DOMLL = {
     nodelist : [],
     observer : new IntersectionObserver( function(e,o) {
                    T.DOMLL.intersectCB(e,o);
                  }, {
                    rootMargin: '200px'
                  }),
     isInitialized:false,
     init : function(options) {
       if ( T.DOMLL.isInitialized ) return;
       T.DOMLL.nodelist.forEach( function(o,i) {
         T.DOMLL.tObserve( o );
       });
       T.DOMLL.isInitialized=true;
     },
     tObserve : function( o ) {
       var trg=document.querySelector(o.sel);
       trg.setAttribute('data-domllid',o.num)
       T.DOMLL.observer.observe(trg);
     },
     tUnobserve : function( o ) {
       var trg=document.querySelector(o.sel);
       trg.setAttribute('data-domll-loaded',true)
       T.DOMLL.observer.unobserve(trg);
     },
     addNode : function( selector, scriptsrc, callback ) {
       var jsline={sel:selector,src:scriptsrc,cb:callback,num:T.DOMLL.nodelist.length};
       T.DOMLL.nodelist.push(jsline);
       if ( T.DOMLL.isInitalized ) {
         T.DOMLL.tObserve(jsline);
       }
     },
     intersectCB : function( e, obs ) {
       for ( i=0; i<e.length; i++ ) {
        var o= e[i];
        if ( o.isIntersecting ) {
          var ind = parseInt(o.target.dataset.domllid);
          var llentry = T.DOMLL.nodelist[ind];
          var xhr = new XMLHttpRequest();
          var con=document.querySelector(llentry.sel);

          xhr.onreadystatechange = function (e) { 
            if (xhr.readyState == 4 && xhr.status == 200) {
              con.insertAdjacentHTML( 'beforeend', xhr.responseText );
              if ( typeof llentry.cb === 'function' ) llentry.cb();
            }
          }
          xhr.open("GET", llentry.src, true);
          xhr.setRequestHeader('Content-type', 'text/html');
          xhr.send();

          T.DOMLL.tUnobserve(llentry)
        }
       }
     }
   };





}

if ( typeof IntersectionObserver == 'undefined' ) {
  // IE does not know intersection observer, so we load the intersectionObserverPolyfill
  
  var script = document.createElement("script");
  script.type = "text/javascript";
  script.onload = function() {
    jsLazyLoadInit();
    domLazyLoadInit();
  };
  script.src = 'https://stats.t-online.de/intersectionobserverpolyfill-js/id_86294288/tid_js/index_ts386294288.js';
  document.getElementsByTagName("head")[0].appendChild(script);

}
else {
  jsLazyLoadInit();
  domLazyLoadInit();
}

document.addEventListener("DOMContentLoaded", function () {
  if ( typeof T.JSLL === 'undefined' ) jsLazyLoadInit();
  T.JSLL.init();
  T.DOMLL.init();
});


/* rl13-include-funcs-js 64568282.31 */

/* funcs-basics-js 70252046.7 */
var W=window, D=document, E=document.documentElement, N=navigator, L=location, R=RegExp, U='undefined', B, H=TgET('head')[0], C, x, i;  /* B=D.body im <body>: TfA() */
var TisjQ=W.jQuery?1:0, T$=TisjQ?jQuery.noConflict():function(){}, T$W=T$(window)||{}, T$D=T$(document)||{}, T$E=T$(E)||{}, T$H=T$(H)||{}, T$B;  /* T$B=T$(B): TfA() */

ADRnd=Math.random().toString().substr(2,9);


if(!W.T) {var T={T0:'',EGO:'',SN:0,ID:0,TI:0,SLIN:0,PN:'',PORT:0,ART:0,PRE:0,KG:'',R0:'',PA:'',TPD:'',TS:0}, TMS={IVW:''}}  //ggf. Reset
T.HOME = (T.ID==46||T.ID==82441520)?1:0;  //Homepage JA/NEIN
//if(T.HOME&&L.search.indexOf('?TEST=T.DCL')!=-1) alert(T.DCL);

/* Client 64568414.25 */
  
/* Kommentare & Beschreibung siehe Reiter "Sonstige" */
  
/* Client C (UserAgent, Browser, Betriebssystem/OS, Touchscreen, Mobilgeraet/Smartphone, Flash, Cookie, Viewport, aufArbeit, ...) */
if(!C) {var CcC = function(ua) {
  var _ua=ua||N.userAgent.toLowerCase(), _v=0.0, _vv=0, _vs='0.0', _n=0, _x, _b='';  /* UserAgent: uebergebener, sonst eigener; ... */
  
  /* Hilfsfunktion fU(s): String s im UserAgent enthalten? Return true/false = J/N */
  function fU(s) {return _ua.indexOf(s)!=-1}
  /* Hilfsfunktion fUL(a,b,c): einer der Strings a,b,c,... im UserAgent enthalten? (Durchlauf rueckwaerts!) Return 1/0 = J/N */
  function fUL(a,b,c) {var i=arguments.length; while(i--) if(_ua.indexOf(arguments[i])!=-1) return 1; return 0}
  /* Hilfsfunktion fV(s,n): Auslesen Browser-Version & -Name in globale Variablen _v, _vs & _n; s = Such-String (Teil), n = Name */
  function fV(s,n) {var v=_ua.match(RegExp(s+'[ \\/]?(\\d*\\.?[0-9a-z]*)(\\.[0-9a-z]*)?(\\.[0-9a-z]*)?')); if(v) _v=parseFloat(_vs=(v[1]||0)+(v[2]||'')+(v[3]||'')); _n=n}
  /* Hilfsfunktion fVV(o,p): Zusammenfassung (Browser-)Versionen am Objekt o (C, C.TB), ggf. fuer Properties p; Params = globale Var. _v & _vs ==> Aufruf nach fV() */
  function fVV(o,p) {var p=p||'bVer';  if(_v==(_vv=parseInt(_v))&&_vv) _v += '.0';  o[p]=_v, o[p+'M']=_vv, o[p+'S']=_vs}
  /* Hilfsfunktion fVN(o): Zusammenfassung Browser-Name(n) am Objekt o (C, C.TB); Params = globale Var. _v, _vv & _n ==> Aufruf nach fV() */
  function fVN(o) {o.bName=_n, o.bBrows= (!_n||!_v)? 0 : _n+_vv}
  /* Hilfsfunktion fV2(s): Ermittlung spezielle OS-Version (IE, Android); Return OS-Version */
  function fV2(s) {return (s=_ua.match(RegExp('[ \\(]'+s+' ?(\\d*(?:[_\\.]\\d*)*)')))&&s[1]? s[1].replace(/_/g,'.') : 0}  /* (fuer Editor: ")" = Klammer-ZU wg. Klammer-AUF in String) */
  /* Hilfsfunktion fVC(v,n,x): Vergleich aktuelle Version v mit uebergebener Zahl n fuer x=1: >=, x=-1: <=, sonst n/0; Return: Version v (bei true) / 0 (bei false) */
  function fVC(v,n,x) {var V=parseFloat(v); return x && (x>0?V>=n:V<=n) || V==(n||V)? V : 0};
  
  var C = {  /* Client C (Definition & Reset; zunaechst lokal) */
    ver: '1.221',  /* Version */
    ua: _ua,  /* UserAgent */
    bVer:0, bVerM:0, bVerS:0, bName:0, bBrows:0,  /* Browser-Version & -Name */
    isIE:0, isEDGE:0, isFF:0, isCH:0, isSF:0, isOP:0, isWK:0, isNN:0, isMZ:0, isTB:0, TB:null,  /* Browser-Typ */
    os:0, osVer:0, osVerM:0, osVerS:0, isIos:0, isWin:0, isWinPhone:0, isAndr:0,  /* Betriebssystem */
    vpW:-1, vpH:-1, vp:{w0:-1, h0:-1, SB:17},  /* Viewport (Resetwerte = -1 (keine negativen Abmessungen moeglich); Scrollbalken =! 17px breit! (FF,CH,IE>=8(IE8+4pxRahmen),...) ) */
    scrW:screen.width, scrH:screen.height, scrPR:W.devicePixelRatio||1,  /* Screen: Breite, Hoehe, Pixel-Faktor */
    swf:0, swfM:0, swfS:0, swf3:0,  /* Flash-Version */
    _:0
  };
  
  /* Browser: MS Internet Explorer IE (2), MS Edge EDGE (3), Firefox FF (4), Chrome CH (5), Safari SF (6), Opera OP (1), Webkit/Khtml WK (7) [, Netscape NN (8), Mozilla/Gecko MZ (9), Netsc./Moz. NN (10=8)], Telekom-Browser [=FF] (letzter/11) */
  if(fU('opera')||fU('opr')) {if(fU('version')||fU('opr')) fV('version|opr',_b='OP'); else {_ua=(_x=_ua).replace('opera m','OperA m'); fV('opera',_b='OP'); _ua=_x} C.isOP=1}
  else if(fU('msie')||fU('trident')) {fV('(?:msie|trident.* rv:|trident.* iemobile)',_b='IE'); C.isIE=1}  /* IE bis einschl. IE11 */
  else if(fU('edge')) {fV('edge',_b='EDGE'); C.isEDGE=1}  /* MS-Edge, nach IE11 ("IE12") */
  else if(fU('firefox')) {fV('firefox',_b='FF'); C.isFF=1; if(fU('dtb')) C.TB={bOrg:_b}}
  else if(fU('chrome')) {fV('chrome',_b='CH'); C.isCH=1}
  else if(fU('safari')) {if(fU('version')) fV('version',_b='SF'); else fV('safari',_b='SF'); if(_v>=85){_v=parseFloat(_vs='0.'+_vs)}; C.isSF=1}
  else if(fU('khtml')) {fV('khtml','WK'); C.isWK=1}
  else if(fU('netscape')) {fV('netscape\\d*','NN'); C.isNN=1} else if(fU('gecko')) {fV('rv:','MZ'); C.isMZ=1} else if(fU('mozilla')) {fV('mozilla','NN'); C.isNN=1}  /*???*/
  fVV(C); fVN(C);  /* Browser-Version & -Name */
  if(C.TB) {C.isTB=1; fV('dtb','TB'); fVV(C.TB); fVN(C.TB)}  /* Telekom-Browser, Version & Name */
  /* <html>-Tag: class="Tisjs TisFF/CH/..." (eigentlich nur im Portal T.PORT=1, nicht im Vertical T.PORT=0 (dort <body>-Tag))!!! */
  _x = TgET('html')[0]; _x.className = 'Tisjs' + ((_x=_x.className.replace(/Tnojs */,'')) ? ' '+_x : '') + (_b ? ' Tis'+_b : '')
  /* Browser-Version dynamisch (allgemein); siehe auch C.isIEVer() */
  C.isBVer = function(v,x) {return fVC(C.bVer,v,x)};
  /* IE-Version dynamisch: C.isIEVer() = VersionsNr. fuer IE, sonst = 0; C.isIEVer(9) = 9 f. IE9; C.isIEVer(9,1) = 9/10/11/... f. IE>=9; C.isIEVer(11,-1) = 11/10/9/... f. IE<=10 */
  C.isIEVer = function(v,x) {return C.isIE && fVC(C.bVer,v,x)};
  
  /* Touchscreen, Mobilgeraet/Smartphone */
  C.isTouch = ('ontouchstart' in W || N.MaxTouchPoints || N.msMaxTouchPoints || fUL('touch','windows phone','(ipod','(ipad','(iphone'))? 1 : 0;  /* Abfrage ipod usw. wie einzeln; ")))" wg. Klammern-AUF in Strings */
  if((C.isIpad = fU('(ipad')? 1 : 0) && C.scrW>=1024) {C.isIpadPro = 1; C.isIpad = 0} else C.isIpadPro = 0; C.isIphone = fU('(iphone')? 1 : 0;  C.isIpod = fU('(ipod')? 1 : 0;  /* entweder iPad oder iPad Pro!; ")))" wg. Klammern-AUF in Strings */
  C.isBberry = fU('blackberry')? 1 : 0;
  
  /* Betriebssystem/OS (_vs => C.osVer) */
  if(fU('win')) {  /* Windows */
    if(fU('windows phone')) {  /* oder: if(fU('mobile'))... (wie bei mac) */
      C.isWinPhone = 1;  C.os = 'WPH';  _vs = fV2('windows phone(?: os)?');
    }
    else {
      C.isWin = 1;  C.os = 'WIN';
      _vs = (_x=_ua.replace('windows xp','winnt5.1').match(/win(?:dows )?nt ?(\d*\.?\d*)/))? _x[1] : 0;  /* Windows-NT-Version */
    }
  }
  else if(fU('mac ')) {  /* Mac */
    if(fU('like mac os')||fU('mobile')) {C.os = 'IOS'; C.isIos = 1}  /* "like mac os..."/"mobile": iOS */
    else C.os = 'MAC';
    _vs = fV2('os(?: x)?');
  }
  else if(fU('android')) {  /* Android */
    C.isAndr = 1; C.os = 'AND'; _vs = fV2('android-?');
  }
  else if(_ua.replace('firefox','').indexOf('x')!=-1) C.os = 'UNX';  /* Unix, Linux, ... */
  _v=parseFloat(_vs); fVV(C,'osVer');  /* OS-Version, Major-Version, Version als String */
  /* Betriebssystem-Versionen dynamisch (analog C.isIEVer(v,x)): Allgemein C.isOsVer(f,x) / C.isAndr_(5) = 5 f. Android-5 / C.isIos_(9,-1) = 9 f. IOS-9/8/7/... */
  C.isOsVer = function(v,x) {return fVC(C.osVer,v,x)};
  C.isAndrVer = function(v,x) {return C.isAndr && fVC(C.osVer,v,x)};
  C.isIosVer = function(v,x) {return C.isIos && fVC(C.osVer,v,x)};
  /* Windows-Name (aus NT-Version) ["'5.0':..." sonst "5:..."!] */
  C.osWNTName = function() {var v=C.osVer, n; return C.isWin ? v&&v<=4 ? 'Windows NT' : (n={'5.0':'2000', '5.1':'XP', '5.2':'XP x64', '6.0':'Vista', '6.1':'7', '6.2':'8', '6.3':'8.1', '6.4':'10 Preview', '10.0':'10'}[v]) ? 'Windows '+n : 0 : 0};
  /* 64bit-OS/CPU; Abfrage: wow64 / win64 / x64; / x86_64 / amd64 [/ ia64 / ppc64 / sparc64 / irix64 / x86-64 / x64_64 / ...] */
  C.isBit64 = function() {return (N.platform.toLowerCase().indexOf('win64')!=-1 || _ua.match(/\w64\b/) && fUL('amd64','x86_64','x64;','win64','wow64')) ? 1 : 0};
  
  /* Flash-Version, Cookie, Java */
  _vs = 0, _b = function(v) {return v.match(/\d+/g).join('.')};  /* Hilfsfunktion: bringt Original-Versions-String in Form '12.3.4' bzw. '12.3.4.5' */
  if(((_x=N.plugins)&&(_x=_x['Shockwave Flash'])) || ((_x=N.mimeTypes)&&(_x=_x['application/x-shockwave-flash'])&&(_x=_x.enabledPlugin))) 
    _vs = _x.version || _b(_x.description);  /* alle Browser inkl. IE>=11 per Plugin oder MimeType */
  else if(W.ActiveXObject) try{_x=new ActiveXObject('ShockwaveFlash.ShockwaveFlash').GetVariable("$version"); _vs = _b(_x)} catch(e){}  /* IE<=10 (try gegen JS-ERROR ohne Flash) */
  _v = parseFloat(_vs); fVV(C,'swf');  /* Flash-Version: C.swfS = komplett '12.3.4.5', C.swf = normal 12.3, C.swfM = major 12 */
  C.swf3 = (function(){var x=C.swfS; if(x) {x=x.split('.'); x.length=3; x=x.join('.')} return x})()  /* C.swf3 = 3-Elemente '12.3.4' */
  C.cook = typeof(_x=N.cookieEnabled)==U? false : _x? 1 : 0;
  C.java = typeof(N.javaEnabled)==U? false : N.javaEnabled()? 1 : 0;
  
  /* Viewport: getWidth&Height; getScrollBars (x=1: Basisbreite & -hoehe neu messen) */
  C.vpgWH = W.innerWidth? function(){C.vpH=W.innerHeight; return C.vpW=W.innerWidth} : function(){C.vpH=E.offsetHeight; return C.vpW=E.offsetWidth};
  C.vpgSB = function(x) {
    var vp=C.vp, r=0; C.vpgWH(); if(vp.w0<0||x) {vp.w0=C.vpW; vp.h0=C.vpH}
    if(vp.w0-E.clientWidth  >= vp.SB) r|=2;  /* vertikaler / senkrechter Scrollbalken (2/Bit1) */
    if(vp.h0-E.clientHeight >= vp.SB) r|=1;  /* horizontaler/waagrechter Scrollbalken (1/Bit0) */
    return r;
  };
  
  /* Initialisierung */
  C.atWork = false;  /* auf Arbeit, im Buero */
  C.iniTmDf = 0;  /* Zeitdifferenz */
  C.ini = function() {  /* Initialisierungsfunktion */
    if(W.T&&T.PORT&&T.R0&&L.protocol!='https:') {  /* nur im Portal + wenn AJAX-URL vorhanden + wenn nicht https */
      T$.get(T.R0+(T.HOME?'?_'+T.T0:''), null, function(d,s,x) {if(x) {
        d=x.getResponseHeader('Date');  C.iniTmDf = d? Date.parse(d)-TgT() : 0;
        s=x.getResponseHeader('Via');  C.atWork = (T.PRE||s&&s.match(/proxy\d*\.t-online\.net/))? 1 : 0;
      }})
    }
  }
  
  /* alle Properties/Eigenschaften und Methoden/Funktionen von C mit ihren Werten als String zurueckgeben; cr = Carriage Return / Zeilenumbruch-Zeichen */
  C.getProps = function(cr) {
    cr=cr||'\n'; var el,elx,str='',p=0,m=0;
    for(el in C) {p++; if(typeof(elx=C[el])=='function') m++; str+='.'+el+':|'+elx+'|'+cr}
    return 'C: Anzahl Properties: '+p+', davon Methoden: '+m+cr + str;
  }
  
  /* Ergebnis zurueckgeben */
  return C;
}
  
C = CcC((W.TuaX||'').toLowerCase());  /* globales C-Client-Objekt */
  
