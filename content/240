  if(typeof consentManager !== 'undefined'){
    function ConsentAdHandler() {
      var adQueue = [];
      var isConsented = false;
      this.register = function(){
        if(isConsented) {
          if(arguments.length == 1 && typeof arguments[0] === 'function') {
            arguments[0]();
          } else {
            IM.GlobalAdTag.register.apply(null, arguments);
          }
        } else {
          adQueue.push(arguments);
        }
        console.log('%c  adQueue', 'background: #D22; color: #222', adQueue);
      };
      this.grantedRender = function(){
        isConsented = true;
        while(ad = adQueue.shift()){
          if(ad.length == 1 && typeof ad[0] === 'function') {
            ad[0]();
          } else {
            IM.GlobalAdTag.register.apply(null, ad);
          }
        }
      };
    };
    var consentAdHandler = new ConsentAdHandler();
    var IM = {
      GlobalAdTag: {}
    };
    IM.GlobalAdTag.register = consentAdHandler.register;
