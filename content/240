 (function() {
 var sibisinitialized = false;
 var rsfs = document.getElementById('regioSchaufensterSIB');
 var ssrc = '/-/id_86566654/tid_js/index.js';
 function theloader() {
 console.log('MrRu regio: theloader');

 if(sibisinitialized) return;
 sibisinitialized = true;

 var script = document.createElement("script")
 script.type = "text/javascript";
 script.onload = function() {
 var regioSchaufenster = new RegioSchaufensterSIB(rsfs);
 };
 script.src = ssrc;
 document.getElementsByTagName("head")[0].appendChild(script);
 }


 function callback(mutationList, observer) {
 for(var i = 0; i < mutationList.length; i++) {
 mutation = mutationList[i];
 switch (mutation.type) {
 case 'childList':
 observer.disconnect();
 theloader();
 break;
 }
 }
 }


 if(!abp && rsfs.previousElementSibling && rsfs.previousElementSibling.id && rsfs.previousElementSibling.id.indexOf('sa') > -1) {
 var targetNode = rsfs.previousElementSibling;
 var observerOptions = {
 childList: true,
 attributes: true
 };
 var observer = new MutationObserver(callback);
 observer.observe(targetNode, observerOptions);
 } else if ( T.JSLL ) {
 if(sibisinitialized) return;
 sibisinitialized = true;

