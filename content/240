      if(typeof(T) == U) T = {};
      for(i = 0; i < k.length; i++) eval('if(typeof(T.' + k[i] + ')==U)' + 'T.' + k[i] + '=v[i]');
      TSx = T.TS ? T.TS : Math.floor(TD / 1000);
      if(typeof(TMS)==U) TMS = { IVW: ['https://', 'toi.ivwbox.de/cgi-bin/ivw/', 'CP'] };
    } this.d = 1;
  }
};

