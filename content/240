<script>
 function notifySA(containerId) {
 var cbmini = document.getElementById('6800001sa');
 var ref = cbmini.parentNode;


 var observer = new MutationObserver(function(mutations) {
 mutations.forEach(function(mutation) {
 if(mutation.type == "attributes") {
 if(mutation.target.className.indexOf('sdgRemoved') > -1) {
 ref.parentNode.style.display = 'none';
 }
 }
 });
 });
 observer.observe(ref, {
 attributes: true //configure it to listen to attribute changes
 });

 if(T.DCL == 3) {
 ref.style.textAlign = 'center';
 ref.parentNode.classList.add('Tmc6', 'Tmc6--ad');

 if (typeof consentAdHandler != 'undefined') {
 consentAdHandler.register('topmobile3', ref);
 } else {
 IM.GlobalAdTag.register('topmobile3', ref);
 }

