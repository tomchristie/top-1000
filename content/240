        var daxValue = data.performance.byPeriods.Intraday;
        var formattedValue = percentFormatter.format(data.performance.byPeriods.Intraday).replace(/\s/g, "")
        var color = daxValue > 0 ? 'positive' : 'negative';
        var prefix = daxValue > 0 ? '+' : '';
        T$('.TSRDax .value').addClass(color).text(`${prefix}${formattedValue}`).removeClass('loading');
      }).fail(function(){
        T$('.TSRDax .value').remove();
      })
    }
  })
})();
</script>
</div>
<!--noindex:-->
