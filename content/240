var gaLabel = getTestVariation('consent');
  var cpLayerLogin = function (){
    cp('authenticate', { prompt: 'login'});
    dataLayer.push({
      'event': 'gtmEvent',
      'eventCategory': 'consent management',
      'eventAction': 'pur login',
      'eventLabel': gaLabel,
      'nonInteraction': false
    });
  };

  var cpLayerSignup = function (){
    cp('authenticate', { prompt: 'signup' });
    dataLayer.push({
      'event': 'gtmEvent',
      'eventCategory': 'consent management',
      'eventAction': 'pur abonnieren',
      'eventLabel': gaLabel
    });
  };

  var cpLayerMail = function (){
    dataLayer.push({
      'event': 'gtmEvent',
      'eventCategory': 'consent management',
      'eventAction': 'mail-button',
      'eventLabel': gaLabel
    });
    window.location.href="https://login.t-online.de/?returnToUrl=https://email.t-online.de/?wdycf=www.t-online.de";
  };

  var cpLayerGA = function (action) {
    dataLayer.push({
      'event': 'gtmEvent',
      'eventCategory': 'consent management',
      'eventAction': action,
      'eventLabel': gaLabel
    });
    switch(action) {
      case 'datenschutzhinweise-pm':
      case 'datenschutzhinweise':
      case 'datenschutz':
        var href = 'https://www.t-online.de/datenschutz/';
        break;
      case 'impressum':
        var href = 'https://www.t-online.de/impressum/id_12511434/index';
        break;
      case 'faq':
        var href = 'https://pur.t-online.de/';
        break;
      case 'nutzungsbedingungen':
        var href = 'https://www.t-online.de/allgemeine-geschaeftsbedingungen/id_90193178/index';
        break;
    }
    setTimeout(function() {
      window.location.href = href;
    }, 100);
  };

</script>
