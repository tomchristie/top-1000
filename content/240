    if(a<=2 && (e=st[n+'_exp']||0) && TgT()>Date.parse(e)) {TdWS(n,a); return null}  //WS expired/abgelaufen (falls gesetzt)
    return st[n+'_jsn']!='0' ? JSON.parse(ws) : unescape(ws);  //WS-Wert zurückgeben
  }
  else if(a&1) {Tgws_=-1; return TgC(n)}  //Cookie
  return Tgws_=null;
}
/* TdWS(): "deleteWebStorage/Cookie (name[, art])" */
function TdWS(n,a) {
