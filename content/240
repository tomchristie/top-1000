cp('authenticate', function (error, user) {
  if (error) {
    console.log('%c ContentPass Integration - error ', 'background: #C00, color: #fff');
    return;
  }

  if (user.hasValidSubscription()) {
    console.log('%c ContentPass Integration - hasValidSubscription ', 'background: #fbb');
    window._sp_.config.targetingParams.pur = "true";
    window._sp_.executeMessaging();
  } else {
    console.log('%c ContentPass Integration - no validSubscription ', 'background: #fbb'); 
    window._sp_.executeMessaging();
  }
});


function ConsentManager() {
  console.log('%cConsentManager call stack', 'background: lightblue', arguments.callee, ConsentManager.caller);
  var consentCallbacks = [];
  var purCallbacks = [];
  var state = 'not interacted';
  var init = function() {
    console.log('%cConsentManager init call stack', 'background: lightblue', arguments.callee, ConsentManager.caller);
    cp('authenticate', function(error, user) {
      console.log('%cConsentManager auth (error, user.isLoggedIn(), user.hasValidSubscription())', 'background: lightblue', error, user.isLoggedIn(), user.hasValidSubscription());
      if (error) {
        console.log('%cConsentManager eventlistener', 'background: lightblue', error);
      }
      if (state !== 'not interacted') {
        console.log('%cConsentManager auth zweites callback abgefangen', 'background: lightblue', error, user.isLoggedIn(), user.hasValidSubscription());
        return;
      }
      if (user.hasValidSubscription()) {
        state = 'pur';
        runCallbacks(purCallbacks);
      } else {
        console.log('%cConsentManager auth, else', 'background: lightblue');
        __tcfapi('addEventListener', 2, function(tcData, success) {
          console.log('%cConsentManager eventlistener', 'background: lightblue', tcData);
          if ((['useractioncomplete', 'tcloaded']).indexOf(tcData.eventStatus) > -1) {
            state = 'consent';
            runCallbacks(consentCallbacks, tcData);
            __tcfapi('removeEventListener', 2, function(success) {}, tcData.listenerId);
          }
        });
      }
    });
  };
  var runCallbacks = function(callbacks, tcData) {
    while (callbacks.length > 0) {
      callback = callbacks.shift();
      try {
        if (typeof tcData !== 'undefined') {
          callback(tcData);
        } else {
          callback();
        }
      } catch (e) {
        console.log('%cConsentManager trycatch', 'background: red', e);
      }
    }
  };
  this.add = function() {
    if (arguments[0] && typeof arguments[0] === 'object') {
      consentCallback = arguments[0]['consented'] || function() {};
      purCallback = arguments[0]['pur'] || function() {};
    } else {
      consentCallback = (arguments[0] && typeof arguments[0] === 'function') ? arguments[0] : function() {};
      purCallback = (arguments[1] && typeof arguments[1] === 'function') ? arguments[1] : function() {};
    }
    switch (state) {
      case 'consent':
        consentCallback();
        break;
      case 'pur':
        purCallback();
        break;
      default:
        purCallbacks.push(purCallback);
        consentCallbacks.push(consentCallback);
        break;
    }
  };
  this.getCurrentState = function() {
    return state;
  }
  init();
};
