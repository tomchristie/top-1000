window &amp;&amp; window.sj_evt &amp;&amp; sj_evt.fire(&quot;OnScriptLoad:AriaTelemetry&quot;);
var AriaTelemetry;
(function (AriaTelemetry) {
    var ready = function () { return sj_evt.fire(&quot;OnScriptLoad:AriaTelemetry&quot;); };
    ready();
    var Impl;
    (function (Impl) {
        if (!Impl.ariaTelemetryOnScriptLoad) {
            sj_evt.bind(&quot;ajax.load&quot;, ready);
        }
        Impl.ariaTelemetryOnScriptLoad = true;
