$(document).ready(function() {
	$(document).scroll(function(){
		var bkTop=$(this).scrollTop();
		if(bkTop > 200){
			$(".topb").show()
		}
		else{
			$(".topb").hide()	
		}			
	})	
	$(".topb").click(function(){
        $('html , body').animate({scrollTop: 0},500);
    });	
});
</script>
