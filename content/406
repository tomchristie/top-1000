#floating-header .block-tags dl .list-tags li a:hover {background-color:#00bec8;}

#floating-header .media-list {float:right; display:inline-block; height:32px; padding:0;}
#floating-header .media-list ul {display:table; height:100%;}
#floating-header .media-list li {display:table-cell; height:100%; vertical-align:middle; padding:0 0 0 20px;}
#floating-header .media-list li img {max-height:32px; height:12px;}
#floating-header .media-list li a {position:relative; border-bottom:1px solid #000; display:block; padding:0 2px 4px 12px;}
#floating-header .media-list li a:hover{ opacity:0.6; filter:alpha(opacity=60); -ms-filter:"alpha(opacity=60)"; -moz-opacity:0.6; -khtml-opacity:0.6;}
#floating-header .media-list li a:before {content: ""; left:3px !important; box-sizing: border-box; width: 5px !important; height: 4px !important; border: 4px solid transparent; border-left: 6px solid #c33; background: none !important;
	position: absolute; margin-top: -5px !important; top: 50%;}
