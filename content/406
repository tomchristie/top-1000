<script language="JavaScript" type="text/JavaScript">
//Lazy Loadを起動する
$(".thumbs img.lazy").lazyload();
$(".block-sub-ad img.lazy").lazyload();
$(".block-sub-attention img.lazy").lazyload();
$(".block-sub-bnr img.lazy").lazyload();
$(".block-sub-gallery img.lazy").not(".thumbs img.lazy").lazyload({ threshold:1500 });
$(".block-sub-goods img.lazy").lazyload();
$(".block-sub-latest img.lazy").lazyload();
$(".block-sub-news img.lazy").lazyload();
$(".block-sub-rank img.lazy").lazyload();
$(".block-sub-rank-list img.lazy").lazyload();
$(".block-sub-recommend img.lazy").lazyload();
$(".block-sub-talent img.lazy").lazyload();
$(".block-snap-vote img.lazy").lazyload();
$("img.lazy")
	.not(".thumbs img.lazy")
	.not(".block-sub-ad img.lazy")
	.not(".block-sub-attention img.lazy")
	.not(".block-sub-bnr img.lazy")
	.not(".block-sub-gallery img.lazy")
	.not(".block-sub-goods img.lazy")
	.not(".block-sub-latest img.lazy")
	.not(".block-sub-news img.lazy")
	.not(".block-sub-rank img.lazy")
	.not(".block-sub-rank-list img.lazy")
	.not(".block-sub-recommend img.lazy")
	.not(".block-sub-talent img.lazy")
	.not(".block-snap-vote img.lazy")
	.lazyload().update();
</script>


