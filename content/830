                    <script>
    var pAb = {
        id: '2018-12-10_AD_MOBILE',
        variations: [
            { id: 'A', 'dfpId': 289 },
            { id: 'B', 'dfpId': 290 },
        ]
    }

    function getCookie(name) {
        var matches = document.cookie.match(new RegExp(
            "(?:^|; )" + name.replace(/([\.$?*|{}\(\)\[\]\\\/\+^])/g, '\\$1') + "=([^;]*)"
        ));
        return matches ? decodeURIComponent(matches[1]) : undefined;
    }

    function rand(min, max) {
        var argc = arguments.length;
        if (argc === 0) {
            min = 0;
            max = 2147483647;
        } else if (argc === 1) {
            throw new Error('Warning: rand() expects exactly 2 parameters, 1 given');
        } else {
            min = parseInt(min, 10);
            max = parseInt(max, 10);
        }
        return Math.floor(Math.random() * (max - min + 1)) + min;
    }

    function getSetAbVar(pAb) {
        var c = getCookie('_pro_abVar'),
            abVar = {
                expId: pAb.id,
                getName: function() {
                    return this.expId + '.' + this.varId
                }
            };

        if (c) {
            var m = c.match(new RegExp(pAb.id + "\.(A|B|C|D|E|F|G|H)"));
            if (m) {
                abVar.varId = m[1]
                abVar.varIndex = pAb.variations.reduce(function(memo, item, index) {
                    if (item.id === abVar.varId) {
                        memo = index;
                    }
                    return memo;
                }, undefined)
            }
        }

        if (typeof abVar.varIndex === 'undefined') {
            abVar.varIndex = rand(0, pAb.variations.length - 1);
            abVar.varId = pAb.variations[abVar.varIndex].id
            var date = new Date();
            date.setDate(date.getDate() + 30);
            document.cookie = "_pro_abVar=" + abVar.getName() + "; path=/; expires=" + date.toUTCString();
        }

        return {
            abVar: abVar.getName(),
            dfpId: pAb.variations[abVar.varIndex].dfpId,
        }
    }

