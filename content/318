<script>
(function () {
	function get(endPoint, onSuccess) {
		var xhr = new XMLHttpRequest();

		xhr.onreadystatechange = function () {
			if (xhr.readyState === 4) {
				if (xhr.status === 200) {
					onSuccess(xhr.responseText);
				}
			}
		};

		xhr.open('GET', endPoint);
		xhr.setRequestHeader('Content-Type', 'text/html');
		xhr.setRequestHeader('X-Requested-With', 'XMLHttpRequest');
		xhr.send();
	}

	function createElementFromHTML(html) {
		var div = document.createElement('div');
		div.innerHTML = html.trim();
		return div.firstChild;
	}

	function generateArticleHtml(article) {
		var articleHtml = "";
		var articleUrl = article.querySelector("a").href;
		articleUrl = articleUrl ? articleUrl.replace("/smp/", "/").replace("-s.html", "-n1.html") : "";
		var articleTitle = article.querySelector("a").innerText;
		articleTitle = articleTitle.match(/(.+)\n/) ? articleTitle.match(/(.+)\n/)[1] : articleTitle;

		articleHtml += '<article class=" sc sc__medium | mb-3 pb-2 "><div class=" df jc-sb | "><div class=" df flex-col "><h2 class="sc__headline gray3 ma-0 mr-auto | font-palt font-1 normal"><a href="' + articleUrl + '" class=" dark | no-underline hover:underline underline-offset-0 " data-story-link="true">' + articleTitle + '</a></h2></div></div><div class=" ma-0 "><div class=" sc__sections article-section | df fwrap | font-0 "><div class=" mr-2 pt-1 | overflow-hidden "><a class=" darkBlueBg white |  tag-bean | pv-1 ph-1 md:ph-2 br-1 | fixed-font-0 font-palt | nowrap no-underline  | overflow-hidden " href="/geino/pressrelease/newslists/pressrelease-n.html">プレスリリース</a></div></div></div></article>';

		return articleHtml;
	}

	get('/smp/geino/pressrelease/newslists/pressrelease-n.html', function (res) {
		var parser = new DOMParser();
		var htmlDocument = parser.parseFromString(res, "text/html");
		var mainContent = htmlDocument.documentElement.querySelector("#contentsContainer");

		var pressreleaseArticles = [].slice.call(mainContent.querySelectorAll(".list-source li"));
		pressreleaseArticles.length = 4;

		var targetDOM = document.querySelector("#pressrelease");
		var rootDOM = createElementFromHTML(
			'<section class=" storycard-feed | pt-3 pb-4 "><h2 class="storycard-header | df ai-c | darkBlue | bold font-1 | mb-2 pv-2 "><span class="dark">プレスリリース</span><span class="mr-0 ml-auto dark font-sm">powered by <a href="http://www.atpress.ne.jp/" target="_blank">@Press</a> , <a href="http://prw.kyodonews.jp/opn/" target="_blank">共同通信ＰＲＷ</a> , <a href="https://www.dreamnews.jp/" target="_blank">ドリームニュース</a></span></h2><div class="storycard-feed__content | grid " data-desktop="1" data-tablet="1" data-mobile="1"><div class="grid__col--lg-12 grid__col--md-12 grid__col--sm-12 "></div><a class=" storycard-feed__category-link darkBlueBg  |  no-underline hover:underline underline-offset-0  |  mr-0 ml-auto " href="/geino/pressrelease/newslists/pressrelease-n.html">もっと見る</a></div></section>'
		);
		var articleList = rootDOM.querySelector(".storycard-feed__content > div");
		var articlesHtml = "";

		pressreleaseArticles.forEach(function(article) {
			articlesHtml += generateArticleHtml(article);
		});

		articleList.innerHTML = articlesHtml;
		targetDOM.append(rootDOM);
	});
})();
</script></div><div id="fusion-static-exit:f0fCSP3ltXTkaf0" style="display:none" data-fusion-component="f0fCSP3ltXTkaf0"></div><div class=" chain-columns grid | mh-auto ph-2 ld:ph-0 ld:max-width "><div class="grid__col--lg-6 grid__col--md-12 grid__col--sm-12 "></div></div><div class="gptAd"><div id="div-gpt-ad-Mobile_PR_Imgtxt_6th"></div><script>(function() {
