                          }).catch(function(error){
                            headerBidderBack("OPEp")
                          });
                        }catch(error){
                          headerBidderBack("OPEp")
                        }
                      })({OPEp: 'https://profiles.tagger.opecloud.com/api/v1/mediaconsortium/profile'});
                    }
                  }
                  requestBids();
                  window.setTimeout(function () {
                    sendAdserverRequest()
                  }, failSafeTimeout)
