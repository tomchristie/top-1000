                      headerBidderBack("OPEp");
                    } else {
                      (function(req){
                        window.setTimeout(function() {
                          headerBidderBack("OPEp");
                        }, TAGGER_TIMEOUT);
                        try{
                          fetch(req["OPEp"] + '?url=' + encodeURIComponent(window.location.href), {
                            referrerPolicy: 'no-referrer-when-downgrade',
                            mode: 'cors',
                            credentials: 'include'
                          }).then(function(response){
