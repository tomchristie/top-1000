<script>
  triggerPopin = () => {
   var memberStatus = 'NOT_SUBSCRIBED';
    // activation de l'appel popin uniquement sur desktop
    if (window.screen.width > 576) {
      var openPopin = document.getElementsByClassName("js--popinClubR--open");
      if (!openPopin) return;
      //Close popin
      var closePopin = document.getElementsByClassName("js--popinClubR--close");
      for (var j = 0; j < closePopin.length; j++) {
        var elemClose = closePopin[j];
        elemClose.onclick = function () {
          document.getElementById("popinClubR").style.display = "none";
          document.location.reload(true);
        };
      };
      for(var i = 0; i < openPopin.length; i++) {
        var elemOpen = openPopin[i];
        elemOpen.onclick = function(e) {
          e.preventDefault();
          if (memberStatus == 'NOT_SUBSCRIBED') {
            console.log('not subscribed, launch ajax...');
            var xhr = new XMLHttpRequest();
            xhr.open("GET", "/user?action=clubpresubscribeajax", true);
            xhr.responseType = "text";
            xhr.send();
            xhr.onload = function() {
              var response = JSON.parse(xhr.responseText);
              var isResponseSuccess = response.success;
              if (isResponseSuccess) {
                document.getElementById("popinClubR").style.display = "flex";
              }
            }
          }
        };
      };
    };
};

document.addEventListener("DOMContentLoaded", triggerPopin);

 </script><script>
        document.addEventListener("DOMContentLoaded", function() {
          var url = window.location.href;
          let urlSplitTracking = url.split('=');
          urlSplitTracking = urlSplitTracking[0].split('&');
          urlSplitTracking = urlSplitTracking[0].split('#');
      
          var urlLastCharacter = parseInt(urlSplitTracking[0].slice(-1));

          var tabItem = document.getElementsByClassName('tab-item');
          // display du bon onglet en fonction de l'url
          for (var i = 0; i < tabItem.length; i++) {
            if (urlLastCharacter == (i + 1)) {
              tabToggle(document.querySelector("a[data-id='" + (i + 1) + "']"), false);
            }
            //menu visible quand on arrive d'une autre page et qu'il y a un tracking
            if(urlSplitTracking[1]) {
                var tabSelected = document.querySelector(".tab-item--selected")
                tabSelected.scrollIntoView({block: "center"});
            }
          }
      
          // changement d'onglets au click sur la page
          for(var j = 0; j < tabItem.length; j++) {
            var elem = tabItem[j];
            elem.onclick = function(event) {
              event.preventDefault();
              //prevent reloading page when a tab-item is clicked
              if (history && history.pushState) {
                //Only do this if history.pushState is supported by the browser
                history.pushState({id:'event'}, 'event', event.target.href);
              }
              tabToggle(this);
            };
          }
          function tabToggle(buttonTargeted) {
            if (navigator.userAgent.search('BuyerApp') > -1) {
              event.preventDefault();
            }
            var tabNavAll = buttonTargeted.parentElement.children;
            var tabNavSelected = buttonTargeted;
            var tabNavSelectedNumber = tabNavSelected.getAttribute('data-id');
            var tabNavSelectedParent = tabNavSelected.parentElement;
            for (var i=0; i < tabNavAll.length; i++) {
              tabNavAll[i].classList.remove('tab-item--selected');
              tabNavAll[i].setAttribute("aria-selected", "false");
            }
            tabNavSelected.classList.add('tab-item--selected');
            tabNavSelected.setAttribute("aria-selected", "true");
            var tabSectionAll = tabNavSelected.parentElement.parentElement.parentElement.parentElement.getElementsByClassName('tab-content--section');
            for (var j=0; j < tabSectionAll.length; j++) {
              tabSectionAll[j].setAttribute("aria-hidden", "true");
              tabSectionAll[j].style.display = 'none';
              var tabSectionNumber = tabSectionAll[j].getAttribute('data-id');
              if (tabSectionNumber === tabNavSelectedNumber) {
                tabSectionAll[j].setAttribute("aria-hidden", "false");
                tabSectionAll[j].style.display = 'block';
              }
            }
            // if (window.screen.width <= 992) {
            //   setTimeout(function(){
            //     window.scrollTo(tabSectionAll[0].parentElement.offsetLeft,tabSectionAll[0].parentElement.offsetTop);
            //   }, 20);
            // }
          }
        });
