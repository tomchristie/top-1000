  <script type="text/javascript">
  (function() {	
    try {
      function loadMegabanner() {
        var url = 'https://static.fr.shopping.rakuten.com/visuels/0_content_square/marketing/merchandising-campaigns/merchandising-campaigns.json';
        
        var xhr = new XMLHttpRequest();
        xhr.onreadystatechange = function() { 
          if (xhr.readyState == XMLHttpRequest.DONE) {
            if (xhr.status == 200) {
              data = xhr.response;
              if(data) {
                try {
                  homeMegaBanernHandler(data);
                } catch (e) {
				  console.log(e)
                  pushFallback();
                }
              } else
                pushFallback();
            } else
              pushFallback();
          }
        };
        xhr.responseType = 'json';
        if(/(preview)|(editors)/i.test(document.location.host) || window.draftCampaigns)
          xhr.open('GET', url.replace('.json', '-preview.json'), true);
        else if (window.kmlSimulator)
          xhr.open('GET', url.replace('.json', '-extension.json'), true);
        else	
          xhr.open('GET', url, true);
        xhr.send(null);
      }
  
      function isDisplayed(programmation) {	
        var prog = false;
        programmation.forEach(function(i) {
        
          i.start[1].length === 4 ? i.start[1] = '0' + i.start[1] : null;
          i.end[1].length === 4 ? i.end[1] = '0' + i.end[1] : null;
          
          var dateDuJour = new Date();
          if(window.simulationDate)
            dateDuJour = new Date(simulationDate);
          
          prog = prog || new Date(i.start[0] + "T" + i.start[1] + ':00').getTime() < dateDuJour.getTime() && new Date(i.end[0] + "T" + i.end[1] + ':00').getTime() > dateDuJour.getTime()
        });
        return prog;
      }
  
      function getMegabanners(data) {
        
        var device = 'DESKTOP', isLogged = 'Non';
        false ? device = 'MOBILE' : false;
        false ? isLogged = 'Oui' : false;
		
		var priceClubStatus = 'NOT_SUBSCRIBED';
		priceClubStatus === 'NOT_SUBSCRIBED' ? priceClubStatus = 'UNSUBSCRIBED' : false;
        
        var asyncDataLayer = {
          'clubMemberStatus': priceClubStatus,
          'userID': 0,
          'logged': isLogged,
          'device': device
        };
  
        var campaigns = data.filter(function(campaign) {
          
          // On filtre sur le datalayer
          var filter = true;
          Object.keys(campaign.targeting).forEach(function(e) {
            if(!campaign.targeting[e].includes(asyncDataLayer[e])) {
              filter = false;
            }
          });
          
          // On filtre sur les dates
          filter = filter && isDisplayed(campaign.programmation);
              
          return filter;
        });
        return campaigns;
      }
  
      function homeMegaBanernHandler(data) {
  
        var homeMegabanners = getMegabanners(data.homeMegabanner); 
        if(!homeMegabanners.length)
          throw 'Aucune campagne';
	
        pushMegaBanners(homeMegabanners);
      }
        
      function pushMegaBanners(homeMegabanners) {
  
        homeMegabanners.forEach(function(homeMegabanner , i) {
          var megaBannerHTML = '\
          <div class="hp--megaban--slide">\
            <style>\
            #megabanner-'+ homeMegabanner.id +' {\
              background: '+ homeMegabanner.creation.background_color +';\
              background: -webkit-'+ homeMegabanner.creation.background_color +';\
            }\
            #megabanner-'+ homeMegabanner.id +' .hp--megaban--cta {\
              color: '+ homeMegabanner.creation.cta_color +';\
              border-color: '+ homeMegabanner.creation.cta_color +';\
            }\
            #megabanner-'+ homeMegabanner.id +' a:hover .hp--megaban--cta {\
              background-color: '+ homeMegabanner.creation.cta_color +';\
              color: '+ homeMegabanner.creation.cta_color_txt +';\
            }\
            #megabanner-'+ homeMegabanner.id +' .hp--megaban--cta--container {\
              top: '+ (homeMegabanner.creation.cta_position_top || 70) +'%;\
              left: '+ (homeMegabanner.creation.cta_position_left || 6) +'%;\
            }\
            @media all and (max-width: 576px) {\
              #megabanner-'+ homeMegabanner.id +' .hp--megaban--background {\
              background-image: url(https://images.fr.shopping.rakuten.com/visuels'+ homeMegabanner.creation.image_mobile +');\
              background-position: center;\
              }\
            }\
            </style>\
            <div id="megabanner-'+ homeMegabanner.id +'" class="kml-megabanner-slide flex justify-center items-center bg-center contain w-100">\
              <a href="'+ homeMegabanner.creation.link +'" class="hp--megaban--background bg-center contain db no-underline w-100 flex justify-center">\
                <div class="hp--megaban--content relative">\
                <img data-nolazy class="hp--megaban--img relative" src="https://images.fr.shopping.rakuten.com/visuels'+ homeMegabanner.creation.image_desktop +'" alt="megaban homepage rakuten">\
                <div class="dn dib-ns absolute hp--megaban--cta--container" style="position: absolute;">\
                  <p class="hp--megaban--cta f14 pointer no-underline bg-transparent db tc">'+ homeMegabanner.creation.cta_text +'</p>\
                </div>\
                </div>\
              </a>\
            </div>\
          </div>';

          document.getElementById('slider-megaban').insertAdjacentHTML('beforeend', megaBannerHTML);
          if( homeMegabanner.creation.newTab && homeMegabanner.creation.newTab === 'Oui') {
            const link = document.querySelector('[href="'+ homeMegabanner.creation.link +'"]')
            link.setAttribute("target","_blank")
          }

		  if(i === 0) {
		    setTimeout(function() {
				sendAtPublisherTag(homeMegabanner.id, 'impression')
			}, 3000);
		  }
        });		
        initCarouselMegabanner();
      }
          
      function initCarouselMegabanner() {
        //DEBUT fix for issues on swipe on IOS.11, prevent scroll down when scrolling horizontally
        // Slider Megaban
        document.addEventListener("DOMContentLoaded", function() {
        var options = {
        passive: false,
        once: true,
        };
        document.addEventListener(
        'touchstart', // change to touchstart for mobile
        function(event) {
          document.addEventListener(
          'touchmove', // change to touchmove for mobile
          function(event) {
            event.preventDefault();
          },
          options
          );
        },
        options
        );
        const sliderMegaban = document.getElementById('slider-megaban');
        function onTouchStart(e) {
          this.firstClientX = e.touches[0].clientX;
          this.firstClientY = e.touches[0].clientY;
        }
        function preventTouch(e) {
          const minValue = 5; // threshold
          this.clientX = e.touches[0].clientX - this.firstClientX;
          this.clientY = e.touches[0].clientY - this.firstClientY;
          // Vertical scrolling does not work when you start swiping horizontally.
          if(Math.abs(this.clientX) > minValue){ 
            e.preventDefault();
            e.returnValue = false;
            return false;
          }
        }
        sliderMegaban.addEventListener('touchstart', onTouchStart);
        sliderMegaban.addEventListener('touchmove', preventTouch, {passive:false});
        });
        //FIN Fix
  
        rktDefer.scriptLoaded('merchCampaign', 'lazyloadTriggerSlideMegaban', {
          targetId: 'slider-megaban',
          rawTemplate: true,
          skipUnlazyLoadingImages: true,
          cb: function() {
            triggerPopin()
          },
          carousel: {
            infinite: true,
            slidesToShow: 1,
            slidesToScroll: 1,
            variableWidth: false,
            swipe: true, // To activate or desactivate Swipe on desktop for peakday (To select code)
            autoplay: true,
            autoplaySpeed: 5000,
            pauseOnHover: true,
            dots: true
          }
          });
		  
		  setTimeout(function() {
			sendTracking()
		  }, 3000)
      }
      loadMegabanner();
    } catch(e) {
	console.log(e)
      pushFallback();
    }
    
    function pushFallback() {
      var megaBannerHTML = '\
      <div id="megabanner-0" style="background-color: #000" class="hp--megaban--slide flex justify-center items-center bg-center contain w-100">\
        <a href="https://fr.shopping.rakuten.com/event/rakuten-deals" class="hp--megaban--background bg-center contain db no-underline w-100 flex justify-center">\
          <div class="hp--megaban--content relative">\
          <img data-nolazy class="hp--megaban--img relative" src="https://images.fr.shopping.rakuten.com/visuels/2019-01-07-RD/autopromo/2000x600_RD_V2.jpg" alt="megaban homepage rakuten">\
          <div class="dn dib-ns absolute hp--megaban--cta--container" style="position: absolute;">\
            <p class="hp--megaban--cta f14 pointer no-underline bg-transparent db tc">Découvrir</p>\
          </div>\
          </div>\
        </a>\
      </div>';	
      document.getElementById('slider-megaban').insertAdjacentHTML('beforeend', megaBannerHTML);
    }
	
	function sendTracking() {
		var targetNodes = document.querySelectorAll('#slider-megaban .slick-slide');
		var config = { attributes: true };
		var callback = function(mutationsList) {
			for(var mutation of mutationsList) {
				if(mutation.attributeName === 'class' && mutation.target.classList.contains('slick-current') && document.visibilityState === 'visible' && window.scrollY <= 292) {
					if(mutation.target.querySelector('.kml-megabanner-slide'))
						sendAtPublisherTag(mutation.target.querySelector('.w-100').attributes.id.value.replace('megabanner-', ''), 'impression')
				}
			}
		};
		var observer = new MutationObserver(callback);
		targetNodes.forEach(function(targetNode) {
			observer.observe(targetNode, config);
			
			targetNode.querySelector('a').addEventListener('click', function(elem) {
				var id = this.parentNode.attributes.id.value.replace('megabanner-', '');
				sendAtPublisherTag(id, 'click')
			})
		});
		
	}
	
	function sendAtPublisherTagFunction(id, type, tagKameleoon) {
		var event = {};
		event[type] = {
			campaignId: '[Merchandising]',
			creation: '[HomeMegabanner]',
			variant: '['+ id +']',
			advertiserId: '[Rakuten]' 
		};
		if(typeof ATInternet !== 'undefined') {
			tagKameleoon.publisher.set(event);
			tagKameleoon.dispatch();
		}
	}
	
    function sendAtPublisherTag(id, type) {
        if(window.Didomi) {
          window.didomiOnReady = window.didomiOnReady || [];
          window.didomiOnReady.push(() => {
            var tagKameleoon = new window.ATInternet.Tracker.Tag();
            sendAtPublisherTagFunction(id, type, tagKameleoon);
          });
        } else {
            var tagKameleoon = new window.ATInternet.Tracker.Tag();
            sendAtPublisherTagFunction(id, type, tagKameleoon);
        }
    }
  })();
</script><div class="hp--megaban--bandeau flex justify-center justify-between-l items-center w-100 pa-0 pv3-ns ph4-ns black"></div>

<script>

document.addEventListener("DOMContentLoaded", function() {
// Construction bandeau RSP
const nbSuperPoints = 0;
const isSubscribedToPriceClub = false;
const firstNameOrPseudo = "";
(function displayBandeauRSP() {
const observer = new MutationObserver(function(mutations) {
  if(window.screen.width > 992) {
    const trackingPriceClub = function trackingPriceClub(user) {
    switch (user) {
      case "nc-ni":
        return '<span class="f14 black underline pv2 tr db">Découvrir le Club R</span>';
      case "c-ni":
        return '<span class="f14 black underline pv2 tr db">Découvrir le Club R</span>';
      case "c-i-nsp":
        return '<span class="f14 black underline pv2 tr db">En savoir plus</span>';
      case "c-i-sp":
        return '<span class="f14 black underline pv2 tr db">Voir le détail de mes Points</span>';
    }
  };
  const isMember = function isMember() {
    const hasRSP = function hasRSP() {
      return nbSuperPoints > 0 ?
        '<a class="no-underline db flex justify-between items-center w-100" href="/usersecure?action=clubaccount#xtatc=PUB-[PMC]-[H]-[HomePage]-[Ban-ClubR]-[C-I]-[Detailsuperpoints]-[]"><p class="f14 pv2 pr2 lh-title flex items-end"><img data-nolazy class="hp--megaban--bandeau-logo mr2" src="https://fr.shopping.rakuten.com/visuels/Club-Rakuten/page/LogoCRdesk.svg" alt="ClubR"><span class="b">Bienvenue ' + firstNameOrPseudo +
        '  !&#160;</span><span class="dn db-l"> Vous avez&nbsp;</span><span class="b">' +
        new Intl.NumberFormat("fr-FR", { style: "currency", currency: "EUR" })
        .format(nbSuperPoints / 100).replace(",00", "") +
        '&nbsp;</span> à dépenser <span class="dn cb-l">sur votre prochaine commande</span></p>' + trackingPriceClub("c-i-sp") +'</a>'
        :
        '<a class="no-underline c-i-nsp db flex justify-between items-center w-100" href="/event/rakuten-deals#xtatc=PUB-[PMC]-[H]-[HomePage]-[Ban-ClubR]-[C-I]-[Ensavoirplus]-[]"><p class="f14 pv2 pr2 lh-title no-underline dib flex items-end"><img data-nolazy class="hp--megaban--bandeau-logo mr2" src="https://fr.shopping.rakuten.com/visuels/Club-Rakuten/page/LogoCRdesk.svg" alt="ClubR"><span class="b">Bienvenue ' + firstNameOrPseudo +
        ' !&#160;</span><span>En tant que membre privilégié, vous profitez de tous nos événements et bénéficiez de 3% minimum remboursés sur des millions de produits*, toute l’année !</span></p>' +
        trackingPriceClub("c-i-nsp") +'</a>';
    };

    return isSubscribedToPriceClub ?
      hasRSP()
      :
      '<a class="no-underline db flex justify-between items-center w-100" href="/event/club-rakuten#xtatc=PUB-[PMC]-[H]-[HomePage]-[Ban-Club]-[C-NI]-[RejoindreleClubR]-[]"><p class="f14 pv2 pr2 lh-title no-underline dib flex items-end"><img data-nolazy class="hp--megaban--bandeau-logo mr2" src="https://fr.shopping.rakuten.com/visuels/Club-Rakuten/page/LogoCRdesk.svg" alt="ClubR"><span class="b">Bienvenue ' + firstNameOrPseudo +
      ' !&#160;</span><span>inscrivez-vous et profitez de 3% remboursés sur tous vos achats, toute l&apos;année !</span></p>' +
      trackingPriceClub("c-ni")+'</a>';
  };

  const bandeauRSPHTML = isSubscribedToPriceClub ?
    isMember()
    :
    '<a class="no-underline db flex justify-between items-center w-100" href="/event/club-rakuten#xtatc=PUB-[PMC]-[H]-[HomePage]-[Ban-ClubR]-[NC-NI]-[RejoindreleClubR]-[]"><p class="f14 pv2 pr2 lh-title no-underline dib flex items-end"><img data-nolazy class="hp--megaban--bandeau-logo mr2" src="https://fr.shopping.rakuten.com/visuels/Club-Rakuten/page/LogoCRdesk.svg" alt="ClubR"><span class="b">Gratuit & sans engagement,&#160;</span>inscrivez-vous et profitez de 3% remboursés sur tous vos achats, toute l&apos;année !</p>' +
    trackingPriceClub("nc-ni")+'</a>';

  document.getElementsByClassName("hp--megaban--bandeau")[0].insertAdjacentHTML("beforeend", bandeauRSPHTML);
  return observer.disconnect();
  }

//version mobile & app
  if(window.screen.width <= 992) {
  const bandeauRSPHTML = isSubscribedToPriceClub ?
    '<a class="f14 black no-underline w-100 pv2 ph3 lh-title dib flex justify-between items-center" href="/event/rakuten-deals-offres-membres#xtatc=PUB-[PMC]-[H]-[HomePage]-[Ban-ClubR]-[I]-[]-[]"><p class="flex items-center"><img data-nolazy class="hp--megaban--bandeau-logo mr2" src="https://fr.shopping.rakuten.com/visuels/Club-Rakuten/page/LogoCRdesk.svg" alt="ClubR"><span class="b">Profitez de nos offres exclusives !</span></p>'+
    '<svg class="hp--megaban--bandeau-rsp--chevron" data-name="Calque 1" xmlns="http://www.w3.org/2000/svg" viewbox="0 0 26 26">'+
      '<defs><style>.chevron-1{fill:#4a4a4a;}</style></defs>'+
      '<title>chevron_bottom</title><path class="chevron-1" d="M14.76,18.91,25.67,8a.95.95,0,0,0-1.2-1.47l-.09.07L13.66,17.3a.91.91,0,0,1-1.28,0l0,0L1.68,6.64A1,1,0,0,0,.44,6.45,1,1,0,0,0,.18,7.84.64.64,0,0,0,.28,8l11,11A2.47,2.47,0,0,0,14.76,18.91Z"/></svg>'+
    '</a>'
    :
    '<a class="f14 black no-underline w-100 pv2 ph3 lh-title dib flex justify-between items-center" href="/event/club-rakuten#xtatc=PUB-[PMC]-[H]-[HomePage]-[Ban-ClubR]-[NC-NI]-[RejoindreleClubR]-[]"><p class="flex items-center"><img data-nolazy class="hp--megaban--bandeau-logo mr2" src="https://fr.shopping.rakuten.com/visuels/Club-Rakuten/page/LogoCRdesk.svg" alt="ClubR"><span class="b">Gratuit & sans engagement&#160;</span>Inscrivez-vous !</p>'+
      '<svg class="hp--megaban--bandeau-rsp--chevron" data-name="Calque 1" xmlns="http://www.w3.org/2000/svg" viewbox="0 0 26 26">'+
      '<defs><style>.chevron-1{fill:#4a4a4a;}</style></defs>'+
      '<title>chevron_bottom</title><path class="chevron-1" d="M14.76,18.91,25.67,8a.95.95,0,0,0-1.2-1.47l-.09.07L13.66,17.3a.91.91,0,0,1-1.28,0l0,0L1.68,6.64A1,1,0,0,0,.44,6.45,1,1,0,0,0,.18,7.84.64.64,0,0,0,.28,8l11,11A2.47,2.47,0,0,0,14.76,18.91Z"/></svg>'+
    '</a>';

  document.getElementsByClassName("hp--megaban--bandeau")[0].insertAdjacentHTML("beforeend", bandeauRSPHTML);
  return observer.disconnect();
  }
  
});

return observer.observe(document.documentElement, {
  childList: true,
  subtree: true,
});
})();

});
