<script>
  function scrollCarousel(arrow) {
    const direction = arrow.getAttribute("data-direction"),
      innerCaroussel = arrow.parentNode.getElementsByClassName("evt--actualites--slider--container-overflow")[0];
    "left" === direction && (innerCaroussel.scrollLeft -= .8 * innerCaroussel.clientWidth), "right" === direction && (innerCaroussel.scrollLeft += .8 * innerCaroussel.clientWidth)
  }
</script>
<div class="grid_container pt4 ">

    <div class="grid_row row_nogutter grid_col-8 grid_col-12-BelowLandscape center pa0 flex items-center br4" data-popin data-js="jblfr">


    
        <div class=" dn db-ns" >
        
            <img data-nolazy class="w-100 br3 tc center shadow-1" src="https://fr.shopping.rakuten.com/visuels/0_merch/OP/Cross/2021/CREW/JBL/encart_pub-JBL03.jpg" alt="JBL">
 
        </div>

 
        <div class=" db dn-ns mb4" >
        
            <img data-nolazy class="w-100 br3 tc center shadow-1" src="https://fr.shopping.rakuten.com/visuels/0_merch/OP/Cross/2021/CREW/JBL/encart_pub-JBL-mob2.png" alt="JBL">
 
        </div>



     </div>
