<div id="pmchPubMob" class="dn">
    <script>
const isAppPubMob = false;
window.googletag = window.googletag || {cmd: []};
window.didomiOnReady = window.didomiOnReady || [];
window.didomiOnReady.push(function() {
Didomi.getObservableOnUserConsentStatusForVendor('google').subscribe((consentStatus) => {
 if (consentStatus) {
   googletag.cmd.push(function () {
     if (window.screen.width < 576) {
        googletag.defineSlot('/57948771/Priceminister_FR_MSITE/homepage/homepage/pave_mtf', [300, 250], 'div-gpt-ad-1616065587254-0').addService(googletag.pubads());      
        googletag.pubads().enableSingleRequest();
        googletag.pubads().collapseEmptyDivs();
        googletag.enableServices();
     }
   });
 }
});
});
    </script>
    <div class="flex justify-center items-center w-100">
        <div id='div-gpt-ad-1616065587254-0' style='width: 300px; height: 250px;' class="mv3 flex justify-center ba b--light-gray">
            <script>
            if (window.screen.width < 576 && !isAppPubMob) {
              googletag.cmd.push(function() { googletag.display('div-gpt-ad-1616065587254-0'); });
            }
            </script>
        </div>
    </div>
</div>

<script>
if (window.screen.width < 576 && !isAppPubMob) {
var containerPmchPubMob = document.getElementById('pmchPubMob');
containerPmchPubMob.style.display = 'block';
adBandeauHPContainer ? adBandeauHPContainer.style.display = 'none' : null;
}
</script>
<div class="HP-Pub-bandeau flex justify-center">
    <div class="HP-Pub-bandeau-container flex flex-column justify-center items-center"></div>
</div>

<script>
        /*push Pub*/
        const isAppPub = false;
        const adBandeauHPContainer = document.querySelector('.HP-Pub-bandeau-container');
        
        var googletag = googletag || {};
        googletag.cmd = googletag.cmd || [];
        const pushAdBandeau = function () {
            googletag.cmd.push(function () {
                googletag.defineSlot('/57948771/PriceMinister_FR/home/home_bandeau_mtf', [[728, 90], [1000, 90]], 'div-gpt-ad-1616065750203-0').addService(googletag.pubads());
                googletag.pubads().enableSingleRequest();
                googletag.pubads().collapseEmptyDivs();
                googletag.enableServices();
            });
        }

        /*init block pub*/
        const initAdBlockBandeau = function () {
            document.getElementById('div-gpt-ad-1616065750203-0') ? document.getElementById('div-gpt-ad-1616065750203-0')
            .remove() : null;
            document.querySelector('.CSCriteoBandeau') ? document.querySelector('.CSCriteoBandeau').remove() : null;
            adBandeauHPContainer.insertAdjacentHTML('afterbegin',
                '<div id="div-gpt-ad-1616065750203-0" class="mv4 ba b--light-gray"></div><div class="CSCriteoBandeau" id="crt-1263264" style="height:90px;width:728px"></div>'
                );
            pushAdBandeau();
            googletag.cmd.push(function () {
                googletag.display('div-gpt-ad-1616065750203-0');
            });
        }

        /* if adblocker*/
        const initCriteoAdsBandeau = function () {
            const criteoId = document.querySelector('.CSCriteoBandeau') ? document.querySelector('.CSCriteoBandeau').id.replace('crt-', '') : null;
            if (criteoId) {
                document.querySelector('.CSCriteoBandeau').style.display = 'block'
                window.Didomi.getObservableOnUserConsentStatusForVendor('91').subscribe((consentStatus) => {
                    window.Criteo = window.Criteo || {};
                    window.Criteo.events = window.Criteo.events || [];
                    if (consentStatus) {
                        window.Criteo.events.push(function () {
                            Criteo.DisplayAcceptableAdIfAdblocked({
                                'zoneid': criteoId,
                                'containerid': 'crt-' + criteoId,
                                'overrideZoneFloor': false,
                                'collapseContainerIfNotAdblocked': true
                            });
                        })
                    }
                });
            }
        }

        if (!isAppPub) {
            window.addEventListener('resize', initAdBlockBandeau);
            window.didomiOnReady = window.didomiOnReady || [];
            window.didomiOnReady.push(function () {
                initAdBlockBandeau();
                initCriteoAdsBandeau();
            });
        }
</script>
