<script>
  document.addEventListener("DOMContentLoaded", function() {
      var allSEOButtons = Array.from(document.querySelectorAll('.js--seemore'));
      for (var i=0; i<allSEOButtons.length; i++) {
          var element = allSEOButtons[i];
          element.onclick = function(event) {
              toggleElementSEO(this);
          }
      }
      function toggleElementSEO(linkTargeted) {
      if (linkTargeted.classList.contains("active")) {
        linkTargeted.classList.remove("active");
      } else {
        linkTargeted.classList.add("active");
      }
      if (linkTargeted.parentElement.getElementsByClassName('links-container')[0].classList.contains("collapsed")) {
        linkTargeted.parentElement.getElementsByClassName('links-container')[0].classList.remove("collapsed");
        linkTargeted.parentElement.getElementsByClassName('links-container')[0].classList.add("to-collapse");
        linkTargeted.getElementsByTagName('P')[0].innerHTML = "voir moins";
      } else {
        linkTargeted.parentElement.getElementsByClassName('links-container')[0].classList.add("collapsed");
        linkTargeted.parentElement.getElementsByClassName('links-container')[0].classList.remove("to-collapse");
        linkTargeted.getElementsByTagName('P')[0].innerHTML = "voir plus";
      }
    }
    // le bouton "voir plus" est caché s'il n'y a pas assez d'éléments (si height du container < 140px)
    const linksContainers = document.getElementsByClassName("links-container");
    for (var i=0; i < linksContainers.length; i++) {
      const seeMoreContainer = document.getElementsByClassName("links-container")[i].parentElement.getElementsByClassName('js--seemore')[0];
      const linksContainerHeight = document.getElementsByClassName("links-container")[i].clientHeight;
      if (linksContainerHeight < 140) {
        seeMoreContainer.classList.remove("flex");
        seeMoreContainer.classList.add("dn");
      }
    }
  });
</script>
