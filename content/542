/*
 ==================================
    to detect current breakpoint
 ================================== */

/*
     ORDER is important !
*/

body::before {
    content: "max";
    display: none;
}

@media screen and (max-width: 1491px) {
    body::before {
        content: "wide";
        display: none;
    }
}

@media screen and (max-width: 1199px) {
    body::before {
        content: "desktop";
        display: none;
    }
}

@media screen and (max-width: 991px) {
    body::before {
        content: "tablet";
        display: none;
    }
}

@media screen and (max-width: 767px) {
    body::before {
        content: "landscape";
        display: none;
    }
}

@media screen and (max-width: 575px) {
    body::before {
        content: "phone";
        display: none;
    }
}

