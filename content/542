    let allmadCtas = {};
    const getAllmadCtas = madCtas => {
        var xhr = new XMLHttpRequest;
        xhr.open("GET", "https://fr.shopping.rakuten.com/visuels/content-short/lucy/mad/json/mads.json"), xhr.onload =
            function () {
                if (200 === xhr.status) {
                    allmadCtas = JSON.parse(xhr.responseText);
                    const madCtasArray = allmadCtas.mads;
                    for (let madCta in madCtas) {
                        const madCtaId = madCtas[madCta];
                        madCtasArray.forEach(item => {
                            const selectionId = item.id;
                            selectionId === madCtaId && displaymadCtas(item)
                        })
                    }
                }
            }, xhr.send()
    };

