        <script>
            document.addEventListener("DOMContentLoaded", function () {
                /*** toggle sous-catégories Desktop ***/
                var allBtnToggle = Array.from(document.querySelectorAll('.js-toggle'));
                var allListOverflow = Array.from(document.querySelectorAll('.hp--sdt--item--overflow'));
                var allBtnClose = Array.from(document.querySelectorAll('.icon-close-tab'));

                function seeMore(linkTargeted) {
                    linkTargeted.classList.add("active");
                    linkTargeted.getElementsByTagName('P')[0].innerHTML = "Voir moins";
                    linkTargeted.parentElement.getElementsByClassName('linksSDT-container')[0].classList.remove(
                        "list-collapsed");
                    linkTargeted.parentElement.getElementsByClassName('linksSDT-container')[0].classList.add(
                        "list-to-collapse");
                    linkTargeted.parentElement.getElementsByClassName('linksSDT-container')[0].getElementsByClassName(
                        'hp--sdt--item--overflow')[0].style.display = 'flex';
                }

                function hideMore(linkTargeted) {
                    linkTargeted.classList.remove("active");
                    linkTargeted.getElementsByTagName('P')[0].innerHTML = "Voir plus";
                    linkTargeted.parentElement.getElementsByClassName('linksSDT-container')[0].classList.add(
                        "list-collapsed");
                    linkTargeted.parentElement.getElementsByClassName('linksSDT-container')[0].classList.remove(
                        "list-to-collapse");
                    linkTargeted.parentElement.getElementsByClassName('linksSDT-container')[0].getElementsByClassName(
                        'hp--sdt--item--overflow')[0].style.display = 'none';
                }

                allBtnToggle.forEach(button => button.addEventListener('mouseenter', function (event) {
                    seeMore(this)
                }));
                allBtnToggle.forEach(button => button.addEventListener('mouseleave', function (event) {
                    hideMore(this)
                }));
                allBtnClose.forEach(button => button.addEventListener('click', function (event) {
                    button.parentElement.style.display = 'none';
                    var toggle = button.parentElement.parentElement.parentElement.getElementsByClassName(
                        'js-toggle');
                    toggle[0].classList.remove("active");
                    toggle[0].getElementsByTagName('P')[0].innerHTML = "Voir plus";
                }));

                allListOverflow.forEach(list => list.addEventListener('mouseenter', function () {
                    list.style.display = 'flex';
                    var toggle = list.parentElement.parentElement.getElementsByClassName('js-toggle');
                    toggle[0].classList.add("active");
                    toggle[0].getElementsByTagName('P')[0].innerHTML = "Voir moins";
                }));

                allListOverflow.forEach(list => list.addEventListener('mouseleave', function () {
                    list.style.display = 'none';
                    var toggle = list.parentElement.parentElement.getElementsByClassName('js-toggle');
                    toggle[0].classList.remove("active");
                    toggle[0].getElementsByTagName('P')[0].innerHTML = "Voir plus";
                }));

                /*** toggle sous-catégories mode mobile & tab ***/
                var allIconPlus = Array.from(document.querySelectorAll('.hp--sdt--item'));

                function toggleElement(linkTargeted) {
                    if (linkTargeted.getElementsByClassName('linksSDT-container')[0].classList.contains(
                        "list-collapsed")) {
                        linkTargeted.getElementsByClassName('icon-plus')[0].classList.toggle("open");
                        linkTargeted.getElementsByClassName('linksSDT-container')[0].classList.remove("list-collapsed");
                        linkTargeted.getElementsByClassName('linksSDT-container')[0].classList.add("list-to-collapse");
                        linkTargeted.getElementsByClassName('hp--sdt--item--img-tab')[0].style.background = "#f7f7f7";

                    } else {
                        linkTargeted.getElementsByClassName('icon-plus')[0].classList.toggle("open");
                        linkTargeted.getElementsByClassName('linksSDT-container')[0].classList.add("list-collapsed");
                        linkTargeted.getElementsByClassName('linksSDT-container')[0].classList.remove("list-to-collapse");
                        linkTargeted.getElementsByClassName('hp--sdt--item--img-tab')[0].style.background = "none";
                    }
                }

                allIconPlus.forEach(icon => icon.addEventListener('click', function (event) {
                    toggleElement(this)
                }));

            });
        </script>
    

