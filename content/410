<script>
    jQuery(function () {
        init();

        jQuery('.profile_photo_item').click(function () {
            // 获取当前元素得序号
            var index = jQuery(this).index();
            // 如果点击得是当前元素，不进行处理
            if ($(".content_item").eq(index).hasClass("button_adv_active")) {
                return;
            }

            // 更改当前点击元素背景颜色
            jQuery(this).css("background-image", "url(" + "'./Images/profile_photo_" + index + "_color.png')");
            // 之前点亮的元素背景变灰
            var without_color_bg = jQuery(".photo_active").css("background-image").replace("_color", "");
            jQuery(".photo_active").css("background-image", without_color_bg);
            // 使当前元素置位最顶层
            var zIndex = $(jQuery(".profile_photo_list").find(".photo_active")[0]).css("z-index");
            jQuery(this).css("z-index", parseInt(zIndex) + 1);
            jQuery('.profile_photo_item').removeClass("photo_active");
            jQuery(this).addClass("photo_active");

            var $item;

            jQuery(".detail_button").hide();
            jQuery(".detail_button").eq(index).show();

            if (jQuery(".content_item").eq(index).length > 0) {

                $item = jQuery(".content_item").eq(index);
                moveIn($item)
            }

            if (jQuery(".content_item.button_adv_active").length > 0) {
                $pre = jQuery(".content_item.button_adv_active");
                moveOut($pre, $item);
            }
            
            $item.addClass("button_adv_active");

            // 元素的滑动切换效果
           /* jQuery(".content_item").css("dispaly","block").slideUp('normal', 'linear', function () {

            });
            jQuery(".content_item").eq(jQuery(this).index()).slideDown('normal', 'linear', function () {

            });*/
           
        });

        function moveIn($item) {
            move($item);
            $item.css("opacity", 1);
        }

        function moveOut($item, $next) {
            move($item, $next)
            $item.removeClass("button_adv_active");
            $item.css("opacity", 0);
        }

        function move($item, $next) {
            var top = parseInt($item.css("top").replace("px", ""));
            var nextTop = 0;

            if ($next) {
                nextTop = parseInt($next.css("top").replace("px", ""));
            }

            if (top > 0) {
                up($item);
            } else if (top < 0) {
                down($item);
            } else if (nextTop > 0) {
                up($item);
            } else if (nextTop < 0) {
                down($item);
            } else {
                up($item);
            }
        }

        function up($item) {
            var top = parseInt($item.css("top").replace("px", ""));
            $item.css("top", (top - 147) + "px");
        }

        function down($item) {
            var top = parseInt($item.css("top").replace("px", ""));
            $item.css("top", (top + 147) + "px");
        }

        function init() {
            // 获取当前元素得序号
            var index = 0;
            var $item;
            jQuery(".detail_button").hide();
            jQuery(".detail_button").eq(index).show();

            if (jQuery(".content_item").eq(index).length > 0) {

                $item = jQuery(".content_item").eq(index);
                moveIn($item)
            }

            if (jQuery(".content_item.button_adv_active").length > 0) {
                $pre = jQuery(".content_item.button_adv_active");
                moveOut($pre, $item);
            }

            $item.addClass("button_adv_active");
        }
    });
</script>

    
