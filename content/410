<script type="text/javascript" defer="defer">
    jQuery(function () {

        var itemLength = jQuery(".cmb-his-item").length;
        if (itemLength == 0) {
            return;
        }
        jQuery(".cmb-his-ctrl-span-total").text(itemLength);

        var startPosition = {};
        startPosition.x = parseFloat(jQuery(".cmb-his-item").eq(0).css("left"));
        startPosition.y = parseFloat(jQuery(".cmb-his-item").eq(0).css("bottom"));

        var secondP = {};
        secondP.x = 600;
        secondP.y = -111;

        var thirdP = {};
        thirdP.x = 1110;
        thirdP.y = -25;
        var cm = new CircularMotion({ itemDiv: "cmb-his-item", startPosition: startPosition, secondPosition: secondP, thirdPosition:thirdP}, jQuery);
        cm.initListPosition();

        var activeCm = new initActive(cm);

        var duringTime = 300;
        var initCount = 4;

        findContentInContainer();
        hideAllItems();

        var listenDom = AnimateUtils.listenDomToWindow({ domClass: "cmb-his-container" }, jQuery);
        listenDom.open(function () {
            initAllItems();
            listenDom.close();
        });

        var autoExecute = AnimateUtils.autoExecute({
            func: function () {
                jQuery(".cmb-his-right").click();
            },
            delay: 3000
        })


        function hideAllItems() {
            jQuery(".cmb-his-active .cmb-his-item-body").hide();

            var titles = jQuery(".cmb-his-item-title");
            for (var i = 1; i < initCount; i++) {
                titles.eq(i).hide();
            }
        }

        function initAllItems() {
            showItem(jQuery(".cmb-his-active .cmb-his-item-body"));
            var index = 1;
            showItems(index, function () {
                autoExecute.start();
                jQuery(".cmb-his-container").hover(function () {
                    autoExecute.stop();
                }, function () {
                    autoExecute.start();
                })
            })
        }

        function showItem($item) {
            $item.css("opacity", 0);
            $item.css("display", "");
            $item.css("bottom", "-30px");
            $item.animate({ opacity: 1,bottom: "0px" }, duringTime*1.5);
        }

        function showItems(index, func) {
            setTimeout(function () {
                showItem(jQuery(".cmb-his-item-title").eq(index));
                index++;
                if (index >= initCount) {
                    if (func && typeof func == "function") {
                        func();
                    }
                    return;
                }
                showItems(index, func);
            }, duringTime*1.5);
        }


        jQuery(".cmb-his-left").on("click", function () {

            if (activeCm.cm.isMoving || cm.isMoving) {
                return;
            }
            activeCm.minusCurIndex();

            findContentInContainer();
            if (checkContentInContainer(jQuery(".cmb-his-container"), jQuery(".cmb-his-item").eq(activeCm.curIndex))) {
                activeCm.cm.next(function () {
                    showActive();
                }, function () {
                    adjustActive(activeCm)
                });
            } else {
                cm.prev();
                activeCm.cm.prev(function () {
                    
                }, function () {
                   showActive();
                    activeCm.cm.next(null, function () {
                        adjustActive(activeCm)
                    });
                });
            }
            
        })

        jQuery(".cmb-his-right").on("click", function () {
            if (activeCm.cm.isMoving || cm.isMoving) {
                return;
            }
            activeCm.addCurIndex();

            findContentInContainer();

            if (checkContentInContainer(jQuery(".cmb-his-container"), jQuery(".cmb-his-item").eq(activeCm.curIndex))) {
                activeCm.cm.prev(function () {
                    showActive();
                }, function () {
                    adjustActive(activeCm)
                });
                
            } else {
                cm.next();
                activeCm.cm.next(function () {
                    
                }, function () {
                    showActive();
                    activeCm.cm.prev(null,function () {
                        adjustActive(activeCm)
                    });
                });
            }
           
            

        })

        jQuery(".cmb-his-item-body").hover(function () {
            
            jQuery(this).stop(true, true).animate(holdContentInContainer(jQuery(".cmb-his-container"), jQuery(this), {
                width: "500px",
                height: "162px"
            }), duringTime);
           ;
            jQuery(this).find(".cmb-his-item-body-title").stop(true, true).animate({
                top: "200px",
                opacity: 0
            }, duringTime);

            jQuery(this).find(".cmb-his-item-body-img").stop(true, true).animate({
                width: "175px",
                height: "160px"
            }, duringTime);
            jQuery(this).find(".cmb-his-item-body-content").stop(true, true).delay(duringTime / 2).animate({
                left: "208px",
                opacity: 1
            }, duringTime);

            jQuery(this).find(".cmb-his-item-body-img img").stop(true, true).animate({
                left: "-20px"
            }, duringTime);

           
        }, function () {
            jQuery(this).stop(true, true).animate({
                width: "120px",
                height: "175px",
                right: "45px"
            }, duringTime);
            jQuery(this).find(".cmb-his-item-body-title").stop(true, true).animate({
                top: "140px",
                opacity: 1
            }, duringTime);
            jQuery(this).find(".cmb-his-item-body-img").stop(true, true).animate({
                width: "120px",
                height: "84px"
            }, duringTime);
            jQuery(this).find(".cmb-his-item-body-content").stop(true, true).animate({
                left: "0px",
                opacity: 0
            }, duringTime);
            jQuery(this).find(".cmb-his-item-body-img img").stop(true, true).animate({
                left: "0px"
            }, duringTime);
        });


        function initActive(cm) {
            var me = this;
            jQuery(".cmb-his-item").eq(0).addClass("cmb-his-active");
            jQuery(".cmb-his-active-point").css("bottom", offestActive(cm.options.startPosition).y + "px");
            jQuery(".cmb-his-active-point").css("left", offestActive(cm.options.startPosition).x + "px");
            var avtiveCm = new CircularMotion({ itemDiv: "cmb-his-active-point", startPosition: offestActive(cm.options.startPosition), secondPosition: offestActive(cm.options.secondPosition), thirdPosition: offestActive(cm.options.thirdPosition) }, jQuery)


            me.cm = avtiveCm;
            me.curIndex = 0;

            showActive(me.curIndex);

            function offestActive(point) {
                return {
                    x: point.x - 6,
                    y: point.y - 6
                }
            }

            me.offestActive = offestActive;

            me.addCurIndex = function () {


                if (me.curIndex == cm.currentLastIndex) {
                    cm.resetFirstToLast();
                }

                me.curIndex++;
                if (me.curIndex > cm.$item.length - 1) {
                    me.curIndex = 0;
                }
            }

            me.minusCurIndex = function () {

                if (me.curIndex == cm.currentFirstIndex) {
                    cm.resetLastToFirst();
                }

                me.curIndex--;
                if (me.curIndex < 0) {
                    me.curIndex = cm.$item.length - 1;
                }
            }
        }

        function adjustActive(active) {
            var abottom = parseFloat(jQuery(".cmb-his-active-point").css("bottom").replace("px", ""));
            var aleft = parseFloat(jQuery(".cmb-his-active-point").css("left").replace("px", ""));

            var bottom = parseFloat(jQuery(".cmb-his-active").css("bottom").replace("px", ""));
            var left = parseFloat(jQuery(".cmb-his-active").css("left").replace("px", ""));

            var shouldPosition = active.offestActive({ x: left, y: bottom });

            if (shouldPosition.x != aleft) {
                jQuery(".cmb-his-active-point").css("left", shouldPosition.x + "px");
            }

            if (shouldPosition.y != abottom) {
                jQuery(".cmb-his-active-point").css("bottom", shouldPosition.y + "px");
            }

        }

        function showActive(curIndex) {

            
            jQuery(".cmb-his-item.cmb-his-active").addClass("cmb-his-prev");
            

            jQuery(".cmb-his-item.cmb-his-prev .cmb-his-item-body").animate({
                opacity: 0,
                bottom: "-20px"
            }, duringTime * 2)

            jQuery(".cmb-his-item.cmb-his-active").removeClass("cmb-his-active");

            setTimeout(function () {
                jQuery(".cmb-his-item.cmb-his-prev").removeClass("cmb-his-prev");
            }, duringTime * 2)
            
            jQuery(".cmb-his-item").eq((curIndex || curIndex === 0) ? curIndex : activeCm.curIndex).addClass("cmb-his-active");

            jQuery(".cmb-his-item.cmb-his-active .cmb-his-item-body").animate({
                opacity: 1,
                bottom: "0px"
            }, duringTime * 2)

            jQuery(".cmb-his-ctrl-span-cur").text((curIndex || curIndex === 0) ? curIndex + 1 : activeCm.curIndex + 1)
        }
        

        function holdContentInContainer($container, $content, option) {

            var width = $content.width();
            var add = parseFloat(option.width.replace("px", "")) - width;

            var containerBound = $container[0].getBoundingClientRect();
            var contentBound = $content[0].getBoundingClientRect();

            var _option = {};

            if (contentBound.right + add > containerBound.right) {
                var offset = contentBound.right + add - containerBound.right;
                var right = parseFloat($content.css("right").replace("px", ""));
                right += offset;
                _option.right = right + "px";
            }

            return Object.assign(_option, option);

        }

        

        function findContentInContainer() {

            jQuery(".cmb-his-item").each(function () {
                var containerBound = jQuery(".cmb-his-container")[0].getBoundingClientRect();

                if (jQuery(this).hasClass("cmb-his-item-hide")) {
                    jQuery(this).removeClass("cmb-his-item-hide");
                }

                var contentBound = jQuery(this)[0].getBoundingClientRect();


                if ((contentBound.top > containerBound.top && contentBound.bottom < containerBound.bottom)
                    && nearbyItem(jQuery(this))
                ) {
                    jQuery(this).removeClass("cmb-his-item-hide");
                } else {
                    jQuery(this).addClass("cmb-his-item-hide");
                }

            })
        }

        function nearbyItem($this) {
            var curIndex = jQuery(".cmb-his-item.cmb-his-active").index();
            var length = jQuery(".cmb-his-item").length;
            return ((Math.abs(curIndex - $this.index()) < 10) || Math.min($this.index() - curIndex, length - ($this.index() - curIndex)));

        }

        function checkContentInContainer($container, $content) {
            var containerBound = $container[0].getBoundingClientRect();
            var contentBound = $content[0].getBoundingClientRect();


            if (contentBound.top > containerBound.top
                && contentBound.bottom < containerBound.bottom
                && contentBound.left > containerBound.left
                && contentBound.right < containerBound.right
            ) {
                return true;
            } else {
                return false;
            }
        }


    })
</script>
