                        <h4><a href="/personal/allinonecard/">一卡通</a></h4>
                    </li>
                    

<div id="subHeader-Allinonecard">

</div>


<script type="text/javascript">

    var webName = 'Allinonecard';
    var linkUrl = '/personal/allinonecard/CardInfo.aspx';
    var itemcount = '0';
    var stringcount = '0';
    var parenturl = '0';
    var notequals = '';
    linkUrl = btoa(linkUrl);

    if (webName) {

        (function (webName, linkUrl, itemcount, stringcount, notequals) {
            var url = "/SrvAjax/CmsWebApi.ashx?type=subheader&web=" + webName + "&linkUrl=" + linkUrl + "&itemcount=" + itemcount + "&stringcount=" + stringcount + "&parenturl=" + parenturl;
            jQuery(function () {
                jQuery.ajax({
                    type: "GET",
                    contentType: "application/json;charset=UTF-8",
                    url: url,
                    success: function (res) {

                        var result = JSON.parse(res);
                        

                        if (result.errCode === "0") {
                            var html = "";
                            for (var i = 0; i < result.data.length; i++) {
                                if (notequals === '' || result.data[i].Name.indexOf(notequals) == -1) {
                                    html += "<li><p><a href='" + result.data[i].LinkUrl + "' target='" + result.data[i].Target + "'>" + result.data[i].Title + "</a></p></li>"
                                } 
                                
                               
                                
                            }
                            jQuery("#subHeader-" + webName).html(html);
                        }

                    }

                })
            })
        })(webName, linkUrl, itemcount, stringcount, notequals)

        
    }

    

</script>
                </ul>
            </div>
            <div class="menu_column">
                <ul>
                    <li>
