  pdpush('pageview');
</script>




  <script>
    // home page more articles /
    var start_feed = 1;
    var limit_feed = 6;
    $('.PartialCategoryArticleTiles-load-more').on('click', function (e) {
      e.preventDefault();
      $.getJSON('/homefeed/' + start_feed + '/' + limit_feed + '/feed.json', function (data) {
        start_feed += limit_feed;
        data.forEach(function (e) {
          var ht = `

<div>
                
<a href='` + e.url + `' class="PartialCategoryArticleTiles-tile">

<img src="` + e.thumb + `" />
<div class="PartialCategoryArticleTiles-textwrap">
<!--<div class="PartialCategoryArticleTiles-category"></div>-->
<div class="PartialCategoryArticleTiles-title">` + e.post_title + `</div>
</div>

</a>
                
</div>
      `;

          $('.PartialCategoryArticleTiles-categories').append(ht);
        });
      });
    });
  </script>

</html>