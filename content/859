    <script type="text/javascript">
        // Load More
        $('#btn-load-more-wp').click(function(){
            var element = this;
            $(this).hide();
            // $(this).parent().append('<div id="load-more-loading" style="text-align:center;z-index:1000;position:relative"><img src="https://asset.kompas.com/data/2017/wp/images/loading.gif"></div>');
            $.ajax({
                url:$(this).attr('data-url'),
                type:'post',
                data:{xmlpath:$(this).attr('data-xmlpath')},
                dataType:'json',
                success:function(str){
                    $($(element).attr('data-append')).append(str['result']);
                    $('#load-more-loading').remove();
                }
            });
        });
        $(document).ready(function() {
            $(window).scroll(function() {
                if ($("#btn-load-more-wp").length > 0 && ($('#btn-load-more-wp').is(':visible'))) {
                    if ($(window).scrollTop() >= ($(document).height() - $(window).height())*0.5) {
                        $("#btn-load-more-wp").trigger('click');
                        return false;
                    }
                }
            });
        });
    </script>
    <script>
    var observer_kilas = new IntersectionObserver(function(entires){
        entires.forEach(function(entry) {
            if(entry.intersectionRatio > 0){
                show_embed_kilas(entry.target)                
            }
        });
    }, {
        threshold: 0
    });        

    observer_kilas.observe(document.getElementById("kilas-inject"));
    function show_embed_kilas(element){
        console.log('show_embed_kilas');
        observer_kilas.unobserve(element);
        observer_kilas.disconnect();        

        var kilasxhttp = new XMLHttpRequest();
        kilasxhttp.onreadystatechange = function() {
            if (this.readyState == 4 && this.status == 200) {
                var kilasres = JSON.parse(kilasxhttp.responseText);
                document.getElementById("kilas-inject").innerHTML = kilasres.data;
            }
        };
        kilasxhttp.open("GET", "https://www.kompas.com/home/kilas", true);
        kilasxhttp.send();
    }
</script>    <script type="text/javascript">

    var observer = new IntersectionObserver(function(entires){
        entires.forEach(function(entry) {
            var visiblePct = Math.floor(entry.intersectionRatio * 100);
            if(visiblePct == 100){
                show_embed_youtube(entry.target, entry.target.getAttribute("data-id"));
            }
        });
    }, {
        threshold: 1.0
    });        

    var video_inject = document.getElementsByClassName('video-inject');
    for(var i = 0; i < video_inject.length; i++)
    {
        observer.observe(video_inject[i]);
    }
    
    function show_embed_youtube(element, i){
        console.log('video inject')
        observer.unobserve(element);
        if(element.innerHTML == ''){
            var video_json = '[]';
            var video_embed = '';
            var video_src = JSON.parse(video_json);
            video_embed = '<div class="latest ga--video mt2 clearfix">'
            video_embed += '<div class="article__list article__list--video clearfix">'
            video_embed += '<div class="article__list__asset clearfix">'
            video_embed += '<iframe width="100%" height="320" src="'+video_src[i].src+'" scrolling="no" frameborder="0" allowtransparency="true"></iframe>'
            video_embed += '</div>'
            video_embed += '<div class="article__list__title">'
            video_embed += '<h3 class="article__title article__title--medium"><a class="article__link" target="_blank" href="'+video_src[i].url+'">'+video_src[i].title+'</a></h3>'
            video_embed += '</div>'
            video_embed += '<div class="article__list__info">'
            video_embed += '<div class="article__time">'
            video_embed += '<svg class="icon icon-time" title="icon-time">'
            video_embed += '<use xlink:href="#icon-time"/>'
            video_embed += '</svg>'+video_src[i].time
            video_embed += '</div>'
            video_embed += '<div class="article__date">'+video_src[i].date+'</div>'
            video_embed += '</div>'
            video_embed += '<div class="clearfix"></div>'
            video_embed += '</div>'
            video_embed += '</div>'
            element.innerHTML = video_embed;
        }
    }
</script>
