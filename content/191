    <script type="text/javascript" charset="utf-8">
        showSlideData(function () {
            $('#slideWrap').slide({ fade: true});
            jOperation.sendRequest('#slideWrap .athm-slide__item[data-operation-extend]');
            jOperation.sendVisibleExposure('#slideWrap .athm-slide__item', 0);
            $("#slideWrap").on('slid.fe.slide', function (e, slide) {
                var _a = slide.$item.eq(slide.number - 1);
                if (typeof (_a.attr('data-operation-extend')) != 'undefined') {
                    jOperation.sendVisibleExposure('#slideWrap .athm-slide__item', slide.number - 1);
                }
            });
            //article
            jOperation.sendRequest('#newTabOne_slide .athm-slide__item[data-operation-extend]');
            jOperation.sendVisibleExposure('#newTabOne_slide .athm-slide__item', 0);
            $("#newTabOne_slide").on('slid.fe.slide', function (e, slide) {
                var _a = slide.$item.eq(slide.number - 1);
                if (typeof (_a.attr('data-operation-extend')) != 'undefined') {
                    jOperation.sendVisibleExposure('#newTabOne_slide .athm-slide__item', slide.number - 1);
                }
            });
        });
    </script>
