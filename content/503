    if (!isLoggedin && ((document.domain.indexOf("thailand") == -1 && document.domain.endsWith(".intel.com")) || document.getElementById("recode50header") != null) && (navigator != null && navigator.userAgent.indexOf("CloudManagerTest") == -1)) {
        try {
            var xmlhttp = new XMLHttpRequest();
            xmlhttp.open('HEAD','https://sfederation.intel.com/federation/status.asp', false);
            xmlhttp.withCredentials="true";
            xmlhttp.send();
            if (200 == xmlhttp.status) {
                var currentUrl = window.location.href;
                var cookiename = "IGHFtarget";
                var samlcookieValue = cookiename + "=" + encodeURIComponent(currentUrl) + ";secure;path=/;domain=.intel.com";
