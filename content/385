    })
    $('.rank-list ul > li > .title p').on('mouseenter mouseleave', function () {
        $(this).addClass('bright').siblings().removeClass('bright')
    })
    // 产品报价
    $('.content .fr .chanpin.show>li:not(:first-child)').hover(function () {
        $(this).addClass('details').siblings().removeClass('details')
        $(this).find('.details').removeClass('details')
    }, function () {
        $('.content .fr .chanpin>li').removeClass('details')
    })
    
  $('.content .fr .chanpin:nth-of-type(2) > li:not(:first-child)').hover(
        function(){
            $(this).find('.img-txt').show()
            $(this).find('.txt').hide()
            $(this).siblings().find('.img-txt').hide()
            $(this).siblings().find('.txt').css('display','block')
            if($(this).find('p').height()>=35){
                $(this).find('span').hide()
                $(this).find('.my-time').show()
            }else{
                $(this).find('span').show()
                $(this).find('.my-time').hide()
            }
        }
    )

    function aclick(obj){
        $.each($(obj),function(index,val){
      val.index = index;
      $(val).hover(function(){
          if(this.index == 0){
                $(obj).parent('div').siblings('ol').eq(0).show()
                $(obj).parent('div').siblings('ol').eq(1).hide()
            }else{
                $(obj).parent('div').siblings('ol').eq(0).hide()
                $(obj).parent('div').siblings('ol').eq(1).show()
            }
        })
    })
    }
    aclick("#list_1 .title")
    aclick("#list_2 .title")
    aclick("#list_3 .title")
    aclick("#list_4 .title")
    aclick("#list_5 .title")
    $('.search form i').on('click',function(){
        $('.search form').submit()
    })
</script>
